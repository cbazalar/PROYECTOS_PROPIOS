	          AND adi.clie_oid_clie= mc.oid_clie
	          <isNotEmpty property="listaParametros">	
				  <iterate prepend="AND" property="listaParametros" open="(" close=")" conjunction="OR">
      	 		adi.esta_oid_esta_clie= #listaParametros[]#
            </iterate>  
			</isNotEmpty>  
			
				      
     
     </select>
     

    <update id="updateOCCDataCrediticiaConsultora" parameterClass="java.util.Map">
    	update int_solic_conso_cabec
		   set VAL_NDOC_IDEN_INFO_COME = #num_Documento#,
		       VAL_NOMB_INFO_COME      = #nombre#,
		       VAL_RESU_INFO_COME      = #resultado#,
		       VAL_ESTA_INFO_COME      = #estado#,
		       IND_SITU_CRED           = #indicador_cred#
		 where cod_clie = #cod_clie#
    </update>    
   	

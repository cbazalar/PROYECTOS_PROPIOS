      <!-- RECHAZO DE DOCUMENTO-->
	<update id="updateRechazoOCC" parameterClass="java.util.Map">
		UPDATE INT_SOLIC_CONSO_CABEC C
			   SET C.IND_ERRO_RECH = '1',
			       C.COD_MOTI_RECH = #gestionDocumento.motivoRechazo#			   
			 WHERE EXISTS (SELECT  1 
			                 FROM STO_DOCUM_DIGIT  D
			                WHERE D.SEC_NUME_DOCU = C.SEC_NUME_DOCU 
			                  AND D.NUM_LOTE = C.NUM_LOTE
			                  AND D.NUM_PROC = #historico.numeroProceso#
			                 )
  	</update>
  	<update id="executeRecoverRejectOCC" parameterClass="java.util.Map">
		UPDATE INT_SOLIC_CONSO_CABEC C
			   SET C.IND_ERRO_RECH = '0'
			 WHERE EXISTS (SELECT  1 
			                 FROM STO_DOCUM_DIGIT  D
			                WHERE D.SEC_NUME_DOCU = C.SEC_NUME_DOCU 
			                  AND D.NUM_LOTE = C.NUM_LOTE
			                  AND D.NUM_PROC = #historico.numeroProceso#
			                 )
  	</update>
  	
  	 <!-- APROBAR DESAPROBAR-->
  	<update id="executeOCCAprobarDeuda" parameterClass="java.util.Map">
	      UPDATE INT_SOLIC_CONSO_CABEC T
	         SET T.IND_ADMI_CART = '1',	         
	             T.USU_ADMI_CART = #usuario.login#,
	             T.FEC_ADMI_CART = SYSDATE
	       WHERE T.NUM_LOTE = #gestionDocumento.lote#
	         AND T.SEC_NUME_DOCU = #gestionDocumento.numeroDocumento#
	         AND EXISTS (SELECT 1
                            FROM STO_DOCUM_DIGIT D
                           WHERE D.IND_ENVI = '0'
                             AND D.IND_RECH = '0'
                             AND D.COD_PAIS = T.COD_PAIS
                             AND D.COD_TIPO_DOCU = #gestionDocumento.documento#
                             AND D.NUM_LOTE = T.NUM_LOTE
                             AND D.SEC_NUME_DOCU = T.SEC_NUME_DOCU)
  </update>
  <update id="executeOCCDesaprobarDeuda" parameterClass="java.util.Map">
	  UPDATE INT_SOLIC_CONSO_CABEC T
	     SET T.IND_ADMI_CART = '0',	         
	         T.USU_ADMI_CART = #usuario.login#,
	         T.FEC_ADMI_CART = SYSDATE
	   WHERE T.NUM_LOTE = #gestionDocumento.lote#
	     AND T.SEC_NUME_DOCU = #gestionDocumento.numeroDocumento#
	     AND EXISTS (SELECT 1
                            FROM STO_DOCUM_DIGIT D
                           WHERE D.IND_ENVI = '0'
                             AND D.IND_RECH = '0'
                             AND D.COD_PAIS = T.COD_PAIS
                             AND D.COD_TIPO_DOCU = #gestionDocumento.documento#
                             AND D.NUM_LOTE = T.NUM_LOTE
                             AND D.SEC_NUME_DOCU = T.SEC_NUME_DOCU)
  </update>
	

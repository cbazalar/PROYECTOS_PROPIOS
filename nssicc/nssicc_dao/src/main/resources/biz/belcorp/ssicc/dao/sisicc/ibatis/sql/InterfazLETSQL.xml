  <delete id="deleteInterfazLETRecepcionarCursos" parameterClass="java.util.Map">					
       DELETE FROM LEC_PROGR_CURSO	    										                						
  </delete>
  
  <insert id="insertInterfazLETRecepcionarCursos" parameterClass="java.util.Map">					
       INSERT INTO LEC_PROGR_CURSO
         (CAM_REGI,
          LNIV_COD_NIVE,
          COD_CONS_LIDE,
          FEC_APRO,
          IND_APRO,
          IND_ACTI,
          USU_CREA,
          FEC_CREA)
       VALUES
         (#campanaCurso#,
          #nivelLider#,
          #codigoLider#,
          TO_DATE(#fechaAprobacion#,'YYYYMMDD'),
          'A',
          '1',
          #codigoUsuario#,
          SYSDATE
         )   	    										                						
  </insert>
      

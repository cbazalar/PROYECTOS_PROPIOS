   <update id="updateInformacionInfoCom" parameterClass="java.util.Map">
   		UPDATE INT_SOLIC_CONSO_CREDI
   		SET 
   			<isNotEmpty property="montoInfoCom">
   			  	VAL_MONT_INFO_COME =to_number(#montoInfoCom#,'9999999999.99'), 
   			</isNotEmpty>   		    
   				VAL_ESTA_INFO_COME =#estado#,	   		
   				IND_ERRO_INFO_COME =#error# ,   					
   				VAL_NOMB_INFO_COME = substr(#nombresInfocorp#,0,400),
   				VAL_EXPL_INFO_COME = substr(#explicacionInfocorp#,0,1000),  
   				IND_TELE_OK   = nvl(#indTeleOk#,0),			
   				IND_SITU_CRED = nvl(#indSituCred#,0),
   				IND_SIN_SALD_AMBA_MARC = nvl(#indAmbaMarc#,0)
   		WHERE NUM_LOTE = #numLote#
   		 AND NUM_DOCU_IDEN =#numeroDocumentoIdentidad#
   </update>
  


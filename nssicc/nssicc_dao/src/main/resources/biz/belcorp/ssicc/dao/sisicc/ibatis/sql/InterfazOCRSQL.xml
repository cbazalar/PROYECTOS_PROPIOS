  	 <select id="getSTOParametroGeneralOCR" parameterClass="java.util.Map" resultClass="string">
	   SELECT VAL_PARAM FROM STO_PARAM_GENER_OCCRR A
	   WHERE A.COD_PAIS = #codigoPais#
         AND A.COD_PARA = #codigoParametro#
     </select>

     <delete id="deleteOCRCUVErradosScaneo" parameterClass="java.util.Map">
        DELETE FROM OCR_CVENT_ERRAD A
	    WHERE A.COD_PAIS = #codigoPais#
          AND A.COD_PERI = #codigoPeriodo#
          AND A.FEC_PROC = to_date(#fechaArchivoFacturacion#,'dd/mm/yyyy')
	 </delete>

	<insert id="insertOCRCUVErradosScaneo" parameterClass="java.util.Map">
        INSERT INTO OCR_CVENT_ERRAD(
			COD_PAIS,
			COD_PERI,
			FEC_PROC,
			COD_VENT,
			COD_CLIE,
			NUM_PREI,
			NUM_INDI,
			NUM_CANT,
			USU_DIGI,
			FEC_DIGI
		)
        VALUES (
		#codigoPais#,
		#codigoPeriodo#,
		to_date(#fechaArchivoFacturacion#,'dd/mm/yyyy'),
		TRIM(#codigoProducto#), 
		TRIM(#codigoConsultora#),
		TRIM(#numPreImpreso#),
		#indiceProducto#,
		#cantidad#,
		#codigoUsuario#, 
        SYSDATE
        )
	</insert>


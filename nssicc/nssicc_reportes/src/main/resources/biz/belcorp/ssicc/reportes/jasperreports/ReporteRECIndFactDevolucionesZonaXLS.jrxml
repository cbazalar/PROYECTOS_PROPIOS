	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPeriodoInicio" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicio")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFin" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFin")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select per.VAL_NOMB_PERI PERIODO,
       f.cod_ZONA ZONA,
       op.cod_oper OPERACION,
       top.val_tipo_oper TIPO_OPERACION,
       sum(y.NUM_UNID_ATEN)*(-1) UNIDADES_DEVUELTAS,
       --     sum(y.VAL_PREC_NETO_TOTA_LOCA) MONTO
       sum(decode(y.VAL_PREC_CATA_UNIT_LOCA,
                  0,
                  0,
                  y.VAL_PREC_NETO_TOTA_LOCA)*(-1)) MONTO_NETO
  from ped_solic_cabec    x,
       ped_solic_posic    y,
       mae_clien          a,
       zon_terri_admin    c,
       zon_terri          d,
       zon_secci          e,
       zon_zona           f,
       zon_regio          g,
       mae_produ          s,
       gen_i18n_sicc_pais v,
       ped_solic_cabec    cons,
       cra_perio          per,
       rec_opera_recla    ro,
       rec_solic_opera    sop,
       rec_tipos_opera    top,
       rec_opera          op
 where x.CLIE_OID_CLIE = a.OID_CLIE
   and x.ZTAD_OID_TERR_ADMI = c.OID_TERR_ADMI
   and c.ZSCC_OID_SECC = e.OID_SECC
   and c.TERR_OID_TERR = d.OID_TERR
   and e.ZZON_OID_ZONA = f.OID_ZONA
   and f.ZORG_OID_REGI = g.OID_REGI
   and x.PERD_OID_PERI <= GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFin})
   and x.PERD_OID_PERI >= GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoInicio})
   AND y.VAL_CODI_VENT IS NOT NULL
   and x.PERD_OID_PERI = per.oid_peri
   and cons.TSPA_OID_TIPO_SOLI_PAIS in (
		REC_PKG_REPOR.GEN_FN_DEVUE_OID_TSOLI_PAIS('C57'),
		REC_PKG_REPOR.GEN_FN_DEVUE_OID_TSOLI_PAIS('C56'),
		REC_PKG_REPOR.GEN_FN_DEVUE_OID_TSOLI_PAIS('C55')
		) 
   -- REEMPLAZAR POR FUNCIÓN CÓDIGO ES 'C57' 'C56' 'C55' 
   and cons.oid_soli_cabe = x.soca_oid_soli_cabe
   and cons.CLIE_OID_CLIE = a.OID_CLIE
   and y.PROD_OID_PROD = s.OID_PROD
   and s.OID_PROD = v.VAL_OID
   and x.OID_SOLI_CABE = y.SOCA_OID_SOLI_CABE
   and v.ATTR_ENTI = 'MAE_PRODU'
   and y.ESPO_OID_ESTA_POSI <> 2
   AND x.MODU_OID_MODU = 15
   and x.oid_soli_cabe = sop.soca_oid_soli_cabe
   and sop.opre_oid_oper_recl = ro.oid_oper_recl
   and ro.TIOP_OID_TIPO_OPER = top.oid_tipo_oper
   and op.IND_ANUL <> 1
   and top.rope_oid_oper = op.oid_oper
 GROUP BY per.VAL_NOMB_PERI, f.cod_ZONA, op.cod_oper, top.val_tipo_oper]]></queryString>


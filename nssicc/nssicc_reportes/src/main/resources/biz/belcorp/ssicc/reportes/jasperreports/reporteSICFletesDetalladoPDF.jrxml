	<queryString><![CDATA[select spc.cod_peri PERIODO, g.DES_REGI REGION, 
       f.DES_ZONA ZONA,
       gen_pkg_gener.GEN_FN_DEVUELVE_COD_CLIE(a.Clie_Oid_Clie) CODIGO, 
       gen_pkg_gener.GEN_FN_CLIEN_DATOS_OID(a.Clie_Oid_Clie, 'NOM_CLIE') AS nombre,
       a.VAL_TOTA_PAGA_LOCA-a.VAL_IMPO_FLET_TOTA_LOCA MONTO_VENTA,
	   a.VAL_IMPO_FLET_LOCA FLETE,
       a.VAL_RECA_FLET_LOCA RECARGO,
       a.VAL_IMPO_FLET_TOTA_LOCA FLETE_TOTAL,
       TO_CHAR(a.fec_fact,'DD/MM/YYYY') FEC_FACT
from ped_solic_cabec a,     
     zon_terri_admin d, 
     zon_secci h, 
     zon_zona f, 
     zon_regio g,
	 ped_estad_solic pes,
	 cra_perio cp,
	 seg_perio_corpo spc
where a.ZTAD_OID_TERR_ADMI=d.OID_TERR_ADMI 
	  AND ((NVL (pes.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
	  AND pes.oid_esta_soli(+) = a.esso_oid_esta_soli
	  and a.perd_oid_peri=cp.oid_peri
      and cp.peri_oid_peri=spc.oid_peri
      and d.ZSCC_OID_SECC=h.OID_SECC 
      and h.ZZON_OID_ZONA=f.OID_ZONA 
      and f.ZORG_OID_REGI=g.OID_REGI
      and a.TSPA_OID_TIPO_SOLI_PAIS= int_pkg_recla.gen_fn_devue_oid_tipo_solpa('SOC')
      and a.FEC_FACT is not null 
	  and spc.cod_peri=$P{codigoPeriodo}
      and a.VAL_IMPO_FLET_TOTA_LOCA>0
      AND ($P{fechaFacturacion} IS NULL OR a.fec_fact = TO_DATE($P{fechaFacturacion}, 'DD/MM/YYYY'))
      $P!{condicion}
order by 1,2]]></queryString>


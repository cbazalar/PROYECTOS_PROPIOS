	<queryString><![CDATA[SELECT COD_OPER,
       REC_REPOR_OPERA_UNIDA_ADMIN.DES_REGI,
       REC_REPOR_OPERA_UNIDA_ADMIN.des_zona as DES_ZONA,
       VAL_DESC_LARG,
       VAL_TIPO_OPER,
       CRA_PERIO.VAL_NOMB_PERI,
       SUM(NUM_GUIA) AS ATEN_RECL,
       SUM(NUM_UNID_RECL) AS CANT_DEVU,
       SUM(IMP_ABON) AS MONT_DEVU,
       REC_REPOR_OPERA_UNIDA_ADMIN.VAL_TIPO_PEDI
FROM   REC_REPOR_OPERA_UNIDA_ADMIN, CRA_PERIO
WHERE CRA_PERIO.OID_PERI= REC_REPOR_OPERA_UNIDA_ADMIN.PERD_OID_PERI
AND REC_REPOR_OPERA_UNIDA_ADMIN.NUM_SECU = $P{nextValue}	
GROUP BY COD_OPER,
         VAL_DESC_LARG,
         VAL_TIPO_OPER,
         CRA_PERIO.VAL_NOMB_PERI,   
         REC_REPOR_OPERA_UNIDA_ADMIN.DES_REGI ,      
         REC_REPOR_OPERA_UNIDA_ADMIN.DES_ZONA,
         REC_REPOR_OPERA_UNIDA_ADMIN.VAL_TIPO_PEDI
order by REC_REPOR_OPERA_UNIDA_ADMIN.DES_REGI ,      
         REC_REPOR_OPERA_UNIDA_ADMIN.DES_ZONA,
         COD_OPER]]></queryString>


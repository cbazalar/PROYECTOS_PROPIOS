	<queryString><![CDATA[SELECT INC_CONCU_PARAM_GENER.VAL_NOMB,
       INC_CONCU_PARAM_GENER.NUM_CONC,
       INC_REQUI_PREMI.VAL_MONT_MINI_CONC,
       INC_REQUI_PREMI.OID_REQU_PREM,
       INC_REQUI_PREMI.NUM_PEDI,
       INC_REQUI_PREMI.VAL_CUOT_INGR,
       INC_REQUI_PREMI.IND_PEDI_PERI,
       INC_REQUI_PREMI.VAL_MONT_MINI_PEDI,
       INC_REQUI_PREMI.IND_PAGO_TIEM,
       INC_REQUI_PREMI.NUM_DIAS_GRAC,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(INC_REQUI_PREMI.PERD_OID_PERI_DESD,'CRA_PERIO','es') as des_peri_desd,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(INC_REQUI_PREMI.PERD_OID_PERI,'CRA_PERIO','es') as des_peri,       
       INC_REQUI_PREMI.IND_ASIS_CURS ,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(INC_CURSO_EXIGI_PREMI.TICU_OID_TIPO_CURS,'EDU_TIPO_CURSO','es') as des_tipo_curso,   
       (SELECT COUNT(*)
        FROM inc_produ_exigi_premi       
         WHERE  inc_produ_exigi_premi.repr_oid_requ_prem = INC_REQUI_PREMI.OID_REQU_PREM) as tot_prod_exig
 FROM  inc_concu_param_gener,
       inc_requi_premi,       
       inc_curso_exigi_premi
     
 WHERE  inc_concu_param_gener.oid_para_gral = inc_requi_premi.copa_oid_para_gral       
        AND inc_requi_premi.oid_requ_prem = inc_curso_exigi_premi.repr_oid_requ_prem (+)     
     AND inc_concu_param_gener.pais_oid_pais = gen_pkg_gener.gen_fn_devuelve_id_pais($P{codigoPais})
   AND inc_concu_param_gener.marc_oid_marc = DECODE ($P{codigoMarca},'',inc_concu_param_gener.marc_oid_marc,gen_pkg_gener.gen_fn_devuelve_id_marca($P{codigoMarca}) )
   AND inc_concu_param_gener.cana_oid_cana = DECODE ($P{codigoCanal},'',inc_concu_param_gener.cana_oid_cana,gen_pkg_gener.gen_fn_devuelve_id_canal($P{codigoCanal}))		
   AND INC_CONCU_PARAM_GENER.Num_Conc = DECODE ($P{codigoConcurso}, '', INC_CONCU_PARAM_GENER.Num_Conc,$P{codigoConcurso})]]></queryString>


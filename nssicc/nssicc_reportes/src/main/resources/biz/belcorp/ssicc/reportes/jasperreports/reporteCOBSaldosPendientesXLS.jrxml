	<queryString><![CDATA[WITH TEMP1 AS
( SELECT 
       spc.cod_peri,
       mcc.perd_oid_peri,
       mcc.clie_oid_clie , 
       MAX(mcc.num_iden_cuot) Boleta,
       MIN(mcc.fec_docu) Fecha,
       SUM(mcc.imp_movi) Facturado,
       SUM(mcc.imp_pend) Saldos  ,
       MAX(mcc.fec_venc) Fec_Venc   
   FROM 
    ccc_movim_cuent_corri mcc,
    cra_perio cp,
    seg_perio_corpo spc
   WHERE mcc.perd_oid_peri = cp.oid_peri
     AND cp.peri_oid_peri = spc.oid_peri
     AND mcc.IMP_pend > 0
   AND mcc.perd_oid_peri >= $P{oidPeriodoInicial}
   AND mcc.perd_oid_peri <= $P{oidPeriodoFinal}
   HAVING SUM(mcc.imp_pend)>0       
   GROUP BY spc.cod_peri,mcc.perd_oid_peri,mcc.clie_oid_clie)
 SELECT 
  s.cod_peri Camp,
  zr.cod_regi Region,       
  zz.cod_zona Zona,
  zs.cod_secc Secc,      
  zt.cod_terr Terr, 
  mc.cod_clie Codigo_Cli, 
  mc.cod_digi_ctrl cod_digi_ctrl,      
  NVL(TRIM(mc.val_nom1),' ') || ' ' || NVL(TRIM(mc.val_nom2),' ') Nombres,   
  NVL(TRIM(mc.val_ape1),' ') Ap_Pate,
  NVL(TRIM(mc.val_ape2),' ') Ap_Mate,       
  mci.num_docu_iden DNI,              
   (SELECT zon_valor_estru_geopo.des_geog
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 IS NULL
       AND zon_valor_estru_geopo.orde_3 IS NULL
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1)  AS Dpto,
   (SELECT zon_valor_estru_geopo.des_geog 
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
       AND zon_valor_estru_geopo.orde_3 IS NULL
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1) AS Provincia,  
  (SELECT zon_valor_estru_geopo.des_geog
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
       AND zon_valor_estru_geopo.orde_3 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,13,6)))
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1) AS Distrito,
      DECODE(TRIM(stv.des_abrv_tipo_via),NULL,'',stv.des_abrv_tipo_via || ' ' ) ||
      DECODE(TRIM(mcd.val_nomb_via),NULL,'',mcd.val_nomb_via || ' ' ) ||
      DECODE(TRIM(mcd.num_ppal),NULL,'',mcd.num_ppal || ' ' ) ||
      DECODE(TRIM(mcd.val_inte),NULL,'',mcd.val_inte || ' ' ) ||
      DECODE(TRIM(mcd.val_manz),NULL,'',mcd.val_manz || ' ' ) ||
      DECODE(TRIM(mcd.val_lote),NULL,'',mcd.val_lote || ' ' ) ||
      DECODE(TRIM(mcd.val_Km),NULL,'',mcd.val_km || ' ' ) || 
      DECODE(TRIM(mcd.val_obse),NULL,'',mcd.val_obse) AS Direccion,
  tf.val_text_comu AS telf,
  tm.val_text_comu AS CEL,  
  mce.camp_ingr CAM_INGR,            
 (SELECT zon_valor_estru_geopo.des_geog
  FROM zon_valor_estru_geopo
  WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
    AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
    AND zon_valor_estru_geopo.orde_3 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,13,6)))
    AND zon_valor_estru_geopo.orde_4 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,19,6)))
    AND ROWNUM = 1) urba,                                                        
  S.Boleta,
  S.FECHA,       
  S.FACTURADO,
  S.Saldos  ,
  S.FEC_VENC,
  em.val_text_comu AS VAL_EMAI,
  TO_CHAR(MDA.FEC_NACI, 'dd/mm/yyyy') FEC_NACI,
  gen_pkg_gener.gen_fn_devue_pedid_orig (mc.cod_clie, s.cod_peri) ORI_PEDI,
  gen_pkg_gener.gen_fn_devue_prome_pagos_ultim(mc.oid_clie,$P{codigoPeriodo})DIAS_PAGOS             
 FROM    
  TEMP1 S,  
  mae_clien mc,
  mae_clien_datos_adici mda,
  mae_clien_ident mci,
  mae_clien_unida_admin mcua,
  mae_clien_direc mcd,
  seg_tipo_via stv,
  mae_clien_estat mce,
  zon_terri_admin zta,
  zon_terri zt,
  zon_secci zs,
  zon_zona zz,
  zon_regio zr,
 (SELECT mtf.clie_oid_clie, mtf.val_text_comu
  FROM mae_clien_comun mtf
  WHERE  mtf.ticm_oid_tipo_comu = 1) tf,
 (SELECT mtt.clie_oid_clie, mtt.val_text_comu
  FROM mae_clien_comun mtt         
  WHERE mtt.ticm_oid_tipo_comu = 7) tt,
 (SELECT mtm.clie_oid_clie, mtm.val_text_comu
  FROM mae_clien_comun mtm            
  WHERE mtm.ticm_oid_tipo_comu = 6) tm,
  (SELECT tf.clie_oid_clie, tf.val_text_comu
  FROM mae_clien_comun tf             
  WHERE tf.ticm_oid_tipo_comu = 3) em
 WHERE MC.OID_CLIE = S.CLIE_OID_CLIE  
   AND MC.OID_CLIE = MCI.CLIE_OID_CLIE
   AND MCI.VAL_IDEN_DOCU_PRIN = 1
   AND  mc.oid_clie = mda.clie_oid_clie 
   AND mc.oid_clie = mcd.clie_oid_clie
   AND mcd.tivi_oid_tipo_via = stv.oid_tipo_via 
   AND mc.oid_clie = mcua.clie_oid_clie
   and mcua.ind_acti = 1
   AND mcd.ind_dire_ppal = 1
   AND mcd.ind_elim = 0
   AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
   AND zta.terr_oid_terr = zt.oid_terr
   AND zta.zscc_oid_secc = zs.oid_secc
   AND zs.zzon_oid_zona = zz.oid_zona
   AND zz.zorg_oid_regi = zr.oid_regi
   AND mc.oid_clie = mce.oid_clie(+)
   AND mc.oid_clie = tf.clie_oid_clie(+)    
   AND mc.oid_clie = tt.clie_oid_clie(+)
   AND mc.oid_clie = tm.clie_oid_clie(+) 
   AND mc.oid_clie = em.clie_oid_clie(+)   
   $P!{condicion}]]></queryString>


	</parameter>
	<queryString><![CDATA[select clie.cod_clie AUTOGENERADO,
						       clid.num_docu_iden NUMEROCEDULA,
						       ( select clid2.num_docu_iden
						           from mae_clien_ident clid2, mae_tipo_docum tdoc
						          where clid2.clie_oid_clie = clid.clie_oid_clie and clid2.tdoc_oid_tipo_docu=tdoc.oid_tipo_docu
						            and tdoc.val_sigl = 'RUT'
						            and clid2.num_docu_iden is not null ) NUMERORUT,
						       clda.cod_prov CODIGOPROVEEDOR
						  from zon_histo_geren       gere,
						       mae_clien             clie,
						       mae_clien_ident       clid,
						       mae_clien_datos_adici clda
						 where 1 = 1
						   and gere.gere = clie.cod_clie
						   and clie.oid_clie = clid.clie_oid_clie
						   and clie.oid_clie = clda.clie_oid_clie
						   and gere.perd_oid_peri_desd = ( select oid_peri from cra_perio x where x.val_nomb_peri like '%'||$P{codigoPeriodo}||'%' )
						   and clid.val_iden_docu_prin = 1
						   and clda.cod_prov is null
						   and gere.cod_regi is not null
						   and gere.cod_zona is not null
						   and gere.cod_secc is not null]]></queryString>


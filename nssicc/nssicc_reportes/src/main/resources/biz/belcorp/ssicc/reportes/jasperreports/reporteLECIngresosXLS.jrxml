<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="reporteLECIngresosXLS"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Landscape"
		 pageWidth="1543"
		 pageHeight="778"
		 columnWidth="1543"
		 columnSpacing="0"
		 leftMargin="0"
		 rightMargin="0"
		 topMargin="0"
		 bottomMargin="0"
		 whenNoDataType="AllSectionsNoDetail"
		 isTitleNewPage="false"
		 isSummaryNewPage="false"
		 resourceBundle="ReportResources"
		 whenResourceMissingType="Empty"
		 isIgnorePagination="true">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="campanyaProceso" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("campanyaProceso")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionRegion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionRegion")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionRegion1" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionRegion1")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionTramos" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionTramos")]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR_1" isForPrompting="false" class="net.sf.jasperreports.engine.JasperReport">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("SUBREPORT_DIR1")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT ROWNUM NRO,
       T.COD_REGI,
       T.COD_ZONA,
       T.COD_SECC,
       ( SELECT COD_CLIE || ' ' || VAL_NOM1 || ' ' || VAL_APE1 || ' ' || VAL_APE2
           FROM MAE_CLIEN
          WHERE COD_CLIE = T.COD_LIDE ) LIDER,
       T.COD_TERR,
       T.COD_CONS,
       T.NOMBRE_CONSULTORA,
       T.CAMP_PEDI,
       nvl(flex.val_cuot_21di_pedi_vige,0)  MONTO_PEDIDO_FLEXIPAGO,
       T.BOLETA_DESPACHO,
       T.MONTO_TOTAL,
       T.VENTA_RETAIL,
       T.RECAUDO_TOTAL,       
       T.FECHA_EMISION,
       T.MONTO_NETO,
       T.FECHA_PAGO,
       T.DIFER_DIAS,
       T.NETO_RECUPERADO,
       T.PORC_COMISION,
       T.INGRESOS,
       T.PEDIDO_CONSECU,
       T.NIVEL_EXITO,
       T.COD_LIDE,
	   (T.RECAUDO_TOTAL - T.VAL_MONT_ABON) RECAUDO_NOCOMIS 
  FROM (SELECT COD_REGI,
               COD_ZONA,
               COD_SECC,
               COD_LIDE,
               COD_TERR,
               COD_CONS,
               NOMBRE_CONSULTORA,
               CAMP_PEDI,
               0 MONTO_PEDIDO_FLEXIPAGO,
               BOLETA_DESPACHO,
			   MONTO_TOTAL,
			   VENTA_RETAIL,
			   RECAUDO_TOTAL,
			   VAL_MONT_ABON,
               TO_CHAR(FECHA_EMISION, 'dd/mm/yyyy') FECHA_EMISION,
               MONTO_NETO MONTO_NETO,
               TO_CHAR(FECHA_PAGO, 'dd/mm/yyyy') FECHA_PAGO,
               DIFER_DIAS,
               NETO_RECUPERADO NETO_RECUPERADO,
               PORC_COMISION PORC_COMISION,
               INGRESOS INGRESOS,
               PEDIDO_CONSECU,
               NIVEL_EXITO,
               CAM_RECU,
               SOCA_OID_SOLI_CABE
          FROM (SELECT DR.COD_REGI,
                       DR.COD_ZONA,
                       DR.COD_SECC,
                       DR.COD_LIDE,
                       TR.COD_TERR,
                       DR.COD_CONS,
                       CL.VAL_NOM1 || ' ' || CL.VAL_APE1 || ' ' ||
                       CL.VAL_APE2 NOMBRE_CONSULTORA,
                                              
                       NVL2(DR.SOCA_OID_SOLI_CABE, CR.VAL_NOMB_PERI, NULL) CAMP_PEDI,                       
                       0 MONTO_PEDIDO_FLEXIPAGO,                                              
                       NVL2(DR.SOCA_OID_SOLI_CABE, DR.VAL_NUME_SOLI, NULL) BOLETA_DESPACHO,
                       
                       (SELECT
                       NVL(SUM(NVL(LSDR.VAL_MONT_CARG_TOTA, 0) - (NVL(LSDR.VAL_MONT_CDRS_TOTA, 0) + NVL(LSDR.VAL_MONT_ABON_PDTE_TOTA, 0)) ), 0)                        
                       FROM LEC_LIDER_SECCI_DETAL_RECUP LSDR 
                       WHERE LSDR.CAM_RECU = DR.CAM_RECU
                       AND LSDR.COD_CONS = DR.COD_CONS
                       AND LSDR.SOCA_OID_SOLI_CABE = DR.SOCA_OID_SOLI_CABE                       
                       ) MONTO_TOTAL,
                       
                       (SELECT 
                       NVL(SUM(NVL(LSDR.VAL_MONT_RTAL, 0)), 0)
                       FROM LEC_LIDER_SECCI_DETAL_RECUP LSDR 
                       WHERE LSDR.CAM_RECU = DR.CAM_RECU
                       AND LSDR.COD_CONS = DR.COD_CONS
                       AND LSDR.MVCC_OID_MOVI_CC  = 0) VENTA_RETAIL,
                       								
                       0 RECAUDO_TOTAL,
					   0 VAL_MONT_ABON,
                       
                       DR.FEC_CARG FECHA_EMISION,
                       
                       (
                        SELECT
                        SUM(NVL(LLSDRMN.VAL_MONT_CARG, 0) - (NVL(LLSDRMN.VAL_MONT_CDRS, 0) + NVL(LLSDRMN.VAL_MONT_ABON_PDTE, 0)))                         
                        FROM LEC_LIDER_SECCI_DETAL_RECUP LLSDRMN
                        WHERE LLSDRMN.CAM_RECU = DR.CAM_RECU
                        AND LLSDRMN.COD_CONS = DR.COD_CONS
                        AND LLSDRMN.SOCA_OID_SOLI_CABE = DR.SOCA_OID_SOLI_CABE
                       ) MONTO_NETO,
                       
                       NULL FECHA_PAGO,
                       0 DIFER_DIAS,
                       0 NETO_RECUPERADO,
                       0 PORC_COMISION,
                       0 INGRESOS,
                       DECODE( DR.IND_PEDI_CONS, '1', 'SI', 'NO' ) PEDIDO_CONSECU,
                       ( SELECT NI.DES_NIVE
                           FROM LEC_LIDER_NIVEL LN,
                                LEC_NIVEL       NI
                          WHERE LN.COD_LIDE = DR.COD_LIDE
                            AND LN.LNIV_COD_NIVE = NI.COD_NIVE
                            AND LN.IND_TIPO_NIVE = 'R'
                            AND LN.IND_ACTI = '1'
                            AND  LN.LPRO_COD_PROG = DR.LPRO_COD_PROG
                            AND $P{campanyaProceso} >= LN.CAM_INIC
                            AND ( $P{campanyaProceso}  <= LN.CAM_FIN OR LN.CAM_FIN IS NULL ) ) NIVEL_EXITO,
                       DR.CAM_RECU,
                       DR.SOCA_OID_SOLI_CABE,
                       DR.POR_FACT_NCOM,
                       DR.IND_FLEX
                  FROM LEC_LIDER_SECCI_DETAL_RECUP DR,
                       PED_SOLIC_CABEC                         PSC,
                       CRA_PERIO                               CR,
                       MAE_CLIEN                               CL,
                       MAE_CLIEN_UNIDA_ADMIN                   UA,
                       ZON_TERRI_ADMIN                         ZTA,
                       ZON_TERRI                               TR
                 WHERE DR.SOCA_OID_SOLI_CABE = PSC.OID_SOLI_CABE
                   AND PSC.PERD_OID_PERI = CR.OID_PERI
                   AND DR.COD_CONS = CL.COD_CLIE
                   AND CL.OID_CLIE = UA.CLIE_OID_CLIE
                   AND UA.ZTAD_OID_TERR_ADMI = ZTA.OID_TERR_ADMI
                   AND ZTA.TERR_OID_TERR = TR.OID_TERR
                   AND (SELECT CP.OID_PERI
                          FROM CRA_PERIO CP, SEG_PERIO_CORPO SP
                         WHERE SP.COD_PERI = $P{campanyaProceso}
                           AND SP.OID_PERI = CP.PERI_OID_PERI) >= UA.PERD_OID_PERI_INI
                   AND ((SELECT CP.OID_PERI
                           FROM CRA_PERIO CP, SEG_PERIO_CORPO SP
                          WHERE SP.COD_PERI = $P{campanyaProceso}
                            AND SP.OID_PERI = CP.PERI_OID_PERI) <= UA.PERD_OID_PERI_FIN OR UA.PERD_OID_PERI_FIN IS NULL)
                   AND DR.CAM_RECU =  $P{campanyaProceso}
                   AND ((DR.SOCA_OID_SOLI_CABE IS NOT NULL AND DR.VAL_MONT_CARG > 0) OR DR.SOCA_OID_SOLI_CABE IS NULL   OR DR.ind_flex = 2)
                                      
                  $P!{condicionRegion}  -----------  CONSIDERAR FILTRO AQUI
                  
                   AND NOT EXISTS ( SELECT *
                                      FROM LEC_LIDER_SECCI_DRECU_TRAMO SDT
                                     WHERE SDT.MVCC_OID_MOVI_CC = DR.MVCC_OID_MOVI_CC
                                       AND SDT.COD_LIDE = DR.COD_LIDE
                                       AND SDT.CAM_RECU = DR.CAM_RECU
                                       AND SDT.COD_CONS = DR.COD_CONS
                                       AND SDT.LPCT_COD_TRAM = $P{condicionTramos}  
                                    ) 
                UNION ALL
                SELECT COD_REGI,
                       COD_ZONA,
                       COD_SECC,
                       COD_LIDE,
                       COD_TERR,
                       COD_CONS,
                       NOMBRE_CONSULTORA,
                       CAMP_PEDI,
                       MONTO_PEDIDO_FLEXIPAGO,
                       BOLETA_DESPACHO,
					   MONTO_TOTAL,
					   VENTA_RETAIL,
					   RECAUDO_TOTAL,
					   VAL_MONT_ABON,
                       FECHA_EMISION,
                       MONTO_NETO,
                       FECHA_PAGO,
                       DIFER_DIAS,
                       NETO_RECUPERADO,
                       ROUND(PORC_COMISION, NUM_DECIMAL) PORC_COMISION,
                       ROUND(NETO_RECUPERADO * (PORC_COMISION / 100),
                             NUM_DECIMAL) INGRESOS,
                       PEDIDO_CONSECU,
                       NIVEL_EXITO,
                       CAM_RECU,
                       SOCA_OID_SOLI_CABE,
                       POR_FACT_NCOM,
                       IND_FLEX
                  FROM (SELECT DR.COD_REGI,
                               DR.COD_ZONA,
                               DR.COD_SECC,
                               DR.COD_LIDE,
                               TR.COD_TERR,
                               DR.COD_CONS,
                               CL.VAL_NOM1 || ' ' || CL.VAL_APE1 || ' ' ||
                               CL.VAL_APE2 NOMBRE_CONSULTORA,
                               
		                       NVL2(DR.SOCA_OID_SOLI_CABE, CR.VAL_NOMB_PERI, NULL) CAMP_PEDI,                       
		                       0 MONTO_PEDIDO_FLEXIPAGO,                                              
		                       NVL2(DR.SOCA_OID_SOLI_CABE, DR.VAL_NUME_SOLI, NULL) BOLETA_DESPACHO,
		                       
		                       (SELECT
		                       NVL(SUM(NVL(LSDR.VAL_MONT_CARG_TOTA, 0) - (NVL(LSDR.VAL_MONT_CDRS_TOTA, 0) + NVL(LSDR.VAL_MONT_ABON_PDTE_TOTA, 0)) ), 0)                        
		                       FROM LEC_LIDER_SECCI_DETAL_RECUP LSDR 
		                       WHERE LSDR.CAM_RECU = DR.CAM_RECU
		                       AND LSDR.COD_CONS = DR.COD_CONS
		                       AND LSDR.SOCA_OID_SOLI_CABE = DR.SOCA_OID_SOLI_CABE                       
		                       ) MONTO_TOTAL,
							   					   
		                       (SELECT 
								NVL(SUM(NVL(LSDR.VAL_MONT_RTAL, 0)), 0)
								FROM LEC_LIDER_SECCI_DETAL_RECUP LSDR 
								WHERE LSDR.CAM_RECU = DR.CAM_RECU
								AND LSDR.COD_CONS = DR.COD_CONS
								AND LSDR.MVCC_OID_MOVI_CC  = 0) VENTA_RETAIL,	
															
                               NVL((
                                SELECT SUM(LRT.VAL_MONT_ABON_TOTA)
                                FROM LEC_LIDER_SECCI_DRECU_TRAMO LRT 
                                WHERE LRT.PAIS_COD_PAIS = DR.PAIS_COD_PAIS
                                AND LRT.COD_REGI = DR.COD_REGI
                                AND LRT.COD_ZONA = DR.COD_ZONA
                                AND LRT.COD_SECC = DR.COD_SECC
                                AND LRT.COD_CONS = DR.COD_CONS
                                AND LRT.CAM_RECU = DR.CAM_RECU
                                AND LRT.MVCC_OID_MOVI_CC IN(
                                    SELECT
                                    MVCC_OID_MOVI_CC
                                    FROM LEC_LIDER_SECCI_DETAL_RECUP LSDR 
                                    WHERE LSDR.PAIS_COD_PAIS = LRT.PAIS_COD_PAIS
                                    AND LSDR.CAM_RECU = LRT.CAM_RECU
                                    AND LSDR.COD_CONS = LRT.COD_CONS                               
                                    AND LSDR.SOCA_OID_SOLI_CABE = DR.SOCA_OID_SOLI_CABE)                               
                               ), 0) RECAUDO_TOTAL,
							   
                               NVL(DTR.VAL_MONT_ABON, 0) VAL_MONT_ABON,
							   
                               DR.FEC_CARG FECHA_EMISION,
                               (
                        		SELECT
                        		SUM(NVL(LLSDRMN.VAL_MONT_CARG, 0) - (NVL(LLSDRMN.VAL_MONT_CDRS, 0) + NVL(LLSDRMN.VAL_MONT_ABON_PDTE, 0)))                         
                        		FROM LEC_LIDER_SECCI_DETAL_RECUP LLSDRMN
                        		WHERE LLSDRMN.CAM_RECU = DR.CAM_RECU
                        		AND LLSDRMN.COD_CONS = DR.COD_CONS
                        		AND LLSDRMN.SOCA_OID_SOLI_CABE = DR.SOCA_OID_SOLI_CABE
                               ) MONTO_NETO,
                               
                              TBPAGOS.FECHA_PAGO,
                               (TRUNC(TBPAGOS.FECHA_PAGO) - TRUNC(DR.FEC_CARG)) DIFER_DIAS,
                               ROUND(TBPAGOS.PAGO -
                                     (TBPAGOS.PAGO * DR.POR_FACT_NCOM),
                                     (SELECT NUM_DECI
                                        FROM SEG_PAIS P, SEG_MONED M
                                       WHERE P.MONE_OID_MONE = M.OID_MONE
                                         AND P.COD_PAIS =  $P{codigoPais} 
                                      )) NETO_RECUPERADO,
                               DTR.VAL_PORC_COMI PORC_COMISION,
                               0 INGRESOS,
                               DECODE(DR.IND_PEDI_CONS, '1', 'SI', 'NO') PEDIDO_CONSECU,
                               (SELECT NI.DES_NIVE
                                  FROM LEC_LIDER_NIVEL LN,
                                       LEC_NIVEL       NI
                                 WHERE LN.COD_LIDE = DR.COD_LIDE
                                   AND LN.LNIV_COD_NIVE = NI.COD_NIVE
                                   AND LN.IND_TIPO_NIVE = 'R'
                                   AND $P{campanyaProceso} >= LN.CAM_INIC
                                   AND ( $P{campanyaProceso}  <= LN.CAM_FIN OR
                                       LN.CAM_FIN IS NULL)) NIVEL_EXITO,
                               DR.CAM_RECU,
                               DR.SOCA_OID_SOLI_CABE,
                               DR.POR_FACT_NCOM,
                               (SELECT NUM_DECI
                                  FROM SEG_PAIS P, SEG_MONED M
                                 WHERE P.MONE_OID_MONE = M.OID_MONE
                                   AND P.COD_PAIS =   $P{codigoPais} 
                                ) NUM_DECIMAL,
                               DR.IND_FLEX, DR.VAL_MONT_CDRS
                          FROM LEC_LIDER_SECCI_DETAL_RECUP DR,
                               LEC_LIDER_SECCI_DRECU_TRAMO DTR,
                               (SELECT CCM.FEC_ULTI_MOVI FECHA_PAGO,
                                       CCM.IMP_PAGO      PAGO,
                                       CCM.OID_MOVI_CC   OIDMOVICC
                                  FROM CCC_MOVIM_CUENT_CORRI      CCM,
                                       CCC_SUBPR                  B,
                                       CCC_PROCE                  C,
                                       LEC_PROC_SUBPR D,
                                       LEC_TIPO_MOVIM E
                                 WHERE CCM.SUBP_OID_SUBP_ULTI = B.OID_SUBP
                                   AND B.CCPR_OID_PROC = C.OID_PROC
                                   AND B.COD_SUBP = D.COD_SUBP
                                   AND C.COD_PROC = D.COD_PROC
                                   AND D.LTMO_COD_MOVI = E.COD_MOVI
                                   AND E.IND_TIPO_MOVI IN ('P')
                                UNION ALL
                                SELECT CCH.FEC_MOVI         FECHA_PAGO,
                                       CCH.IMP_PAGO         PAGO,
                                       CCH.MVCC_OID_MOVI_CC OIDMOVICC
                                  FROM CCC_HISTO_MOVIM_CC         CCH,
                                       CCC_SUBPR                  B,
                                       CCC_PROCE                  C,
                                       LEC_PROC_SUBPR D,
                                       LEC_TIPO_MOVIM E
                                 WHERE CCH.SUBP_OID_SUBP = B.OID_SUBP
                                   AND B.CCPR_OID_PROC = C.OID_PROC
                                   AND B.COD_SUBP = D.COD_SUBP
                                   AND C.COD_PROC = D.COD_PROC
                                   AND D.LTMO_COD_MOVI = E.COD_MOVI
                                   AND E.IND_TIPO_MOVI IN ('P')) TBPAGOS,
                               PED_SOLIC_CABEC PSC,
                               CRA_PERIO CR,
                               MAE_CLIEN CL,
                               MAE_CLIEN_UNIDA_ADMIN UA,
                               ZON_TERRI_ADMIN ZTA,
                               ZON_TERRI TR
                         WHERE DTR.CAM_RECU = DR.CAM_RECU
                           AND DTR.COD_LIDE = DR.COD_LIDE
                           AND DTR.COD_CONS = DR.COD_CONS
                           AND DTR.MVCC_OID_MOVI_CC = DR.MVCC_OID_MOVI_CC
                           AND DTR.MVCC_OID_MOVI_CC = TBPAGOS.OIDMOVICC
                           AND DR.SOCA_OID_SOLI_CABE = PSC.OID_SOLI_CABE
                           AND PSC.PERD_OID_PERI = CR.OID_PERI
                           AND DR.COD_CONS = CL.COD_CLIE
                           AND CL.OID_CLIE = UA.CLIE_OID_CLIE
                           AND UA.ZTAD_OID_TERR_ADMI = ZTA.OID_TERR_ADMI
                           AND ZTA.TERR_OID_TERR = TR.OID_TERR
                           AND (SELECT CP.OID_PERI
                                  FROM CRA_PERIO CP, SEG_PERIO_CORPO SP
                                 WHERE SP.COD_PERI =   $P{campanyaProceso}
                                   AND SP.OID_PERI = CP.PERI_OID_PERI) >=
                               UA.PERD_OID_PERI_INI
                           AND ((SELECT CP.OID_PERI
                                   FROM CRA_PERIO CP, SEG_PERIO_CORPO SP
                                  WHERE SP.COD_PERI = $P{campanyaProceso}
                                    AND SP.OID_PERI = CP.PERI_OID_PERI) <=
                               UA.PERD_OID_PERI_FIN OR
                               UA.PERD_OID_PERI_FIN IS NULL)
                           AND DR.CAM_RECU = $P{campanyaProceso} 
                           AND DTR.LPCT_COD_TRAM = $P{condicionTramos}
                           
                           AND ((DR.SOCA_OID_SOLI_CABE IS NOT NULL AND DR.VAL_MONT_CARG > 0) OR DR.SOCA_OID_SOLI_CABE IS NULL  OR DR.ind_flex = 2)
                             
                           $P!{condicionRegion1} -------------  CONSIDERAR FILTRO AQUI
                           
                           AND (CASE
                                 WHEN DTR.LPCT_COD_TRAM = 1 THEN
                                  TBPAGOS.FECHA_PAGO
                               END <= DTR.FEC_LIMI_ABON OR
                               (CASE
                                 WHEN DTR.LPCT_COD_TRAM = 2 THEN
                                  TBPAGOS.FECHA_PAGO
                               END >=
                               NVL(DTR.FEC_INI_ABON, TBPAGOS.FECHA_PAGO) AND
                               CASE
                                 WHEN DTR.LPCT_COD_TRAM = 2 THEN
                                  TBPAGOS.FECHA_PAGO
                               END <= DTR.FEC_LIMI_ABON))) )
         ) T,
         ( SELECT flexi.cod_peri,
                  flexi.cod_clie,
                  flexi.soca_oid_soli_cabe,
                  flexi.val_cuot_21di_pedi_vige
             FROM flx_gener_finan_consu_flexi flexi
            WHERE cod_peri = $P{campanyaProceso} 
            AND ind_gene_fina_flex = '1') flex
WHERE t.cam_recu = flex.cod_peri (+)
   AND t.cod_cons = flex.cod_clie (+)
   AND t.soca_oid_soli_cabe = flex.soca_oid_soli_cabe (+) 
ORDER BY  2, 3, 4, 7, 11 ASC]]></queryString>

	<field name="NRO" class="java.math.BigDecimal"/>
	<field name="COD_REGI" class="java.lang.String"/>
	<field name="COD_ZONA" class="java.lang.String"/>
	<field name="COD_SECC" class="java.lang.String"/>
	<field name="LIDER" class="java.lang.String"/>
	<field name="COD_TERR" class="java.math.BigDecimal"/>
	<field name="COD_CONS" class="java.lang.String"/>
	<field name="NOMBRE_CONSULTORA" class="java.lang.String"/>
	<field name="CAMP_PEDI" class="java.lang.String"/>
	<field name="MONTO_PEDIDO_FLEXIPAGO" class="java.math.BigDecimal"/>
	<field name="BOLETA_DESPACHO" class="java.math.BigDecimal"/>
	<field name="FECHA_EMISION" class="java.lang.String"/>
	<field name="MONTO_NETO" class="java.math.BigDecimal"/>
	<field name="FECHA_PAGO" class="java.lang.String"/>
	<field name="DIFER_DIAS" class="java.math.BigDecimal"/>
	<field name="NETO_RECUPERADO" class="java.math.BigDecimal"/>
	<field name="PORC_COMISION" class="java.math.BigDecimal"/>
	<field name="INGRESOS" class="java.math.BigDecimal"/>
	<field name="PEDIDO_CONSECU" class="java.lang.String"/>
	<field name="NIVEL_EXITO" class="java.lang.String"/>
	<field name="COD_LIDE" class="java.lang.String"/>
	<field name="MONTO_TOTAL" class="java.math.BigDecimal"/>
	<field name="VENTA_RETAIL" class="java.math.BigDecimal"/>
	<field name="RECAUDO_TOTAL" class="java.math.BigDecimal"/>
	<field name="RECAUDO_NOCOMIS" class="java.math.BigDecimal"/>

	<variable name="NRO_CORRELATIVO" class="java.lang.Integer" resetType="Group" resetGroup="SECCION" calculation="Count">
		<variableExpression><![CDATA[$V{NRO_CORRELATIVO}]]></variableExpression>
		<initialValueExpression><![CDATA[new Integer(1)]]></initialValueExpression>
	</variable>

		<group  name="REGION" >
			<groupExpression><![CDATA[]]></groupExpression>
			<groupHeader>
			<band height="0"  isSplitAllowed="false" >
			</band>
			</groupHeader>
			<groupFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
			</groupFooter>
		</group>
		<group  name="ZONA" >
			<groupExpression><![CDATA[]]></groupExpression>
			<groupHeader>
			<band height="0"  isSplitAllowed="false" >
			</band>
			</groupHeader>
			<groupFooter>
			<band height="0"  isSplitAllowed="true" >
			</band>
			</groupFooter>
		</group>
		<group  name="SECCION" >
			<groupExpression><![CDATA[]]></groupExpression>
			<groupHeader>
			<band height="0"  isSplitAllowed="false" >
			</band>
			</groupHeader>
			<groupFooter>
			<band height="0"  isSplitAllowed="false" >
			</band>
			</groupFooter>
		</group>
		<group  name="LIDER" isStartNewPage="true" isReprintHeaderOnEachPage="true" >
			<groupExpression><![CDATA[]]></groupExpression>
			<groupHeader>
			<band height="0"  isSplitAllowed="false" >
			</band>
			</groupHeader>
			<groupFooter>

	<queryString><![CDATA[SELECT r.des_regi DES_REGI, b.cod_vali COD_VALI, a.cod_clie COD_CLIE
  FROM STO_HISTO_DOCUM_DIGIT a,
       (SELECT DISTINCT cod_pais,
                        cod_tipo_docu,
                        num_lote,
                        cod_vali,
                        sec_nume_docu
          FROM STO_HISTO_AUDIT_EXCEP x
         WHERE EXISTS (SELECT NULL
                  FROM ped_histo_solic_conso_cabec y
                 WHERE y.sec_nume_docu = x.sec_nume_docu
                 AND y.cod_peri = $P{periodo}
                   AND y.ind_proc_gp2 = 0
                   AND NOT EXISTS
                 (SELECT NULL
                          FROM ped_histo_solic_conso_cabec h
                         WHERE h.ind_proc_gp2 = 1
                         AND h.cod_peri  = $P{periodo}
                           AND h.cod_peri = y.cod_peri
                           AND h.cod_clie = y.cod_clie))) b,
       sto_param_secue_valid c,
       zon_regio r
 WHERE a.cod_pais = b.cod_pais
   AND a.cod_tipo_docu = b.cod_tipo_docu
   AND a.num_lote = b.num_lote
   AND a.sec_nume_docu = b.sec_nume_docu
   AND b.cod_pais = c.cod_pais
   AND b.cod_vali = c.cod_vali
   AND b.cod_tipo_docu = c.cod_tipo_docu
   AND a.cod_regi = r.cod_regi   
   AND ((trunc(a.FEC_digi) >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY')) or ($P{fechaDesde} is null))
   AND ((TRUNC(a.FEC_digi) <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY')) or ($P{fechaHasta} is null))
   AND a.cod_peri = $P{periodo}
   AND b.cod_pais = $P{codigoPais}
   AND b.cod_tipo_docu IN
       (SELECT cod_tipo_docu
          FROM sto_tipo_docum_digit
         WHERE ('C' = $P{tipoReporte} AND cod_tipo_docu = $P{tipoDocumento})
            OR ('D' = $P{tipoReporte} AND cod_tipo_docu_cabe = $P{tipoDocumento})
            OR ('T' = $P{tipoReporte} AND (cod_tipo_docu = $P{tipoDocumento} 
            OR cod_tipo_docu_cabe = $P{tipoDocumento})))]]></queryString>


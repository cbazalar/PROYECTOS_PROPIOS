	<queryString><![CDATA[SELECT (CASE WHEN $P{nombre}=NULL AND $P{lineazona} = NULL THEN(


SELECT det.val_nomb_pica PICADORA
       FROM APE_LISTA_PICAD_DETAL DET 
       INNER JOIN APE_LISTA_PICAD_CABEC CAB
       ON DET.LPCA_OID_LIST_PICA_CABE = CAB.OID_LIST_PICA_CABE
       INNER JOIN  APP_CONFI_CENTR_DISTR CE 
       ON CAB.CCDI_OID_CONF_CENT_DIST =  CE.OID_CONF_CENT_DIST
       AND CE.COD_CENT_DIST=$P{centro}
       INNER JOIN APE_MAPA_CENTR_DISTR_DETAL MAPADET ON DET.MCDD_OID_MAPA_CENT_DIST_DETA = MAPADET.OID_MAPA_CENT_DIST_DETA
       AND DET.NUM_UNID_PICA <> DET.NUM_UNID_CHEQ
       INNER JOIN PED_SOLIC_CABEC SOL 
       ON SOL.OID_SOLI_CABE = CAB.SOCA_OID_SOLI_CABE
       AND SOL.ZZON_OID_ZONA= (SELECT Z.OID_ZONA
                                                                FROM ZON_ZONA Z 
                                                                WHERE Z.OID_ZONA=SOL.ZZON_OID_ZONA 
                                                                AND Z.COD_ZONA=$P{codigozona}) 
       INNER JOIN APE_MAPA_CENTR_DISTR_CABEC MAPA
       ON MAPA.CCDI_OID_CONF_CENT_DIST = CE.OID_CONF_CENT_DIST
       INNER JOIN MAE_PRODU PRO ON DET.PROD_OID_PROD = PRO.OID_PROD
       AND SOL.PERD_OID_PERI = (SELECT P.OID_PERI
                                                       FROM CRA_PERIO P 
                                                       WHERE P.OID_PERI = SOL.PERD_OID_PERI 
                                                       AND P.VAL_NOMB_PERI like $P{campana}
                                                       AND P.MARC_OID_MARC = (SELECT M.OID_MARC 
                                                                                                    FROM SEG_MARCA M 
                                                                                                    WHERE M.OID_MARC = P.MARC_OID_MARC 
                                                                                                    AND M.COD_MARC=$P{marca})
                                                       AND P.CANA_OID_CANA = (SELECT C.OID_CANA
                                                                                                    FROM SEG_CANAL C 
                                                                                                    WHERE C.OID_CANA = P.CANA_OID_CANA
                                                                                                    AND C.COD_CANA = $P{canal})
                                                       )
                         )
                         WHEN $P{nombre} = NULL AND $P{lineazona} <> NULL THEN(
      
    
    SELECT det.val_nomb_pica PICADORA
                
       FROM APE_LISTA_PICAD_DETAL DET 
       INNER JOIN APE_LISTA_PICAD_CABEC CAB
       ON DET.LPCA_OID_LIST_PICA_CABE = CAB.OID_LIST_PICA_CABE
       INNER JOIN  APP_CONFI_CENTR_DISTR CE 
       ON CAB.CCDI_OID_CONF_CENT_DIST =  CE.OID_CONF_CENT_DIST
        INNER JOIN APE_MAPA_CENTR_DISTR_DETAL MAPADET ON DET.MCDD_OID_MAPA_CENT_DIST_DETA = MAPADET.OID_MAPA_CENT_DIST_DETA
       AND CE.COD_CENT_DIST=$P{centro}
       AND DET.NUM_UNID_PICA <> DET.NUM_UNID_CHEQ
       INNER JOIN PED_SOLIC_CABEC SOL 
       ON SOL.OID_SOLI_CABE = CAB.SOCA_OID_SOLI_CABE
       AND SOL.ZZON_OID_ZONA= (SELECT Z.OID_ZONA
                                                                FROM ZON_ZONA Z 
                                                                WHERE Z.OID_ZONA=SOL.ZZON_OID_ZONA 
                                                                AND Z.COD_ZONA=$P{codigozona}) 
       INNER JOIN APE_MAPA_CENTR_DISTR_CABEC MAPA
       ON MAPA.CCDI_OID_CONF_CENT_DIST = CE.OID_CONF_CENT_DIST
       INNER JOIN APE_MAPA_ZONA_CABEC MAPZONA ON MAPZONA.MCDC_OID_MAPA_CENT_DIST_CABE= MAPA.OID_MAPA_CENT_DIST_CABE
       AND MAPZONA.COD_MAPA_ZONA=4    
        INNER JOIN MAE_PRODU PRODUC ON DET.PROD_OID_PROD = PRODUC.OID_PROD
       
       AND SOL.PERD_OID_PERI = (SELECT P.OID_PERI
                                                       FROM CRA_PERIO P 
                                                       WHERE P.OID_PERI = SOL.PERD_OID_PERI 
                                                       AND P.VAL_NOMB_PERI like $P{campana}
                                                       AND P.MARC_OID_MARC = (SELECT M.OID_MARC 
                                                                                                    FROM SEG_MARCA M 
                                                                                                    WHERE M.OID_MARC = P.MARC_OID_MARC 
                                                                                                    AND M.COD_MARC=$P{marca})
                                                       AND P.CANA_OID_CANA = (SELECT C.OID_CANA
                                                                                                    FROM SEG_CANAL C 
                                                                                                    WHERE C.OID_CANA = P.CANA_OID_CANA
                                                                                                    AND C.COD_CANA = $P{canal})
                                                       ))
                                                       ELSE (
                                                       
      
    
    SELECT det.val_nomb_pica PICADORA
       FROM APE_LISTA_PICAD_DETAL DET 
       INNER JOIN APE_LISTA_PICAD_CABEC CAB
       ON DET.LPCA_OID_LIST_PICA_CABE = CAB.OID_LIST_PICA_CABE
       INNER JOIN  APP_CONFI_CENTR_DISTR CE 
       ON CAB.CCDI_OID_CONF_CENT_DIST =  CE.OID_CONF_CENT_DIST
       AND CE.COD_CENT_DIST=$P{centro}
       AND DET.NUM_UNID_PICA <> DET.NUM_UNID_CHEQ
       AND DET.VAL_NOMB_PICA=$P{nombrePicador}
       INNER JOIN PED_SOLIC_CABEC SOL 
       ON SOL.OID_SOLI_CABE = CAB.SOCA_OID_SOLI_CABE
       INNER JOIN APE_MAPA_CENTR_DISTR_CABEC MAPA
       ON MAPA.CCDI_OID_CONF_CENT_DIST = CE.OID_CONF_CENT_DIST
       INNER JOIN MAE_PRODU PRODUC ON DET.PROD_OID_PROD = PRODUC.OID_PROD
       INNER JOIN APE_MAPA_ZONA_CABEC MAPZONA ON MAPZONA.MCDC_OID_MAPA_CENT_DIST_CABE= MAPA.OID_MAPA_CENT_DIST_CABE
       INNER JOIN APE_MAPA_CENTR_DISTR_DETAL MAPADET ON DET.MCDD_OID_MAPA_CENT_DIST_DETA = MAPADET.OID_MAPA_CENT_DIST_DETA
       AND MAPZONA.COD_MAPA_ZONA=$P{mapaz}    
       AND SOL.PERD_OID_PERI = (SELECT P.OID_PERI
                                                       FROM CRA_PERIO P 
                                                       WHERE P.OID_PERI = SOL.PERD_OID_PERI 
                                                       AND P.VAL_NOMB_PERI like $P{campana}
                                                       AND P.MARC_OID_MARC = (SELECT M.OID_MARC 
                                                                                                    FROM SEG_MARCA M 
                                                                                                    WHERE M.OID_MARC = P.MARC_OID_MARC 
                                                                                                    AND M.COD_MARC=$P{marca})
                                                       AND P.CANA_OID_CANA = (SELECT C.OID_CANA
                                                                                                    FROM SEG_CANAL C 
                                                                                                    WHERE C.OID_CANA = P.CANA_OID_CANA
                                                                                                    AND C.COD_CANA = $P{canal})
                                                       )
                                                       
                                                      ) END ),det.val_nomb_pica PICADORA,
                 PRODUC.COD_SAP PRODUCTO,
                 PRODUC.DES_CORT DESCRIPCION,
                 SOL.NUM_UNID_POR_ATEN_TOTA FALTANTES,
                 SOL.NUM_UNID_ATEN_TOTA SOBRANTES,
                 DET.NUM_UNID_PICA PICADAS,
                 '1' ERROR,
                 '1' RELATIVO,
                  MAPADET.NUM_ANAQ ACTUAL,
                  (SELECT DETALLITO.NUM_ANAQ FROM APE_MAPA_CENTR_DISTR_DETAL DETALLITO WHERE DETALLITO.OID_MAPA_CENT_DIST_DETA = MAPADET.MCDD_OID_MAPA_CENT_DIST_DETA) ORIGINAL,
	          (select lia.DES_MARC from SEG_MARCA lia where lia.COD_MARC =$P{marca}) MARCA,
				  (select gen.VAL_I18N from GEN_I18N_SICC_COMUN gen where gen.VAL_OID = (select li.OID_CANA  from SEG_CANAL li where li.COD_CANA =$P{canal}) and gen.ATTR_ENTI = 'SEG_CANAL') CANAL,
				  (select per.val_nomb_peri from cra_perio per where per.oid_peri = sol.perd_oid_peri) campana,
	   (select gen.VAL_I18N from GEN_I18N_SICC_PAIS gen where gen.VAL_OID = (select ce.OID_CONF_CENT_DIST  from APP_CONFI_CENTR_DISTR ce where ce.COD_CENT_DIST =$P{centro}) and gen.ATTR_ENTI = 'APP_CONFI_CENTR_DISTR') centro,
	   (SELECT GEN.VAL_I18N FROM  APE_MAPA_ZONA_CABEC MAPA,GEN_I18N_SICC_PAIS GEN WHERE MAPA.COD_MAPA_ZONA=$P{mapaz} AND GEN.ATTR_ENTI='APE_MAPA_ZONA_CABEC' AND MAPA.OID_MAPA_ZONA_CABE=GEN.VAL_OID) ZONA,
		  (SELECT GEN.VAL_I18N FROM APE_MAPA_CENTR_DISTR_CABEC MAPAC, GEN_I18N_SICC_PAIS GEN  WHERE MAPAC.NUM_CODI_MAPA=$P{mapacd} AND GEN.VAL_OID= MAPAC.OID_MAPA_CENT_DIST_CABE AND GEN.ATTR_ENTI='APE_MAPA_CENTR_DISTR_CABEC') MAPAZONA
        FROM APE_LISTA_PICAD_DETAL DET 
       INNER JOIN APE_LISTA_PICAD_CABEC CAB
       ON DET.LPCA_OID_LIST_PICA_CABE = CAB.OID_LIST_PICA_CABE
       INNER JOIN  APP_CONFI_CENTR_DISTR CE 
       ON CAB.CCDI_OID_CONF_CENT_DIST =  CE.OID_CONF_CENT_DIST
       AND CE.COD_CENT_DIST=$P{centro}
       INNER JOIN APE_MAPA_CENTR_DISTR_DETAL MAPADET ON DET.MCDD_OID_MAPA_CENT_DIST_DETA = MAPADET.OID_MAPA_CENT_DIST_DETA
       
       INNER JOIN PED_SOLIC_CABEC SOL 
       ON SOL.OID_SOLI_CABE = CAB.SOCA_OID_SOLI_CABE
       INNER JOIN APE_MAPA_CENTR_DISTR_CABEC MAPA
       ON MAPA.CCDI_OID_CONF_CENT_DIST = CE.OID_CONF_CENT_DIST
       INNER JOIN MAE_PRODU PRODUC ON DET.PROD_OID_PROD = PRODUC.OID_PROD
       INNER JOIN APE_MAPA_ZONA_CABEC MAPZONA ON MAPZONA.MCDC_OID_MAPA_CENT_DIST_CABE= MAPA.OID_MAPA_CENT_DIST_CABE]]></queryString>


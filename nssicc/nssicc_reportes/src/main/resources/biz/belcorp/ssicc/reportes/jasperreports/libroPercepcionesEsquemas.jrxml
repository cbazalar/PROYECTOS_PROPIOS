	<parameter name="fechaDesde" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaHasta" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaHasta")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPais" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPaisLbel" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPaisLbel")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT COD_CANA, COD_ACCE, COD_SBAC, COD_CLIE, TIP_DOID, NUM_DOID, NOM_CONS, SUM_VENTAS, SUM_PERCEPCION, SUM_COBRADO, SOCIEDAD
FROM ((SELECT TEMP.COD_CANA,
       TEMP.COD_ACCE,
			 TEMP.COD_SBAC,
			 TEMP.COD_CLIE,
			 PER_PKG_REPOR_PERCE.PER_FN_DEVUE_DOCUM_HOMOL($P{codigoPais},TEMP.TIP_DOID) as TIP_DOID,
			 TRIM(TEMP.NUM_DOID) AS NUM_DOID,
			 MAX(TEMP.NOM_CONS) AS NOM_CONS,	   
			 SUM(TEMP.VENTAS) SUM_VENTAS,
			 SUM(TEMP.PERCEPCION) SUM_PERCEPCION,
			 SUM(TEMP.COBRADO) SUM_COBRADO,
			 TEMP.SOCIEDAD
FROM        
(SELECT  PER.COD_CANA AS COD_CANA,
        PER.COD_ACCE AS COD_ACCE,
			  PER.COD_SBAC AS COD_SBAC,
			  PER.COD_CLIE AS COD_CLIE,
			  PER.TIP_DOID AS TIP_DOID,
			  PER.NUM_DOID AS NUM_DOID,
			  PER_PKG_REPOR_PERCE.PER_FN_NOMBR_CONSU_LIBRO_PERCE(PER.PAIS_COD_PAIS, PER.COD_CLIE, PER.APE_PATE, PER.APE_MATE, PER.VAL_NOMB) AS NOM_CONS,	
			  PER.MON_TODL AS VENTAS,
			  SUM(PER.MON_PERC) AS PERCEPCION,
			  SUM(PER.MON_PAGO) AS COBRADO,
			  SOCI.VAL_DENO as SOCIEDAD,	
			  PER.NUM_DOLE as NUM_DOLE		
	 FROM PER_PERCE_CONSO PER,
				SEG_PAIS  PAIS,
				SEG_SOCIE SOCI
 WHERE PER.PAIS_COD_PAIS = $P{codigoPais}
  			AND	  PER.PAIS_COD_PAIS = PAIS.COD_PAIS
  			AND   SOCI.COD_SOCI =  PER.COD_SOCI   		 
            AND   (PER.FEC_COPE >= TO_DATE ($P{fechaDesde},'dd/MM/yyyy'))
   			AND   (PER.FEC_COPE < TO_DATE ($P{fechaHasta},'dd/MM/yyyy') + 1)
   		    AND (PER.FLA_CANC is null or PER.FLA_CANC <> '1')          
     		AND (PER.FLA_CANC is null or PER.FLA_CANC <> 'A')	
 GROUP BY 
          PER.PAIS_COD_PAIS,
          PER.COD_CANA,
          PER.COD_ACCE,
				  PER.COD_SBAC,
					PER.COD_CLIE,
					PER.TIP_DOID,
					PER.NUM_DOID,
					SOCI.VAL_DENO,
					PER.NUM_DOLE,
					PER.SER_DOLE,
					PER.MON_TODL,
					PER.APE_PATE, 
					PER.APE_MATE, 
					PER.VAL_NOMB ) TEMP
 GROUP BY COD_CANA,
           COD_ACCE,
    			 COD_SBAC,
    			 COD_CLIE,
    			 TIP_DOID,
    			 TRIM(NUM_DOID),
    			 SOCIEDAD)
UNION
(SELECT TEMP.COD_CANA,
       TEMP.COD_ACCE,
			 TEMP.COD_SBAC,
			 TEMP.COD_CLIE,
			 PER_PKG_REPOR_PERCE_LBEL.PER_FN_DEVUE_DOCUM_HOMOL($P{codigoPaisLbel},TEMP.TIP_DOID) as TIP_DOID,
			 TRIM(TEMP.NUM_DOID) AS NUM_DOID,
			 MAX(TEMP.NOM_CONS) AS NOM_CONS,	   
			 SUM(TEMP.VENTAS) SUM_VENTAS,
			 SUM(TEMP.PERCEPCION) SUM_PERCEPCION,
			 SUM(TEMP.COBRADO) SUM_COBRADO,
			 TEMP.SOCIEDAD
FROM        
(SELECT  PER.COD_CANA AS COD_CANA,
        PER.COD_ACCE AS COD_ACCE,
			  PER.COD_SBAC AS COD_SBAC,
			  PER.COD_CLIE AS COD_CLIE,
			  PER.TIP_DOID AS TIP_DOID,
			  PER.NUM_DOID AS NUM_DOID,
			  PER_PKG_REPOR_PERCE_LBEL.PER_FN_NOMBR_CONSU_LIBRO_PERCE(PER.PAIS_COD_PAIS, PER.COD_CLIE, PER.APE_PATE, PER.APE_MATE, PER.VAL_NOMB) AS NOM_CONS,	
			  PER.MON_TODL AS VENTAS,
			  SUM(PER.MON_PERC) AS PERCEPCION,
			  SUM(PER.MON_PAGO) AS COBRADO,
			  SOCI.VAL_DENO as SOCIEDAD,	
			  PER.NUM_DOLE as NUM_DOLE		
	 FROM PER_PERCE_CONSO_LBEL PER,
				SEG_PAIS  PAIS,
				SEG_SOCIE SOCI
 WHERE PER.PAIS_COD_PAIS = $P{codigoPaisLbel}
  			AND	 PER.PAIS_COD_PAIS = PAIS.COD_PAIS
  			AND  SOCI.COD_SOCI =  PER.COD_SOCI   		 
            AND (PER.FEC_COPE >= TO_DATE ($P{fechaDesde},'dd/MM/yyyy'))
   			AND (PER.FEC_COPE < TO_DATE ($P{fechaHasta},'dd/MM/yyyy') + 1)
   		    AND (PER.FLA_CANC is null or PER.FLA_CANC <> '1')          
     		AND (PER.FLA_CANC is null or PER.FLA_CANC <> 'A')	 	 			  
 GROUP BY 
          PER.PAIS_COD_PAIS,
          PER.COD_CANA,
          PER.COD_ACCE,
				  PER.COD_SBAC,
					PER.COD_CLIE,
					PER.TIP_DOID,
					PER.NUM_DOID,
					SOCI.VAL_DENO,
					PER.NUM_DOLE,
					PER.SER_DOLE,
					PER.MON_TODL,
					PER.APE_PATE, 
					PER.APE_MATE, 
					PER.VAL_NOMB ) TEMP
 GROUP BY COD_CANA,
           COD_ACCE,
    			 COD_SBAC,
    			 COD_CLIE,
    			 TIP_DOID,
    			 TRIM(NUM_DOID),
    			 SOCIEDAD))	    			 				 	
ORDER BY COD_CANA, COD_ACCE, COD_SBAC, COD_CLIE]]></queryString>


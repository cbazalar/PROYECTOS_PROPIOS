	<queryString><![CDATA[SELECT 
   est.cod_peri,
   est.des_regi,
   est.des_zona,
   est.cod_secc,
   mc.cod_clie,
   TRIM(mc.val_nom1 || ' ' || mc.val_nom2 || ' ' || mc.val_ape1 || ' ' || mc.val_ape2) nom_clie,
   FIN_PKG_GENER.FIN_FN_OBTIE_DIREC_CLIEN(mc.oid_clie)val_dire ,
   FIN_PKG_GENER.FIN_FN_OBTIE_DIREC_CLIEN_REFER(mc.oid_clie) val_refe,
   FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(mc.oid_clie,'TF') num_tele_fijo,
   FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(mc.oid_clie,'TT') num_tele_trab,
   FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(mc.oid_clie,'TM') num_tele_movi,
   FIN_PKG_GENER.FIN_FN_OBTIE_CAMPA_ULTIM_PEDID(mc.oid_clie,oid_peri) camp_ulti_pedi,
   FIN_PKG_GENER.FIN_FN_OBTIE_DESCR_ESTAT_CLIEN(mc.oid_clie) val_desc_esta_clie,
   est.imp_fact_neto imp_fact_neto,
   est.imp_paga imp_recup,
   est.imp_pend imp_pend,
   FIN_PKG_GENER.FIN_FN_OBTIE_DESCR_ESTAT_CLIEN(mc.oid_clie) val_desc_esta_clie,
   CASE
      WHEN est.imp_fact_neto> 0 THEN
         ROUND(est.imp_paga*100/est.imp_fact_neto,2)
      ELSE 0
   END por_recu   
FROM cob_repor_estad_recup_cobra est,
     mae_clien mc
WHERE est.oid_clie=mc.oid_clie 
AND est.cod_pais= $P{codigoPais}
AND est.cod_soci = $P{codigoSociedad}
AND est.cod_peri>= $P{codigoPeriodoInicial}
AND est.cod_peri <= $P{codigoPeriodoFinal}
AND est.imp_pend > 0
$P!{condicion}]]></queryString>


	<parameter name="numeroDias" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("numeroDias")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT MC.COD_CLIE,
       CEL.VAL_TEXT_COMU
  FROM MAE_CLIEN MC,
       (SELECT CLIE_OID_CLIE,
               VAL_TEXT_COMU
          FROM MAE_CLIEN_COMUN TC2,
       MAE_TIPO_COMUN TC
         WHERE TC2.TICM_OID_TIPO_COMU = TC.OID_TIPO_COMU
           AND TC.COD_TIPO_COMU='TM'
           AND TRIM(TC2.VAL_TEXT_COMU) IS NOT NULL) CEL,
       (SELECT DISTINCT MCC.CLIE_OID_CLIE
          FROM CCC_MOVIM_CUENT_CORRI MCC
         WHERE MCC.IMP_PEND > 0
           AND SYSDATE - MCC.FEC_DOCU > $P{numeroDias}) DEUDA
 WHERE MC.OID_CLIE = CEL.CLIE_OID_CLIE
   AND MC.OID_CLIE = DEUDA.CLIE_OID_CLIE]]></queryString>


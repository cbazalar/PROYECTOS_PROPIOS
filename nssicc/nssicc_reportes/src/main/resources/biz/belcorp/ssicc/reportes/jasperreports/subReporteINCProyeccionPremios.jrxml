	<queryString><![CDATA[select  x.NUM_NIVE, 
        x.NUM_PREM, 
        x.COD_SAP, 
        x.DES_PROD, 
        sum(x.NUM_UNID) as NUM_UNID, 
        (select count(*) 
         from ( 
            select num_nive, num_prem, cod_sap, des_prod, clie_oid_clie 
            from INC_REPOR_PROY_PREM 
            group by num_nive, num_prem, cod_sap, des_prod, clie_oid_clie 
         ) t 
        where t.num_nive = x.num_nive 
         and t.num_prem = x.num_prem 
         and t.cod_sap = x.cod_sap 
         and t.des_prod = x.des_prod) pos_gana     
from INC_REPOR_PROY_PREM x
where 1=1 
$P!{condicion}
group by x.NUM_NIVE, 
        x.NUM_PREM, 
        x.COD_SAP, 
        x.DES_PROD 
ORDER BY x.NUM_NIVE,x.NUM_PREM,x.COD_SAP ASC]]></queryString>


	<queryString><![CDATA[SELECT PER.FEC_COPE,
       PER.SER_COPE,
       PER.NUM_COPE,
       PER.TIP_DOID,
       PER.NUM_DOID,
       TRIM(VAL_NOM1)||' '||TRIM(VAL_NOM2)||' '||TRIM(VAL_APE1)||' '||TRIM(VAL_APE2) AS NOM_CONS,
       PER.TIP_DOLE,
       PER.SER_DOLE,
       PER.NUM_DOLE,
       PER.MON_TODL,
       0 AS PERCEP,
       PER.MON_TODL AS DEBE,
       0 AS HABER,
       PER.MON_TODL AS SALDO,
       SOCI.VAL_DENO as SOCIEDAD,
       SOCI.VAL_IDEN_FISC AS SOC_RUC
FROM   PER_PERCE_CONSO PER, 
       MAE_CLIEN CLI,
       SEG_PAIS  PAIS    ,
        SEG_SOCIE SOCI
WHERE  PER.PAIS_COD_PAIS   = $P{codigoPais} AND
       PER.PAIS_COD_PAIS = PAIS.COD_PAIS AND
       CLI.PAIS_OID_PAIS  = PAIS.OID_PAIS  AND
       CLI.COD_CLIE       = PER.COD_CLIE   AND
       PER.FEC_DOLE >= TO_DATE ($P{fechaDesde},'dd/MM/yyyy') AND 
       PER.FEC_DOLE <= TO_DATE ($P{fechaHasta},'dd/MM/yyyy') AND       
       SOCI.COD_SOCI =  PER.COD_SOCI
GROUP BY PER.FEC_COPE,PER.SER_COPE,PER.NUM_COPE,PER.TIP_DOID,PER.NUM_DOID,
       TRIM(VAL_NOM1)||' '||TRIM(VAL_NOM2)||' '||TRIM(VAL_APE1)||' '||TRIM(VAL_APE2), PER.TIP_DOLE,
       PER.SER_DOLE,PER.NUM_DOLE,PER.MON_TODL,0,PER.MON_TODL,0,PER.MON_TODL, SOCI.VAL_DENO,SOCI.VAL_IDEN_FISC]]></queryString>


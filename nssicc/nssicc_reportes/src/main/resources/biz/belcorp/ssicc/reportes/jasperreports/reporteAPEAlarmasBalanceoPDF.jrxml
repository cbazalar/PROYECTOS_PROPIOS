	<queryString><![CDATA[/* FORMATTED ON 2008/10/24 10:17 (FORMATTER PLUS V4.8.5) */
SELECT DISTINCT 
	   PRO.COD_SAP CODIGO,
       (SELECT SEG.VAL_I18N
           FROM GEN_I18N_SICC_PAIS SEG
           WHERE SEG.ATTR_ENTI = 'MAE_PRODU'
       	         AND SEG.VAL_OID = PRO.OID_PROD) DESCRIPCION,
       MAPA.NUM_ANAQ ANAQUEL, PRO.VAL_DIME_ALTO ALTO,
       PRO.VAL_DIME_LARG LARGO, PRO.VAL_DIME_ANCH ANCHO,
       PRO.NUM_CAPA_OPER OPERARIO, PRO.VAL_PESO PESO,
       PRO.VAL_VOLU VOLUMEN, PRO.VAL_COST_ESTD COSTO,
       PRO.NUM_UNID_CAJA_PROD UNIDADES, DET.NUM_UNIDA ESTIMADAS
FROM APE_ASIGN_PRODU_ANAQU_CABEC C INNER JOIN APE_ASIGN_PRODU_ANAQU_DETAL DET 
ON DET.APAC_OID_ASIG_PROD_ANAQ_CABE = C.OID_ASIG_PROD_ANAQ_CABE
AND C.IND_ACTI_FACT ='S'
AND C.PERD_OID_PERI = (SELECT PE.OID_PERI
                          FROM CRA_PERIO PE
                          WHERE PE.VAL_NOMB_PERI LIKE $P{campana})
INNER JOIN MAE_PRODU PRO ON PRO.OID_PROD = DET.PROD_OID_PROD
INNER JOIN APE_MAPA_CENTR_DISTR_DETAL MAPA ON MAPA.OID_MAPA_CENT_DIST_DETA = DET.MCDD_OID_MAPA_CENT_DIST_DETA
INNER JOIN APE_MAPA_CENTR_DISTR_CABEC CABECMAPA 
ON CABECMAPA.OID_MAPA_CENT_DIST_CABE =MAPA.MCDC_OID_MAPA_CENT_DIST_CABE
INNER JOIN APP_CONFI_CENTR_DISTR CENTRO ON CENTRO.COD_CENT_DIST =$P{centro}
AND CABECMAPA.CCDI_OID_CONF_CENT_DIST = CENTRO.OID_CONF_CENT_DIST
INNER JOIN APE_LINEA_ARMAD LINEA ON LINEA.CCDI_OID_CONF_CENT_DIST = CENTRO.OID_CONF_CENT_DIST
AND LINEA.NUM_CODI_LINE = $P{codigoLinea} AND PRO.VAL_VOLU =0]]></queryString>


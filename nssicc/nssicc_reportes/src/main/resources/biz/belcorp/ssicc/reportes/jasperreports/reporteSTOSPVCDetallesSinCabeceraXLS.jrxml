	<queryString><![CDATA[SELECT COD_PAIS,
	   COD_PERI,
	   NUM_LOTE,
	   COD_CLIE,
	   NUM_DOCU,
	   NUM_DETA,
	   TO_CHAR(FEC_DIGI,'DD/MM/YYYY') FEC_DIGI,
	   USU_DIGI
FROM INT_SOLIC_POVEN_DETAL
WHERE (COD_PERI = $P{codigoPeriodo} OR ($P{codigoPeriodo} IS NULL))  
AND (COD_CLIE = $P{codigoCliente} OR ($P{codigoCliente} IS NULL))  
AND (NUM_LOTE = $P{numLote} OR ($P{numLote} IS NULL))
AND ( (FEC_DIGI>=TO_DATE($P{fechaInicio},'DD/MM/YYYY HH24:MI')) or ($P{fechaInicio} is null))
AND ( (FEC_DIGI<=TO_DATE($P{fechaFin},'DD/MM/YYYY HH24:MI')) or ($P{fechaFin} is null))
GROUP BY COD_PAIS,
	     COD_PERI,
	   NUM_LOTE,
	   COD_CLIE,
	   NUM_DOCU,
	   NUM_DETA,
	   FEC_DIGI,
	   USU_DIGI]]></queryString>


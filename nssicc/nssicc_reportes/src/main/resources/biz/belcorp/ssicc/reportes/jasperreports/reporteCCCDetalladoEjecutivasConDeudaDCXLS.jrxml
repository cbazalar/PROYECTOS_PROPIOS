	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[with  ejec as
(
select  sec.CLIE_OID_CLIE, mae.cod_clie, sec.ZZON_OID_ZONA, sec.COD_SECC    
from   zon_secci  sec,
       mae_clien mae
where sec.CLIE_OID_CLIE = mae.OID_CLIE
and   sec.IND_ACTI = 1
),
deuda as
(
SELECT  cc.perd_oid_peri, cc.clie_oid_clie, sum(cc.imp_movi) t_imp_movi,  sum(cc.imp_pend) t_imp_pend
FROM    ccc_movim_cuent_corri cc,
        ejec
WHERE  cc.clie_oid_clie = ejec.clie_oid_clie 
AND    cc.imp_pend <> 0 
GROUP BY   cc.perd_oid_peri, cc.clie_oid_clie      
 ),
datos as  
( select mae.COD_CLIE, sec.CLIE_OID_CLIE, reg.COD_REGI, zon.COD_ZONA, sec.COD_SECC,
       mae.VAL_NOM1 || ' ' || mae.VAL_NOM2 nombres, mae.VAL_APE1 || ' ' || mae.VAL_APE2 apellidos,          
       deuda.perd_oid_peri,  t_imp_movi,  t_imp_pend 
from   ejec  sec,
       mae_clien   mae,
       zon_zona  zon,
       zon_regio  reg,
       deuda
where mae.OID_CLIE = sec.CLIE_OID_CLIE 
and   zon.OID_ZONA = sec.ZZON_OID_ZONA
and   reg.OID_REGI = zon.ZORG_OID_REGI
and   zon.IND_ACTI = 1
and   reg.IND_ACTI = 1 
and   deuda.clie_oid_clie(+) = sec.clie_oid_clie )
select datos.COD_CLIE, datos.CLIE_OID_CLIE, datos.COD_REGI, datos.COD_ZONA, datos.COD_SECC,
    datos.nombres, datos.apellidos,  datos.t_imp_movi, datos.t_imp_pend, cor.COD_PERI 
from  datos,
      cra_perio cam,
      seg_perio_corpo cor
where cam.PERI_OID_PERI =  cor.OID_PERI
and  cam.OID_PERI = datos.perd_oid_peri 
and cor.cod_peri >= $P{codigoPeriodoInicial}
and cor.cod_peri <= $P{codigoPeriodoFinal}
order by  datos.COD_CLIE, cor.cod_peri]]></queryString>


	<queryString><![CDATA[SELECT   inc.oid_bols_falt,
         r.cod_regi, 
         r.des_regi,
         zon.cod_zona, 
         zon.des_zona,
         te.cod_terr, 
         cli.cod_clie,
         trim(cli.val_ape1)||' '||trim(cli.val_ape2)||' '||trim(cli.val_nom1)||' '||trim(cli.val_nom2) as nom_clie, 
         sc.fec_fact,
         pe.val_nomb_peri, 
         ped.cod_grup_proc, 
         sc.oid_soli_cabe, 
         inc.fec_solu,
         produ.cod_sap, 
         pos.val_codi_vent_fict, 
         INT_PKG_RECLA.GEN_FN_DESC_PROD (produ.Oid_Prod) as des_cort,
         inc.num_unid_falt
    FROM inc_bolsa_falta inc,
         ped_solic_cabec sc,
         ped_solic_posic pos,
         zon_zona zon,
         zon_terri te,
         zon_regio r,
         mae_clien cli,
         cra_perio pe,
         ped_grupo_proce ped,
         mae_produ produ
   WHERE (    (sc.oid_soli_cabe = pos.soca_oid_soli_cabe)
          AND (pos.oid_soli_posi = inc.sopo_oid_soli_posi)
          AND (zon.oid_zona = sc.zzon_oid_zona)
          AND (te.oid_terr = sc.terr_oid_terr)
          AND (r.oid_regi = zon.zorg_oid_regi)
          AND (cli.oid_clie = sc.clie_oid_clie)
          AND (pe.oid_peri = sc.perd_oid_peri)
          AND (ped.oid_grup_proc = sc.grpr_oid_grup_proc)	
          AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) >= $P{codigoPeriodoInicio}
          AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) <= $P{codigoPeriodoFin}  	    
          AND (produ.oid_prod = pos.prod_oid_prod)
          AND (inc.fec_solu IS NULL)
		      AND sc.IND_TS_NO_CONSO = 1
          $P!{condicion}
          AND ((ped.COD_GRUP_PROC<>'GP5'  AND sc.FEC_FACT is null)
               OR (ped.COD_GRUP_PROC='GP5'  
                  AND Decode($P{fechaFacturacionInicio}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionInicio}, 'dd/mm/yyyy'))  <=sc.FEC_FACT
                  AND Decode($P{fechaFacturacionFin}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionFin}, 'dd/mm/yyyy'))  >=sc.FEC_FACT))
)
ORDER BY r.cod_regi ASC, zon.cod_zona ASC, te.cod_terr ASC, cli.cod_clie ASC]]></queryString>


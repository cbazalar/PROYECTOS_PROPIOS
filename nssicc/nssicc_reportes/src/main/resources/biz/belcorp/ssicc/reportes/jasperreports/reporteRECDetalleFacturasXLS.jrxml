	<queryString><![CDATA[select DISTINCT 
m.cod_clie AS cliente,
refe.val_nume_soli AS pedido,
refeposic.val_codi_vent AS CUV,
mp.cod_sap,
G.VAL_I18N AS PRODUCTO,
refeposic.num_unid_dema,
refeposic.num_unid_aten,
REFEPOSIC.VAL_PREC_CATA_UNIT_LOCA AS PRECIO_CATALOGO_UNIT,
DECODE(REFEPOSIC.VAL_PREC_CATA_UNIT_LOCA,0,0,REFEPOSIC.VAL_PREC_FACT_UNIT_LOCA) AS PRECIO_FACTURA_UNIT,
nvl(REFEPOSIC.VAL_PORC_DESC,0) AS PORCENTAJE_DTO,
refeposic.num_unid_aten*DECODE(REFEPOSIC.VAL_PREC_CATA_UNIT_LOCA,0,0,REFEPOSIC.VAL_PREC_FACT_UNIT_LOCA)  AS TOTAL_FACTURA,
decode(mp.COD_IND_DENT_CAJA,'F','NO','SI') es_dentro_de_caja,
DECODE(REFEHIJO.MODU_OID_MODU,13,'SI','NO') es_premio,
DECODE(REFEHIJO.MODU_OID_MODU,15,'SI','NO') es_cdr
from ped_solic_cabec c,
ped_solic_cabec refe,
ped_solic_cabec refehijo,
ped_solic_posic refeposic,
mae_clien m,
cra_perio cra,
mae_produ mp,
gen_i18n_sicc_pais g
where c.TSPA_OID_TIPO_SOLI_PAIS in (
SELECT TSP.OID_TIPO_SOLI_PAIS
FROM ped_tipo_solic_pais tsp, ped_tipo_solic  ts
WHERE tsp.tsol_oid_tipo_soli = ts.oid_tipo_soli
and    tsp.tsol_oid_tipo_cons in(
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C52') FROM DUAL
union
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C65') FROM DUAL
union
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C56') FROM DUAL
)
)
and refeposic.soca_oid_soli_cabe = refehijo.oid_soli_cabe
and refehijo.soca_oid_soli_cabe = refe.oid_soli_cabe
and mp.oid_prod = refeposic.prod_oid_prod
and mp.oid_prod = g.val_oid
and g.ATTR_ENTI = 'MAE_PRODU'
and c.soca_oid_docu_refe = refe.oid_soli_cabe
and m.oid_clie = c.clie_oid_clie
and cra.OID_PERI = refe.PERD_OID_PERI
and to_char(c.fec_cron, 'dd/mm/yyyy') = $P{fechaAnulacion}
and c.PERD_OID_PERI = $P{oidPeriodo}
$P!{condicionRegiones}
$P!{codigoConsultora}
]]></queryString>


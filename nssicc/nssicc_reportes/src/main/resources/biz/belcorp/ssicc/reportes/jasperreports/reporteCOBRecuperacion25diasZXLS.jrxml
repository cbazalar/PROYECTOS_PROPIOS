	<queryString><![CDATA[SELECT aa.cod_peri,
            aa.cod_regi,aa.des_regi,
            aa.cod_zona,aa.des_zona,
            sum(aa.imp_fact) Total_Facturado,
            sum(aa.imp_fact_neto) FacturadoNeto,
            sum(aa.cob_dias_21) Cob21,
            round((sum(aa.cob_dias_21)/sum(aa.imp_fact_neto))*100,2) porcentaje21,
            sum(aa.cob_dias_25) Cob25,
            round((sum(aa.cob_dias_25)/sum(aa.imp_fact_neto))*100,2) porcentaje25,
            sum(aa.cob_dias_31) Cob31,
            round((sum(aa.cob_dias_31)/sum(aa.imp_fact_neto))*100,2) porcentaje31
FROM cob_repor_estad_recup_cobra aa
WHERE cod_peri >= $P{codperInicio} and 
      cod_peri <= $P{codperFin} and
      imp_fact_neto <> 0 and
      cod_soci  = $P{codigoSociedad}
      $P!{condicion} 
GROUP BY aa.cod_peri,
         aa.cod_regi,
         aa.des_regi,
         aa.cod_zona,
         aa.des_zona
ORDER BY aa.cod_peri,
         aa.cod_regi,
         aa.des_regi,
         aa.cod_zona,
         aa.des_zona]]></queryString>


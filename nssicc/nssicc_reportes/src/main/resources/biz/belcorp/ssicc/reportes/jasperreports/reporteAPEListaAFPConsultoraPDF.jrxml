	<parameter name="fechaFacturacion" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFacturacion")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoLote" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoLote")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  
distinct   (SELECT seg.val_i18n
          FROM  gen_i18n_sicc_pais seg
         WHERE seg.attr_enti = 'MAE_PRODU'
           AND seg.val_oid = producto.oid_prod)DES_CORT, PRODUCTO.COD_SAP PRODUCTO, Z.COD_ZONA || (SELECT R.COD_REGI FROM ZON_REGIO R WHERE R.OID_REGI = Z.ZORG_OID_REGI AND ROWNUM=1) || (SELECT SE.COD_SECC  FROM ZON_SECCI SE WHERE SE.ZZON_OID_ZONA = Z.OID_ZONA AND ROWNUM=1) || (SELECT (SELECT T.COD_TERR FROM ZON_TERRI T WHERE T.OID_TERR= TER.TERR_OID_TERR AND ROWNUM=1) FROM ZON_TERRI_ADMIN TER WHERE TER.OID_TERR_ADMI= cab.ZTAD_OID_TERR_ADMI AND ROWNUM=1) ZONASA,
	       (SELECT PERIO.VAL_NOMB_PERI FROM  CRA_PERIO PERIO WHERE PERIO.OID_PERI = cab.PERD_OID_PERI) CAMPANAP ,
  		   C.COD_CLIE CLIENTE,
		   TRIM(C.VAL_NOM1) || ' ' || TRIM(VAL_NOM2) || ' ' || TRIM(VAL_APE1) || ' ' || TRIM(VAL_APE2) NOMBRE,
       pos.num_unid_aten NUM_UNID_PROD,
       CAB1.VAL_NUME_SOLI SOLICITUD
 FROM  PED_SOLIC_CABEC CAB INNER JOIN  PED_SOLIC_CABEC CAB1
 ON CAB1.OID_SOLI_CABE= CAB.SOCA_OID_SOLI_CABE INNER JOIN
 PED_SOLIC_POSIC POS 
ON cab.oid_soli_cabe = pos.soca_oid_soli_cabe and  CAB1.FEC_FACT=TO_DATE($P{fechaFacturacion},'dd/mm/yyyy')
INNER JOIN  MAE_PRODU PRODUCTO ON PRODUCTO.OID_PROD = pos.PROD_OID_PROD
AND CAB1.IND_TS_NO_CONSO=0
INNER JOIN  Mae_Clien C ON C.OID_CLIE = cab.CLIE_OID_CLIE
INNER JOIN  ZON_ZONA Z ON Z.OID_ZONA = cab.ZZON_OID_ZONA
AND POS.Ind_Dent_Fuer_Caja_Bols='F'
 and  pos.num_unid_aten>0
group by 
 C.VAL_NOM1, VAL_NOM2, VAL_APE1, VAL_APE2, cab1.VAL_NUME_SOLI,
 C.COD_CLIE,
 producto.oid_prod,COD_SAP, COD_ZONA,Z.ZORG_OID_REGI,Z.OID_ZONA,cab.ZTAD_OID_TERR_ADMI,
 cab.PERD_OID_PERI, pos.num_unid_aten
 order by nombre]]></queryString>


	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPais" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoAnt" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoAnt")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoSig" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoSig")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select  campana , cod_regi, cod_zona, cod_secc, gere Codigo, CL.VAL_NOM1 || ' ' || CL.VAL_APE1 || ' ' || CL.VAL_APE2 Nombres,   Tipo, Secc_Anterior, Motivo Motivo_Baja
from  (
select replace($P{codigoPeriodo},'%','')  campana, g.cod_regi, g.cod_zona, G.COD_SECC, g.gere, 'Alta' Tipo,null  Secc_anterior, null motivo             ---- Campaña de Proceso
 from zon_histo_geren g
where (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo}) >= g.PERD_OID_PERI_DESD               ---- Campaña de Proceso
and ((select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo}) <= g.PERD_OID_PERI_HAST or g.PERD_OID_PERI_HAST is null )       ---- Campaña de Proceso
and length(ua) = 9 and
g.gere not in 
 ( select  gant.gere
 from zon_histo_geren gant
where (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoAnt}) >= gant.PERD_OID_PERI_DESD               -----    Campaña anterior 
and ((select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoAnt}) <= gant.PERD_OID_PERI_HAST or gant.PERD_OID_PERI_HAST is null )     -----    Campaña anterior 
and length(ua) = 9 
 )
UNION
select replace($P{codigoPeriodo},'%','') campana, g2.cod_regi, g2.cod_zona, G2.COD_SECC, g2.gere, 'Baja' Tipo,null  Secc_anterior,  ds.des_tipo_desv  motivo    ---- Campaña de Proceso
 from zon_histo_geren g2, zon_tipo_desvi ds
where (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo}) >= g2.PERD_OID_PERI_DESD     ---- Campaña de Proceso
and G2.PERD_OID_PERI_HAST = (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo})          ---- Campaña de Proceso
and length(ua) = 9 and
G2.IND_DESV_AUTO = ds.cod_tipo_desv
and g2.gere not in 
 ( select  gant.gere
 from zon_histo_geren gant
where (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoSig}) >= gant.PERD_OID_PERI_DESD              -----    Campaña Siguiente
and ((select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoSig}) <= gant.PERD_OID_PERI_HAST or gant.PERD_OID_PERI_HAST is null )               -----    Campaña Siguiente
and length(ua) = 9 
 )
 UNION
 select replace($P{codigoPeriodo},'%','') campana, g2.cod_regi, g2.cod_zona, G2.COD_SECC, g2.gere, 'Rotación' Tipo, GANT.COD_ZONA || GANT.COD_SECC  Secc_anterior,  null  motivo            ---- Campaña de Proceso
 from zon_histo_geren g2, zon_histo_geren gant
where (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo}) >= g2.PERD_OID_PERI_DESD                 ---- Campaña de Proceso
and ((select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodo}) <= g2.PERD_OID_PERI_HAST or g2.PERD_OID_PERI_HAST is null )                 ---- Campaña de Proceso
and length(g2.ua) = 9 
and g2.gere  =  gant.gere
and  (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoAnt}) >= gant.PERD_OID_PERI_DESD                 -----    Campaña anterior 
and Gant.PERD_OID_PERI_HAST = (select oid_peri from cra_perio where val_nomb_peri like $P{codigoPeriodoAnt})                    -----    Campaña anterior 
and length(gant.ua) = 9 
 ) , mae_clien cl
 where gere = cl.cod_clie
order by cod_regi, cod_zona, cod_secc
]]></queryString>


	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT $P{codigoPeriodo} COD_PERI,
       REG.DES_REGI COD_REGI,
       ZON.COD_ZONA COD_ZONA,
       TO_CHAR(SOL.FEC_FACT,'DD/MM/YYYY') FECHA,
       COUNT(*) CANTIDAD
  FROM PED_SOLIC_CABEC CON,
       PED_SOLIC_CABEC SOL,
       ZON_ZONA ZON,
       ZON_REGIO REG
 WHERE CON.FEC_FACT IS NOT NULL
   AND CON.OID_SOLI_CABE = SOL.SOCA_OID_SOLI_CABE
   AND SOL.TSPA_OID_TIPO_SOLI_PAIS = fin_pkg_gener.FIN_FN_OBTIE_OID_SOLIC_PAIS('SOC') 
   AND SOL.perd_oid_peri = (SELECT cp.oid_peri 
                              FROM cra_perio cp,
                                   seg_perio_corpo sp 
                             WHERE cp.peri_oid_peri = sp.oid_peri 
                               AND sp.cod_peri=$P{codigoPeriodo})
   AND SOL.ZZON_OID_ZONA = ZON.OID_ZONA  
   AND ZON.ZORG_OID_REGI = REG.OID_REGI
 GROUP BY REG.DES_REGI, ZON.COD_ZONA, TO_CHAR(SOL.FEC_FACT,'DD/MM/YYYY')
 ORDER BY 3, 2]]></queryString>


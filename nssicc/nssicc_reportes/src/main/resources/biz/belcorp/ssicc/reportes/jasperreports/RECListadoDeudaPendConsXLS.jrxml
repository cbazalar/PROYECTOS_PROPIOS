	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPeriodoInicio" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicio")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFin" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFin")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaFacturacion" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFacturacion")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicion" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicion")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT   r.des_regi, 
         zon.des_zona, 
         te.cod_terr, 
         cli.cod_clie, 
         cli.val_ape1||' '||cli.val_ape2||' '|| cli.val_nom1||' '|| cli.val_nom2 as nom_clie,
         per.fec_fact,per.NUM_SOLI_CONS,
         SUM(per.imp_cuot) TOTAL_CUOTAS, SUM(per.imp_cuot_pend) TOTAL_CUOTAS_PDTE,
         SUM(per.imp_inaf_perc) TOTAL_PERC_INAF, SUM(per.imp_pend_inaf_perc) TOTAL_PERC_INAF_PDTE, 
		     SUM(per.imp_afec_perc) TOTAL_PERC_AFEC, SUM(per.imp_pend_afec_perc) TOTAL_PERC_AFEC_PDTE
    FROM per_cuent_corri_docle per,
         mae_clien_unida_admin uni_adm,
         mae_clien cli,
         zon_terri_admin ter_adm,
         zon_terri te,
         zon_secci secc,
         zon_zona zon,
         zon_regio r,
	 (SELECT * FROM cra_perio where oid_peri= gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoInicio})) peri_i,
 		 (SELECT * FROM cra_perio where oid_peri= gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFin})) peri_f
     WHERE (    (cli.oid_clie = uni_adm.clie_oid_clie)
          AND (cli.cod_clie = per.cod_clie)
          AND (ter_adm.oid_terr_admi = uni_adm.ztad_oid_terr_admi)
          AND (te.oid_terr = ter_adm.terr_oid_terr)
          AND (secc.oid_secc = ter_adm.zscc_oid_secc)
          AND (zon.oid_zona = secc.zzon_oid_zona)
          AND (r.oid_regi = zon.zorg_oid_regi)
	$P!{condicion}
  AND per.fec_fact = Decode($P{fechaFacturacion}, null,per.fec_fact, '',per.fec_fact,$P{fechaFacturacion} )
		  AND (peri_i.fec_inic <= per.fec_fact)
		  AND (peri_f.fec_fina >= per.fec_fact)
         )
GROUP BY r.des_regi, 
         zon.des_zona, te.cod_terr, cli.cod_clie,cli.val_ape1,
         cli.val_ape2, cli.val_nom1, cli.val_nom2, per.fec_fact,per.NUM_SOLI_CONS
ORDER BY r.des_regi ASC, zon.des_zona ASC, te.cod_terr ASC, cli.cod_clie ASC]]></queryString>


	<queryString><![CDATA[SELECT REG.COD_REGI AS REGION,
ZON.COD_ZONA AS ZONA,
CLI.COD_CLIE AS CONSULTORA,
(select num_docu_iden from mae_clien_ident iden where iden.CLIE_OID_CLIE=sol.clie_oid_clie
and tdoc_oid_tipo_docu=2001) as CEDULA, 
CLI.VAL_APE1||' '||CLI.VAL_APE2||', '||CLI.VAL_NOM1||' '||CLI.VAL_NOM2 AS NOMBRES,
(select val_text_comu from mae_clien_comun com where com.CLIE_OID_CLIE=sol.clie_oid_clie
and ticm_oid_tipo_comu=1) as Telefono, 
(select val_text_comu from mae_clien_comun com where com.CLIE_OID_CLIE=sol.clie_oid_clie
and ticm_oid_tipo_comu=6) as Celular,
( SELECT (SELECT des_geog
               FROM zon_valor_estru_geopo
              WHERE orde_1 =
                       (SELECT orde_1
                          FROM zon_valor_estru_geopo
                         WHERE OID_VALO_ESTR_GEOP = d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_2 IS NULL
                    AND orde_3 IS NULL
                    AND orde_4 IS NULL)
         || ' '
         || (SELECT des_geog
               FROM zon_valor_estru_geopo
              WHERE orde_1 =
                       (SELECT orde_1
                          FROM zon_valor_estru_geopo
                         WHERE OID_VALO_ESTR_GEOP = d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_2 =
                           (SELECT orde_2
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_3 IS NULL
                    AND orde_4 IS NULL)
         || ' '
         || (SELECT des_geog
               FROM zon_valor_estru_geopo
              WHERE orde_1 =
                       (SELECT orde_1
                          FROM zon_valor_estru_geopo
                         WHERE OID_VALO_ESTR_GEOP = d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_2 =
                           (SELECT orde_2
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_3 =
                           (SELECT orde_3
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_4 IS NULL)
         || ' '
         || (SELECT des_geog
               FROM zon_valor_estru_geopo
              WHERE orde_1 =
                       (SELECT orde_1
                          FROM zon_valor_estru_geopo
                         WHERE OID_VALO_ESTR_GEOP = d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_2 =
                           (SELECT orde_2
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_3 =
                           (SELECT orde_3
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP)
                    AND orde_4 =
                           (SELECT orde_4
                              FROM zon_valor_estru_geopo
                             WHERE OID_VALO_ESTR_GEOP =
                                      d.VEPO_OID_VALO_ESTR_GEOP))
            UBIGEO
    FROM zon_terri_admin c,
         mae_clien_unida_admin ua,
         zon_terri d,
         zon_secci e,
         zon_zona f,
         zon_regio g
   WHERE cli.oid_clie = ua.clie_oid_clie
         AND ua.ind_acti = 1
         AND ua.ZTAD_OID_TERR_ADMI = c.OID_TERR_ADMI
         AND c.ZSCC_OID_SECC = e.OID_SECC
         AND c.TERR_OID_TERR = d.OID_TERR
         AND e.ZZON_OID_ZONA = f.OID_ZONA
         AND f.ZORG_OID_REGI = g.OID_REGI) as UBIGEO,

PRD.COD_SAP AS PRODUCTO,
POS.VAL_CODI_VENT AS COD_VTA,
GEN.VAL_I18N AS DESCRIPCION,
(POS.NUM_UNID_POR_ATEN-POS.NUM_UNID_ATEN) AS FALTANTES,
PER.VAL_NOMB_PERI AS CAMPANIA,
TO_CHAR(SOL.FEC_FACT,'YYYY-MM-DD') AS FECHA_FACT
FROM PED_SOLIC_CABEC SOL,PED_SOLIC_POSIC POS,MAE_PRODU PRD,
GEN_I18N_SICC_PAIS GEN,MAE_CLIEN CLI,CRA_PERIO PER,
ZON_ZONA ZON,ZON_REGIO REG,PED_TIPO_SOLIC_PAIS TSPA,PED_TIPO_SOLIC TSOL
WHERE PER.VAL_NOMB_PERI=$P{codigoPeriodo}
AND SOL.PERD_OID_PERI=PER.OID_PERI
AND SOL.TSPA_OID_TIPO_SOLI_PAIS=TSPA.OID_TIPO_SOLI_PAIS
AND TSPA.TSOL_OID_TIPO_SOLI=TSOL.OID_TIPO_SOLI
AND TSOL.COD_TIPO_SOLI='SOC' -- ORDEN DE COMPRA
AND SOL.FEC_FACT IS NOT NULL
AND SOL.OID_SOLI_CABE=POS.SOCA_OID_SOLI_CABE
AND PRD.OID_PROD=POS.PROD_OID_PROD
AND GEN.ATTR_ENTI='MAE_PRODU'
AND GEN.VAL_OID=PRD.OID_PROD
AND SOL.CLIE_OID_CLIE=CLI.OID_CLIE
AND POS.NUM_UNID_POR_ATEN>POS.NUM_UNID_ATEN
AND SOL.ZZON_OID_ZONA=ZON.OID_ZONA
AND ZON.ZORG_OID_REGI=REG.OID_REGI
$P!{codigoRegion}]]></queryString>


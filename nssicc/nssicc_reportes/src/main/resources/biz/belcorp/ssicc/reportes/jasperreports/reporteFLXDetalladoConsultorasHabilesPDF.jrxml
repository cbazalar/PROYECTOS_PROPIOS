	</parameter>
	<parameter name="condicionZona" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionZona")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoCampana" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoCampana")]]></defaultValueExpression>
	</parameter>
	<parameter name="indClienteCedula" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("indClienteCedula")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
   zz.cod_zona,
   zs.cod_secc,
   (select case
         WHEN min(h.cod_peri_comu) = $P{codigoCampana} THEN
          '1 - Consultoras invitadas en Campaña ' || $P{codigoCampana}
         WHEN cup_pkg_prog_nuevas.gen_fn_dev_nsgte_campa(min(h.cod_peri_comu),
                                                                     1) =
              $P{codigoCampana} THEN
          '2 - Consultoras invitadas en Campaña ' || cup_pkg_prog_nuevas.gen_fn_dev_nsgte_campa(ch.cod_peri_comu,-1)
         ELSE
          '3 - Consultoras Invitadas en Campañas Anteriores a ' || cup_pkg_prog_nuevas.gen_fn_dev_nsgte_campa(ch.cod_peri_comu,-1)
       END 
  from Flx_Consu_Habil_Flexi h
 where h.cod_clie = mc.cod_clie) numInvi,  
   CASE
    WHEN $P{indClienteCedula} = 1 THEN 
       (SELECT num_docu_iden 
	  FROM mae_clien_ident 
	 WHERE clie_oid_clie = mc.oid_clie 
	   AND val_iden_docu_prin = 1)
    ELSE ch.cod_clie 
   END cod_clie,
   mc.val_nom1 || ' ' || mc.val_nom2 || ' ' || mc.val_ape1 || ' ' || mc.val_ape2 nom_clie,
   ch.val_pedi_base,
   ch.val_line_cred,
   CASE
    WHEN ch.ind_acti = 1 THEN 0
     ELSE 1
   END ind_acti,
   ch.ind_acti ind_habil,
   DECODE(ch.ind_acti,1,'Si','No') ind_firm_cart,
   fcc.des_cali,
         nvl((select min(cfc.cod_peri)
            from flx_cuota_flexi_factu_cabec cfc
           where cfc.cod_clie = mc.cod_clie
           group by cfc.cod_clie), '') min_flexi,
         nvl((select max(cfc.cod_peri)
            from flx_cuota_flexi_factu_cabec cfc
           where cfc.cod_clie = mc.cod_clie
           group by cfc.cod_clie), '') max_flexi,
    case
      when ch.ind_acti = 0 and ch.ind_canc = 0 then 'Inactiva'
      when ch.ind_acti = 1 and ch.ind_canc = 0 then 'Activa'   
      when ch.ind_acti = 0 and ch.ind_canc = 1 then 'Cancelada' 
       else ''
   end estatus 
 FROM 
   flx_consu_habil_flexi ch,
   flx_calif_compo       fcc,
  mae_clien mc,
  mae_clien_unida_admin mcua,
  zon_terri_admin zta,
  zon_secci zs,
  zon_zona zz,
  zon_regio zr
 WHERE ch.cod_clie = mc.cod_clie   
   AND mc.oid_clie = mcua.clie_oid_clie
   AND mcua.ind_acti = 1
   AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
   AND zta.zscc_oid_secc = zs.oid_secc
   AND zs.zzon_oid_zona = zz.oid_zona
   AND zz.zorg_oid_regi = zr.oid_regi
   AND ch.cod_peri_comu =  $P{codigoCampana}
   AND ch.ind_habi = 1 
   AND ch.ind_cali_comp = fcc.cod_cali
   AND exists (select null from mae_clien_datos_adici mad
               where mad.clie_oid_clie = mc.oid_clie
               and mad.esta_oid_esta_clie not in (5,7) ) 
   $P!{condicionRegion}
   $P!{condicionZona}
   $P!{condicionSeccion} 
 ORDER BY zr.cod_regi , zz.cod_zona, zs.cod_secc, numInvi, mc.val_nom1 || ' ' || mc.val_nom2 || ' ' || mc.val_ape1 || ' ' || mc.val_ape2]]></queryString>


	<queryString><![CDATA[SELECT a.cod_clie, 
       a.nom_clie NOMBRE, 
       a.cod_peri CAMPANA, 
       a.cod_regi REGION, 
       a.cod_zona ZONA, 
       a.cod_terr TERRITORIO, 
       a.val_mont_pedi MTO_PED, 
       a.val_sald_deud MTO_DEUDA,
       (SELECT d.des_larg_mens 
          FROM sto_histo_detal_docum_excep c, sto_mensa_valid d 
         WHERE c.sec_nume_docu = a.sec_nume_docu 
           AND c.ind_apro = 0 
           AND c.cod_vali = d.cod_vali 
           AND rownum = 1) MOTIVO_RECHAZO
  FROM ped_histo_solic_conso_cabec a, 
       sto_histo_docum_digit b
 WHERE A.SEC_NUME_DOCU = B.SEC_NUME_DOCU 
   AND A.COD_PERI = $P{codigoPeriodo}
   AND A.IND_OCS_PROC = 0
 ORDER BY A.COD_ZONA]]></queryString>


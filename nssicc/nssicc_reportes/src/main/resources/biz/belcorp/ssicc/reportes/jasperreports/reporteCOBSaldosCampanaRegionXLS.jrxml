	<queryString><![CDATA[SELECT   b.val_nomb_peri CAMPANNA, 
  reg.cod_regi REGION, 
  MAX(reg.des_regi) DE_REGION,
  SUM (c.imp_movi) VENTA, 
  SUM (c.imp_pend) SALDO
FROM ccc_movim_cuent_corri c,
  cra_perio b,
  seg_perio_corpo cor,
  mae_clien_unida_admin ua,
  zon_terri_admin za,
  zon_secci sec,
  zon_regio reg,
  zon_zona zon,
  seg_marca mar,
  seg_canal can
WHERE c.clie_oid_clie = ua.clie_oid_clie
  AND ua.ztad_oid_terr_admi = za.oid_terr_admi
  AND za.zscc_oid_secc = sec.oid_secc
  AND sec.zzon_oid_zona = zon.oid_zona
  AND zon.zorg_oid_regi = reg.oid_regi
  AND ua.ind_acti = 1
  AND can.cod_cana = $P{codigoCanal}  /* codigo de canal seleccionado por el usuario*/
  AND mar.cod_marc = $P{codigoMarca}  /* codigo de marca seleccionado por el usuario */
  AND c.perd_oid_peri = b.oid_peri
  AND b.peri_oid_peri = cor.oid_peri
  AND c.imp_pend <> 0
  AND za.pais_oid_pais = $P{oidPais}
  AND za.marc_oid_marc = mar.oid_marc
  AND za.cana_oid_cana = can.oid_cana
  AND c.subp_oid_subp_crea IN (
         SELECT subp.oid_subp
              FROM ccc_subpr subp, ccc_proce pro
             WHERE pro.oid_proc = subp.ccpr_oid_proc
               AND (   pro.cod_proc = 'CCC001' AND subp.cod_subp = 1
                    OR pro.cod_proc = 'CCC003' AND subp.cod_subp = 4
                    OR pro.cod_proc = 'CCC006' AND subp.cod_subp = 1
                    OR pro.cod_proc = 'CCCEN1' AND subp.cod_subp = 1
                    OR pro.cod_proc = 'CCCEP1' AND subp.cod_subp = 1
                    OR pro.cod_proc = 'CCCEP2' AND subp.cod_subp = 2
                    OR pro.cod_proc = 'CCCEV1' AND subp.cod_subp = 1
                    OR pro.cod_proc = 'CCCEV2' AND subp.cod_subp = 2
                   ))
     /* rango de campaÃ±as solicitado por el usuario */
     AND (cor.cod_peri >= $P{periodoDesde} AND cor.cod_peri <= $P{periodoHasta})
GROUP BY b.val_nomb_peri, reg.cod_regi]]></queryString>


	<queryString><![CDATA[with temp as
(
select zorg.cod_regi,
       zorg.des_regi,
       zzon.cod_zona,
       zzon.des_zona,
       zscc.cod_secc,
       zscc.des_secci,
       lide.gere cod_lide,
       lide.val_ape1 || ' ' || lide.val_ape2 || ' ' || lide.val_nom1 || ' ' || val_nom2 nomb_lide,
       (
        select  LN.DES_NIVE
          from lec_lider_nivel lln, lec_nivel ln
         where lln.lniv_cod_nive = ln.cod_nive
           and lln.cod_lide = lide.gere
           and lln.lpro_cod_prog = $P{codigoPrograma}
           and $P{campanyaProceso} >= lln.cam_inic
           and ($P{campanyaProceso} <= lln.cam_fin or lln.cam_fin is null)
           and lln.ind_tipo_nive = 'R'
       ) cod_nive,
       (
        select lsop.num_pedi_obje_fina
          from lec_lider_secci_objet_pedid lsop
         where lsop.lpro_cod_pais = $P{codigoPais}
           and lsop.cam_obje = $P{campanyaProceso}
           and lsop.cod_regi = lide.cod_regi
           and lsop.cod_zona = lide.cod_zona
           and lsop.cod_secc = lide.cod_secc
       ) val_meta_pedi,
       (
        select num_pedi
          from lec_lider_secci_resul lisr
         where lisr.pais_cod_pais = $P{codigoPais}
           and lisr.lpro_cod_prog = $P{codigoPrograma}
           and lisr.cod_regi = zorg.cod_regi
           and lisr.cod_zona = zzon.cod_zona
           and lisr.cod_secc = zscc.cod_secc
           and lisr.cod_lide = lide.gere
           and lisr.cam_resu = $P{campanyaProceso}
       ) val_pedi_real,
       0 val_pedi_falt, -- Calcular en el reporte
       0 val_porc_logr_pedi, -- Calcular en el reporte
       (
        select num_pedi_cons
          from lec_lider_secci_resul lisr
         where lisr.pais_cod_pais = $P{codigoPais}
           and lisr.lpro_cod_prog = $P{codigoPrograma}
           and lisr.cod_regi = zorg.cod_regi
           and lisr.cod_zona = zzon.cod_zona
           and lisr.cod_secc = zscc.cod_secc
           and lisr.cod_lide = lide.gere
           and lisr.cam_resu = $P{campanyaProceso}
       ) val_nume_pedi_cons,
       (
        select num_pedi_ncon
          from lec_lider_secci_resul lisr
         where lisr.pais_cod_pais = $P{codigoPais}
           and lisr.lpro_cod_prog = $P{codigoPrograma}
           and lisr.cod_regi = zorg.cod_regi
           and lisr.cod_zona = zzon.cod_zona
           and lisr.cod_secc = zscc.cod_secc
           and lisr.cod_lide = lide.gere
           and lisr.cam_resu = $P{campanyaProceso}
       ) val_nume_pedi_ncon,
  --agregue
       (
        select LPN.NUM_META_INGR
          from lec_lider_nivel lln,
                  lec_progr_nivel lpn
           where  lln.cod_lide = lide.gere
           and lln.lpro_cod_prog = $P{codigoPrograma}
           and $P{campanyaProceso} between lln.cam_inic and nvl(lln.cam_fin,$P{campanyaProceso})
           and lln.ind_tipo_nive = 'R'
           and lln.lniv_cod_nive = LPN.LNIV_COD_NIVE (+)
           and LLN.LPRO_COD_PROG = LPN.LPRO_COD_PROG (+)
       ) val_meta_Ingreso,
       (
        select num_ingr_secc
          from lec_lider_secci_resul lisr
         where lisr.pais_cod_pais = $P{codigoPais}
           and lisr.lpro_cod_prog = $P{codigoPrograma}
           and lisr.cod_regi = lide.cod_regi
           and lisr.cod_zona = lide.cod_zona
           and lisr.cod_secc = lide.cod_secc
           and lisr.cod_lide = lide.gere
           and lisr.cam_resu = $P{campanyaProceso}
       )  val_real_Ingreso, 
       ---  Ingres Faltantes
       -- FIN agregue
       ------  INICIO  RCR  PER-SICC-2015-0548 
              (
        select LPN.NUM_META_CAPI
          from LEC_lider_nivel lln,
                  LEC_progr_nivel lpn
           where  lln.cod_lide = lide.gere
           and lln.lpro_cod_prog = $P{codigoPrograma}
           and $P{campanyaProceso} between lln.cam_inic and nvl(lln.cam_fin, $P{campanyaProceso})
           and lln.ind_tipo_nive = 'R'
           and lln.lniv_cod_nive = LPN.LNIV_COD_NIVE (+)
           and LLN.LPRO_COD_PROG = LPN.LPRO_COD_PROG (+)
       ) val_meta_capital,
       (
        select num_ingr_secc + LISR.NUM_REIN_SECC - LISR.NUM_EGRE_SECC
          from LEC_lider_secci_resul lisr
         where lisr.pais_cod_pais = $P{codigoPais}
           and lisr.lpro_cod_prog = $P{codigoPrograma}
           and lisr.cod_regi = lide.cod_regi
           and lisr.cod_zona = lide.cod_zona
           and lisr.cod_secc = lide.cod_secc
           and lisr.cod_lide = lide.gere
           and lisr.cam_resu = $P{campanyaProceso}
       )  val_real_capital, 
----------  FIN RCR
       (
        select val_rete_secc
          from lec_lider_secci_resul_bono lrbo
         where lrbo.pais_cod_pais = $P{codigoPais}
           and lrbo.lpro_cod_prog = $P{codigoPrograma}
           and lrbo.cod_regi = zorg.cod_regi
           and lrbo.cod_zona = zzon.cod_zona
           and lrbo.cod_secc = zscc.cod_secc
           and lrbo.cod_lide = lide.gere
           and lrbo.cam_resu = $P{campanyaProceso}
           and ltbo_cod_tipo_bono = '07'
       ) val_nume_cons,
       (
        select lcot.val_porc_cobr
          from lec_progr_ambit_geogr lpag,
               lec_progr_cobra_objet_tramo lcot
         where 1=1
           and lpag.pais_cod_pais = lcot.pais_cod_pais
           and lpag.lpro_cod_prog = lcot.lpro_cod_prog
           and lpag.ltug_cod_tipo_uso_geog = lcot.ltug_cod_tipo_uso_geog
           and lpag.sec_ambi_geog = lcot.lpag_sec_ambi_geog
           --
           and lpag.pais_cod_pais = $P{codigoPais}
           and lpag.lpro_cod_prog = $P{codigoPrograma}
           and lpag.ltug_cod_tipo_uso_geog = '01'
           and nvl(lpag.cod_regi,zorg.cod_regi) = zorg.cod_regi
           and nvl(lpag.cod_zona,zzon.cod_zona) = zzon.cod_zona
           --
           and lcot.lpct_cod_tram = 1
           and lcot.ind_acti = '1' -- Nuevo
       )val_meta_cobr,
       (
        select val_porc_recu_real
          from lec_lider_secci_recup_tramo lrrt -- val_porc_recu_real
         where lrrt.pais_cod_pais = $P{codigoPais}
           and lrrt.lpro_cod_prog = $P{codigoPrograma2}
           and lrrt.cod_regi = zorg.cod_regi
           and lrrt.cod_zona = zzon.cod_zona
           and lrrt.cod_secc = zscc.cod_secc
           and lrrt.cod_lide = lide.gere
           and lrrt.cam_recu = $P{campanyaRecaudo}
           and lrrt.lpct_cod_tram = 1 -- validar el valor cuando tiene mas de 1 tramo
       ) val_real_cobr,
       (
        select val_mont_reca_pedi_cons
          from lec_lider_secci_recup_tramo lrrt
         where lrrt.pais_cod_pais = $P{codigoPais}
           and lrrt.lpro_cod_prog = $P{codigoPrograma2}
           and lrrt.cod_regi = zorg.cod_regi
           and lrrt.cod_zona = zzon.cod_zona
           and lrrt.cod_secc = zscc.cod_secc
           and lrrt.cod_lide = lide.gere
           and lrrt.cam_recu = $P{campanyaRecaudo}
           and lrrt.lpct_cod_tram = 1 -- validar el valor cuando tiene mas de 1 tramo
       ) val_neto_recu_pedi_cons,
       (
        select val_mont_reca_pedi_ncon
          from lec_lider_secci_recup_tramo lrrt
         where lrrt.pais_cod_pais =  $P{codigoPais}
           and lrrt.lpro_cod_prog = $P{codigoPrograma2}
           and lrrt.cod_regi = zorg.cod_regi
           and lrrt.cod_zona = zzon.cod_zona
           and lrrt.cod_secc = zscc.cod_secc
           and lrrt.cod_lide = lide.gere
           and lrrt.cam_recu = $P{campanyaRecaudo}
           and lrrt.lpct_cod_tram = 1 -- validar el valor cuando tiene mas de 1 tramo
       ) val_neto_recu_pedi_ncon,
       (
        select val_mont_recu
          from lec_lider_secci_recup_tramo lrrt
         where lrrt.pais_cod_pais = $P{codigoPais}
           and lrrt.lpro_cod_prog = $P{codigoPrograma2}
           and lrrt.cod_regi = zorg.cod_regi
           and lrrt.cod_zona = zzon.cod_zona
           and lrrt.cod_secc = zscc.cod_secc
           and lrrt.cod_lide = lide.gere
           and lrrt.cam_recu = $P{campanyaRecaudo}
           and lrrt.lpct_cod_tram = 1 -- validar el valor cuando tiene mas de 1 tramo
       ) val_neto_camp  
  from zon_secci zscc,
       zon_zona zzon,
       zon_regio zorg,
       cra_perio perd,
       seg_perio_corpo peri,
       cra_perio perd_b,
       seg_perio_corpo peri_b,
       (
         select gere.*, clie.val_ape1, clie.val_ape2, clie.val_nom1, clie.val_nom2
           from zon_histo_geren gere,
                cra_perio perd_c,
                seg_perio_corpo peri_c,
                cra_perio perd_d,
                seg_perio_corpo peri_d,
                mae_clien clie
          where gere.gere = clie.cod_clie
            and gere.perd_oid_peri_desd = perd_c.oid_peri
            and perd_c.peri_oid_peri = peri_c.oid_peri
            and nvl(gere.perd_oid_peri_hast, GEN_PKG_GENER.gen_fn_devuelve_id_cra_perio2($P{campanyaProceso})) = perd_d.oid_peri
            and perd_d.peri_oid_peri = peri_d.oid_peri
            and gere.cod_regi is not null
            and gere.cod_zona is not null
            and gere.cod_secc is not null
            and $P{campanyaProceso} between peri_c.cod_peri and peri_d.cod_peri
       ) lide
 where 1=1
   and zscc.zzon_oid_zona = zzon.oid_zona
   and zzon.zorg_oid_regi = zorg.oid_regi
   and zscc.perd_oid_peri_inic = perd.oid_peri
   and perd.peri_oid_peri = peri.oid_peri
   and nvl(zscc.perd_oid_peri_fina,GEN_PKG_GENER.gen_fn_devuelve_id_cra_perio2($P{campanyaProceso})) = perd_b.oid_peri
   and perd_b.peri_oid_peri = peri_b.oid_peri
   and $P{campanyaProceso} between peri.cod_peri and peri_b.cod_peri
   and zorg.cod_regi || zzon.cod_zona || zscc.cod_secc = lide.cod_regi || lide.cod_zona || lide.cod_secc 
   $P!{condicion} 
   $P!{condicionZonaCorreo}
  )

select cod_regi, 
       des_regi, 
       cod_zona, 
       des_zona, 
       cod_secc, 
       des_secci, 
       cod_lide, 
       nomb_lide, 
       cod_nive,
       val_meta_pedi, 
       val_pedi_real, 
       CASE WHEN val_meta_pedi > val_pedi_real THEN TO_CHAR((val_pedi_real - val_meta_pedi)) 
            WHEN val_meta_pedi < val_pedi_real THEN '+'||TO_CHAR((val_pedi_real - val_meta_pedi)) END val_pedi_falt,
       round((val_pedi_real/val_meta_pedi)*100,2) val_porc_logr_pedi,
       val_nume_pedi_cons, 
       val_nume_pedi_ncon,
  -- agregue
       val_meta_Ingreso,
       val_real_ingreso,
       nvl(val_real_ingreso,0)  -  nvl(val_meta_Ingreso,0)  val_dife_ingreso,
       CASE WHEN nvl(val_real_ingreso,0)  -  nvl(val_meta_Ingreso,0) > 0 THEN
         '+' || TO_CHAR(nvl(val_real_ingreso,0)  -  nvl(val_meta_Ingreso,0))
       else
          TO_CHAR(nvl(val_real_ingreso,0)  -  nvl(val_meta_Ingreso,0))
       end val_dife_ingreso02,
       
       case WHEN  val_meta_ingreso > 0 then 
       ROUND(((val_real_ingreso/val_meta_ingreso)*100),0)
       else 0
       END   val_porc_logr_Ingre,       
       -- FIN agregue      
       ---- INICIO  RCR  PER-SICC-2015-0548
       val_meta_capital,
       val_real_capital,
       nvl(val_real_capital,0)  -  nvl(val_meta_capital,0)  val_dife_capital,
       CASE WHEN nvl(val_real_capital,0)  -  nvl(val_meta_capital,0) > 0 THEN
         '+' || TO_CHAR(nvl(val_real_capital,0)  -  nvl(val_meta_capital,0))
       else
          TO_CHAR(nvl(val_real_capital,0)  -  nvl(val_meta_capital,0))
       end val_dife_capital02,       
       case WHEN  val_meta_capital > 0 then 
       ROUND(((val_real_capital/val_meta_capital)*100),0)
       else 0
       END   val_porc_logr_capital,       
       --- FIN RCR 
       val_nume_cons,
       val_meta_cobr, 
       val_real_cobr, 
       round(val_neto_recu_pedi_cons,2) val_neto_recu_pedi_cons , 
       round(val_neto_recu_pedi_ncon,2) val_neto_recu_pedi_ncon, 
       round(val_neto_camp,2) val_neto_camp
  from temp
 order by temp.cod_regi, temp.cod_zona, temp.cod_secc]]></queryString>


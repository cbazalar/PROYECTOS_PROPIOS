	</parameter>
	<parameter name="fechaHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT c.des_cc BANCO, COUNT(*) CANTIDAD, sum(b.val_pago) IMPORTE,a.tior_tipo_orig_dato codigo,per.des_tipo_orig_dato Descipcion_Dato
FROM per_movim_banca_cabec a,
     per_movim_banca_detal b,
     ccc_cuent_corri_banca c,
     per_tipo_orige_datos per
WHERE a.num_lote_inte = b.moca_num_lote_inte
AND a.cod_ccba = c.cod_cc
AND a.tior_tipo_orig_dato = b.tior_tipo_orig_dato
AND b.fec_proc >= to_date($P{fechaDesde},'DD/MM/YYYY')
AND b.fec_proc <= to_date($P{fechaHasta},'DD/MM/YYYY')
and  per.cod_tipo_orig_dato=a.tior_tipo_orig_dato
GROUP BY c.des_cc,a.tior_tipo_orig_dato,per.des_tipo_orig_dato]]></queryString>


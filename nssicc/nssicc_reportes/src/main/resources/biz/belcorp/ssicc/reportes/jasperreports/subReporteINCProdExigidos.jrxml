	<parameter name="parameter" isForPrompting="true" class="java.lang.Double"/>
	<parameter name="plantilla" isForPrompting="false" class="java.lang.Double"/>
	<queryString><![CDATA[select OID_PROD_EXIG,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.MAPR_OID_MARC_PROD,'SEG_MARCA_PRODU','es') as des_marca_exigi,      
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.Nego_Oid_Nego,'MAE_NEGOC','es') as des_negoc_exigi,            
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.UNEG_OID_UNID_NEGO,'MAE_UNIDA_NEGOC','es') as des_unid_negoc_exigi,                           
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.Tofe_Oid_Tipo_Ofer,'PRE_TIPO_OFERT','es') as des_tipo_ofert_exigi,      
       decode(mae_produ.oid_prod,null,'',mae_produ.cod_sap || ' - ') || GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.PROD_OID_PROD,'MAE_PRODU','es') as des_prod_exigi,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.PERD_OID_PERI_DESD,'CRA_PERIO','es') as des_peri_desd_exigi,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(inc_produ_exigi.Perd_Oid_Peri_Hast,'CRA_PERIO','es') as des_peri_exigi,
       inc_produ_exigi.num_unid_exig,
       inc_produ_exigi.imp_mont_exig,
       inc_produ_exigi.num_punt_exig,
       ind_agrup,
       null as CUV
FROM   inc_produ_exigi, mae_produ
where  inc_produ_exigi.prdu_oid_prod = $P{parameter}
and inc_produ_exigi.prod_oid_prod = mae_produ.oid_prod(+)
and ofde_oid_deta_ofer is null
UNION
SELECT OID_PROD_EXIG,
       null  as des_marca_exigi,
       null as des_negoc_exigi,
       null as des_unid_negoc_exigi,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(b.Tofe_Oid_Tipo_Ofer,'PRE_TIPO_OFERT','es') as des_tipo_ofert_exigi,                                        
       e.cod_sap || ' - ' || GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(b.PROD_OID_PROD,'MAE_PRODU','es') as des_prod_exigi,
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(d.PERD_OID_PERI,'CRA_PERIO','es') as des_peri_desd_exigi,     
       GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(d.PERD_OID_PERI,'CRA_PERIO','es') as des_peri_exigi,
       a.num_unid_exig,
       a.imp_mont_exig,
       a.num_punt_exig,
       ind_agrup,      
       B.val_codi_vent cuv      
FROM   inc_produ_exigi a, pre_ofert_detal b, pre_matri_factu c, pre_matri_factu_cabec d, mae_produ e
where  a.prdu_oid_prod = $P{parameter}
and b.oid_deta_ofer = c.ofde_oid_deta_ofer
and c.MFCA_OID_CABE = d.oid_cabe
and a.ofde_oid_deta_ofer = b.oid_deta_ofer
and e.oid_prod = b.prod_oid_prod
and a.ofde_oid_deta_ofer is not null]]></queryString>


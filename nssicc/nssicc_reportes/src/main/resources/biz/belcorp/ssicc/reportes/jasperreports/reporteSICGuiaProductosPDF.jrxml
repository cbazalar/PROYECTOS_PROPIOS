	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT pre_catal_orden.num_orde Catalogo_Numero_orden,
       pre_catal.DES_CATA,
       pre_ofert_detal.num_pagi_cata,
       pre_estra.cod_estr,
       mae_produ.COD_SAP,
       mae_produ.CODI_ANTI,
       pre_ofert_detal.VAL_CODI_VENT,
       gen_i18n_sicc_pais.VAL_I18N DESC_PROD,
       pre_ofert_detal.VAL_TEXT_BREV TEXTO_BREVE,
       pre_ofert_detal.IMP_PREC_CATA PRECIO_CATALOGO,
       pre_ofert_Detal.IMP_PREC_POSI PRECIO_CONTABLE,
       pre_tipo_ofert.COD_TIPO_OFER TIPO_OFERTA,
       pre_ofert_detal.VAL_FACT_REPE FACTOR_REPET,
       pre_ofert_detal.IND_IMPR_GP,
       pre_ofert_detal.IND_DIGI,
       pre_ofert_detal.IND_PROD_PRIN,
       pre_ofert_detal.GOFE_OID_GRUP_OFER GRUPO,
       pre_grupo_ofert.IND_CNDO,
       pre_grupo_ofert.NUM_GRUP
  FROM pre_ofert,
       pre_ofert_detal,
       mae_produ,
       mae_unida_negoc,
       mae_negoc,
       pre_catal,
       pre_catal_orden,
       pre_estra,
       pre_grupo_ofert,
       gen_i18n_sicc_pais,
       cra_perio,
       seg_perio_corpo,
       pre_matri_factu_cabec,
       pre_tipo_ofert
 WHERE gen_i18n_sicc_pais.ATTR_ENTI = 'MAE_PRODU'
   and mae_produ.OID_PROD = gen_i18n_sicc_pais.VAL_OID
   and pre_tipo_ofert.oid_tipo_ofer = pre_ofert_detal.tofe_oid_tipo_ofer
   AND pre_ofert.mfca_oid_cabe = pre_matri_factu_cabec.OID_CABE
   and pre_matri_factu_cabec.PERD_OID_PERI = cra_perio.OID_PERI
   and cra_perio.PERI_OID_PERI = seg_perio_corpo.oid_peri
   and seg_perio_corpo.cod_peri = $P{codigoPeriodo}
   AND pre_ofert_detal.ofer_oid_ofer = pre_ofert.oid_ofer
   AND pre_ofert_detal.ocat_oid_catal = pre_catal.oid_cata(+)
   AND pre_ofert.coes_oid_estr = pre_estra.oid_estr
   AND pre_ofert_detal.prod_oid_prod = mae_produ.oid_prod
   AND pre_catal_orden.ocat_oid_cata(+) = pre_catal.oid_cata
   AND mae_produ.nego_oid_nego = mae_negoc.oid_nego(+)
   AND mae_produ.uneg_oid_unid_nego = mae_unida_negoc.oid_unid_nego(+)
   and pre_ofert_detal.GOFE_OID_GRUP_OFER =
       pre_grupo_ofert.OID_GRUP_OFER(+)
   and pre_ofert_detal.IMP_PREC_CATA = '0'
   and pre_ofert_Detal.IMP_PREC_POSI = '0'
   and pre_estra.COD_ESTR <> '014'
 ORDER BY -- Orden de Catalogo 
          PRE_CATAL_ORDEN.NUM_ORDE,
          NVL(PRE_OFERT_DETAL.NUM_PAGI_CATA, 0),
          PRE_ESTRA.COD_ESTR,
          PRE_OFERT.NUM_OFER,
          pre_ofert_detal.IND_PROD_PRIN desc,
          NVL(pre_grupo_ofert.IND_CNDO, '0') desc,
          NVL(pre_grupo_ofert.NUM_GRUP, '0') asc,
          NVL(PRE_OFERT_DETAL.VAL_TEXT_BREV, 'aaa') asc]]></queryString>


	</parameter>
	<queryString><![CDATA[with  ret as  
(  SELECT wr.clie_oid_clie, spc.cod_peri, wr.perd_oid_peri, cra.FEC_FINA        
   FROM mae_clien_histo_estat  wr,
        mae_clien_tipo_subti sbt,
        cra_perio  cra,
        seg_perio_corpo spc
      WHERE wr.perd_oid_peri_peri_fin is null
      AND   wr.esta_oid_esta_clie = 5
      AND   sbt.clie_oid_clie = wr.clie_oid_clie
      AND   sbt.ticl_oid_tipo_clie = 2     
      AND   cra.peri_oid_peri = spc.oid_peri
      AND   spc.cod_peri >=  $P{codigoPeriodoDesde}
      AND   spc.cod_peri <=  $P{codigoPeriodoHasta}
      AND   cra.oid_peri = wr.perd_oid_peri
 ),
 sret as
( SELECT ret.clie_oid_clie, ret.cod_peri, ret.perd_oid_peri, ret.FEC_FINA, 
         ccc_pkg_gener.CCC_FN_OBTIE_SALDO_CONTA(ret.clie_oid_clie, ret.fec_fina ) deu_ret  
 FROM ret )
 
 SELECT sret.COD_PERI,  REG.COD_REGI, ZON.COD_ZONA, mae.cod_clie, MAE.VAL_NOM1 || ' ' || MAE.VAL_NOM2 nom_clie, MAE.VAL_APE1, MAE.VAL_APE2, 
         sret.deu_ret, sret.fec_fina, tdo.cod_tipo_docu, tdo.val_sigl, ide.num_docu_iden,
        tc.VAL_TEXT_COMU TELEF_CASA, tcl.VAL_TEXT_COMU CELULAR,
        tv.DES_ABRV_TIPO_VIA || ' ' ||   mcd.VAL_NOMB_VIA||' '||mcd.NUM_PPAL DIRECCION , mcd.VAL_OBSE Referencia, 
       CASE    WHEN SUBSTR (mcd.cod_unid_geog, 1, 6) IS NULL
        THEN NULL
        ELSE (SELECT nivel1.des_geog
           FROM zon_valor_estru_geopo nivel1
           WHERE nivel1.orde_1 = substr(mcd.COD_UNID_GEOG,1,6)
              AND nivel1.orde_2 IS NULL
              AND nivel1.orde_3 IS NULL
              AND nivel1.orde_4 IS NULL
              AND nivel1.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END departamento,
        CASE    WHEN (SUBSTR (mcd.cod_unid_geog, 7, 6) IS NULL)
        THEN NULL
        ELSE (SELECT nivel2.des_geog
           FROM zon_valor_estru_geopo nivel2
           WHERE nivel2.orde_1 = SUBSTR (mcd.cod_unid_geog, 1, 6)
              AND nivel2.orde_2 = SUBSTR (mcd.cod_unid_geog, 7, 6)
              AND nivel2.orde_3 IS NULL
              AND nivel2.orde_4 IS NULL
              AND nivel2.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END provincia,
       CASE    WHEN (SUBSTR (mcd.cod_unid_geog, 13, 6) IS NULL)
        THEN NULL
        ELSE (SELECT nivel3.des_geog
           FROM zon_valor_estru_geopo nivel3
           WHERE nivel3.orde_1 = SUBSTR (mcd.cod_unid_geog, 1, 6)
              AND nivel3.orde_2 = SUBSTR (mcd.cod_unid_geog, 7, 6)
              AND nivel3.orde_3 = SUBSTR (mcd.cod_unid_geog, 13, 6)
              AND nivel3.orde_4 IS NULL
              AND nivel3.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END distrito,
        CASE    WHEN (substr(mcd.COD_UNID_GEOG,19,6)  IS NULL)
        THEN NULL
        ELSE (SELECT nivel4.des_geog
           FROM zon_valor_estru_geopo nivel4
           WHERE  nivel4.orde_1 = SUBSTR (mcd.cod_unid_geog, 1, 6)
              AND  nivel4.orde_2 = SUBSTR (mcd.cod_unid_geog, 7, 6)
              AND  nivel4.orde_3 = SUBSTR (mcd.cod_unid_geog, 13, 6)
              AND  nivel4.orde_4 = SUBSTR (mcd.cod_unid_geog, 19, 6)
              AND ROWNUM = 1)
        END Centro_Poblado  
 from sret,
      mae_clien mae,
      mae_clien_unida_admin ua,
      zon_terri_admin za,
      zon_secci sec,
      zon_regio reg,
      zon_zona zon,
      mae_clien_comun tc,
      mae_clien_comun tcl,
      seg_tipo_via tv,
      mae_clien_direc mcd,
      zon_valor_estru_geopo egeop,
      zon_terri  terr,
      mae_clien_ident ide,
      mae_tipo_docum tdo 
 WHERE  deu_ret <=  $P{importeHasta}
 and  mae.OID_CLIE = sret.clie_oid_clie
 AND MAE.oid_clie = ua.clie_oid_clie
 AND ua.ztad_oid_terr_admi = za.oid_terr_admi
 AND za.zscc_oid_secc = sec.oid_secc
 AND za.TERR_OID_TERR = terr.OID_TERR
 AND sec.zzon_oid_zona = zon.oid_zona
 and zon.ZORG_OID_REGI = reg.OID_REGI
 AND ua.ind_acti = 1
 AND mae.OID_CLIE = tc.CLIE_OID_CLIE (+)  AND  1 = tc.TICM_OID_TIPO_COMU (+)
 AND mae.OID_CLIE = tcl.CLIE_OID_CLIE (+) AND  6 = tcl.TICM_OID_TIPO_COMU (+)  
 AND mae.OID_CLIE = mcd.CLIE_OID_CLIE(+)  AND  mcd.IND_DIRE_PPAL = 1  AND  mcd.ind_elim = 0
 AND terr.vepo_oid_valo_estr_geop = egeop.oid_valo_estr_geop(+) 
 AND mcd.TIVI_OID_TIPO_VIA = tv.OID_TIPO_VIA
 AND ide.clie_oid_clie = sret.clie_oid_clie
 AND ide.TDOC_OID_TIPO_DOCU = tdo.OID_TIPO_DOCU
 AND ide.VAL_IDEN_DOCU_PRIN = 1 
$P!{condicion}
ORDER BY cod_peri, cod_regi, cod_zona, cod_clie]]></queryString>


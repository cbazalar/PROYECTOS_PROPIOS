	<queryString><![CDATA[select DISTINCT 
zr.des_regi region,
zz.des_zona zona,
zs.DES_SECCI seccion,
zt.COD_TERR territorio,
m.cod_clie cliente,
cra.val_nomb_peri periodopedido,
afectado.val_nume_soli pedidoafectado,
to_char(afectado.fec_fact, 'dd/mm/yyyy') fechapedido,
to_char(fac.fec_fact, 'dd/mm/yyyy') fechanc,
fac.NUM_DOCU_LEGA,
fac.NUM_DOCU_CONT_INTE,
ide.num_docu_iden cedula,
m.VAL_APE1||' '||m.VAL_APE2||', '||m.VAL_NOM1||' '||m.VAL_NOM2 as nombre,
GEN_PKG_GENER.GEN_FN_CLIEN_DATOS_OID(m.OID_CLIE, 'DES_DIRE') DIRECCION,
(-1)*fac.VAL_TOTA_PAGA_LOCA total_a_pagar,
(-1)*fac.VAL_PREC_NETO_TOTA_LOCA total_a_pagar_sin_impuesto,
(-1)*fac.IMP_IMPU_TOTA_DOCU impuesto,
(-1)*fac.VAL_PREC_CATA_TOTA_LOCA total_catalogo,
(-1)*fac.IMP_DES1_TOTA_LOCA descuentos,
(-1)*(select sum(decode(l.val_prec_cata_unit_loca,0,0,l.val_prec_fact_tota_loca))
from fac_docum_conta_linea l
where l.dcca_oid_cabe= fac.oid_cabe) total_catalogo_sin_descuentos,
(-1)*fac.IMP_FLET_TOTA_LOCA flete, 
(select val_tasa_impu from PED_TASA_IMPUE where val_indi_impu='IVA') TASA_IMPUESTO,
(-1)*fac.imp_redo_loca redondeo
from ped_solic_cabec c,
ped_solic_cabec afectado,
mae_clien m,
zon_zona zz,
zon_regio zr,
zon_secci zs,
zon_terri zt,
zon_terri_admin zta,
cra_perio cra,
mae_clien_ident ide,
fac_docum_conta_cabec fac
where c.TSPA_OID_TIPO_SOLI_PAIS in (
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C52') FROM DUAL
union
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C65') FROM DUAL
union
SELECT INT_PKG_RECLA.GEN_FN_DEVUE_OID_TIPO_SOLPA('C56') FROM DUAL
)
and afectado.oid_soli_cabe = c.SOCA_OID_DOCU_REFE 
and fac.TIDO_OID_TIPO_DOCU = (select oid_tipo_docu from fac_tipo_docum where cod_tipo_docu='021')
and fac.SOCA_OID_SOLI_CABE = c.OID_SOLI_CABE
and ide.VAL_IDEN_DOCU_PRIN = 1
and ide.clie_oid_clie = m.oid_clie
and m.oid_clie = c.clie_oid_clie
and c.ZZON_OID_ZONA=zz.oid_zona
and c.ZTAD_OID_TERR_ADMI = zta.OID_TERR_ADMI
and zta.TERR_OID_TERR = zt.OID_TERR
and zta.ZSCC_OID_SECC = zs.OID_SECC
and zs.ZZON_OID_ZONA = zz.OID_ZONA
and zz.ZORG_OID_REGI = zr.OID_REGI
and cra.OID_PERI = afectado.PERD_OID_PERI
and to_char(c.fec_cron, 'dd/mm/yyyy') = $P{fechaAnulacion}
and afectado.PERD_OID_PERI = $P{oidPeriodo}
$P!{condicionRegiones}
$P!{condicionZonas}
$P!{codigoConsultora}
]]></queryString>


		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoSociedad" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoSociedad")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
   cod_peri,
   facturado imp_fact,
   pendiente imp_pend,
   ROUND((pendiente/facturado)*100,2) val_porc1,
   saldo_campaña sald_camp,
   ROUND((pendiente/saldo_campaña)*100,2) val_porc2  
FROM (
SELECT 
                mcc.perd_oid_peri,
                spc.cod_peri, 
               SUM(mcc.imp_movi) facturado , 
               SUM(mcc.imp_pend) pendiente,               
               (SELECT SUM(x.imp_pend)
                FROM ccc_movim_cuent_corri x
                WHERE x.perd_oid_peri = mcc.perd_oid_peri
                AND x.imp_movi>0) saldo_campaña                
FROM ccc_movim_cuent_corri mcc,
     cra_perio cp,
     seg_perio_corpo spc
WHERE mcc.perd_oid_peri=cp.oid_peri  
AND spc.oid_peri=cp.peri_oid_peri
AND mcc.imp_movi > 0 
AND spc.cod_peri>=$P{codigoPeriodoInicial}
AND spc.cod_peri<=$P{codigoPeriodoFinal}
AND EXISTS  (
SELECT NULL
FROM mae_clien_histo_estat x
WHERE x.perd_oid_peri= mcc.perd_oid_peri
AND x.esta_oid_esta_clie IN (8,2)
AND x.clie_oid_clie=mcc.clie_oid_clie)
GROUP BY mcc.perd_oid_peri,spc.cod_peri
ORDER BY 1 ASC)]]></queryString>


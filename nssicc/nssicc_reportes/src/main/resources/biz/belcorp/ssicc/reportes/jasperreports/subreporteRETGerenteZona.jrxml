	<queryString><![CDATA[SELECT COD_ZONA,
       COD_GERE_ZONA,
       SUM_VENT,
       SUM_COMI,
       (SELECT TRIM(VAL_APE1)||' '||TRIM(VAL_APE2)||' '||TRIM(VAL_NOM1)||' '||TRIM(VAL_NOM2)
           FROM MAE_CLIEN WHERE COD_CLIE = Cod_Gere_Zona) NOM_GERE
 FROM
(select rep.cod_zona,
       rep.cod_gere_zona,
       SUM(rep.imp_vent) SUM_VENT,
       SUM(rep.imp_comi) SUM_COMI
 from  ret_repor_comis_gzona rep
 where cod_pais = $P{codigoPais}
   AND TRUNC(FEC_INIC) >= TO_DATE($P{fechaInicio}, 'DD/MM/YYYY')
   AND TRUNC(FEC_FINA) <= TO_DATE($P{fechaFinal}, 'DD/MM/YYYY')
   $P!{condicionRegion}
   $P!{condicionZona}
 group by rep.cod_zona, rep.cod_gere_zona
)
ORDER BY  cod_zona, cod_gere_zona
]]></queryString>


	<queryString><![CDATA[SELECT 
gen_pkg_gener.gen_fn_clien_datos_oid(mc.oid_clie,'COD_REGI') AS cod_regi,
gen_pkg_gener.gen_fn_clien_datos_oid(mc.oid_clie,'COD_ZONA') AS cod_zona,
gen_pkg_gener.gen_fn_clien_datos_oid(mc.oid_clie,'COD_SECC') AS cod_secc,
spc.cod_peri cod_peri_fact,
to_char(psc.fec_fact, 'DD/MM/YYYY') as fec_fact,
mc.cod_clie,
fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1) AS cod_peri_fina,
(SELECT ff.ind_gene_fina_flex
  FROM flx_gener_finan_consu_flexi ff
  WHERE ff.oid_clie = psc.clie_oid_clie
    AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
    AND ff.cod_moti_rech is NULL) ind_gene_fina_flex,
  (SELECT ff.val_mont_mini_paga
   FROM flx_gener_finan_consu_flexi ff
   WHERE ff.oid_clie = psc.clie_oid_clie
     AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
     AND ff.cod_moti_rech is NULL) val_mont_mini,
  (SELECT ff.val_sald_deud_fina
   FROM flx_gener_finan_consu_flexi ff
   WHERE ff.oid_clie = psc.clie_oid_clie
     AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
     AND ff.cod_moti_rech is NULL) val_sald_deud_fina, 
    (SELECT ff.val_sald_maxi_camp
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie
    AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
    AND ff.cod_moti_rech is NULL) val_sald_maxi_camp, 
    (SELECT ff.val_mont_flex_fina
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie
    AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
    AND ff.cod_moti_rech is NULL) val_mont_flex_fina,
    (SELECT ff.val_mont_cuot_fle1
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie
    AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
    AND ff.cod_moti_rech is NULL) val_mont_cuot_fle1,
    (SELECT ff.val_mont_cuot_fle2
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie
    AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
    AND ff.cod_moti_rech is NULL) val_mont_cuot_fle2,
    (SELECT ff.cod_moti_rech
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie    
    AND ff.cod_peri = spc.cod_peri) AS cod_moti_rech,
    (SELECT ff.val_mont_flex_fina_proy
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie    
    AND ff.cod_peri = spc.cod_peri) AS val_mont_flex_fina_proy,
    (SELECT ff.val_mont_mini_paga
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie    
    AND ff.cod_peri = spc.cod_peri) AS val_mont_mini_paga,
    (SELECT ff.val_mont_paga_sinf
    FROM flx_gener_finan_consu_flexi ff
    WHERE ff.oid_clie = psc.clie_oid_clie    
    AND ff.cod_peri = spc.cod_peri) AS val_mont_paga_sinf                
FROM ped_solic_cabec psc,
     ped_tipo_solic_pais ptsp,
     ped_tipo_solic pts,
     cra_perio cp,
     seg_perio_corpo spc,
     mae_clien mc, 
     mae_clien_unida_admin mcua, 
     zon_terri_admin zta, 
     zon_secci zs,
     zon_zona zz,
     zon_regio zr
WHERE psc.clie_oid_clie = mc.oid_clie 
AND psc.tspa_oid_tipo_soli_pais = ptsp.oid_tipo_soli_pais
AND ptsp.tsol_oid_tipo_soli = pts.oid_tipo_soli
AND psc.perd_oid_peri = cp.oid_peri
AND cp.peri_oid_peri = spc.oid_peri
AND mc.oid_clie = mcua.clie_oid_clie
AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
AND zta.zscc_oid_secc = zs.oid_secc
AND zs.zzon_oid_zona = zz.oid_zona
AND zz.zorg_oid_regi = zr.oid_regi
AND pts.cod_tipo_soli = 'SDFA'
AND psc.ind_oc = 0
AND spc.cod_peri = $P{codigoCampana}
AND ($P{fechaInicio} is null OR ( TRUNC(psc.fec_fact) >= TO_DATE($P{fechaInicio}, 'DD/MM/YYYY') ))
AND ($P{fechaFin} is null OR ( TRUNC(psc.fec_fact) <= TO_DATE($P{fechaFin}, 'DD/MM/YYYY') ))
$P!{condicionRegion}
$P!{condicionZona}
$P!{condicionSeccion}
AND EXISTS (
SELECT NULL
FROM flx_gener_finan_consu_flexi ff
WHERE ff.oid_clie = psc.clie_oid_clie
   AND ff.cod_peri = fin_pkg_gener.fin_fn_obtie_nsgte_campa(spc.cod_peri,-1)
   AND ff.cod_moti_rech IS NULL)
ORDER BY psc.fec_fact ASC]]></queryString>


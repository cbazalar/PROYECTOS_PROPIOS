	<queryString><![CDATA[SELECT
    p.oid_para_gral OID_PARA
    ,p.cod_regi REGION
    ,p.des_regi NOM_REGION
    ,p.cod_zona ZONA
    ,p.cod_secc SECCION
    ,p.lider LIDER
    ,p.nombres NOMBRES
    ,p.concurso CONCURSO
    ,sum(p.puntos_campania) puntos_campania
    ,sum(p.punt_pedi_obje) puntos_pedi_obje 
    ,sum(p.punt_recom_2ped) puntos_recom_2ped
    ,sum(p.punt_recom_3ped) puntos_recom_3ped
    ,sum(p.punt_meta_ing_efec) puntos_meta
    ,sum(p.punt_adici_meta_ing) puntos_bono_meta
    ,sum(p.entrega_premio) entrega_premio
    ,sum(p.puntos_bonif) puntos_bonif
FROM ( 
    SELECT
        t.oid_para_gral 
        ,t.campania  
        ,t.cod_regi 
        ,t.des_regi 
        ,t.cod_zona 
        ,t.cod_secc 
        ,t.lider 
        ,t.nombres 
        ,t.concurso 
        ,sum(t.puntos) puntos_campania
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and (val_desc like '%Abono por Número de Pedidos Sección%' or val_desc like '%ABONO POR NUMERO PEDIDOS DE SECCION%')),0) punt_pedi_obje
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and (val_desc like '%Abono por Recomendación Efectiva(2P)%' or val_desc like '%ABONO POR RECOMENDACION EFECTIVA ( 2 PEDIDOS CONSECUTIVOS )%')),0) punt_recom_2ped
         ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and (val_desc like '%Abono por Recomendación Efectiva(3P)%' or val_desc like '%ABONO POR RECOMENDACION EFECTIVA ( 3 PEDIDOS CONSECUTIVOS )%')),0) punt_recom_3ped
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and (val_desc like '%Abono por Cumplir Meta de Ingresos Efectivos%' or val_desc like '%ABONO POR CUMPLIR LA META DE INGRESOS EFECTIVOS%')),0) punt_meta_ing_efec
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and (val_desc like '%Abono por Adicionales a la Meta de Ingresos Efectivos%' or val_desc like '%ABONO POR ADICIONALES A LA META DE INGRESOS EFECTIVOS%')),0) punt_adici_meta_ing
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and val_desc like '%Entrega de Premio%'),0) entrega_premio                  
        ,NVL((select sum(num_punt)
          from inc_cuent_corri_punto
          where copa_oid_para_gral = t.oid_para_gral
          and clie_oid_clie = t.oid_clie
          and perd_oid_peri = t.oid_peri
          and val_desc not like '%Abono por Número de Pedidos Sección%'
          and val_desc not like '%ABONO POR NUMERO PEDIDOS DE SECCION%'
          and val_desc not like '%Abono por Recomendación Efectiva(2P)%'
          and val_desc not like '%ABONO POR RECOMENDACION EFECTIVA ( 2 PEDIDOS CONSECUTIVOS )%'
          and val_desc not like '%Abono por Recomendación Efectiva(3P)%'
          and val_desc not like '%ABONO POR RECOMENDACION EFECTIVA ( 3 PEDIDOS CONSECUTIVOS )%'      
          and val_desc not like '%Abono por Cumplir Meta de Ingresos Efectivos%'
          and val_desc not like '%ABONO POR CUMPLIR LA META DE INGRESOS EFECTIVOS%'
          and val_desc not like '%Abono por Adicionales a la Meta de Ingresos Efectivos%'
          and val_desc not like '%ABONO POR ADICIONALES A LA META DE INGRESOS EFECTIVOS%'
          and val_desc not like '%Entrega de Premio%'),0) puntos_bonif         
    FROM (
    select 
        cp.oid_peri 
        , pc.cod_peri campania
        , cpg.oid_para_gral
        , cpg.num_conc concurso
        , r.cod_regi
        , r.des_regi
        , z.cod_zona
        , z.des_zona
        , s.cod_secc
        , mc.oid_clie
        , mc.cod_clie lider 
        , TRIM(mc.val_nom1) || ' ' || TRIM(mc.val_nom2) || ' ' || TRIM(mc.val_ape1) || ' ' || TRIM(mc.val_ape2) nombres
        , sum(ccp.num_punt) puntos
    from 
        inc_cuent_corri_punto ccp
        ,inc_concu_param_gener cpg
        ,cra_perio cp
        ,seg_perio_corpo pc
        ,mae_clien mc
        ,zon_secci s
        ,zon_zona z
        ,zon_regio r
    where
 ccp.copa_oid_para_gral = cpg.oid_para_gral
 and cpg.NUM_CONC =$P{codigoConcurso}
    and ccp.perd_oid_peri = cp.oid_peri
    and cp.peri_oid_peri = pc.oid_peri
    and ccp.clie_oid_clie = mc.oid_clie
    and mc.oid_clie = s.clie_oid_clie(+)
    and s.zzon_oid_zona = z.oid_zona(+)
    and z.zorg_oid_Regi = r.oid_regi(+)
    and s.ind_acti(+) = 1
    and s.ind_borr(+) = 0
    and z.ind_acti(+) = 1
    and z.ind_borr(+) = 0
    group by 
        cp.oid_peri, pc.cod_peri, cpg.oid_para_gral, cpg.num_conc
        , r.cod_regi, r.des_regi, z.cod_zona, z.des_zona, s.cod_secc
        , mc.oid_clie, mc.cod_clie, mc.val_nom1, mc.val_nom2, mc.val_ape1, mc.val_ape2
    ) t
WHERE    t.campania <= $P{codigoPeriodoFin} 
and t.campania >= $P{codigoPeriodoInicio}
 $P!{condicionRegion}
 $P!{condicionZona}  
$P!{condicionSeccion}
    GROUP BY 
        t.oid_peri, t.campania, t.oid_para_gral, t.concurso
        ,t.cod_regi, t.des_regi, t.cod_zona, t.des_zona, t.cod_secc
        ,t.lider,t.oid_clie,t.nombres
) p
GROUP BY 
    p.oid_para_gral
    ,p.cod_regi
    ,p.des_regi
    ,p.cod_zona
    ,p.cod_secc
    ,p.lider
    ,p.nombres
    ,p.concurso
ORDER BY
    p.cod_regi
    ,p.des_regi
    ,p.cod_zona
    ,p.cod_secc
    ,p.lider]]></queryString>


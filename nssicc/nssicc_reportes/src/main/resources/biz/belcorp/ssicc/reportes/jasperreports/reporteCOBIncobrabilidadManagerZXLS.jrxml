	<queryString><![CDATA[SELECT 
    cob_pkg_repor_cobra.COB_FN_OBTEN_ETAPA_PERIO_ZONA($P{codigoPais},
					               $P{codigoSociedad},
						       c.cod_peri,
						       $P{codigoRegion},
						       $P{codigoZona}) COD_ETAP_DEUD,
    c.cod_peri , 
    c.cod_regi,
    c.des_regi,
    NVL((SELECT m.val_nom1 || ' ' || m.val_nom2 || ' ' || m.val_ape1 || ' ' || m.val_ape2 
	           FROM own_peru.mae_clien m
             WHERE m.oid_clie=oid_clie_regi),' ' ) nom_clie_regi,               
   c.cod_zona,
   c.des_zona,
    NVL((SELECT m.val_nom1 || ' ' || m.val_nom2 || ' ' || m.val_ape1 || ' ' || m.val_ape2 
	           FROM own_peru.mae_clien m
             WHERE m.oid_clie=oid_clie_zona),' ' ) nom_clie_zona,           
    c.cod_secc,
    c.des_secc,
NVL((SELECT m.val_nom1 || ' ' || m.val_nom2 || ' ' || m.val_ape1 || ' ' || m.val_ape2 
	           FROM own_peru.mae_clien m
             WHERE m.oid_clie=oid_clie_secc),' ' ) nom_clie_secc,           
   NVL(SUM(c.imp_movi),0) venta,
   NVL(SUM(c.imp_sald),0) saldo
FROM cob_repor_incob_ua c
WHERE c.cod_pais=$P{codigoPais}
AND c.cod_soci=$P{codigoSociedad}
AND c.cod_peri>=$P{codigoPeriodoInicial}
AND c.cod_peri<=$P{codigoPeriodoFinal}
and c.cod_regi =$P{codigoRegion}
AND c.cod_zona = $P{codigoZona}
GROUP BY 
	 c.cod_peri , 
	 c.cod_regi,
	 c.des_regi,
	 c.oid_clie_regi,
	 c.cod_zona,
	 c.des_zona,
	 c.oid_clie_zona,
	 c.cod_secc,
	 c.des_secc,
	 c.oid_clie_secc]]></queryString>


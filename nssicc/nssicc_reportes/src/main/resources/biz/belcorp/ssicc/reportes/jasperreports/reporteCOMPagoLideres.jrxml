		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodo" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoComisionIngreso" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoComisionIngreso")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoComisionRecu1" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoComisionRecu1")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT DISTINCT
	C.COD_PLAN, 
        A.COD_LIDE_SECC,
        A.NOM_LIDE_SECC,
	COM_PKG_REPOR.COM_FN_DEVUE_COMIS_LIDER_SECCI(A.PERI_COD_PERI, A.COD_LIDE_SECC, $P{codigoComisionIngreso}, $P{codigoComisionRecu1}) AS IMP_COMI,
        SYSDATE AS FEC_PAGO,
        C.NUM_LIAH
   FROM COM_COMIS_PERIO_CALCU A,
	MAE_CLIEN_LIDER C 
   WHERE A.PERI_COD_PERI = $P{codigoPeriodo}
	 AND C.PAIS_COD_PAIS = $P{codigoPais}
	 AND C.COD_PLAN IS NOT NULL 
	 AND C.COD_CLID = A.COD_LIDE_SECC
	 AND COM_PKG_REPOR.COM_FN_DEVUE_COMIS_LIDER_SECCI(A.PERI_COD_PERI, A.COD_LIDE_SECC, $P{codigoComisionIngreso}, $P{codigoComisionRecu1}) > 0
         AND NOT EXISTS (SELECT X.COD_CLIB
			  FROM MAE_CLIEN_LIDER_BLOQU X
			  WHERE
 				X.PAIS_COD_PAIS = C.PAIS_COD_PAIS
			   AND X.COD_CLIB = A.COD_LIDE_SECC 
			   AND ($P{codigoPeriodo} >= X.PER_INIC)
			   AND ($P{codigoPeriodo} <= X.PER_FINA OR X.PER_FINA IS NULL) 
			   AND X.EST_CLIB = '1'
			)
				 
ORDER BY A.COD_LIDE_SECC]]></queryString>


	<queryString><![CDATA[SELECT DISTINCT
       c.cod_clie                                                                                CODIGO,
       trim(c.val_nom1)||' '|| trim(c.val_nom2)||' '|| trim(c.val_ape1)||' '|| trim(c.val_ape2)  NOMBRE,
       (SELECT r.cod_regi
          FROM zon_regio r
         WHERE r.oid_regi = z.zorg_oid_regi
           AND ROWNUM = 1) || z.cod_zona || (SELECT s.cod_secc
                                               FROM zon_secci s
                                              WHERE s.zzon_oid_zona = z.oid_zona AND ROWNUM = 1) TERRITORIO,
       soli.val_nume_soli                                                                        FACTURA,
       SUM(det.num_unid_prod) OVER (partition by cab.oid_list_pica_cabe)                                     CANTIDAD,
       z.des_zona                                                                                ZONA,
       (SELECT DISTINCT gen.val_i18n descripcion
          FROM ped_tipo_solic soli, 
               gen_i18n_sicc_comun gen
				 WHERE gen.val_oid = sol.oid_tipo_soli
           AND gen.attr_enti = 'PED_TIPO_SOLIC')                                                 SOLI
  FROM ped_solic_cabec soli,
       ape_lista_picad_cabec cab,
	     ape_lista_picad_detal det,
	     mae_clien c,
	     zon_zona z,
	     ped_tipo_solic_pais tiposol,
	     ped_tipo_solic sol
 WHERE cab.fec_factu =  TO_DATE ($P{fechaFacturacion}, 'dd/mm/yyyy')
   AND det.prod_oid_prod = $P{oidProducto}
   AND cab.soca_oid_soli_cabe = soli.oid_soli_cabe
   AND cab.oid_list_pica_cabe = det.lpca_oid_list_pica_cabe
   AND c.oid_clie = soli.clie_oid_clie
   AND z.oid_zona = soli.zzon_oid_zona
   AND tiposol.oid_tipo_soli_pais = soli.tspa_oid_tipo_soli_pais
   AND sol.oid_tipo_soli = tsol_oid_tipo_soli
   
   $P!{condicionZona}
   $P!{condicionTipoSolicitud}
   
ORDER BY 3, 1]]></queryString>


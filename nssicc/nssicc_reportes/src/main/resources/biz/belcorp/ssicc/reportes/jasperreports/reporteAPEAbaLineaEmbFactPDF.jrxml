	<queryString><![CDATA[SELECT periodo_anterior.producto                   OID_PRODUCTO_ANT,
       periodo_actual.producto                     OID_PRODUCTO_ACT,
       producto.cod_sap                            COD_PRODUCTO,
       i18.val_i18n                                DES_PRODUCTO,
       periodo_anterior.anaquel                    NUM_ANAQUEL_ANT,
       periodo_actual.anaquel                      NUM_ANAQUEL_ACT,
       NVL(producto.num_unid_caja_prod,0)          UNI_CAJAS,
       (CASE 
          WHEN (NVL(producto.num_unid_caja_prod,0) = 0 AND NVL(productoApe.num_unid_caja_maes,0) = 0 ) THEN 0
          WHEN (NVL(producto.num_unid_caja_prod,0) = 0 AND NVL(productoApe.num_unid_caja_maes,0) <> 0 ) THEN 
                 periodo_actual.unidades / productoApe.num_unid_caja_maes
          ELSE periodo_actual.unidades/ producto.num_unid_caja_prod
       END)                                        CAJ_ABASTECER,
       periodo_actual.unidades                     NUM_ABASTECER,
       NVL(estimado.num_unid_esti_mark,0)          NUM_ESTIMADOS,
       (CASE 
         WHEN (periodo_anterior.producto IS NULL AND periodo_actual.producto IS NOT NULL ) THEN 'ENTRA'
         WHEN (periodo_anterior.producto IS NOT NULL) AND (periodo_actual.producto IS NULL) THEN 'SALE'
         WHEN (periodo_anterior.producto <> periodo_actual.producto ) AND (periodo_anterior.producto IS NOT NULL) AND (periodo_actual.producto IS NOT NULL) THEN 'CAMBIA'
         WHEN (periodo_anterior.producto = periodo_actual.producto )  AND (periodo_anterior.producto IS NOT NULL) AND (periodo_actual.producto IS NOT NULL) THEN 'QUEDA'
        END)                                       MOVIMIENTO,
       decode(NVL(producto.num_unid_caja_prod,0),0,0, NVL(estimado.num_unid_esti_mark,0) /  NVL(producto.num_unid_caja_prod,0))  CAJ_MOVER,
       $P{desMarca}													       MARCA,
       $P{desCanal}													       CANAL,
       $P{desCentro}													     CENTRO,
       $P{desLinea}													       LINEA
  FROM
       ( SELECT mapadet.oid_mapa_cent_dist_deta    mapa,
                mapadet.num_anaq                   anaquel,
                asignaciondet.prod_oid_prod        producto
           FROM ape_asign_produ_anaqu_cabec      asignacioncab,
                ape_asign_produ_anaqu_detal      asignaciondet,
                ape_mapa_centr_distr_cabec       mapacab,
                ape_mapa_centr_distr_detal       mapadet,
                ape_subli_armad                  sublinea
          WHERE asignacioncab.mcdc_oid_mapa_cent_dist_cabe = $P{oidMapaCentro}
            AND asignacioncab.perd_oid_peri = $P{oidPeriodoAnterior}
            AND asignacioncab.ind_acti_fact = 'S'
            AND asignacioncab.oid_asig_prod_anaq_cabe = asignaciondet.apac_oid_asig_prod_anaq_cabe
            AND asignaciondet.mcdd_oid_mapa_cent_dist_deta = mapadet.oid_mapa_cent_dist_deta
            AND mapacab.oid_mapa_cent_dist_cabe = asignacioncab.mcdc_oid_mapa_cent_dist_cabe
            AND mapacab.oid_mapa_cent_dist_cabe = mapadet.mcdc_oid_mapa_cent_dist_cabe
            AND mapadet.sbar_oid_subl_arma = sublinea.oid_subl_arma
            AND sublinea.liar_oid_line_arma = $P{oidLinea}
       ) periodo_anterior,
       ( SELECT mapadet.oid_mapa_cent_dist_deta    mapa,
                mapadet.num_anaq                   anaquel,
                asignaciondet.prod_oid_prod        producto,
                asignaciondet.num_unida            unidades
           FROM ape_asign_produ_anaqu_cabec      asignacioncab,
                ape_asign_produ_anaqu_detal      asignaciondet,
                ape_mapa_centr_distr_cabec       mapacab,
                ape_mapa_centr_distr_detal       mapadet,
                ape_subli_armad                  sublinea
          WHERE asignacioncab.mcdc_oid_mapa_cent_dist_cabe = $P{oidMapaCentro}
            AND asignacioncab.perd_oid_peri = $P{oidPeriodo}
            AND asignacioncab.val_vers = $P{codVersion}
            AND asignacioncab.ind_acti_fact = 'S'
            AND asignacioncab.oid_asig_prod_anaq_cabe = asignaciondet.apac_oid_asig_prod_anaq_cabe
            AND asignaciondet.mcdd_oid_mapa_cent_dist_deta = mapadet.oid_mapa_cent_dist_deta
            AND mapacab.oid_mapa_cent_dist_cabe = asignacioncab.mcdc_oid_mapa_cent_dist_cabe
            AND mapacab.oid_mapa_cent_dist_cabe = mapadet.mcdc_oid_mapa_cent_dist_cabe
            AND mapadet.sbar_oid_subl_arma = sublinea.oid_subl_arma
            AND sublinea.liar_oid_line_arma = $P{oidLinea}
       ) periodo_actual,
       mae_produ                   producto,
       gen_i18n_sicc_pais          i18,
       fac_histo_estim_deman_antic estimado,
       ape_produ                   productoApe
 WHERE periodo_actual.mapa = periodo_anterior.mapa
   AND periodo_actual.producto = producto.oid_prod (+)
   AND i18.attr_enti (+)= 'MAE_PRODU'
   AND i18.attr_num_atri (+)= 1
   AND i18.idio_oid_idio (+)= 1
   AND i18.val_oid (+)= producto.oid_prod
   AND ((periodo_actual.producto IS NOT NULL) OR (periodo_anterior.producto IS NOT NULL))
   AND estimado.prod_oid_prod (+)= producto.oid_prod
   AND estimado.perd_oid_peri (+) = $P{oidPeriodo}
   AND productoApe.oid_prod(+)= producto.oid_prod
 $P!{orden}]]></queryString>


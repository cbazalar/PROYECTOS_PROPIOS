	<parameter name="codigoPeriodoInicio" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicio")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFin" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFin")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select per.VAL_NOMB_PERI AS PERIODO,
       f.COD_ZONA ZONA,
       (SELECT G.VAL_I18N
          FROM PED_TIPO_SOLIC_PAIS TSP,
               PED_TIPO_SOLIC      TS,
               GEN_I18N_SICC_COMUN G
         WHERE TSP.TSOL_OID_TIPO_SOLI = TS.OID_TIPO_SOLI
           AND G.VAL_OID = TS.OID_TIPO_SOLI
           AND G.ATTR_ENTI = 'PED_TIPO_SOLIC'
           and tsp.oid_tipo_soli_pais in (X.TSPA_OID_TIPO_SOLI_PAIS)) TIPOATENCION,
       y.VAL_CODI_VENT,
       s.COD_SAP,
       v.VAL_I18N DESCRIPCION,
       sum(y.NUM_UNID_ATEN) UNIDADES_ATENDIDAS,
       sum(decode(y.val_prec_CATA_unit_loca,
                  0,
                  0,
                  y.VAL_PREC_NETO_TOTA_LOCA)) MONTO_NETO
  from ped_solic_cabec    x,
       ped_solic_posic    y,
       mae_clien          a,
       zon_zona           f,
       mae_produ          s,
       gen_i18n_sicc_pais v,
       ped_solic_cabec    cons,
       cra_perio          per,
       pre_ofert_detal    pof,
       pre_tipo_ofert     tipof,
       PED_ESTAD_SOLIC    ESTAD
 where 1 = 1
   AND X.CLIE_OID_CLIE = a.OID_CLIE
   AND X.ZZON_OID_ZONA = F.OID_ZONA
   and x.ESSO_OID_ESTA_SOLI = ESTAD.OID_ESTA_SOLI
   AND ESTAD.COD_ESTA_SOLI not in ('BL', 'AN', 'ER')
   and x.PERD_OID_PERI <= GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFin})              
   and x.PERD_OID_PERI >= GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoInicio})                           
   AND y.VAL_CODI_VENT IS NOT NULL
   and x.PERD_OID_PERI = per.oid_peri
   and cons.TSPA_OID_TIPO_SOLI_PAIS =REC_PKG_REPOR.GEN_FN_DEVUE_OID_TSOLI_PAIS('C1') 
   and cons.oid_soli_cabe = x.soca_oid_soli_cabe
   and cons.CLIE_OID_CLIE = a.OID_CLIE
   and y.PROD_OID_PROD = s.OID_PROD
   and s.OID_PROD = v.VAL_OID
   and x.OID_SOLI_CABE = y.SOCA_OID_SOLI_CABE
   and v.ATTR_ENTI = 'MAE_PRODU'
   and pof.OID_DETA_OFER = y.OFDE_OID_DETA_OFER
   and pof.TOFE_OID_TIPO_OFER = tipof.OID_TIPO_OFER
   and tipof.IND_ESTA = 1
   and y.NUM_UNID_ATEN <> 0
 GROUP BY per.VAL_NOMB_PERI,
          f.COD_ZONA,
          y.VAL_CODI_VENT,
          s.COD_SAP,
          v.VAL_I18N,
          X.TSPA_OID_TIPO_SOLI_PAIS
HAVING sum(y.VAL_PREC_NETO_TOTA_LOCA) > 0]]></queryString>


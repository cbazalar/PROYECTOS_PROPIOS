	<queryString><![CDATA[SELECT OCR_SOLIC_PEDIDOS.GEN_FN_CODSAP_PROD(REC_REPOR_OPERA_UNIDA_ADMIN.PROD_OID_PROD) AS COD_SAP,
       OCR_SOLIC_PEDIDOS.GEN_FN_DESC_PROD(REC_REPOR_OPERA_UNIDA_ADMIN.PROD_OID_PROD) AS DESC_PROD,      
       SUM(num_guia) AS ATEN_RECL,
       SUM(NUM_UNID_RECL) AS CANT_DEVU,
       SUM(IMP_ABON) AS MONT_DEVU,
       temporal.UNID_FACT,
       temporal.MONT_FACT,
       temporal.NUM_PEDI
    FROM   REC_REPOR_OPERA_UNIDA_ADMIN,
    (SELECT temp.prod_oid_prod,
             SUM(temp.VAL_PREC_FACT_TOTA_LOCA)  AS MONT_FACT ,   
             SUM(temp.NUM_UNID_DEMA) AS UNID_FACT,
             SUM(temp.Num_Pedi) AS Num_Pedi
       FROM  ( SELECT perd_oid_peri, 
                      prod_oid_prod,
                      NUM_UNID_DEMA,
                      VAL_PREC_FACT_TOTA_LOCA,   
                      Num_Pedi                    
                 FROM REC_REPOR_OPERA_UNIDA_ADMIN
              GROUP BY perd_oid_peri, 
                       prod_oid_prod,
                       NUM_UNID_DEMA,
                       VAL_PREC_FACT_TOTA_LOCA,
                       num_pedi ) temp
              GROUP BY prod_oid_prod ) temporal
      where temporal.prod_oid_prod     =    REC_REPOR_OPERA_UNIDA_ADMIN.prod_oid_prod
    GROUP BY REC_REPOR_OPERA_UNIDA_ADMIN.PROD_OID_PROD,
         temporal.UNID_FACT,
       temporal.MONT_FACT,
       temporal.NUM_PEDI
   ORDER BY  MONT_DEVU DESC]]></queryString>


	<queryString><![CDATA[select  CCFS.COD_PERI, 
         (select iden.num_docu_iden from mae_clien_ident iden where iden.CLIE_OID_CLIE=psc.clie_oid_clie
          and iden.tdoc_oid_tipo_docu = 2001) as Cedula,  
          PSC1.VAL_NUME_SOLI PEDIDO,
          to_char(PSC1.FEC_FACT,'dd/mm/yyyy') FEC_FACT,
          CCFS.VAL_IMPO_CARG 
	 from CCC_HISTO_CARGO_FAMIL_SEGUR CCFS, 
	      ped_solic_cabec psc, 
          ped_solic_cabec psc1
	where CCFS.OID_SOLI_CABE = PSC.OID_SOLI_CABE
	  and PSC.SOCA_OID_SOLI_CABE = PSC1.OID_SOLI_CABE
      and CCFS.COD_PERI between $P{codigoPeriodoInicial} and $P{codigoPeriodoFinal}
     $P!{condicion}]]></queryString>


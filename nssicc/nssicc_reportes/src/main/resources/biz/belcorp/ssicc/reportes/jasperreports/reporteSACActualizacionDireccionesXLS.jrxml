	<queryString><![CDATA[select f.des_zona,a.cod_clie,  a.val_ape1, a.val_ape2, a.val_nom1, a.val_nom2,
a.fec_crea as fec_crea_cons,b.fec_ulti_actu as fec_modi_direc 
from mae_clien a, mae_clien_direc b, zon_terri c, zon_terri_admin d, zon_secci e, zon_zona f
where a.oid_clie = B.CLIE_OID_CLIE
and trunc(b.fec_ulti_actu) = TO_DATE($P{fechaFacturacion},'DD/MM/YYYY')
and B.IND_ELIM = 0
and b.ind_dire_ppal = 1
and trunc(a.fec_crea) <> TO_DATE($P{fechaFacturacion},'DD/MM/YYYY')
and B.TERR_OID_TERR = C.OID_TERR
and c.oid_terr = D.TERR_OID_TERR
and D.IND_BORR = 0
and D.ZSCC_OID_SECC = e.oid_secc
and E.ZZON_OID_ZONA = f.oid_zona
order by b.fec_ulti_actu]]></queryString>


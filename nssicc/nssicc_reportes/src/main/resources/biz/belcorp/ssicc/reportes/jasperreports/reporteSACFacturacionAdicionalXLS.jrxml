		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFin")]]></defaultValueExpression>
	</parameter>
	<parameter name="periodoInicial" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("periodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="periodoFinal" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("periodoFinal")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionRegionExcel" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionRegionExcel")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionZonasExcel" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionZonasExcel")]]></defaultValueExpression>
	</parameter>
	<parameter name="indicadorFAD" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("indicadorFAD")]]></defaultValueExpression>
	</parameter>
	
	<queryString><![CDATA[
	SELECT cod_pais, cod_peri, cod_clie, nombre, cod_regi, des_regi, cod_zona,
         fec_inic, num_ped, fec_fact, val_prec_neto_tota_loca,
         NVL
            ((SELECT ROUND (SUM (    (    (  val_prec_cata_unit_loca
                                           - NVL (val_impo_desc_unit_loca, 0)
                                          )
                                        * num_unid_dema_real
                                      - num_unid_aten
                                     )
                                   / 1
                                 + (val_tasa_impu / 100)
                                ),
                            2
                           )
                FROM ped_solic_posic
               WHERE soca_oid_soli_cabe = oid_soli_cabe
                 AND num_unid_dema_real > num_unid_aten),
             0
            ) fna,
         num_unid_aten_tota,
         NVL ((SELECT SUM (num_unid_dema_real - num_unid_aten)
                 FROM ped_solic_posic
                WHERE soca_oid_soli_cabe = oid_soli_cabe
                  AND num_unid_dema_real > num_unid_aten),
              0
             ) fna_unid,
         DECODE (tota_ped, 1, 'NO', 'SI') ind_fad,
         (SELECT MAX (NVL (val_porc_desc, 0))
            FROM ped_solic_posic
           WHERE soca_oid_soli_cabe = oid_soli_cabe) escala_descuento
    FROM (SELECT   b.oid_soli_cabe, f.cod_pais, e.cod_peri, c.cod_clie,
                      c.val_nom1
                   || ' '
                   || c.val_nom2
                   || ' '
                   || c.val_ape1
                   || ' '
                   || c.val_ape2 nombre,
                   h.cod_regi, h.des_regi, g.cod_zona, i.fec_inic,
                   k.val_tasa_impu,
                   ROW_NUMBER () OVER (PARTITION BY e.cod_peri, c.cod_clie ORDER BY e.cod_peri,
                    c.cod_clie, a.fec_fact) num_ped,
                   a.fec_fact, a.val_prec_neto_tota_loca,
                   a.num_unid_aten_tota,
                   COUNT (1) OVER (PARTITION BY e.cod_peri, c.cod_clie)
                                                                     tota_ped
              FROM ped_solic_cabec a,
                   ped_solic_cabec b,
                   mae_clien c,
                   cra_perio d,
                   seg_perio_corpo e,
                   seg_pais f,
                   zon_zona g,
                   zon_regio h,
                   cra_crono i,
                   cra_activ j,
                   (SELECT a.val_tasa_impu
                      FROM ped_tasa_impue a, ped_impue_gener b
                     WHERE b.sbac_oid_sbac = 888
                       AND b.taim_oid_tasa_impu = a.oid_tasa_impu) k
             WHERE a.oid_soli_cabe = b.soca_oid_soli_cabe
               AND b.clie_oid_clie = c.oid_clie
               AND b.ind_oc = 1
               AND a.perd_oid_peri = d.oid_peri
               AND d.peri_oid_peri = e.oid_peri
               AND e.cod_peri >= $P{periodoInicial}
               AND e.cod_peri <= $P{periodoFinal}
               AND a.val_tota_paga_loca > 0
               AND a.pais_oid_pais = f.oid_pais
               AND a.zzon_oid_zona = g.oid_zona
               AND g.zorg_oid_regi = h.oid_regi
               AND g.oid_zona = i.zzon_oid_zona
               AND d.oid_peri = i.perd_oid_peri
               AND i.cact_oid_acti = j.oid_acti               
               AND j.cod_acti = 'FA'
               and sto_pkg_proce_valid_occ.sto_fn_valid_fad(d.oid_peri,c.oid_clie) > 0
               AND a.fec_fact >= TO_DATE($P{fechaInicio},'dd/mm/yyyy')
               AND a.fec_fact <= TO_DATE($P{fechaFin},'dd/mm/yyyy')
			   $P!{condicionRegionExcel}
			   $P!{condicionZonasExcel}
          ORDER BY e.cod_peri, c.cod_clie)
   WHERE 1 = 1
 $P!{indicadorFAD}
ORDER BY cod_pais, cod_peri, cod_clie, fec_fact,num_ped
 
]]></queryString>


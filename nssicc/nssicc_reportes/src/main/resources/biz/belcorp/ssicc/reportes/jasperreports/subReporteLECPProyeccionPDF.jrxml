	<parameter name="codigoTramo" isForPrompting="true" class="java.lang.String"/>
	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String"/>
	<parameter name="codigoLider" isForPrompting="true" class="java.lang.String"/>
	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[WITH temp AS (
              SELECT lsrr.cam_recu,
                     lsrr.cod_regi,
                     lsrr.cod_zona,
                     lsrr.cod_secc,
                     lsrr.cod_lide,
                     --
                     ( lsrr.val_mont_carg_tota - lsrr.val_mont_cdrs_tota ) montoTotal_t,
                     ( base.val_mont_carg_tota_c - base.val_mont_cdrs_tota_c ) montoTotal_c,
                     ( base.val_mont_carg_tota_nc - base.val_mont_cdrs_tota_nc ) montoTotal_nc,
                     --
                     lrrt.val_mont_recu_tota recaudoTotal_t,
                     base.val_mont_abon_tota_c recaudoTotal_c,
                     base.val_mont_abon_tota_nc recaudoTotal_nc,
                     --
                     ( lsrr.val_mont_carg - lsrr.val_mont_cdrs ) montoNeto_t,
                     ( base.val_mont_carg_c - base.val_mont_cdrs_c ) montoNeto_c,
                     ( base.val_mont_carg_nc - base.val_mont_cdrs_nc ) montoNeto_nc,
                     --
                     lrrt.val_mont_recu recaNetoComis_t,
                     lrrt.val_mont_reca_pedi_cons recaNetoComis_c,
                     lrrt.val_mont_reca_pedi_ncon recaNetoComis_nc,
                     --
                     NULL val_porc_comi_t,
                     base.val_porc_comi_c,
                     base.val_porc_comi_nc,
                     --
                     ( base.val_gana_comi_c + base.val_gana_comi_nc ) val_gana_comi_t,
                     base.val_gana_comi_c,
                     base.val_gana_comi_nc,
                     DECODE (lrrt.LEOB_COD_ESTA_OBJE,  '02', 'SI',  '01', 'NO',  '03', 'NO') indPagoTole,
                     DECODE (lisr.IND_PEDI_CUMP_EXIG, 1, 'SI', 'NO') indExigePedido,                  
                     DECODE( pcot.val_porc_mini_cobr, 0, pcot.val_porc_cobr, pcot.val_porc_mini_cobr ) val_porc_meta_cobr
                   FROM lec_lider_secci_resul_recup lsrr,
                       lec_lider_secci_recup_tramo lrrt,
                       lec_lider_secci_resul lisr,
                       lec_lider_nivel llni,
                       lec_progr_nivel_tramo pntr,
                       LEC_PROGR_COBRA_OBJET_TRAMO PCOT,
                       (
                        SELECT lsdr.pais_cod_pais,
                               lsdr.lpro_cod_prog,
                               lsdr.cod_regi,
                               lsdr.cod_zona,
                               lsdr.cod_secc,
                               lsdr.cod_lide,
                               lsdr.cam_recu,
                               --
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN lsdr.val_mont_carg ELSE 0 END ) val_mont_carg_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN lsdr.val_mont_carg ELSE 0 END ) val_mont_carg_nc,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN lsdr.val_mont_cdrs ELSE 0 END ) val_mont_cdrs_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN lsdr.val_mont_cdrs ELSE 0 END ) val_mont_cdrs_nc,
                               --
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN lsdr.val_mont_carg_tota ELSE 0 END ) val_mont_carg_tota_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN lsdr.val_mont_carg_tota ELSE 0 END ) val_mont_carg_tota_nc,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN lsdr.val_mont_cdrs_tota ELSE 0 END ) val_mont_cdrs_tota_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN lsdr.val_mont_cdrs_tota ELSE 0 END ) val_mont_cdrs_tota_nc,
                               --
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN ldrt.val_mont_abon ELSE 0 END ) val_mont_abon_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN ldrt.val_mont_abon ELSE 0 END ) val_mont_abon_nc,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN ldrt.val_mont_abon_tota ELSE 0 END ) val_mont_abon_tota_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN ldrt.val_mont_abon_tota ELSE 0 END ) val_mont_abon_tota_nc,
                               --
                               MAX( CASE WHEN lsdr.ind_pedi_cons = '1' THEN ldrt.val_porc_comi ELSE 0 END ) val_porc_comi_c,
                               MAX( CASE WHEN lsdr.ind_pedi_cons = '0' THEN ldrt.val_porc_comi ELSE 0 END ) val_porc_comi_nc,
                               --
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '1' THEN ROUND(ldrt.val_mont_abon * (ldrt.val_porc_comi/100),2) ELSE 0 END ) val_gana_comi_c,
                               SUM( CASE WHEN lsdr.ind_pedi_cons = '0' THEN ROUND(ldrt.val_mont_abon * (ldrt.val_porc_comi/100),2) ELSE 0 END ) val_gana_comi_nc
                          FROM lec_lider_secci_detal_recup lsdr,
                               lec_lider_secci_drecu_tramo ldrt       
                         WHERE lsdr.pais_cod_pais = ldrt.pais_cod_pais(+)
                           AND lsdr.lpro_cod_prog = ldrt.lpro_cod_prog(+)
                           AND lsdr.cod_regi = ldrt.cod_regi(+)
                           AND lsdr.cod_zona = ldrt.cod_zona(+)
                           AND lsdr.cod_secc = ldrt.cod_secc(+)
                           AND lsdr.cod_lide = ldrt.cod_lide(+)
                           AND lsdr.cam_recu = ldrt.cam_recu(+)
                           AND lsdr.cod_cons = ldrt.cod_cons(+)
                           AND lsdr.mvcc_oid_movi_cc = ldrt.mvcc_oid_movi_cc(+)
                           AND ldrt.lpct_cod_tram(+) =  $P{codigoTramo}
                           AND lsdr.pais_cod_pais = $P{codigoPais}
                           AND lsdr.cam_recu = $P{codigoPeriodo}                      
                         GROUP BY lsdr.pais_cod_pais,
                                  lsdr.lpro_cod_prog,
                                  lsdr.cod_regi,
                                  lsdr.cod_zona,
                                  lsdr.cod_secc,
                                  lsdr.cod_lide,
                                  lsdr.cam_recu
                       ) base
                 WHERE 1=1
                   AND lsrr.cam_recu = $P{codigoPeriodo}
                   AND lsrr.cam_recu = lrrt.cam_recu(+)
                   AND lsrr.cod_lide = lrrt.cod_lide(+)
                   AND lrrt.lpct_cod_tram = $P{codigoTramo}
                   AND lsrr.cod_lide = lisr.cod_lide(+)
                   AND lsrr.cam_recu = lisr.cam_resu(+)
                   AND lsrr.cod_lide = llni.cod_lide
                   AND lsrr.cam_recu BETWEEN llni.cam_inic AND NVL(llni.cam_fin,lsrr.cam_recu)
                   AND llni.ind_tipo_nive = 'R'
                   AND lsrr.pais_cod_pais = pntr.pais_cod_pais
                   AND lsrr.lpro_cod_prog = pntr.lpro_cod_prog
                   AND llni.lniv_cod_nive = pntr.lniv_cod_nive
                   AND pntr.cod_tram = $P{codigoTramo}
                   AND lsrr.cod_lide = $P{codigoLider}
                   --
                   AND lsrr.pais_cod_pais = base.pais_cod_pais(+)
                   AND lsrr.lpro_cod_prog = base.lpro_cod_prog(+)
                   AND lsrr.cod_regi = base.cod_regi(+)
                   AND lsrr.cod_zona = base.cod_zona(+)
                   AND lsrr.cod_secc = base.cod_secc(+)
                   AND lsrr.cod_lide = base.cod_lide(+)
                   AND lsrr.cam_recu = base.cam_recu(+)
                   ---
                   AND lrrt.pais_cod_pais = PCOT.PAIS_COD_PAIS
                   AND LRRT.LPRO_COD_PROG   = PCOT.LPRO_COD_PROG
                   AND LRRT.LPAG_SEC_AMBI_GEOG = PCOT.LPAG_SEC_AMBI_GEOG
                   AND  LRRT.LPCT_COD_TRAM = PCOT.LPCT_COD_TRAM
                   AND  PCOT.IND_ACTI = 1
                    )
SELECT cam_recu,
       cod_regi,
       cod_zona,
       cod_secc,
       cod_lide,
       montototal_t,
       montototal_c,
       montototal_nc,
       recaudototal_t,
       recaudototal_c,
       recaudototal_nc,
       CASE
          WHEN recaudototal_t >= ( montototal_t * ( val_porc_meta_cobr/100) ) THEN 0
          ELSE ( montototal_t * ( val_porc_meta_cobr/100) ) - recaudototal_t
       END Monto_Faltante,
       montototal_t  -  recaudototal_t  Monto_faltante_100,
       (montototal_c - recaudototal_c) saldoActual_c,
       (montototal_nc - recaudototal_nc) saldoActual_nc,
       ROUND((recaudototal_t/DECODE(montototal_t,0,1,montototal_t))*100,2) porcRecup_t,
       ROUND((recaudototal_c/DECODE(montototal_c,0,1,montototal_c))*100,2) porcRecup_c,
       ROUND((recaudototal_nc/DECODE(montototal_nc,0,1,montototal_nc))*100,2) porcRecup_nc, 
       montoneto_t,
       montoneto_c,
       montoneto_nc,
       (recaudototal_t - recanetocomis_t) recanetonocomis_t,
       (recaudototal_c - recanetocomis_c) recanetonocomis_c,
       (recaudototal_nc - recanetocomis_nc) recanetonocomis_nc,
       recanetocomis_t,
       recanetocomis_c,
       recanetocomis_nc,
       val_porc_comi_t,
       val_porc_comi_c, 
       val_porc_comi_nc,
       val_gana_comi_t,
       val_gana_comi_c,
       val_gana_comi_nc,
       indpagotole,
       indexigepedido   
  FROM temp]]></queryString>


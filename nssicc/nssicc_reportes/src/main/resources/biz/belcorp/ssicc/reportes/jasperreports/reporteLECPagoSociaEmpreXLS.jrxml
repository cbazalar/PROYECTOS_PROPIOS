	<queryString><![CDATA[SELECT CASE WHEN lide.regionBono IS NOT NULL THEN lide.regionBono ELSE lide.regionRecaudo END codigoRegion,
       CASE WHEN lide.zonaBono IS NOT NULL THEN lide.zonaBono ELSE lide.zonaRecaudo END codigoZona,
       CASE WHEN lide.seccionBono IS NOT NULL THEN lide.seccionBono ELSE lide.seccionRecaudo END codigoSeccion,
       (LEC_PKG_PROCE.LEC_FN_OBTE_CODIG_SAP(lide.gere)) codigoSap,
       clid.num_docu_iden numeroCedula,
       lide.gere codigoLider,
       DECODE(clie.val_ape1,NULL,'',clie.val_ape1) || DECODE(clie.val_ape2,NULL,'',' ') ||
       DECODE(clie.val_ape2,NULL,'',clie.val_ape2) || DECODE(clie.val_nom1,NULL,'',' ') ||
       DECODE(clie.val_nom1,NULL,'',clie.val_nom1) || DECODE(clie.val_nom2,NULL,'',' ') ||
       DECODE(clie.val_nom2,NULL,'',clie.val_nom2)  nombreLider,
       (
         SELECT llni.lniv_cod_nive
           FROM lec_lider_nivel llni
          WHERE llni.lpro_cod_prog = $P{codigoPrograma}
            AND llni.cod_lide = lide.gere
            AND llni.ind_tipo_nive = 'R'
            AND $P{codigoPeriodo} BETWEEN llni.cam_inic AND NVL(llni.cam_fin,$P{codigoPeriodo})
       ) nivelBono,
       (
          SELECT llni.lniv_cod_nive
            FROM lec_lider_nivel llni
           WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
             AND llni.cod_lide = lide.gere
             AND llni.ind_tipo_nive = 'R'
             AND $P{codigoPeriodoRecaudo} BETWEEN llni.cam_inic AND NVL(llni.cam_fin,$P{codigoPeriodoRecaudo})
       ) nivelRecaudo,
       --
        (
         SELECT lsop.num_pedi_obje_fina
           FROM lec_lider_secci_objet_pedid lsop
          WHERE lsop.cod_regi = lide.regionRecaudo
            AND lsop.cod_zona = lide.zonaRecaudo
            AND lsop.cod_secc = lide.seccionRecaudo
            AND lsop.lpro_cod_pais = $P{codigoPais}
            AND lsop.lpro_cod_prog = $P{codigoPrograma2}
            AND lsop.cam_obje(+) = $P{codigoPeriodoRecaudo}
       ) npMeta,
       (
          SELECT lpni.tol_pedi
          FROM lec_progr_nivel lpni
         WHERE lpni.pais_cod_pais = $P{codigoPais}
           AND lpni.lpro_cod_prog = $P{codigoPrograma2}
           AND lpni.lniv_cod_nive = (
                                      SELECT llni.lniv_cod_nive
                                        FROM lec_lider_nivel llni
                                      WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
                                         AND llni.cod_lide = lide.gere
                                         AND llni.ind_tipo_nive = 'R'
                                          AND $P{codigoPeriodoRecaudo} BETWEEN llni.cam_inic AND NVL(llni.cam_fin,$P{codigoPeriodoRecaudo})
                                    )
       ) npTolerancia,
       (
        SELECT lisr.num_pedi
          FROM lec_lider_secci_resul lisr
         WHERE lisr.pais_cod_pais = $P{codigoPais}
           AND lisr.lpro_cod_prog = $P{codigoPrograma2}
           AND lisr.cod_regi = lide.regionRecaudo
           AND lisr.cod_zona = lide.zonaRecaudo
           AND lisr.cod_secc = lide.seccionRecaudo
           AND lisr.cod_lide = lide.gere
           AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
       ) npTotalReal,
       --
        (
         SELECT lsop.val_obje_vent
           FROM lec_lider_secci_objet_pedid lsop
          WHERE lsop.cod_regi = lide.regionRecaudo
            AND lsop.cod_zona = lide.zonaRecaudo
            AND lsop.cod_secc = lide.seccionRecaudo
            AND lsop.lpro_cod_pais = $P{codigoPais}
            AND lsop.lpro_cod_prog = $P{codigoPrograma2}
            AND lsop.cam_obje(+) = $P{codigoPeriodoRecaudo}
       ) MetaVenta,
       ( SELECT lpni.val_mont_tole_vent
          FROM lec_progr_nivel lpni
         WHERE lpni.pais_cod_pais = $P{codigoPais}
           AND lpni.lpro_cod_prog = $P{codigoPrograma2}
           AND lpni.lniv_cod_nive = (
                                      SELECT llni.lniv_cod_nive
                                        FROM lec_lider_nivel llni
                                      WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
                                         AND llni.cod_lide = lide.gere
                                         AND llni.ind_tipo_nive = 'R'
                                          AND $P{codigoPeriodoRecaudo} BETWEEN llni.cam_inic AND NVL(llni.cam_fin,$P{codigoPeriodoRecaudo})
                                    )
       ) ToleranciaVenta,
        (
          SELECT nvl(llni.mon_vent_cata,0) + nvl(mont_vent_rtal,0)
            FROM lec_lider_nivel llni
           WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
             AND llni.cod_lide = lide.gere
             AND llni.ind_tipo_nive = 'P'
             AND $P{codigoPeriodoRecaudo} = llni.cam_inic
       ) VentalReal,
        (
        SELECT leob.des_esta_obje
          FROM lec_lider_secci_resul lisr,
               lec_estad_objet leob
         WHERE lisr.leob_cod_esta_obje = leob.cod_esta_obje
           AND lisr.pais_cod_pais = $P{codigoPais}
           AND lisr.lpro_cod_prog = $P{codigoPrograma2}
           AND lisr.cod_regi = lide.regionRecaudo
           AND lisr.cod_zona = lide.zonaRecaudo
           AND lisr.cod_secc = lide.seccionRecaudo
           AND lisr.cod_lide = lide.gere
           AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
       ) npEstatus,
  -- agregue
      (  SELECT lpni.num_meta_ingr
          FROM lec_progr_nivel lpni
         WHERE lpni.pais_cod_pais = $P{codigoPais}
           AND lpni.lpro_cod_prog = $P{codigoPrograma2}
           AND lpni.lniv_cod_nive =
               (SELECT llni.lniv_cod_nive
                  FROM lec_lider_nivel llni
                 WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
                   AND llni.cod_lide = lide.gere
                   AND llni.ind_tipo_nive = 'R'
                   AND $P{codigoPeriodoRecaudo} BETWEEN llni.cam_inic AND
                       NVL(llni.cam_fin, $P{codigoPeriodoRecaudo}))) nMetaIngreso,
       (
       SELECT lisr.num_ingr_secc
       FROM lec_lider_secci_resul lisr
       WHERE lisr.pais_cod_pais = $P{codigoPais}
         AND lisr.lpro_cod_prog = $P{codigoPrograma2}
         AND lisr.cod_regi = lide.regionRecaudo
         AND lisr.cod_zona = lide.zonaRecaudo
         AND lisr.cod_secc = lide.seccionRecaudo
         AND lisr.cod_lide = lide.gere
         AND lisr.cam_resu = $P{codigoPeriodoRecaudo}) RealIngreso,
        (SELECT leob.des_esta_obje
           FROM lec_lider_secci_resul lisr,
                lec_estad_objet       leob
          WHERE lisr.leob_cod_esta_obje_ingr = leob.cod_esta_obje
            AND lisr.pais_cod_pais = $P{codigoPais}
            AND lisr.lpro_cod_prog = $P{codigoPrograma2}
            AND lisr.cod_regi = lide.regionRecaudo
            AND lisr.cod_zona = lide.zonaRecaudo
            AND lisr.cod_secc = lide.seccionRecaudo
            AND lisr.cod_lide = lide.gere
            AND lisr.cam_resu = $P{codigoPeriodoRecaudo}) npEstatusIngre,
       -- FIN agregue
       -------  INICIO RCR  PER-SICC-2015-0548
          (
            SELECT lpni.num_meta_capi
            FROM lec_progr_nivel lpni
            WHERE lpni.pais_cod_pais = $P{codigoPais}
            AND lpni.lpro_cod_prog = $P{codigoPrograma2}
            AND lpni.lniv_cod_nive = (
              SELECT llni.lniv_cod_nive
              FROM lec_lider_nivel llni
              WHERE llni.lpro_cod_prog = $P{codigoPrograma2}
              AND llni.cod_lide = lide.gere
              AND llni.ind_tipo_nive = 'R'
              AND $P{codigoPeriodoRecaudo} BETWEEN llni.cam_inic AND NVL(llni.cam_fin, $P{codigoPeriodoRecaudo})
              )
            ) nMetaCapit, 
            (
              SELECT lisr.num_ingr_secc + LISR.NUM_REIN_SECC - LISR.NUM_EGRE_SECC
              FROM lec_lider_secci_resul lisr
              WHERE  lisr.pais_cod_pais = $P{codigoPais}
              AND lisr.lpro_cod_prog = $P{codigoPrograma2}
              AND lisr.cod_regi = lide.regionRecaudo
              AND lisr.cod_zona = lide.zonaRecaudo
              AND lisr.cod_secc = lide.seccionRecaudo
              AND lisr.cod_lide = lide.gere
              AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
            ) RealCapit, 
            (
              SELECT leob.des_esta_obje
              FROM lec_lider_secci_resul lisr,
                    lec_estad_objet leob
              WHERE LISR.LEOB_COD_ESTA_OBJE_CAPI = leob.cod_esta_obje
              AND lisr.pais_cod_pais = $P{codigoPais}
              AND lisr.lpro_cod_prog = $P{codigoPrograma2}
              AND lisr.cod_regi = lide.regionRecaudo
              AND lisr.cod_zona = lide.zonaRecaudo
              AND lisr.cod_secc = lide.seccionRecaudo
              AND lisr.cod_lide = lide.gere
              AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
            ) npEstatusCapit,
                      
             ------  FIN    
       (
        SELECT lcot.val_porc_cobr
          FROM lec_progr_ambit_geogr lpag,
               lec_progr_cobra_objet_tramo lcot
         WHERE lpag.pais_cod_pais = lcot.pais_cod_pais
           AND lpag.lpro_cod_prog = lcot.lpro_cod_prog
           AND lpag.ltug_cod_tipo_uso_geog = lcot.ltug_cod_tipo_uso_geog
           AND lpag.sec_ambi_geog = lcot.lpag_sec_ambi_geog
           --
           AND lpag.pais_cod_pais = $P{codigoPais}
           AND lpag.lpro_cod_prog = $P{codigoPrograma}
           AND lpag.ltug_cod_tipo_uso_geog = '01'
           AND NVL(lpag.cod_regi,lide.regionRecaudo) = lide.regionRecaudo
           AND NVL(lpag.cod_zona,lide.zonaRecaudo) = lide.zonaRecaudo
           --
           AND lcot.lpct_cod_tram = $P{codigoTramo}
           AND lcot.ind_acti = '1' -- Nuevo
       ) coMeta,
       (
        SELECT lcot.val_porc_mini_cobr
          FROM lec_progr_ambit_geogr lpag,
               lec_progr_cobra_objet_tramo lcot
         WHERE lpag.pais_cod_pais = lcot.pais_cod_pais
           AND lpag.lpro_cod_prog = lcot.lpro_cod_prog
           AND lpag.ltug_cod_tipo_uso_geog = lcot.ltug_cod_tipo_uso_geog
           AND lpag.sec_ambi_geog = lcot.lpag_sec_ambi_geog
           --
           AND lpag.pais_cod_pais = $P{codigoPais}
           AND lpag.lpro_cod_prog = $P{codigoPrograma}
           AND lpag.ltug_cod_tipo_uso_geog = '01'
           AND nvl(lpag.cod_regi,lide.regionRecaudo) = lide.regionRecaudo
           AND nvl(lpag.cod_zona,lide.zonaRecaudo) = lide.zonaRecaudo
           --
           AND lcot.lpct_cod_tram = $P{codigoTramo}
           AND lcot.ind_acti = '1' -- Nuevo
       ) coTolerancia,
         ( 
           SELECT SUM(x.VAL_MONT_VENT_NETA_CONS)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}
       ) coCompraNeta,

       (     SELECT SUM(x.VAL_MONT_COBR_NETO_CONS + x.VAL_MONT_COBR_NETO_NCON)
         FROM LEC_LIDER_RESUM_RECUP x,
              LEC_PROGR_NIVEL_RANGO Y
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}
           AND x.PAIS_COD_PAIS = Y.PAIS_COD_PAIS
           AND x.LPRO_COD_PROG = Y.LPRO_COD_PROG
           AND X.PNRA_COD_RANG = y.COD_RANG 
       ) coCompraNetaRecup,
  (   
         SELECT SUM(x.VAL_MONT_VENT_BASE)
          FROM LEC_CONSU_VENTA_RETAI x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG  = $P{codigoPrograma2}
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.cod_regi = lide.regionRecaudo
           AND x.cod_zona = lide.zonaRecaudo
           AND x.cod_secc = lide.seccionRecaudo
           AND x.COD_CONS = lide.gere
       ) coVentaRetail,
       ( 
         SELECT MAX(x.VAL_PORC_RECU)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}  
       ) coPorCobranzaReal,
      ( 
      SELECT MAX(x.LEOB_COD_ESTA_OBJE)
         FROM LEC_LIDER_RESUM_RECUP x
             WHERE x.PAIS_COD_PAIS = $P{codigoPais}
               AND x.LPRO_COD_PROG = $P{codigoPrograma2}
               AND x.COD_REGI = lide.regionRecaudo
               AND x.COD_ZONA = lide.zonaRecaudo
               AND x.COD_SECC = lide.seccionRecaudo
               AND x.COD_LIDE = lide.gere
               AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
               AND x.LPCT_COD_TRAM = $P{codigoTramo} 
       ) coEstatus, 
       (
        SELECT lisr.num_pedi_cons
          FROM lec_lider_secci_resul lisr
         WHERE lisr.pais_cod_pais = $P{codigoPais}
           AND lisr.lpro_cod_prog = $P{codigoPrograma2}
           AND lisr.cod_regi = lide.regionRecaudo
           AND lisr.cod_zona = lide.zonaRecaudo
           AND lisr.cod_secc = lide.seccionRecaudo
           AND lisr.cod_lide = lide.gere
           AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
       ) pcNumeroPedidos,
       ( 
         SELECT SUM(x.VAL_MONT_VENT_NETA_CONS)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}  
       ) pcCompraNeta,
       (
         SELECT SUM(x.VAL_MONT_COBR_NETO_CONS)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}  
       ) pcCompraNetaRecup,
       ( 
          SELECT SUM(x.VAL_IMPO_COMI_BASE_CONS)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo} 
       ) pcGanancia,
       --
       (
        SELECT lisr.num_pedi_ncon
          FROM lec_lider_secci_resul lisr
         WHERE lisr.pais_cod_pais = $P{codigoPais}
           AND lisr.lpro_cod_prog = $P{codigoPrograma2}
           AND lisr.cod_regi = lide.regionRecaudo
           AND lisr.cod_zona = lide.zonaRecaudo
           AND lisr.cod_secc = lide.seccionRecaudo
           AND lisr.cod_lide = lide.gere
           AND lisr.cam_resu = $P{codigoPeriodoRecaudo}
       ) ncNumeroPedidos,
       ( 
         SELECT SUM(x.VAL_MONT_VENT_NETA_NCON)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo
           AND x.COD_SECC = lide.seccionRecaudo
           AND x.COD_LIDE = lide.gere
           AND x.CAM_RECA = $P{codigoPeriodoRecaudo}
           AND x.LPCT_COD_TRAM = $P{codigoTramo}
       ) ncCompraNeta,
       (           
         SELECT SUM(x.VAL_MONT_COBR_NETO_NCON)
         FROM LEC_LIDER_RESUM_RECUP x
         WHERE x.PAIS_COD_PAIS = $P{codigoPais}
           AND x.LPRO_COD_PROG = $P{codigoPrograma2}
           AND x.COD_REGI = lide.regionRecaudo
           AND x.COD_ZONA = lide.zonaRecaudo

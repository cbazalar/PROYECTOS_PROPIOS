      SELECT gere.gere,
               MAX(CASE WHEN $P{codigoPeriodoRecaudo} BETWEEN peri.cod_peri AND peri2.cod_peri THEN gere.cod_regi ELSE NULL END) regionRecaudo,
               MAX(CASE WHEN $P{codigoPeriodoRecaudo} BETWEEN peri.cod_peri AND peri2.cod_peri THEN gere.cod_zona ELSE NULL END) zonaRecaudo,
               MAX(CASE WHEN $P{codigoPeriodoRecaudo} BETWEEN peri.cod_peri AND peri2.cod_peri THEN gere.cod_secc ELSE NULL END) seccionRecaudo,
               --
               MAX(CASE WHEN $P{codigoPeriodo} BETWEEN  peri.cod_peri AND peri2.cod_peri THEN gere.cod_regi ELSE NULL END) regionBono,
               MAX(CASE WHEN $P{codigoPeriodo} BETWEEN  peri.cod_peri AND peri2.cod_peri THEN gere.cod_zona ELSE NULL END) zonaBono,
               MAX(CASE WHEN $P{codigoPeriodo} BETWEEN  peri.cod_peri AND peri2.cod_peri THEN gere.cod_secc ELSE NULL END) seccionBono
          FROM zon_histo_geren gere,
               cra_perio perd,
               seg_perio_corpo peri,
               cra_perio perd2,
               seg_perio_corpo peri2
         WHERE 1=1
           AND gere.perd_oid_peri_desd = perd.oid_peri
           AND perd.peri_oid_peri = peri.oid_peri
           AND NVL(gere.perd_oid_peri_hast,$P{oidCampanaBono}) = perd2.oid_peri
           AND perd2.peri_oid_peri = peri2.oid_peri
           --
           AND gere.cod_regi IS NOT NULL
           AND gere.cod_zona IS NOT NULL
           AND gere.cod_secc IS NOT NULL
           AND (
                $P{codigoPeriodoRecaudo} BETWEEN peri.cod_peri AND peri2.cod_peri OR
                $P{codigoPeriodo} BETWEEN  peri.cod_peri AND peri2.cod_peri
               )
         GROUP BY gere.gere
       ) lide,
       mae_clien clie,
       mae_clien_ident clid,
       mae_clien_lider prov
       WHERE lide.gere = clie.cod_clie
   AND clie.oid_clie = clid.clie_oid_clie(+)
   AND lide.gere = prov.cod_clid(+)
   AND clid.val_iden_docu_prin(+) = 1
   $P!{condicion}
   $P!{condicionRegionCorreo}
   $P!{condicionZonaCorreo}
 ORDER BY codigoRegion, codigoZona, codigoSeccion]]></queryString>


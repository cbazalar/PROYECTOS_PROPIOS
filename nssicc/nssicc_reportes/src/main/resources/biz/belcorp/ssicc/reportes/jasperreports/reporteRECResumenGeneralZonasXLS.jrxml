										  c.cod_pais=$P{codigoPais} and  
										  d.COD_PAIS=c.cod_pais and
										  d.cod_peri=c.cod_peri and
										  c.des_zona=d.des_zona and  
										 EXISTS (select 1 
			   		  		 			  from  REC_BOREC_ESPEC_detal x
			   							  where x.BREC_OID_BORE_ESPE_CABE=D.OID_BORE_ESPE_CABE and
			   							  ( x.val_codi_vent=$P{codigoVenta} OR $P{codigoVenta}='TODOS')
			   							  )  
	   			  		   				  ) as total,
	TO_CHAR(SYSDATE,'DD/MM/YYYY') as FECHA_PROCESO,
	c.des_soci as SOCIEDAD
from REC_BOREC_ESPEC_CABEC c 
where ( c.cod_clie=$P{codigoConsultora}
        or $P{codigoConsultora}='TODOS') and 
	   c.cod_peri=$P{codigoPeriodo} and 
	   c.cod_pais=$P{codigoPais}
ORDER BY c.des_zona]]></queryString>


	<queryString><![CDATA[select distinct c.des_zona ,(select count(*) from  
  						   REC_BOREC_ESPEC_CABEC d					
	   			  		   			where  (c.cod_clie=$P{codigoConsultora} 
										or $P{codigoConsultora}='TODOS')and  

	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoSociedad" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoSociedad")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicion")]]></defaultValueExpression>
	</parameter>
	<parameter name="porcentajeRecuperacion" isForPrompting="true" class="java.lang.Integer">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("porcentajeRecuperacion")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT c.cod_peri, 
             c.des_regi,
             c.des_zona,
	     c.des_secc,
             SUM(c.imp_fact) imp_fact , 
             SUM(c.imp_fact_neto) imp_fact_neto, 
             SUM(c.cob_dias_21) cob_21,
             SUM(c.cob_dias_vent) cob_31,
             ROUND(SUM(c.imp_fact_neto)*$P{porcentajeRecuperacion}/100,2) cob_esti_31,
	     CASE
	        WHEN SUM(c.imp_fact_neto)>0 THEN
                   ROUND(SUM(c.cob_dias_vent)/SUM(c.imp_fact_neto)*100,2) 
                ELSE 0
             END porc_recu,
             CASE 
 	        WHEN ROUND(SUM(c.imp_fact_neto)*$P{porcentajeRecuperacion}/100,2) - SUM(c.cob_dias_vent) > 0 THEN ROUND(SUM(c.imp_fact_neto)*$P{porcentajeRecuperacion}/100,2) - SUM(c.cob_dias_vent)
 		ELSE 0   
             END imp_pend_recu_esti,
             MIN(c.fec_cier_vent) fec_cier_zona,
             SUM(c.imp_sald_pend_sac) imp_pend_recu
FROM cob_repor_estad_recup_cobra c
WHERE c.cod_pais= $P{codigoPais}
AND c.cod_soci = $P{codigoSociedad}
AND c.cod_peri>= $P{codigoPeriodoInicial}
AND c.cod_peri <= $P{codigoPeriodoFinal}
$P!{condicion}
GROUP BY c.cod_peri, c.des_regi, c.des_zona, c.des_secc
ORDER BY 1 , 2, 3 ASC]]></queryString>


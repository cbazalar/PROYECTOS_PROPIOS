	<queryString><![CDATA[SELECT 
 spc.cod_peri,
 SUM(mcc.imp_movi) imp_movi,
 SUM(mcc.imp_paga) imp_paga,
 SUM(mcc.imp_pend) imp_pend,
 ROUND(SUM(mcc.imp_paga)*100/SUM(mcc.imp_movi),2) por_recu
FROM 
  ccc_movim_cuent_corri mcc,
  cra_perio cp,
  seg_perio_corpo spc
WHERE mcc.subp_oid_subp_crea = 7007
and mcc.perd_oid_peri = cp.oid_peri
AND cp.peri_oid_peri = spc.oid_peri
GROUP BY spc.cod_peri
order BY 1 ASC]]></queryString>


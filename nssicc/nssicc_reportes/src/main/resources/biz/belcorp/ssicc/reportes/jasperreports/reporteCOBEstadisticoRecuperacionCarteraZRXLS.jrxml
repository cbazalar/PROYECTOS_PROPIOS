	</parameter>
	<queryString><![CDATA[SELECT 
   cbz.cod_peri,        
   cbz.des_regi,        
   cbz.cod_zona,   
   NVL((SELECT zz.des_zona
    	FROM zon_zona zz
    	WHERE zz.cod_zona = cbz.cod_zona),'') des_zona,
   FIN_PKG_GENER.FIN_FN_OBTIE_NOMBR_CLIEN(cbz.oid_clie_zona) nom_gere_zona,
   SUM(cbz.imp_fact) imp_fact,  
   SUM(cbz.imp_abon_nmon) imp_abon_no_mon,       
   SUM(cbz.imp_carg_dire) imp_carg_dire,   
   SUM(cbz.imp_fact_neto ) imp_fact_neto,	       
   SUM(cbz.cob_dias_21) cob_21, 
   SUM(cbz.cob_dias_31) cob_31,   
   SUM(cbz.cob_dias_42) cob_42, 
   SUM(cbz.cob_dias_63) cob_63, 
   SUM(cbz.cob_dias_84) cob_84, 
   SUM(cbz.cob_dias_126) cob_126, 
   SUM(cbz.cob_dias_189) cob_189, 
   SUM(cbz.cob_dias_999) cob_999,       
   SUM(cbz.imp_sald_pend_sac) imp_sald_pend_sac   
FROM cob_repor_estad_recup_cobra cbz
WHERE cbz.cod_pais= $P{codigoPais}
AND cbz.cod_soci = $P{codigoSociedad}
AND cbz.cod_peri>= $P{codigoPeriodoInicial}
AND cbz.cod_peri <= $P{codigoPeriodoFinal}
$P!{condicion}
GROUP BY cbz.cod_peri,        
         cbz.des_regi,        
         cbz.cod_zona,
         cbz.oid_clie_zona
ORDER BY 1,2,3 ASC]]></queryString>


	<queryString><![CDATA[SELECT  vp.cod_regi  COD_REGION,
        zr.des_regi  DES_REGION,
        vp.cam_vige  CAM_VIGENCIA,
        vp.fec_inic  FEC_INICIO,
        vp.fec_fina  FEC_FIN
  FROM  sgr_famse_poliz fp,
	sgr_famse_vigen_poliz vp,
        zon_regio zr
 WHERE  zr.cod_regi = vp.cod_regi
   AND  vp.cam_vige >= $P{codigoPeriodoInicio}
   AND  vp.cam_vige <= $P{codigoPeriodoFin}
   AND  vp.cod_regi = DECODE($P{codigoRegion}, null,vp.cod_regi,'',vp.cod_regi,'Todos',vp.cod_regi,$P{codigoRegion})
   AND  fp.cod_poli = vp.poli_cod_poli
   AND  fp.ind_acti = 1
   AND  vp.est_regi <> '9'
ORDER BY vp.cod_regi,
         vp.cam_vige]]></queryString>


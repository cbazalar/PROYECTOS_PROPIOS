	<queryString><![CDATA[SELECT 
       CAB.FEC_FACT,
       CAB.DES_TIPO_DOCU,
       CAB.DOC_LEGAL_INIC AS FACTURA_INICIAL,
       CAB.DOC_LEGAL_FIN AS FACTURA_FINAL,
       DET.COSMETICOS AS COSMETICOS,
       DET.BIJOUTERIE AS BIJOUTERIE,
       DET.ACCESORIOS AS ACCESORIOS,
       DET.TERCEROS AS TERCEROS,
       DET.INCENTIVOS AS INCENTIVOS,
       DET.PROM_USU AS PROM_USUARIOS,
       DET.MATPROM AS MAT_PROMOCIONAL,
       DET.FLEX AS FLEXIPAGO,
       CAB.IMPORTE_BI AS BASE_IMPONIBLE,
       CAB.DTO+DESCUENTO3_SIN_IMPU AS DESCUENTO,
       CAB.IMPORTE_BI_NETA-DESCUENTO3_SIN_IMPU AS VENTA_PRODUCTOS,
       CAB.REDO AS REDONDEO,
       CAB.FLETE AS ING_FLETE,
       CAB.GA AS ING_GA,
       CAB.DESCUENTO3 AS DESCUENTO3,
       CAB.VENTAS_SIN_IVA-DESCUENTO3_SIN_IMPU AS VENTA_SIN_IVA,
       CAB.IVA AS TOTAL_IVA,
       CAB.TOTAL AS TOTAL_FACTURADO,
       DET.ICE AS ICE,
       CAB.TOTAL + DET.ICE AS TOTAL_FACTURADO_CON_ICE
  FROM (SELECT FEC_FACT,
               DES_TIPO_DOCU,
               VAL_SERI_DOCU_LEGA,
               MIN(NUM_DOCU_LEGA) AS DOC_LEGAL_INIC,
               MAX(NUM_DOCU_LEGA) AS DOC_LEGAL_FIN,
               SUM(IMPORTE_BI) AS IMPORTE_BI,
               SUM(DTO) AS DTO,
               SUM(IMPORTE_BI_NETA) AS IMPORTE_BI_NETA,
               SUM(REDO) AS REDO,
               SUM(FLETE) AS FLETE,
               SUM(GA) AS GA,
               SUM(IMPORTE_BI_NETA) - SUM(REDO) + SUM(FLETE)+ SUM(GA) AS VENTAS_SIN_IVA,
               SUM(IVA) AS IVA,
               SUM(TOTAL) AS TOTAL,
               SUM(DESCUENTO3) AS DESCUENTO3,
               SUM(DESCUENTO3_SIN_IMPU) AS DESCUENTO3_SIN_IMPU
          FROM (SELECT FAC_DOCUM_CONTA_CABEC.OID_CABE,
                       FAC_DOCUM_CONTA_CABEC.FEC_FACT,
                       FAC_TIPO_DOCUM.DES_TIPO_DOCU,
                       FAC_DOCUM_CONTA_CABEC.VAL_SERI_DOCU_LEGA,
                       FAC_DOCUM_CONTA_CABEC.NUM_DOCU_LEGA,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE SUM(FAC_DOCUM_CONTA_LINEA.VAL_PREC_SIN_IMPU_TOTA_LOCA)
                       END IMPORTE_BI,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('C', 'R') THEN 0
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B') THEN
                              SUM(DECODE(FAC_DOCUM_CONTA_LINEA.VAL_PREC_CONT_TOTA_LOCA,0,0,PED_SOLIC_POSIC.VAL_PREC_SIN_IMPU_TOTA_LOCA))
                         ELSE SUM(DECODE(FAC_DOCUM_CONTA_LINEA.VAL_PREC_CONT_TOTA_LOCA,0,0,PED_SOLIC_POSIC.VAL_PREC_SIN_IMPU_TOTA_LOCA)) +
                              SUM(FAC_DOCUM_CONTA_LINEA.IMP_DESC_SIN_IMPU_TOTA_LOCA)
                       END DTO,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE SUM(FAC_DOCUM_CONTA_LINEA.VAL_PREC_SIN_IMPU_TOTA_LOCA) -
                              (SUM(DECODE(FAC_DOCUM_CONTA_LINEA.VAL_PREC_CONT_TOTA_LOCA,0,0,PED_SOLIC_POSIC.VAL_PREC_SIN_IMPU_TOTA_LOCA)) +
                               SUM(FAC_DOCUM_CONTA_LINEA.IMP_DESC_SIN_IMPU_TOTA_LOCA))
                       END IMPORTE_BI_NETA,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE FAC_DOCUM_CONTA_CABEC.IMP_FLET_IMPU_TOTA_LOCA
                       END FLETE,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE nvl(FAC_DOCUM_CONTA_CABEC.VAL_TOTA_GAST_ADMI2_SIN_IMPU,0)+nvl(FAC_DOCUM_CONTA_CABEC.VAL_TOTA_GAST_ADMI_SIN_IMPU,0)
                       END GA,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0                         
                         ELSE FAC_DOCUM_CONTA_CABEC.IMP_REDO_LOCA
                       END REDO,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE FAC_DOCUM_CONTA_CABEC.IMP_IMPU_TOTA_LOCA
                       END IVA,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE FAC_DOCUM_CONTA_CABEC.VAL_TOTA_PAGA_LOCA
                       END TOTAL,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                         ELSE FAC_DOCUM_CONTA_CABEC.imp_des3_total_loca
                       END DESCUENTO3,
                       CASE
                         WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN
                              ('B', 'C', 'R') THEN
                          0
                         ELSE
                          FAC_DOCUM_CONTA_CABEC.IMP_DES3_SIN_IMPU_TOTA
                       END DESCUENTO3_SIN_IMPU
                  FROM FAC_DOCUM_CONTA_CABEC,
                       FAC_DOCUM_CONTA_LINEA,
                       PED_SOLIC_POSIC,
                       PED_SOLIC_CABEC PED,
                       INC_CONCU_TIPO_PROG,
                       FAC_TIPO_DOCUM 
                 WHERE FAC_DOCUM_CONTA_CABEC.OID_CABE = FAC_DOCUM_CONTA_LINEA.DCCA_OID_CABE
                   AND FAC_DOCUM_CONTA_LINEA.SOPO_OID_SOLI_POSI = PED_SOLIC_POSIC.OID_SOLI_POSI
                   AND PED_SOLIC_POSIC.SOCA_OID_SOLI_CABE = PED.OID_SOLI_CABE
                   AND FAC_DOCUM_CONTA_CABEC.ICTP_OID_TIPO_PROG = INC_CONCU_TIPO_PROG.OID_TIPO_PROG(+)
                   AND FAC_DOCUM_CONTA_CABEC.TIDO_OID_TIPO_DOCU = FAC_TIPO_DOCUM.OID_TIPO_DOCU
                   AND FAC_DOCUM_CONTA_CABEC.FEC_FACT >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') 
                   AND FAC_DOCUM_CONTA_CABEC.FEC_FACT <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY')
                   AND FAC_DOCUM_CONTA_CABEC.PERD_OID_PERI IN (SELECT OID_PERI
                                                                                FROM CRA_PERIO
                                                                        WHERE (FEC_INIC >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') AND
                                                                                        FEC_FINA <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY'))
                                                                           OR (FEC_INIC <= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') AND
                                                                                            FEC_FINA >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY'))
                                                                                        OR (FEC_INIC <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY') AND
                                                                                            FEC_FINA >= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY')))
                       AND FAC_TIPO_DOCUM.OID_TIPO_DOCU NOT IN ($P{oidGuiaRemision}) 
                   AND FAC_DOCUM_CONTA_CABEC.TIDO_OID_TIPO_DOCU NOT IN ($P{oidNotaCreditoBol}, $P{oidNotaCreditoFact1}, $P{oidNotaCreditoFact2}, $P{oidNCGlobales})
                 GROUP BY FAC_DOCUM_CONTA_CABEC.FEC_FACT,
                          FAC_TIPO_DOCUM.DES_TIPO_DOCU,
                          FAC_DOCUM_CONTA_CABEC.VAL_SERI_DOCU_LEGA,
                          FAC_DOCUM_CONTA_CABEC.NUM_DOCU_LEGA,
                          INC_CONCU_TIPO_PROG.COD_TIPO_PROG,
                          FAC_DOCUM_CONTA_CABEC.IMP_FLET_IMPU_TOTA_LOCA,
                          nvl(FAC_DOCUM_CONTA_CABEC.VAL_TOTA_GAST_ADMI2_SIN_IMPU,0)+nvl(FAC_DOCUM_CONTA_CABEC.VAL_TOTA_GAST_ADMI_SIN_IMPU,0),
                          FAC_DOCUM_CONTA_CABEC.IMP_REDO_LOCA,
                          FAC_DOCUM_CONTA_CABEC.IMP_IMPU_TOTA_LOCA,
                          FAC_DOCUM_CONTA_CABEC.VAL_TOTA_PAGA_LOCA,
                          FAC_DOCUM_CONTA_CABEC.OID_CABE,
                          fac_docum_conta_cabec.imp_des3_total_loca,
              FAC_DOCUM_CONTA_CABEC.IMP_DES3_SIN_IMPU_TOTA)
         GROUP BY FEC_FACT, DES_TIPO_DOCU, VAL_SERI_DOCU_LEGA
         ORDER BY FEC_FACT, DES_TIPO_DOCU) CAB,
       (SELECT FEC_FACT,
               DES_TIPO_DOCU,
               VAL_SERI_DOCU_LEGA,
               SUM(BI_0101) + SUM(BI_0102) + SUM(BI_0103) + SUM(BI_0104) + SUM(BI_0105) + SUM(BI_0106) + SUM(BI_0107) AS COSMETICOS,
               SUM(BI_0201) + SUM(BI_0202) + SUM(BI_0203) AS BIJOUTERIE,
               SUM(BI_0301) + SUM(BI_0302) + SUM(BI_0303) AS ACCESORIOS,
               SUM(BI_0401) AS TERCEROS,
               SUM(BI_0501) AS PROM_USU,
               SUM(BI_0502) AS INCENTIVOS,
               SUM(BI_0503) + SUM(BI_0504) AS MATPROM,
               SUM(ICE) ICE,
               SUM(FLEX) AS FLEX
          FROM (SELECT FEC_FACT,
                       DES_TIPO_DOCU,
                       VAL_SERI_DOCU_LEGA,
                       MIN(NUM_DOCU_LEGA) AS DOC_LEGAL_INIC,
                       MAX(NUM_DOCU_LEGA) AS DOC_LEGAL_FIN,
                       BPCS,
                       SUM(IMPORTE_ICE) ICE,
                       CASE
                         WHEN COD_SAP = '9999999996' THEN
                          SUM(IMPORTE_BI)
                         ELSE
                          0
                       END AS FLEX,
                       CASE
                         WHEN BPCS = '0101' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0101,
                       CASE   
                         WHEN BPCS = '0102' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0102,
                       CASE
                         WHEN BPCS = '0103' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0103,
                       CASE
                         WHEN BPCS = '0104' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0104,
                       CASE
                         WHEN BPCS = '0105' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0105,
                       CASE
                         WHEN BPCS = '0106' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0106,
                       CASE
                         WHEN BPCS = '0107' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0107,
                       CASE
                         WHEN BPCS = '0201' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0201,
                       CASE
                         WHEN BPCS = '0202' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0202,
                       CASE
                         WHEN BPCS = '0203' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0203,
                       CASE
                         WHEN BPCS = '0301' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0301,
                       CASE
                         WHEN BPCS = '0302' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0302,
                       CASE
                         WHEN BPCS = '0303' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0303,
                       CASE
                         WHEN BPCS = '0401' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0401,
                       CASE
                         WHEN BPCS = '0501' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0501,
                       CASE
                         WHEN BPCS = '0502' and COD_SAP <> '9999999996' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0502,
                       CASE
                         WHEN BPCS = '0503' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0503,
                       CASE
                         WHEN BPCS = '0504' THEN SUM(IMPORTE_BI)
                         ELSE 0
                       END AS BI_0504
                  FROM (SELECT FAC_DOCUM_CONTA_CABEC.FEC_FACT,
                               FAC_TIPO_DOCUM.DES_TIPO_DOCU,
                               FAC_DOCUM_CONTA_CABEC.VAL_SERI_DOCU_LEGA,
                               FAC_DOCUM_CONTA_CABEC.NUM_DOCU_LEGA,
                               PROD.VAL_GRUP_ARTI BPCS,
                               PROD.COD_SAP,
                               CASE
                                 WHEN INC_CONCU_TIPO_PROG.COD_TIPO_PROG IN ('B', 'C', 'R') THEN 0
                                 ELSE SUM(FAC_DOCUM_CONTA_LINEA.VAL_PREC_SIN_IMPU_TOTA_LOCA)
                               END IMPORTE_BI,
                               SUM(NVL(FAC_DOCUM_CONTA_LINEA.IMP_IMPU_TOTA_PROD_NACI, 0)) IMPORTE_ICE
                          FROM FAC_DOCUM_CONTA_CABEC,
                               FAC_DOCUM_CONTA_LINEA,
                               PED_SOLIC_POSIC,
                               PED_SOLIC_CABEC PED,
                               INC_CONCU_TIPO_PROG,
                               FAC_TIPO_DOCUM,
                               MAE_PRODU PROD
                         WHERE FAC_DOCUM_CONTA_CABEC.OID_CABE = FAC_DOCUM_CONTA_LINEA.DCCA_OID_CABE
                           AND FAC_DOCUM_CONTA_LINEA.SOPO_OID_SOLI_POSI = PED_SOLIC_POSIC.OID_SOLI_POSI
                           AND PED_SOLIC_POSIC.SOCA_OID_SOLI_CABE = PED.OID_SOLI_CABE
                           AND PED_SOLIC_POSIC.PROD_OID_PROD = PROD.OID_PROD
                           AND FAC_DOCUM_CONTA_CABEC.ICTP_OID_TIPO_PROG = INC_CONCU_TIPO_PROG.OID_TIPO_PROG(+)
                           AND FAC_DOCUM_CONTA_CABEC.TIDO_OID_TIPO_DOCU = FAC_TIPO_DOCUM.OID_TIPO_DOCU
                          AND FAC_DOCUM_CONTA_CABEC.FEC_FACT >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') 
                           AND FAC_DOCUM_CONTA_CABEC.FEC_FACT <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY')
                           AND FAC_DOCUM_CONTA_CABEC.PERD_OID_PERI IN (SELECT OID_PERI
                                                                                FROM CRA_PERIO
                                                                        WHERE (FEC_INIC >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') AND
                                                                                        FEC_FINA <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY'))
                                                                           OR (FEC_INIC <= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY') AND
                                                                                            FEC_FINA >= TO_DATE($P{fechaDesde}, 'DD/MM/YYYY'))
                                                                                        OR (FEC_INIC <= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY') AND
                                                                                            FEC_FINA >= TO_DATE($P{fechaHasta}, 'DD/MM/YYYY')))
                           AND FAC_TIPO_DOCUM.OID_TIPO_DOCU NOT IN ($P{oidGuiaRemision})
                           AND FAC_DOCUM_CONTA_CABEC.TIDO_OID_TIPO_DOCU NOT IN ($P{oidNotaCreditoBol}, $P{oidNotaCreditoFact1}, $P{oidNotaCreditoFact2}, $P{oidNCGlobales})
                         GROUP BY FAC_DOCUM_CONTA_CABEC.FEC_FACT,
                                  FAC_TIPO_DOCUM.DES_TIPO_DOCU,
                                  FAC_DOCUM_CONTA_CABEC.VAL_SERI_DOCU_LEGA,
                                  FAC_DOCUM_CONTA_CABEC.NUM_DOCU_LEGA,
                                  PROD.VAL_GRUP_ARTI,
                                  PROD.COD_SAP,
                                  INC_CONCU_TIPO_PROG.COD_TIPO_PROG)
                 GROUP BY FEC_FACT, DES_TIPO_DOCU, VAL_SERI_DOCU_LEGA, BPCS, COD_SAP
                 ORDER BY FEC_FACT, DES_TIPO_DOCU)
         GROUP BY FEC_FACT, DES_TIPO_DOCU, VAL_SERI_DOCU_LEGA) DET
 WHERE CAB.FEC_FACT = DET.FEC_FACT(+)
   AND CAB.DES_TIPO_DOCU = DET.DES_TIPO_DOCU(+)
 ORDER BY CAB.FEC_FACT]]></queryString>


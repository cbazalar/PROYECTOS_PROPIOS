	<queryString><![CDATA[SELECT DECODE ( COD_MOTI_DEVO, '','00', COD_MOTI_DEVO ) AS COD_MOTI_DEVO, 
       DECODE ( DES_MOTI_DEVO, '','0. Sin Motivo',DES_MOTI_DEVO ) AS DES_MOTI_DEVO,
       VAL_NOMB_PERI,        
       SUM(NUM_GUIA) AS ATEN_RECL,
       SUM(NUM_UNID_RECL) AS CANT_DEVU,
       SUM(IMP_ABON) AS MONT_DEVU        
FROM
(SELECT 
   ( SELECT COD_MOTI_DEVO FROM REC_MOTIV_DEVOL
     WHERE R.MODV_OID_MOTI_DEVO = OID_MOTI_DEVO ) AS COD_MOTI_DEVO,
   DES_MOTV_DEVO AS DES_MOTI_DEVO,
   PERD_OID_PERI,
   NUM_GUIA,
   NUM_UNID_RECL, 
   IMP_ABON,
   C.VAL_NOMB_PERI            
FROM REC_REPOR_OPERA_UNIDA_ADMIN R, CRA_PERIO C
WHERE C.OID_PERI= R.PERD_OID_PERI 
) 
GROUP BY COD_MOTI_DEVO,DES_MOTI_DEVO, VAL_NOMB_PERI]]></queryString>


	<queryString><![CDATA[SELECT
		C.COD_REGI AS REGION, 
		C.COD_ZONA AS ZONA, 
		C.COD_SECC AS SECCION, 
		C.COD_TERR AS TERRITORIO, 
		C.COD_PERI_PROC AS CAMPANA_PROCESO, 
		MC.COD_CLIE AS CLIENTE, 
		MC.VAL_NOM1||' '||MC.VAL_APE1||' '||MC.VAL_APE2 AS NOM_CONS,
		C.VAL_NUME_BORE AS NUMERO_BOLETA_RECOJO, 
		DECODE(C.NUM_RECO,1,'PRIMER RECOJO',2,'SEGUNDO RECOJO') AS NUMERO_RECOJO, 
		TO_CHAR(C.FEC_INGR,'DD/MM/YYYY') AS FECHA_PRIMER_ENVIO,
		TO_CHAR(C.FEC_RECO,'DD/MM/YYYY') AS FECHA_PRIMER_RECOJO,
		TO_CHAR(C.FEC_ING2,'DD/MM/YYYY') AS FECHA_SEGUNDO_ENVIO,
		TO_CHAR(C.FEC_REC2,'DD/MM/YYYY') AS FECHA_SEGUNDO_RECOJO,
		(
		SELECT E2.DES_ESTA_BORE
		FROM INT_REC_ESTAD_BOREC E2
		WHERE 
		E2.OID_ESTA_BORE = C.ESBO_OID_ESTA_BOR1 
		) ESTADO,
		(
		SELECT E2.DES_ESTA_BORE
		FROM INT_REC_ESTAD_BOREC E2
		WHERE 
		E2.OID_ESTA_BORE = C.ESBO_OID_ESTA_BOR2 
		) RESULTADO,
		LB.COD_PROD COD_SAP_RECL, 
		GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(LB.PROD_OID_PROD ,'MAE_PRODU','es')  AS DES_PROD_RECL, 
		DECODE(LB.COPA_OID_PARA_GRAL,
						  NULL,
						  INT_PKG_RECLA.GEN_FN_DEVUE_COD_VENTA_MFACT(LB.TOFE_OID_TIPO_OFER,
																	 LB.MAFA_OID_MATR_FACT,
																	 LB.PROD_OID_PROD),
						  INT_PKG_RECLA.GEN_FN_DEVUE_COD_VENTA_FICTI(LB.LOPA_OID_LOTE_PREM_ARTI,
																	 LB.PANP_OID_PARA_NIVE_PREM,
																	 LB.COPA_OID_PARA_GRAL,
																	 LB.PROD_OID_PROD)) VAL_CODI_VENT_RECL,
		LB.NUM_UNID_RECL AS UNID_RECL,
		LB.VAL_PREC AS VAL_PREC,
		LB.VAL_PREC_CONT AS VAL_CONT,
		MP.VAL_COST_ESTD AS VAL_COSEST,
		DECODE(LB.COD_PROD_DISC,NULL,LB.COD_PROD, LB.COD_PROD_DISC) AS COD_SAP_RECOGIDO, 
		GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(DECODE(LB.PROD_OID_PROD_DISC,NULL,LB.PROD_OID_PROD, LB.PROD_OID_PROD_DISC) ,'MAE_PRODU','es') AS DES_PROD_RECOGIDO, 
		LB.NUM_UNID_RECO AS UNID_RECOGIDA, 
		LB.COD_OPER AS OPERACION,
		RCR.NUM_RECL,
		TO_CHAR(RCR.FEC_INGR,'DD/MM/YYYY') AS FEC_INGR,
		GEN_PKG_GENER.GEN_FN_DEVUELVE_DES_PERIO( RCR.PERD_OID_PERI_DOCU_REFE ) PER_REFE,
		decode(c.IND_ENVI_CON_PED1,1,'SI','NO') CON_PEDIDO_1,  
        c.VAL_NUME_SOLI_PED1                    NRO_PEDIDO_1,
        decode(c.IND_ENVI_CON_PED2,1,'SI','NO') CON_PEDIDO_2,
        c.VAL_NUME_SOLI_PED2                    NRO_PEDIDO_2,
        C.COD_PERI_DESP1, 
		C.COD_PERI_DESP2		
		FROM INT_REC_LINEA_BOREC LB,
			 INT_REC_CABEC_BOREC C,
			 MAE_CLIEN MC,
			INT_REC_ESTAD_BOREC REB,
			ZON_ZONA Z, 
			ZON_REGIO R,
			MAE_PRODU MP,
			REC_CABEC_RECLA RCR
		WHERE  C.CLIE_OID_CLIE = MC.OID_CLIE
		AND LB.COD_CABE_BORE = C.COD_CABE_BORE
		AND LB.PROD_OID_PROD = MP.OID_PROD(+)
		AND LB.CARE_OID_CABE_RECL = RCR.OID_CABE_RECL(+)
		AND R.COD_REGI= C.COD_REGI
		AND Z.COD_ZONA = C.COD_ZONA
		AND C.ESBO_OID_ESTA_BOR2=REB.OID_ESTA_BORE(+)
		$P!{campania}
		AND (
		($P{fechaInicioFacturacion} IS NULL AND $P{fechaFinFacturacion} IS NULL) OR 
		((C.FEC_INGR <= TO_DATE($P{fechaFinFacturacion},'DD/MM/YYYY') AND C.FEC_INGR >= TO_DATE($P{fechaInicioFacturacion},'DD/MM/YYYY')) OR  
		 (C.FEC_ING2 <= TO_DATE($P{fechaFinFacturacion},'DD/MM/YYYY') AND C.FEC_ING2 >= TO_DATE($P{fechaInicioFacturacion},'DD/MM/YYYY')))
		)
		$P!{region}
		$P!{zona}
		AND ($P{numeroRecojo} IS NULL OR C.NUM_RECO = $P{numeroRecojo})
		$P!{condicion}]]></queryString>


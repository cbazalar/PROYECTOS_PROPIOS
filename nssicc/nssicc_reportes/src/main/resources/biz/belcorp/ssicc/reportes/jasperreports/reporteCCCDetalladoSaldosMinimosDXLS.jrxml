	<parameter name="importeHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("importeHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[with ddeu  as
(
SELECT REG.COD_REGI, reg.oid_regi, ZON.COD_ZONA, zon.OID_ZONA, mcc.CLIE_OID_CLIE, 
        mcc.IMP_MOVI, mcc.IMP_PEND, mcc.FEC_DOCU, mcc.FEC_VENC,
        mcc.PERD_OID_PERI, mcc.VAL_EJER_CUOT, mcc.NUM_IDEN_CUOT, mcc.TSPA_OID_TIPO_SOLI_PAIS, mcc.SOCA_OID_SOLI_CABE,
        ua.ZTAD_OID_TERR_ADMI, za.ZSCC_OID_SECC, sec.COD_SECC, za.TERR_OID_TERR 
from  
      ccc_movim_cuent_corri mcc,
      mae_clien_unida_admin ua,
      zon_terri_admin za,
      zon_secci sec,
      zon_regio reg,
      zon_zona zon        
 where 
      ua.ztad_oid_terr_admi = za.oid_terr_admi
 AND za.zscc_oid_secc = sec.oid_secc
 AND sec.zzon_oid_zona = zon.oid_zona
 and zon.ZORG_OID_REGI = reg.OID_REGI
 AND ua.ind_acti = 1
 and mcc.CLIE_OID_CLIE = ua.CLIE_OID_CLIE
 and mcc.FEC_VENC < trunc(sysdate)
 and mcc.IMP_PEND <> 0
 $P!{condicion}
order by cod_regi, cod_zona, clie_oid_clie, perd_oid_peri ),
tdeu as
( select ddeu.clie_oid_clie, sum( ddeu.imp_pend) tot_deu
    from ddeu
    group by ddeu.clie_oid_clie
    having sum( ddeu.imp_pend) <  $P{importeHasta}
    and sum( ddeu.imp_pend) >  0
     ),
sdeu as     
( select ddeu.*, cor.cod_peri   
  from   ddeu,
         tdeu,
         cra_perio cam,
         seg_perio_corpo cor
  where  ddeu.clie_oid_clie = tdeu.clie_oid_clie
  and    ddeu.perd_oid_peri = cam.oid_peri
  and    cam.PERI_OID_PERI =  cor.OID_PERI
  order by cod_regi, cod_zona, ddeu.clie_oid_clie, perd_oid_peri )
 SELECT sdeu.COD_PERI,  sdeu.COD_REGI, sdeu.COD_ZONA, sdeu.cod_secc, mae.cod_clie, MAE.VAL_NOM1 || ' ' || MAE.VAL_NOM2 nom_clie, MAE.VAL_APE1, MAE.VAL_APE2, 
         sdeu.imp_pend,  tdo.cod_tipo_docu, tdo.val_sigl, ide.num_docu_iden,
        tc.VAL_TEXT_COMU TELEF_CASA, tcl.VAL_TEXT_COMU CELULAR,
        tv.DES_ABRV_TIPO_VIA || ' ' ||   mcd.VAL_NOMB_VIA||' '||mcd.NUM_PPAL DIRECCION , mcd.VAL_OBSE Referencia, 
       CASE    WHEN SUBSTR (mcd.cod_unid_geog, 1, 6) IS NULL
        THEN NULL
        ELSE (SELECT nivel1.des_geog
           FROM zon_valor_estru_geopo nivel1
           WHERE nivel1.orde_1 = substr(mcd.COD_UNID_GEOG,1,6)
              AND nivel1.orde_2 IS NULL
              AND nivel1.orde_3 IS NULL
              AND nivel1.orde_4 IS NULL
              AND nivel1.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END departamento,
        CASE    WHEN (SUBSTR (mcd.cod_unid_geog, 7, 6) IS NULL)
        THEN NULL
        ELSE (SELECT nivel2.des_geog
           FROM zon_valor_estru_geopo nivel2
           WHERE nivel2.orde_1 = SUBSTR (mcd.cod_unid_geog, 1, 6)
              AND nivel2.orde_2 = SUBSTR (mcd.cod_unid_geog, 7, 6)
              AND nivel2.orde_3 IS NULL
              AND nivel2.orde_4 IS NULL
              AND nivel2.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END provincia,
       CASE    WHEN (SUBSTR (mcd.cod_unid_geog, 13, 6) IS NULL)
        THEN NULL
        ELSE (SELECT nivel3.des_geog
           FROM zon_valor_estru_geopo nivel3
           WHERE nivel3.orde_1 = SUBSTR (mcd.cod_unid_geog, 1, 6)
              AND nivel3.orde_2 = SUBSTR (mcd.cod_unid_geog, 7, 6)
              AND nivel3.orde_3 = SUBSTR (mcd.cod_unid_geog, 13, 6)
              AND nivel3.orde_4 IS NULL
              AND nivel3.pais_oid_pais = egeop.pais_oid_pais
              AND ROWNUM = 1)
        END distrito,
        CASE
                      WHEN EXISTS (
                             SELECT oid_soli_cabe
                               FROM ped_solic_cabec p,
                                    ped_tipo_solic_pais tp,
                                    ped_tipo_solic t
                              WHERE p.soca_oid_soli_cabe =
                                                  sdeu.soca_oid_soli_cabe
                                AND p.tspa_oid_tipo_soli_pais =
                                                         tp.oid_tipo_soli_pais
                                AND tp.tsol_oid_tipo_soli = t.oid_tipo_soli
                                AND p.ind_oc = 1
                                AND t.ind_soli_nega = 0)
                         THEN sdeu.val_ejer_cuot || trim(TO_CHAR(sdeu.num_iden_cuot,'00000000'))  
                         else null 
          end  BolDesp,                      
        sdeu.fec_docu,
        sdeu.fec_venc     
 from sdeu,
      mae_clien mae,
      mae_clien_comun tc,
      mae_clien_comun tcl,
      seg_tipo_via tv,
      mae_clien_direc mcd,
      zon_valor_estru_geopo egeop,
      zon_terri  terr,
      mae_clien_ident ide,
      mae_tipo_docum tdo 
 where  mae.OID_CLIE = sdeu.clie_oid_clie
 AND sdeu.TERR_OID_TERR = terr.OID_TERR
 AND mae.OID_CLIE = tc.CLIE_OID_CLIE (+)  AND  1 = tc.TICM_OID_TIPO_COMU (+)
 AND mae.OID_CLIE = tcl.CLIE_OID_CLIE (+) AND  6 = tcl.TICM_OID_TIPO_COMU (+)  
 AND mae.OID_CLIE = mcd.CLIE_OID_CLIE(+)  AND  mcd.IND_DIRE_PPAL = 1  AND  mcd.ind_elim = 0
 AND terr.vepo_oid_valo_estr_geop = egeop.oid_valo_estr_geop(+) 
 AND mcd.TIVI_OID_TIPO_VIA = tv.OID_TIPO_VIA
 AND ide.clie_oid_clie = sdeu.clie_oid_clie
 AND ide.TDOC_OID_TIPO_DOCU = tdo.OID_TIPO_DOCU
 AND ide.VAL_IDEN_DOCU_PRIN = 1]]></queryString>


	<queryString><![CDATA[SELECT 
	   (SELECT P.VAL_NOMB_COMP FROM BAS_PAIS P WHERE P.COD_PAIS = $P{codigoPais}) PAIS,   	
	   A.CAM_PARA_MAV CAMPANHA,
       A.COR_PARA_CONF NUMMAV,
       A.COD_ACTI CODACTIVIDAD,
       G.VAL_I18N ACTIVIDAD,
       (SELECT FC.DES_FORM_COBR
          FROM MAV_FORMA_COBRO FC
         WHERE FC.OID_FORM_COBR = A.FOR_COBR) FORMACOBRO,      
       (SELECT G.VAL_I18N ALMACEN
          FROM GEN_I18N_SICC G
         WHERE G.VAL_OID = A.FOR_PAGO
           AND G.ATTR_ENTI = 'BEL_FORMA_PAGO') FORMAPAGO,
       CASE A.IND_ORIG_MAV
         WHEN 'D' THEN
          'DIGITACION'
         ELSE
          'CARGA MASIVA'
       END ORIGENMAV,
       CASE A.IND_ENVI_MENS
         WHEN 'S' THEN
          'SI'
         ELSE
          'NO'
       END INDMENSAJE,       
       CASE A.IND_UNID_DIF
         WHEN 'S' THEN
          'SI'
         ELSE
          'NO'
       END INDUNDDIFE,
       ( SELECT ECS.DES_ENVI_SOLI
           FROM MAV_PARAM_CONFI MA
           JOIN MAV_ACTIV TV
             ON TV.OID_ACTI = MA.ATOF_ACTI_OID_ACTI
            AND TV.EST_REGI = 1
           JOIN MAV_ACTIV_TIPO_DESPA VTD
             ON VTD.ACTI_OID_ACTI = TV.OID_ACTI
           JOIN MAV_ENVIO_SOLIC ECS
             ON ECS.OID_ENVI_SOLI = VTD.ENVS_OID_ENVI_SOLI
          WHERE VTD.ENVS_OID_ENVI_SOLI = A.ENV_SOLI
            AND ROWNUM = 1) ENVSOLI,       
       (SELECT TD.DES_TIPO_DESP
          FROM MAV_PARAM_CONFI MA
          JOIN MAV_ACTIV TV
            ON TV.OID_ACTI = MA.ATOF_ACTI_OID_ACTI
           AND TV.EST_REGI = 1
          JOIN MAV_ACTIV_TIPO_DESPA VTD
            ON VTD.ACTI_OID_ACTI = TV.OID_ACTI
          JOIN MAV_TIPO_DESPA TD
            ON vtd.tdch_oid_tipo_desp = tD.Oid_Tipo_Desp
         WHERE vtd.oid_acti_tipo_desp = A.ACT_TIPO_DESP
           AND ROWNUM = 1) TIPODESPA,
       A.VAL_BASE_ESTI_DEST ESTIMADO,
       A.COD_TIPO_OFER CODTOFERTA,
       G1.VAL_I18N TIPOOFERTA,
       A.COD_SAP CODIGOSAP,
       (SELECT P.COD_SAP || ' - ' || MEC.VAL_I18N PRODUCTO
		  FROM MAE_PRODU P
		  JOIN GEN_I18N_SICC MEC
		    ON MEC.ATTR_ENTI = 'MAE_PRODU'
		   AND MEC.VAL_OID = P.OID_PROD
		 WHERE P.COD_SAP = A.COD_SAP)DESPRODUCTO,            
       CASE CORES.IND_CONS_REST
         WHEN 'C' THEN
          'CONSIDERACIÓN'
         ELSE
          'RESTRICCIÓN'
       END TIPO,
       CORES.ABR_CONS_REST ABRCORES,
       CORES.DES_CONS_REST,
       CORES.IND_TIPO INDTIPO,
       CASE CORES.IND_DIRI
         WHEN 'C' THEN
          'CONSULTORA'
         ELSE
          'GERENTE'
       END DIRIGIDOA,
       --        CORES.CORE_COR_CONS_REST ,    
       CORES.VAL_CONS_REST_1 CONRES1,
       CORES.VAL_CONS_REST_2 CONRES2,
       CORES.VAL_CONS_REST_3 CONRES3,
       CORES.VAL_CONS_REST_4 CONRES4,
       CORES.VAL_CONS_REST_5 CONRES5,
       CASE A.COD_ESTA_MAV
         WHEN 1 THEN
          'PENDIENTE'
         WHEN 2 THEN
          'COMPLETADO'
         WHEN 3 THEN
          'INICIADO'
         WHEN 4 THEN
          'GENERADO'
         ELSE
          'DESACTIVADO'
       END ESTADOMAV,
       (SELECT G.VAL_I18N
          FROM MAV_PARAM_CONFI MA
          JOIN MAV_ACTIV TV
            ON TV.OID_ACTI = MA.ATOF_ACTI_OID_ACTI
           AND TV.EST_REGI = 1
          JOIN BEL_ALMAC B
            ON TV.ALMA_OID_ALMA = B.OID_ALMA
          JOIN GEN_I18N_SICC G
            ON G.VAL_OID = B.OID_ALMA
           AND G.ATTR_ENTI = 'BEL_ALMAC'
         WHERE MA.ATOF_ACTI_OID_ACTI = A.ATOF_ACTI_OID_ACTI
           AND ROWNUM = 1
        ) ALMACEN,
       (SELECT G.VAL_I18N ALMACEN
          FROM MAV_PARAM_CONFI MA
          JOIN MAV_ACTIV TV
            ON TV.OID_ACTI = MA.ATOF_ACTI_OID_ACTI
           AND TV.EST_REGI = 1
          JOIN MAE_TIPO_CLIEN TC
            ON TC.OID_TIPO_CLIE = TV.TICL_OID_TIPO_CLIE
          JOIN GEN_I18N_SICC G
            ON G.VAL_OID = TC.OID_TIPO_CLIE
           AND G.ATTR_ENTI = 'MAE_TIPO_CLIEN'
         WHERE MA.ATOF_ACTI_OID_ACTI = A.ATOF_ACTI_OID_ACTI
           AND ROWNUM = 1) TIPOCLIENTE,
    A.NUM_UNID_PROD UNIDADES
  FROM MAV_PARAM_CONFI A,
       MAV_ACTIV_TIPO_OFERT ACTIV,
       GEN_I18N_SICC_PAIS G,
       (SELECT *
          FROM OWN_COMUN.GEN_I18N_SICC_COMUN
         WHERE ATTR_ENTI = 'PRE_TIPO_OFERT') G1,
       (SELECT * FROM GEN_I18N_SICC_PAIS WHERE ATTR_ENTI = 'MAE_PRODU') G2,
       (SELECT CAM_PROC_MAV,
               PAIS_COD_PAIS,
               PACO_COR_PARA_CONF,
               COR_CONS_REST,
               ABR_CONS_REST,
               DES_CONS_REST,
               IND_CONS_REST,
               IND_TIPO,
               IND_DIRI,
               CORE_COR_CONS_REST,
               VAL_CONS_REST_1,
               VAL_CONS_REST_2,
               VAL_CONS_REST_3,
               VAL_CONS_REST_4,
               VAL_CONS_REST_5
          FROM (SELECT CRDET.CAM_PROC_MAV,
                       CRDET.PAIS_COD_PAIS,
                       CRDET.PACO_COR_PARA_CONF,
                       CR.COR_CONS_REST,
                       CR.ABR_CONS_REST,
                       CR.DES_CONS_REST,
                       CR.IND_CONS_REST,
                       CR.IND_TIPO,
                       CR.IND_DIRI,
                       CRCAB.CORE_COR_CONS_REST,
                       CRDET.VAL_CONS_REST_1,
                       CRDET.VAL_CONS_REST_2,
                       CRDET.VAL_CONS_REST_3,
                       CRDET.VAL_CONS_REST_4,
                       CRDET.VAL_CONS_REST_5
                  FROM MAV_PARAM_CORES_CABEC CRCAB,
                       MAV_PARAM_CORES_DETAL CRDET,
                       MAV_CONSI_RESTR       CR
                 WHERE CRCAB.PAIS_COD_PAIS = $P{codigoPais}
                   AND CRCAB.PACO_COR_PARA_CONF = $P{correlativo}
                   AND CRCAB.CAM_PROC_MAV = $P{codigoPeriodoBusq}
                   AND CRDET.PAIS_COD_PAIS = CRCAB.PAIS_COD_PAIS
                   AND CRDET.PACO_COR_PARA_CONF = CRCAB.PACO_COR_PARA_CONF
                   AND CRDET.CORE_COR_CONS_REST = CRCAB.CORE_COR_CONS_REST
                   AND CR.COR_CONS_REST = CRCAB.CORE_COR_CONS_REST
                   AND CR.ABR_CONS_REST NOT IN ('UAS', 'TCL')
                   AND CRCAB.EST_REGI = '1'
                   AND CRCAB.IND_ACTI = 1
                   AND CRDET.EST_REGI = '1'
                   AND CRDET.IND_ACTI = 1
                UNION
                SELECT CRCAB.CAM_PROC_MAV,
                       CRCAB.PAIS_COD_PAIS,
                       CRCAB.PACO_COR_PARA_CONF,
                       CR.COR_CONS_REST,
                       CR.ABR_CONS_REST,
                       CR.DES_CONS_REST,
                       CR.IND_CONS_REST,
                       CR.IND_TIPO,
                       CR.IND_DIRI,
                       CRCAB.CORE_COR_CONS_REST,
                       CRDET.VAL_CONS_REST_1,
                       CRDET.VAL_CONS_REST_2,
                       CRDET.VAL_CONS_REST_3,
                       CRDET.VAL_CONS_REST_4,
                       CRDET.VAL_CONS_REST_5
                  FROM MAV_PARAM_CORES_CABEC CRCAB,
                       MAV_PARAM_CORES_DETAL CRDET,
                       MAV_CONSI_RESTR       CR
                 WHERE CRCAB.PAIS_COD_PAIS = $P{codigoPais}
                   AND CRCAB.PACO_COR_PARA_CONF = $P{correlativo}
                   AND CRCAB.CAM_PROC_MAV = $P{codigoPeriodoBusq}
                   AND CR.COR_CONS_REST = CRCAB.CORE_COR_CONS_REST
                   AND CRDET.PAIS_COD_PAIS = CRCAB.PAIS_COD_PAIS
                   AND CRDET.PACO_COR_PARA_CONF = CRCAB.PACO_COR_PARA_CONF
                   AND CRDET.CORE_COR_CONS_REST = CRCAB.CORE_COR_CONS_REST
                   AND CR.ABR_CONS_REST IN ('UAS', 'TCL')
                   AND CRCAB.EST_REGI = '1'
                   AND CRCAB.IND_ACTI = 1
                   AND CRDET.EST_REGI = '1'
                   AND CRDET.IND_ACTI = 1
                   AND (SELECT COUNT(1)
                          FROM MAV_PARAM_CORES_DETAL CRDET
                         WHERE CRDET.PAIS_COD_PAIS = CRCAB.PAIS_COD_PAIS
                           AND CRDET.PACO_COR_PARA_CONF =
                               CRCAB.PACO_COR_PARA_CONF
                           AND CRDET.CORE_COR_CONS_REST =
                               CRCAB.CORE_COR_CONS_REST
                           AND CRDET.EST_REGI = '1'
                           AND CRDET.IND_ACTI = 1) > 0)
         ORDER BY IND_CONS_REST DESC, COR_CONS_REST) CORES
 WHERE 1 = 1
   AND A.CAM_PARA_MAV = $P{codigoPeriodoBusq}
   AND G.VAL_OID = A.ATOF_ACTI_OID_ACTI
   AND G.ATTR_ENTI = 'MAV_ACTIV'
   AND A.ATOF_OID_TIPO_OFER_ACTI = ACTIV.OID_TIPO_OFER_ACTI
   AND ACTIV.TOFE_OID_TIPO_OFER = G1.VAL_OID
   AND A.PROD_OID_PROD = G2.VAL_OID
   AND A.EST_REGI = 1
   AND A.COR_PARA_CONF = $P{correlativo}
   AND A.PAIS_COD_PAIS = CORES.PAIS_COD_PAIS
   AND A.CAM_PARA_MAV = CORES.CAM_PROC_MAV
   AND A.COR_PARA_CONF = CORES.PACO_COR_PARA_CONF
   AND ROWNUM = 1]]></queryString>


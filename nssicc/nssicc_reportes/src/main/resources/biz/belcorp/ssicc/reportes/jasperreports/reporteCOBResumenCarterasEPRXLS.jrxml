	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoEtapaDeuda" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoEtapaDeuda")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 
  e.val_desc des_etap_deud, 
  d.cod_peri,
  zr.cod_regi,
  zr.des_regi,
 TO_CHAR(SUM(d.imp_deud_asig),'999,999,999.99') imp_deud_asig,
 TO_CHAR(SUM(d.imp_deud_canc),'999,999,999.99') imp_deud_canc,
 TO_CHAR(SUM(d.imp_deud_pend),'999,999,999.99') imp_deud_pend,
 ROUND(SUM(d.imp_deud_pend)*100/SUM(d.imp_deud_asig),2) imp_porc_recu,
 CASE
   WHEN MAX(d.fec_cier) - TRUNC(SYSDATE) > 0 THEN TO_CHAR(MAX(d.fec_cier) - TRUNC(SYSDATE))
   ELSE
    'Cartera Cerrada'
 END val_dias_gest 
FROM cob_detal_asign_carte d,
     cob_etapa_deuda_pais e,
     zon_regio zr
WHERE d.cod_etap_deud = e.cod_etap_deud
  AND d.cod_regi = zr.cod_regi 
  AND d.cod_etap_deud = $P{codigoEtapaDeuda}
  AND d.cod_peri >= $P{codigoPeriodoInicial}
  AND d.cod_peri <= $P{codigoPeriodoFinal}
GROUP BY e.val_desc,d.cod_peri,zr.cod_regi,zr.des_regi 
ORDER BY 1,2,3 ASC]]></queryString>


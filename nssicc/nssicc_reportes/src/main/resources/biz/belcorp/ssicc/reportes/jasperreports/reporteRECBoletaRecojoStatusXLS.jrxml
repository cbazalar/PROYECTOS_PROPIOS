	<queryString>
	<![CDATA[


SELECT c.val_nume_bore, 
       c.cod_peri_proc, 
       (SELECT num_recl 
          FROM rec_cabec_recla 
         WHERE oid_cabe_recl = l.care_oid_cabe_recl) nro_cdr, -- num_recl 
       decode(l.copa_oid_para_gral, 
              NULL, 
              int_pkg_recla.gen_fn_devue_cod_venta_mfact(l.tofe_oid_tipo_ofer, 
                                                         l.mafa_oid_matr_fact, 
                                                         l.prod_oid_prod), 
              int_pkg_recla.gen_fn_devue_cod_venta_ficti(l.lopa_oid_lote_prem_arti, 
                                                         l.panp_oid_para_nive_prem, 
                                                         l.copa_oid_para_gral, 
                                                         l.prod_oid_prod)) val_codi_vent, 
       int_pkg_recla.gen_fn_codsap_prod(l.prod_oid_prod) codigo_sap, --codigo_sap 
       int_pkg_recla.gen_fn_desc_prod(l.prod_oid_prod) descripcion_producto, -- descripcion_producto 
       c.cod_clie,
       gen_pkg_gener.gen_fn_clien_datos_oid(c.clie_oid_clie, 'NOM_CLIE') nombre_cliente, -- nombre cliente 
       c.cod_regi, 
       c.cod_zona, 
       gen_pkg_gener.gen_fn_clien_datos(c.cod_clie, 'DES_REGI') region, 
       gen_pkg_gener.gen_fn_clien_datos(c.cod_clie, 'DES_ZONA') zona, 
       l.num_unid_recl, 
       decode(l.ind_disc, 1, 0, l.num_unid_reco) num_unid_reco, 
       0 num_unid_elim, 
       (SELECT a.des_motn_reco_bore 
          FROM int_rec_motno_recoj_borec a 
         WHERE a.oid_motn_reco_bore = c.more_oid_motn_reco_bore) primer_motivo_no_recojo, 
       (SELECT a.des_motn_reco_bore 
          FROM int_rec_motno_recoj_borec a 
         WHERE a.oid_motn_reco_bore = c.more_oid_motn_reco_bor2) segundo_motivo_no_recojo, 
       int_pkg_recla.gen_fn_devue_deest_borec(c.esbo_oid_esta_bor1) gestion, -- gestión 
       int_pkg_recla.gen_fn_devue_deest_borec(c.esbo_oid_esta_bor2) resultado, -- resultado         
       l.val_prec precio_facturado, 
       decode(l.num_unid_recl, 0, 0, 
                         (l.num_unid_recl - 
                          decode(l.ind_disc, 1, 0, l.num_unid_reco)) * 
                          l.val_prec) cargo, 
       decode(l.num_unid_recl, 0, 
                         decode(l.ind_disc, 1, 0, l.num_unid_reco) * 
                          l.val_prec_disc, 0) abono,
	(select dees.val_i18n ESTADO 
	  from mae_clien_datos_adici clda,mae_estat_clien esta, 
	       gen_i18n_sicc_comun dees, mae_clien clie 
	 where clda.esta_oid_esta_clie = esta.oid_esta_clie 
	   and clda.esta_oid_esta_clie = dees.val_oid 
	   and clda.clie_oid_clie = clie.oid_clie 
	   and dees.attr_enti = 'MAE_ESTAT_CLIEN' 
	   and clie.cod_clie = c.cod_clie) ESTADO,
	TO_CHAR(c.fec_ingr,'dd/MM/yyyy')  FECHA_GENER,
	c.val_nume_soli_ped1  CON_PEDIDO, 
    psc.val_nume_soli PEDIDO_APLICA, 
    psc.val_tota_paga_loca MONTO_PEDIDO_APLICA
  FROM int_rec_cabec_borec c, 
       int_rec_linea_borec l, 
       int_rec_estad_borec p, 
       int_rec_estad_borec r, 
       ped_solic_cabec psc 
 WHERE l.cod_cabe_bore = c.cod_cabe_bore 
   AND l.soca_oid_soli_cabe = psc.oid_soli_cabe(+) 
   AND c.cod_peri_proc between $P{codigoPeriodoInicio} and $P{codigoPeriodoFin}   
   AND p.oid_esta_bore = c.esbo_oid_esta_bor1 
   $P!{condicionGestion}
   AND (r.oid_esta_bore = c.esbo_oid_esta_bor2 OR 
       c.esbo_oid_esta_bor2 IS NULL) 
   $P!{condicionResultado}       
   AND l.prod_oid_prod IS NOT NULL 
   AND l.refe_cod_line_bore IS NULL 
   AND (TRUNC(c.FEC_INGR)=TO_DATE($P{fechaProceso},'dd/MM/yyyy') or ($P{fechaProceso} is null))
   $P!{condicionRegion}
   $P!{condicionZona}   
UNION 
SELECT c.val_nume_bore, 
       c.cod_peri_proc, 
       (SELECT num_recl 
          FROM rec_cabec_recla 
         WHERE oid_cabe_recl = l.care_oid_cabe_recl) nro_cdr, -- num_recl 
       l.cod_vent_disc val_codi_vent, 
       int_pkg_recla.gen_fn_codsap_prod(l.prod_oid_prod_disc) codigo_sap, --cod_sap 
       int_pkg_recla.gen_fn_desc_prod(l.prod_oid_prod_disc) descripcion_producto, -- MODIFICADO   
       c.cod_clie,
       gen_pkg_gener.gen_fn_clien_datos_oid(c.clie_oid_clie, 'NOM_CLIE') nombre_cliente, -- nombre cliente 
       c.cod_regi, 
       c.cod_zona, 
       gen_pkg_gener.gen_fn_clien_datos(c.cod_clie, 'DES_REGI') region, 
       gen_pkg_gener.gen_fn_clien_datos(c.cod_clie, 'DES_ZONA') zona, 
       0 num_unid_recl, 
       l.num_unid_reco, 
       0 num_unid_elim, 
       (SELECT a.des_motn_reco_bore 
          FROM int_rec_motno_recoj_borec a 
         WHERE a.oid_motn_reco_bore = c.more_oid_motn_reco_bore) primer_motivo_no_recojo, 
       (SELECT a.des_motn_reco_bore 
          FROM int_rec_motno_recoj_borec a 
         WHERE a.oid_motn_reco_bore = c.more_oid_motn_reco_bor2) segundo_motivo_no_recojo, 
       int_pkg_recla.gen_fn_devue_deest_borec(c.esbo_oid_esta_bor1) gestion, -- gestión 
       int_pkg_recla.gen_fn_devue_deest_borec(c.esbo_oid_esta_bor2) resultado, -- resultado       
       l.val_prec precio_facturado, 
       0 cargo, 
       decode(num_unid_recl, 0, 0, 
                         num_unid_reco * val_prec_disc) abono ,
	(select dees.val_i18n ESTADO 
	  from mae_clien_datos_adici clda,mae_estat_clien esta, 
	       gen_i18n_sicc_comun dees, mae_clien clie 
	 where clda.esta_oid_esta_clie = esta.oid_esta_clie 
	   and clda.esta_oid_esta_clie = dees.val_oid 
	   and clda.clie_oid_clie = clie.oid_clie 
	   and dees.attr_enti = 'MAE_ESTAT_CLIEN' 
	   and clie.cod_clie = c.cod_clie) ESTADO,
	TO_CHAR(c.fec_ing2,'dd/MM/yyyy')  FECHA_GENER,
	c.val_nume_soli_ped2  CON_PEDIDO, 
    psc.val_nume_soli PEDIDO_APLICA, 
    psc.val_tota_paga_loca MONTO_PEDIDO_APLICA 
  FROM int_rec_cabec_borec c, 
       int_rec_linea_borec l, 
       int_rec_estad_borec p, 
       int_rec_estad_borec r, 
       ped_solic_cabec psc
 WHERE l.cod_cabe_bore = c.cod_cabe_bore 
   AND l.soca_oid_soli_cabe = psc.oid_soli_cabe(+) 
   AND c.cod_peri_proc between $P{codigoPeriodoInicio} and $P{codigoPeriodoFin}
   AND p.oid_esta_bore = c.esbo_oid_esta_bor1 
   $P!{condicionGestion}
   AND (r.oid_esta_bore = c.esbo_oid_esta_bor2 OR 
       c.esbo_oid_esta_bor2 IS NULL) 
   $P!{condicionResultado}       
   AND l.prod_oid_prod IS NOT NULL 
   AND l.prod_oid_prod_disc IS NOT NULL 
   AND (TRUNC(c.FEC_INGR)=TO_DATE($P{fechaProceso},'dd/MM/yyyy') or ($P{fechaProceso} is null))
   $P!{condicionRegion}
   $P!{condicionZona}
 ORDER BY cod_regi, cod_zona, cod_clie, val_nume_bore 
	

	]]>
	</queryString>


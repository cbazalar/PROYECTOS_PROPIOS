	</parameter>
	<parameter name="condicionZona" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionZona")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT DISTINCT ircb.cod_regi, ircb.cod_zona, ircb.esbo_oid_esta_bor2,
									NVL ((SELECT des_esta_bore
												FROM int_rec_estad_borec
											   WHERE oid_esta_bore = ircb.esbo_oid_esta_bor2),
											 'SIN CALIFICAR'
											) des_esta_bore,
									SUM (DECODE (ircb.num_reco, 1, 1, 0)) num_reco_1,
									SUM (DECODE (ircb.num_reco, 2, 1, 0)) num_reco_2
								 FROM int_rec_cabec_borec ircb
							  WHERE 1 = 1
									AND (( $P!{fechaIngrReco} >= TO_DATE ($P{fechaInicio}, 'DD/MM/YYYY') AND $P!{fechaIngrReco} <= TO_DATE ($P{fechaFin}, 'DD/MM/YYYY') ) 
									OR ( $P!{fechaIngrReco2} >= TO_DATE ($P{fechaInicio}, 'DD/MM/YYYY') AND $P!{fechaIngrReco2} <= TO_DATE ($P{fechaFin}, 'DD/MM/YYYY') )) 
									$P!{condicionRegion}
									$P!{condicionZona}
							GROUP BY ircb.cod_regi, ircb.cod_zona, ircb.esbo_oid_esta_bor2
							ORDER BY ircb.cod_regi, ircb.cod_zona, ircb.esbo_oid_esta_bor2]]></queryString>


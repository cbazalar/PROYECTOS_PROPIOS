		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaInicio")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaFin" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFin")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaIngrReco" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaIngrReco")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaIngrReco2" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaIngrReco2")]]></defaultValueExpression>
	</parameter>

	<queryString><![CDATA[SELECT 
cod_zona, 
max(nom_gere) as nom_gere,
cod_lide, 
nom_lide, 
cod_secc, 
per_rela,
per_rel1,
per_rel2,
per_rel3,
per_rel4,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_TOTAL_DEUDORAS(cod_zona, cod_secc)  as TOTAL_DEUDORAS,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_DEUDORAS(cod_zona, cod_secc)  as DEUDORAS,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_DEUDORAS1(cod_zona, cod_secc) as DEUDORAS1,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_DEUDORAS2(cod_zona, cod_secc) as DEUDORAS2,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_DEUDORAS3(cod_zona, cod_secc) as DEUDORAS3,
PER_PKG_REPOR_PERCE.PER_FN_OBTIE_DEUDORAS4(cod_zona, cod_secc) as DEUDORAS4,
SUM (SAL_PEND + PAG_DIFE) AS DEUDA,
SUM (SAL_PEN1 + PAG_DIF1) AS DEUDA1,
SUM (SAL_PEN2 + PAG_DIF2) AS DEUDA2,
SUM (SAL_PEN3 + PAG_DIF3) AS DEUDA3,
SUM (SAL_PEN4 + PAG_DIF4) AS DEUDA4
FROM 
  PER_REPOR_FACTU_PENDI_SECCI
where 
  cod_lide is not null 
GROUP BY 
  cod_zona, 
  cod_secc, 
  cod_lide, 
  nom_lide, 
  per_rela,
  per_rel1,
  per_rel2,
  per_rel3,
  per_rel4]]></queryString>


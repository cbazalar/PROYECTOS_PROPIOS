	<queryString><![CDATA[SELECT 
    FEC_FACT,
    COD_PERI,
    DES_TIPO_PROG,
    TIPO_SOLICITUD,
    DES_REGI,
    DES_ZONA,
    COD_CLIE,
    NOMBRE,
    CI,
    BOLETA_DESPACHO,
    AGRUPACION,
    IND_GRATIS,
    COD_VENTA,
    TIPO_OFERTA,
    COD_SAP,
    PRODUCTO,
    SOLIC,  
    ATND,
    PRECIO_UNIT_CAT,
    TOTAL_CATALOGO,
    PORC_DTO,
    DESCUENTO,
    TOT_CON_DTO,
    OBSERVACIONES,
    PEDIDO_SERVICIO,
    CATEGORIA
from
    (
        select
            to_char(cons.fec_fact,'dd/mm/YYYY') FEC_FACT,
            seg_perio_corpo.COD_PERI,
            INC_CONCU_TIPO_PROG.DES_TIPO_PROG,
            ts.VAL_I18N as tipo_solicitud,
            i.VAL_I18N categoria, 
            cons.OID_SOLI_CABE,
            PED_SOLIC_POSIC.OID_SOLI_POSI,    
            DES_REGI,
            cod_zona,
            DES_ZONA,
            cod_terr,
            PED_SOLIC_CABEC_SECUE.NUM_SECU_FACT_DIAR, 
            cod_clie,
            nombre,
            ci,
            cons.VAL_NUME_SOLI as boleta_despacho,
            case 
                when PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA = 0 then 'MAV'
                when PED_SOLIC_POSIC.VAL_PORC_DESC is null then 'PRODUCTOS SIN DESCUENTO'
                when PED_SOLIC_POSIC.VAL_PORC_DESC is not null then 'PRODUCTOS CON DESCUENTO' 
            end as agrupacion,
            case
                when PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA = 0 then 0
                else 8
            end ind_gratis,
            PED_SOLIC_POSIC.VAL_CODI_VENT as cod_venta,
            tof.VAL_I18N as tipo_oferta,
            cod_sap,
            dp.VAL_I18N as producto,
            PED_SOLIC_POSIC.NUM_UNID_DEMA_REAL as solic,
            PED_SOLIC_POSIC.NUM_UNID_ATEN as atnd,
            case
                when PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA = 0 then PED_SOLIC_POSIC.VAL_PREC_CONT_UNIT_LOCA 
                else PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA
            end as Precio_unit_cat,
            case
                when PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA = 0 then PED_SOLIC_POSIC.VAL_PREC_CONT_UNIT_LOCA * PED_SOLIC_POSIC.NUM_UNID_ATEN 
                else PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA * PED_SOLIC_POSIC.NUM_UNID_ATEN
            end as total_catalogo,
            case 
                when PED_SOLIC_POSIC.VAL_PORC_DESC is null then 0
                else PED_SOLIC_POSIC.VAL_PORC_DESC
            end as porc_dto,
            PED_SOLIC_POSIC.VAL_IMPO_DESC_TOTA_LOCA as descuento,
            PED_SOLIC_POSIC.VAL_PREC_FACT_TOTA_LOCA as tot_con_dto,
            case
                when PED_SOLIC_POSIC.VAL_PREC_CATA_UNIT_LOCA = 0 then 'Gratis'
                when PED_SOLIC_POSIC.NUM_UNID_ATEN = 0 then 'Agotado'
                else '' 
            end as observaciones,
            cons.VAL_IMPO_DESC_1_TOTA_LOCA as dto1,
            cons.VAL_PREC_CONT_TOTA_LOCA as dto2,
            cons.VAL_IMPO_FLET_TOTA_LOCA as flete,
            cons.VAL_TOTA_PAGA_LOCA as total,
            case
                when ped.ICTP_OID_TIPO_PROG is not null then 0
                else PED_SOLIC_POSIC.VAL_PREC_CONT_TOTA_LOCA
            end as prec_gratis,
            case
                when ped.ICTP_OID_TIPO_PROG is not null then PED_SOLIC_POSIC.VAL_PREC_CONT_TOTA_LOCA
                else 0
            end as prec_premio,
            cons.VAL_IMPO_REDO_LOCA,
            INC_CONCU_TIPO_PROG.OID_TIPO_PROG,
            ped.COPA_OID_PARA_GENE,
            decode((select sum(ind_oc) from ped_solic_cabec where soca_oid_soli_cabe=cons.oid_soli_cabe),0,'SI','NO') PEDIDO_SERVICIO  
       FROM
            ped_solic_cabec ped,
            ped_solic_cabec cons,
            PED_SOLIC_CABEC_SECUE, 
            PED_SOLIC_POSIC,
            mae_produ,
            cra_perio,
            seg_perio_corpo,
            (select VAL_I18N, VAL_OID from  GEN_I18N_SICC_PAIS where ATTR_ENTI = 'MAE_PRODU') dp,
            (select VAL_I18N, VAL_OID from  own_comun.GEN_I18N_SICC_COMUN where ATTR_ENTI = 'PED_TIPO_SOLIC') ts,
            ped_tipo_solic_pais ptsp,
            ped_tipo_solic pts,
            INC_CONCU_TIPO_PROG,
            PRE_TIPO_OFERT,
            PRE_OFERT_DETAL,
            (select VAL_I18N, VAL_OID from  own_comun.GEN_I18N_SICC_COMUN where ATTR_ENTI = 'PRE_TIPO_OFERT') tof, 
            mae_negoc negoh, v_gen_i18n_sicc i,
            (
                select
                    ZON_REGIO.DES_REGI,
                    cod_zona, 
                    ZON_ZONA.DES_ZONA,
                    cod_terr,
                    oid_clie,
                    cod_clie,
                    VAL_NOM1 ||' '||VAL_NOM2 ||' '||VAL_APE1 ||' '||VAL_APE2 as nombre,
                    NUM_DOCU_IDEN as ci
                from
                     MAE_CLIEN,
                     MAE_CLIEN_IDENT,
                     MAE_CLIEN_UNIDA_ADMIN,
                     ZON_TERRI_ADMIN,
                     ZON_TERRI,
                     ZON_SECCI,
                     ZON_ZONA,    
                     ZON_REGIO
                where
                    1=1    
                    and MAE_CLIEN_UNIDA_ADMIN.CLIE_OID_CLIE = MAE_CLIEN.OID_CLIE
                    and MAE_CLIEN_UNIDA_ADMIN.ZTAD_OID_TERR_ADMI = ZON_TERRI_ADMIN.OID_TERR_ADMI
                    and ZON_TERRI_ADMIN.ZSCC_OID_SECC = ZON_SECCI.OID_SECC
                    and ZON_TERRI_ADMIN.TERR_OID_TERR = ZON_TERRI.OID_TERR
                    and ZON_SECCI.ZZON_OID_ZONA = ZON_ZONA.OID_ZONA
                    and ZON_ZONA.ZORG_OID_REGI = ZON_REGIO.OID_REGI
                    and MAE_CLIEN_UNIDA_ADMIN.IND_ACTI = 1
                    and oid_clie = MAE_CLIEN_IDENT.CLIE_OID_CLIE
                    and MAE_CLIEN_IDENT.VAL_IDEN_DOCU_PRIN = 1
            ) ua
        where
            1=1
            and ped.ICTP_OID_TIPO_PROG = INC_CONCU_TIPO_PROG.OID_TIPO_PROG(+)
            and oid_prod = PED_SOLIC_POSIC.PROD_OID_PROD
            and oid_prod = dp.VAL_OID
            and PED_SOLIC_POSIC.SOCA_OID_SOLI_CABE = ped.OID_SOLI_CABE
            and ped.TSPA_OID_TIPO_SOLI_PAIS = ptsp.OID_TIPO_SOLI_PAIS
            and ptsp.TSOL_OID_TIPO_SOLI = pts.OID_TIPO_SOLI
            and pts.OID_TIPO_SOLI = ts.VAL_OID
            and ped_solic_posic.prod_oid_prod = oid_prod
            and nego_oid_nego = negoh.oid_nego
            and i.attr_enti = 'MAE_NEGOC'
            and i.idio_oid_idio = 1
            and i.val_oid = negoh.oid_nego
            and ped.SOCA_OID_SOLI_CABE = cons.OID_SOLI_CABE
            and cons.perd_oid_peri = cra_perio.oid_peri
            and cra_perio.PERI_OID_PERI = seg_perio_corpo.OID_PERI
            and ua.oid_clie = cons.CLIE_OID_CLIE
            and PED_SOLIC_CABEC_SECUE.SOCA_OID_SOLI_CABE = cons.oid_soli_cabe 
            and cons.val_nume_soli =$P{nroBoleta}
            and cons.val_tota_paga_loca >= 0
            and PED_SOLIC_POSIC.OFDE_OID_DETA_OFER = PRE_OFERT_DETAL.OID_DETA_OFER(+)
            and PRE_OFERT_DETAL.TOFE_OID_TIPO_OFER = PRE_TIPO_OFERT.OID_TIPO_OFER(+)
            and PRE_TIPO_OFERT.OID_TIPO_OFER = tof.VAL_OID(+)
        order by 
            cons.val_nume_soli,
            agrupacion desc,
            PED_SOLIC_POSIC.VAL_CODI_VENT
    )]]></queryString>


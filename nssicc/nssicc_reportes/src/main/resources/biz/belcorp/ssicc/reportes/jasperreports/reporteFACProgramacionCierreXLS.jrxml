		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("tipoCierre")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaFacDesde" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFacDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaFacHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFacHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT X.CAM_PROC,TO_CHAR(X.FEC_CIER, 'DD/MM/YYYY') FEC_CIER,
CASE WHEN X.TIP_CIER = 'Z' THEN 'Cierre Zona'
    WHEN X.TIP_CIER = 'R' THEN 'Cierre Región'
    WHEN X.TIP_CIER = 'C' THEN 'Cierre Campaña'
END TIPO_CIERRE,
X.COD_REGI,
(SELECT R.DES_REGI FROM ZON_REGIO R WHERE R.COD_REGI=X.COD_REGI ) AS DES_REGI,
X.COD_ZONA,
(SELECT Z.DES_ZONA FROM ZON_ZONA Z WHERE Z.COD_ZONA=X.COD_ZONA ) AS DES_ZONA,
X.COD_CAMP,
CASE WHEN X.EST_CIER = 'E' THEN 'Pendiente'
    WHEN X.EST_CIER = 'A' THEN 'Aprobado'
    WHEN X.EST_CIER = 'P' THEN 'Procesado'
   END EST_CIER
 FROM FAC_PROGR_CIERR X WHERE 
X.FEC_CIER BETWEEN
TO_DATE($P{fechaFacDesde}, 'DD/MM/YYYY') AND
TO_DATE($P{fechaFacHasta}, 'DD/MM/YYYY')
AND X.CAM_PROC BETWEEN $P{codigoPeriodoInicio} AND $P{codigoPeriodoFin}
$P!{tipoCierre}
$P!{estado}
ORDER BY X.FEC_CIER DESC]]></queryString>


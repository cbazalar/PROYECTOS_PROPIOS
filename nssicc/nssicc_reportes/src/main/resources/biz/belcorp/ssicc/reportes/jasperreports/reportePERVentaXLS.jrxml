					case
						when bpcs = '0105' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0105,
					case
						when bpcs = '0106' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0106,
					case
						when bpcs = '0106' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0106,
					case
						when bpcs = '0107' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0107,
					case
						when bpcs = '0107' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0107,
					case
						when bpcs = '0201' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0201,
					case
						when bpcs = '0201' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0201,
					case
						when bpcs = '0202' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0202,
					case
						when bpcs = '0202' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0202,
					case
						when bpcs = '0203' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0203,
					case
						when bpcs = '0203' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0203,
					
					case
						when bpcs = '0301' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0301,
					case
						when bpcs = '0301' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0301,
					case
						when bpcs = '0302' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0302,
					case
						when bpcs = '0302' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0302,
					case
						when bpcs = '0303' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0303,
					case
						when bpcs = '0303' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0303,
					case
						when bpcs = '0401' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0401,
					case
						when bpcs = '0401' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0401,
					case
						when bpcs = '0501' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0501,
					case
						when bpcs = '0501' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0501,
					case
						when bpcs = '0502' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0502,
					case
						when bpcs = '0502' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0502,
					case
						when bpcs = '0503' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0503,
					case
						when bpcs = '0503' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0503,
					case
						when bpcs = '0504' then sum(unid_atnd)
						else 0 
					end as unid_atnd_0504,
					case
						when bpcs = '0504' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_0504,
					
					/*case
						when bpcs = 'Unidad Negocios Salud-Bienestar' then sum(unid_atnd)
						else 0 
					end as unid_atnd_salud,
					case
						when bpcs = 'Unidad Negocios Salud-Bienestar' then sum(importe_bi_neta)
						else 0 
					end as bi_neta_salud,*/
					
					--sum(importe_bi) as importe_bi,
					--sum(importe_dto1) as importe_dto1,
					--sum(importe_dto2) as importe_dto2,
					--sum(dto) as dto,
					--sum(importe_bi_neta) as importe_bi_neta,
					sum(flete) as flete,
					--sum(flete) over (partition by 1) as flete1,
					sum(iva) as iva,
					sum(total) as total
				from
					(
						select
							cons.fec_fact,
							oid_cabe,
							FAC_TIPO_DOCUM.DES_TIPO_DOCU,
							FAC_DOCUM_CONTA_CABEC.VAL_SERI_DOCU_LEGA,
							FAC_DOCUM_CONTA_CABEC.NUM_DOCU_LEGA,
							--pn.VAL_I18N as negocio,
							--pun.VAL_I18N as unidad_negocio,
							prod.val_grup_arti bpcs,
							sum(FAC_DOCUM_CONTA_LINEA.NUM_UNID_ATEN) unid_atnd,
							sum(FAC_DOCUM_CONTA_LINEA.val_prec_sin_impu_tota_loca) as importe_bi,
							sum(FAC_DOCUM_CONTA_LINEA.imp_desc_sin_impu_tota_loca) as importe_dto1,
							sum(decode(FAC_DOCUM_CONTA_LINEA.val_prec_cont_tota_loca,0,0,PED_SOLIC_POSIC.val_prec_sin_impu_tota_loca)) as importe_dto2,
							sum(decode(FAC_DOCUM_CONTA_LINEA.val_prec_cont_tota_loca,0,0,PED_SOLIC_POSIC.val_prec_sin_impu_tota_loca)) + sum(FAC_DOCUM_CONTA_LINEA.imp_desc_sin_impu_tota_loca) as dto,
							sum(FAC_DOCUM_CONTA_LINEA.val_prec_sin_impu_tota_loca) - (sum(decode(FAC_DOCUM_CONTA_LINEA.val_prec_cont_tota_loca,0,0,PED_SOLIC_POSIC.val_prec_sin_impu_tota_loca)) + sum(FAC_DOCUM_CONTA_LINEA.imp_desc_sin_impu_tota_loca)) as importe_bi_neta,
							FAC_DOCUM_CONTA_CABEC.imp_impu_tota_loca as iva,
							FAC_DOCUM_CONTA_CABEC.IMP_FLET_IMPU_TOTA_LOCA as flete,
							FAC_DOCUM_CONTA_CABEC.val_tota_paga_loca as total

						from
							ped_solic_cabec ped,
							ped_solic_cabec cons,
							PED_SOLIC_ACUMU_IMPUE,
							PED_SOLIC_POSIC,
							mae_produ prod,
							pre_ofert_detal pod,
							pre_tipo_ofert tof,
							(select VAL_I18N, VAL_OID from GEN_I18N_SICC_PAIS where ATTR_ENTI = 'MAE_PRODU') dp,
							--(select VAL_I18N, VAL_OID from GEN_I18N_SICC_PAIS where ATTR_ENTI = 'MAE_UNIDA_NEGOC') pun,
							--(select VAL_I18N, VAL_OID from GEN_I18N_SICC_PAIS where ATTR_ENTI = 'MAE_NEGOC') pn,		
							(select VAL_I18N, VAL_OID from GEN_I18N_SICC_COMUN where ATTR_ENTI = 'PED_TIPO_SOLIC') ts,
							(select VAL_I18N, VAL_OID from GEN_I18N_SICC_COMUN where ATTR_ENTI = 'PRE_TIPO_OFERT') dtof,
							ped_tipo_solic_pais ptsp,
							ped_tipo_solic pts,
							FAC_DOCUM_CONTA_CABEC,
							FAC_TIPO_DOCUM,
							INC_CONCU_TIPO_PROG,
							FAC_DOCUM_CONTA_LINEA,
							FAC_DOCUM_CONTA_ACUMU_IMPUE,
							FAC_REGIS_UNICO_VENTA,
							(
								select
									ZON_REGIO.DES_REGI,
									ZON_ZONA.DES_ZONA,
									oid_clie,
									cod_clie,
									VAL_NOM1 ||' '||VAL_NOM2 ||' '||VAL_APE1 ||' '||VAL_APE2 as nombre
								from
									MAE_CLIEN,
									MAE_CLIEN_UNIDA_ADMIN,
									ZON_TERRI_ADMIN,
									ZON_SECCI,
									ZON_ZONA,	

	</parameter>
	<queryString><![CDATA[select (select sub.Cod_Subl_Arma from ape_subli_armad sub where sub.liar_oid_line_arma = li.oid_line_arma and rownum=1)  SUBLINEA, 
           detalle.num_anaq ANAQUEL,
           DECODE(DET.COEC_OID_CODI_ERRO_CHEQ ,4, 'Sobrante', 2, 'Error Picado') SOBFAL,
		   cab.oid_list_pica_cabe,
		   (det.num_unid_prod) SOLICITA,
           (det.num_unid_cheq) CANTIDAD,
           producto.cod_sap PRODUCTO,
           (SELECT seg.val_i18n
          FROM gen_i18n_sicc_pais seg
         WHERE seg.attr_enti = 'MAE_PRODU'
           AND seg.val_oid = producto.oid_prod) DESCRIPCION,
           det.val_nomb_pica PICADORAS,
           soli.val_nume_soli PEDIDO,
           det.num_caja CAJA,
	(select gen.VAL_I18N from GEN_I18N_SICC_PAIS gen where gen.VAL_OID = (select li.OID_LINE_ARMA from ape_linea_armad li where li.NUM_CODI_LINE=1) and gen.ATTR_ENTI = 'APE_LINEA_ARMAD') linea
from ape_lista_picad_detal det
inner join ape_lista_picad_cabec cab on det.lpca_oid_list_pica_cabe = cab.oid_list_pica_cabe
inner join ped_solic_cabec soli on soli.oid_soli_cabe = cab.soca_oid_soli_cabe
and soli.pais_oid_pais = (select pais.oid_pais from seg_pais pais where pais.oid_pais = soli.pais_oid_pais and pais.cod_pais =$P{codigoPais})
inner join app_confi_centr_distr centro on centro.oid_conf_cent_dist=cab.ccdi_oid_conf_cent_dist and centro.cod_cent_dist=$P{centro}
inner join ape_linea_armad li on cab.liar_oid_line_arma = li.oid_line_arma AND LI.NUM_CODI_LINE=$P{linea}
inner join mae_produ producto on det.prod_oid_prod = producto.oid_prod
inner join ape_mapa_centr_distr_detal detalle on detalle.oid_mapa_cent_dist_deta=det.mcdd_oid_mapa_cent_dist_deta
and soli.fec_fact = to_date($P{fecha}, 'dd/mm/yyyy')
and det.COEC_OID_CODI_ERRO_CHEQ is not null]]></queryString>


	<queryString><![CDATA[WITH temp1
   AS
    (SELECT 
       z.cod_peri,
       z.cod_regi,      
       z.cod_zona,
       SUM(z.imp_fact_neto) imp_fact_neto_63,
       SUM(z.cob_dias_63) imp_cob_63,
       ROUND(SUM(z.cob_dias_63)*100/SUM(z.imp_fact_neto),2) imp_porc_63
     FROM cob_repor_estad_recup_cobra z
     WHERE z.cod_peri=$P{codigoPeriodo}
     AND z.cod_regi=$P{codigoRegion}
     GROUP BY z.cod_peri,z.cod_regi,z.cod_zona)
     SELECT 
            t1.cod_peri,
            t1.cod_regi,
            zr.des_regi,
            (SELECT TRIM(mc.val_nom1) || ' ' || 
                    TRIM(mc.val_nom2) || ' ' || 
                    TRIM(mc.val_ape1) || ' ' || 
                    TRIM(mc.val_ape2) 
             FROM mae_clien mc
             WHERE mc.oid_clie = zr.clie_oid_clie) nom_clie_regi,
            t1.cod_zona,
            (SELECT TRIM(mc.val_nom1) || ' ' || 
                    TRIM(mc.val_nom2) || ' ' || 
                    TRIM(mc.val_ape1) || ' ' || 
                    TRIM(mc.val_ape2) 
             FROM mae_clien mc
             WHERE mc.oid_clie = zz.clie_oid_clie) nom_clie_zona,
            t1.imp_fact_neto_63,
            t1.imp_cob_63,
            t1.imp_porc_63
     FROM temp1 t1,
          zon_regio zr,
          zon_zona zz
     WHERE t1.cod_regi = zr.cod_regi
     AND t1.cod_zona = zz.cod_zona
     ORDER BY 1,2,3,4,5 ASC



]]></queryString>


	</parameter>
	<parameter name="importeDesde" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("importeDesde")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
 zr.des_regi,
 zz.des_zona,
 mc.cod_clie,
 mc.val_nom1 || ' ' || mc.val_nom2 || ' ' || mc.val_ape1 || ' ' ||
 mc.val_ape2 nom_clie,
 mci.num_docu_iden,
 spc.cod_peri,
 mcc.fec_docu,
 mcc.imp_movi,
 mcc.imp_pend
  FROM ccc_movim_cuent_corri mcc,
       cra_perio             cp,
       seg_perio_corpo       spc,
       mae_clien             mc,
       mae_clien_ident       mci,
       mae_clien_unida_admin mcua,
       zon_terri_admin zta,
       zon_secci zs,
       zon_zona zz,
       zon_regio zr
 WHERE mcc.clie_oid_clie = mc.oid_clie
   AND mc.oid_clie = mci.clie_oid_clie
   AND mci.val_iden_docu_prin = 1
   AND mcc.perd_oid_peri = cp.oid_peri
   AND zta.oid_terr_admi = mcua.ztad_oid_terr_admi
   AND zs.oid_secc = zta.zscc_oid_secc
   AND zz.oid_zona = zs.zzon_oid_zona
   AND zr.oid_regi = zz.zorg_oid_regi
   AND mc.oid_clie = mcua.clie_oid_clie
   AND mcua.ind_acti = '1'
   AND cp.peri_oid_peri = spc.oid_peri
   AND cp.peri_oid_peri = spc.oid_peri
   AND mcc.imp_pend >= DECODE(TO_NUMBER($P{importeDesde},'99999999.99'),null,mcc.imp_pend,TO_NUMBER($P{importeDesde},'99999999.99')) 
   AND spc.cod_peri >= $P{codigoPeriodoInicial}
   AND spc.cod_peri <= $P{codigoPeriodoFinal}
   AND mc.oid_clie IN (SELECT mcc.clie_oid_clie
                         FROM ccc_movim_cuent_corri mcc
                        WHERE mcc.imp_pend >= DECODE(TO_NUMBER($P{importeDesde},'99999999.99'),null,mcc.imp_pend,TO_NUMBER($P{importeDesde},'99999999.99')) 
                        HAVING COUNT(DISTINCT mcc.perd_oid_peri) > 1
                        GROUP BY mcc.clie_oid_clie)
  $P!{condicion}
 ORDER BY cod_clie, cod_peri ASC]]></queryString>


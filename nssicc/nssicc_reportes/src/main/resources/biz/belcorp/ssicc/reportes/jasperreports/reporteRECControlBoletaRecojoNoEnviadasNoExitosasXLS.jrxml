	<queryString><![CDATA[SELECT 
CB.COD_REGI,
CB.COD_ZONA,
MC.COD_CLIE AS CLIENTE, -- CODIGO DE CLIENTE DE BOLETA DE RECOJO
MC.VAL_NOM1||' '||MC.VAL_APE1||' '||MC.VAL_APE2 AS NOM_CONS,
CB.VAL_NUME_BORE AS NUMERO_BOLETA_RECOJO, -- NUMERO DE BOLETA DE RECOJO
CB.COD_PERI_PROC AS CAMPAÑA_PROCESO, -- CODIGO DE CAMPAÑA DE PROCESO
(
SELECT NUM_RECL
FROM REC_CABEC_RECLA
WHERE OID_CABE_RECL = LB.CARE_OID_CABE_RECL
) AS NUM_RECL,
LB.COD_PROD AS COD_SAP,
GEN_PKG_GENER.GEN_FN_DEVUELVE_DESCRIPCION(LB.PROD_OID_PROD ,'MAE_PRODU','es')    DESC_PROD,
(
SELECT MP.CODI_ANTI
FROM MAE_PRODU MP
WHERE MP.OID_PROD = LB.PROD_OID_PROD
) COD_BPCS,
DECODE(LB.COPA_OID_PARA_GRAL,
                  NULL,
                  INT_PKG_RECLA.GEN_FN_DEVUE_COD_VENTA_MFACT(LB.TOFE_OID_TIPO_OFER,
                                                             LB.MAFA_OID_MATR_FACT,
							     LB.PROD_OID_PROD),
                  INT_PKG_RECLA.GEN_FN_DEVUE_COD_VENTA_FICTI(LB.LOPA_OID_LOTE_PREM_ARTI,
                                                             LB.PANP_OID_PARA_NIVE_PREM,
                                                             LB.COPA_OID_PARA_GRAL,
							     LB.PROD_OID_PROD)) VAL_CODI_VENT_RECL,
LB.VAL_PREC,
LB.VAL_PREC_CONT,
LB.NUM_UNID_RECL,
(
SELECT E2.DES_ESTA_BORE
FROM INT_REC_ESTAD_BOREC E2
WHERE 
E2.OID_ESTA_BORE = CB.ESBO_OID_ESTA_BOR2 
) RESULTADO,
(
SELECT DES_MOTN_RECO_BORE
FROM INT_REC_MOTNO_RECOJ_BOREC
WHERE OID_MOTN_RECO_BORE = CB.MORE_OID_MOTN_RECO_BORE
) MOTIVO1,
(
SELECT DES_MOTN_RECO_BORE
FROM INT_REC_MOTNO_RECOJ_BOREC
WHERE OID_MOTN_RECO_BORE = CB.MORE_OID_MOTN_RECO_BOR2
) MOTIVO2

FROM INT_REC_CABEC_BOREC CB,
	 INT_REC_LINEA_BOREC LB,
	 MAE_CLIEN MC,
         INT_REC_ESTAD_BOREC REB
WHERE 	 CB.CLIE_OID_CLIE = MC.OID_CLIE
AND LB.COD_CABE_BORE = CB.COD_CABE_BORE
AND CB.ESBO_OID_ESTA_BOR1 = INT_PKG_RECLA.GEN_FN_DEVUE_OID_ESTAD_BOREC($P{codigoPais},'CE') -- BOLETAS DE RECOJO CERRADAS USAR FUNCION PARA RECUPERAR ESTADO
AND CB.COD_PERI_proc >= $P{codigoPeriodoInicial} -- FILTRO
AND CB.COD_PERI_proc <= $P{codigoPeriodoFinal}-- FILTRO 
AND CB.ESBO_OID_ESTA_BOR2=REB.OID_ESTA_BORE
AND REB.COD_ESTA_BORE NOT IN ('EX','CD')
$P!{condicionRegion}
$P!{condicionZona}
$P!{condicionNovedad}
$P!{condicionRecojo}]]></queryString>


	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="oidPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("oidPeriodo")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaDesde" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select 
       cod_peri COD_PERIODO,
       to_char(fec_proc,'dd/mm/yyyy')  FECHA,
       region  REGION,
       zona     ZONA,
       count(1) RECEPCIONADAS,
       sum(decode(ind_envi, 1, 1, 0)) REGISTRADAS,
       sum(case
             when (select count(1)
                     from ped_solic_cabec     x1,
                          ped_tipo_solic_pais x2,
                          ped_tipo_solic      x3,
                          mae_clien           x4,
                          cra_perio           x5,
                          seg_perio_corpo     x6
                    where x1.tspa_oid_tipo_soli_pais = x2.oid_tipo_soli_pais
                      and x2.tsol_oid_tipo_soli = x3.oid_tipo_soli
                      and x3.cod_tipo_soli = 'SOC'
                      and x1.clie_oid_clie = x4.oid_clie
                      and x1.perd_oid_peri = x5.oid_peri
                      and x5.peri_oid_peri = x6.oid_peri
                      and x6.cod_peri = zz.cod_peri
                      and x4.cod_clie = zz.cod_clie) > 0 then
              1
             else
              0
           end) FACTURADAS,
       sum(decode(ind_envi, 1, 0, 1)) RECHAZADAS
  from (select a.cod_peri,
               a.oid_peri,
               a.fec_proc,
               a.cod_clie,
               b.ind_envi,
                nvl(y3.cod_zona,  a.VAL_ZONA_ARRI) zona,
               y4.des_regi region
          from int_solic_conso_credi a, sto_docum_digit b, zon_terri_admin y1, zon_secci y2, zon_zona y3, zon_regio y4
           where a.oid_peri=$P{oidPeriodo}
        		      AND y1.oid_terr_admi = a.oid_terr_admi
                      and y1.zscc_oid_secc = y2.oid_secc
                      and y2.zzon_oid_zona = y3.oid_zona
                      and y4.oid_regi = y3.zorg_oid_regi       
         AND ( (a.fec_proc>=TO_DATE($P{fechaDesde},'DD/MM/YYYY HH24:MI')) or ($P{fechaDesde} is null))
		 AND ( (a.fec_proc<=TO_DATE($P{fechaHasta},'DD/MM/YYYY HH24:MI')) or ($P{fechaHasta} is null))
         and a.SEC_NUME_DOCU = b.SEC_NUME_DOCU) zz
 group by cod_peri, fec_proc, REGION, zona 
 order by cod_peri, fec_proc, region, zona]]></queryString>


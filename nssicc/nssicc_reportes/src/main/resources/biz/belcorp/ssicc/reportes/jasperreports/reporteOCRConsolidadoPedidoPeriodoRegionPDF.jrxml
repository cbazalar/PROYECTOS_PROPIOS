	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT zonr.COD_REGI,
       zonr.des_regi,
       SUM(NROPEDIDOS) PEDIDOS,
       SUM(NROORDENES) ORDENES,
       CASE
         WHEN COUNT(DISTINCT cierres.COD_REGI) > 0 THEN
          'Si'
         ELSE
          'No'
       END AS REGIONCERRADA
  FROM (SELECT zr.OID_REGI, zr.COD_REGI
          FROM fac_contr_cierr fcc, zon_regio zr, fac_tipos_cierr ftc
         WHERE fcc.PERD_OID_PERI = $P{codigoPeriodo}
           AND fcc.TCIE_OID_TIPO_CIER = ftc.oid_tipo_cier
           AND ftc.cod_tipo_cier = 'R'
           AND fcc.ZORG_OID_REGI = zr.OID_REGI
         GROUP BY zr.oid_regi, zr.COD_REGI) cierres,
       (SELECT reg.cod_regi,
               COUNT(DISTINCT sc.clie_oid_clie) AS NROPEDIDOS,
               COUNT(DISTINCT sc.OID_SOLI_CABE) AS NROORDENES
          FROM ped_solic_cabec       sc,
               mae_clien             mc,
               zon_terri_admin       terri,
               mae_clien_unida_admin adm,
               zon_secci             secc,
               zon_zona              zon,
               zon_regio             reg,
               zon_terri             ter,
               cra_perio             periini,
               cra_perio             perifin,
               cra_perio             periactual,
               ped_tipo_solic_pais   tsp,
               ped_tipo_solic        ts
         WHERE sc.CLIE_OID_CLIE = mc.OID_CLIE
           AND sc.TSPA_OID_TIPO_SOLI_PAIS = tsp.OID_TIPO_SOLI_PAIS
           and tsp.TSOL_OID_TIPO_SOLI = ts.OID_TIPO_SOLI
           and ts.COD_TIPO_SOLI = 'SOC'
           AND sc.PERD_OID_PERI = periactual.oid_peri
           AND sc.FEC_FACT IS NOT NULL
           AND sc.ZTAD_OID_TERR_ADMI = terri.OID_TERR_ADMI
           AND terri.terr_oid_terr = ter.oid_terr
           AND terri.ZSCC_OID_SECC = secc.OID_SECC
           AND secc.ZZON_OID_ZONA = zon.OID_ZONA
           AND zon.ZORG_OID_REGI = reg.OID_REGI
           AND adm.clie_oid_clie = sc.CLIE_OID_CLIE
           AND adm.perd_oid_peri_ini = periini.oid_peri
           AND adm.ztad_oid_terr_admi = terri.oid_terr_admi
           AND periactual.oid_peri = $P{codigoPeriodo}
           AND periactual.fec_inic >= periini.fec_inic
           AND ((adm.perd_oid_peri_fin IS NULL) OR
               (adm.perd_oid_peri_fin = perifin.oid_peri AND
               perifin.fec_fina >= periactual.fec_fina))
           AND terri.ind_borr = 0
           AND secc.ind_borr = 0
           AND secc.ind_acti = 1
         GROUP BY reg.cod_regi) Pedi,
       zon_regio zonr
 WHERE zonr.cod_regi = cierres.cod_regi(+)
   AND zonr.cod_regi = Pedi.cod_regi(+)
 GROUP BY zonr.cod_regi, zonr.des_regi]]></queryString>


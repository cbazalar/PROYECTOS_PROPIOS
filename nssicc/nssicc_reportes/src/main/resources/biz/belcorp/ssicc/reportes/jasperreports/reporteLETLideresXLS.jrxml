	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPeriodoInicio" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicio")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFin" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFin")]]></defaultValueExpression>
	</parameter>
	<parameter name="tipoEstado" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("tipoEstado")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[WITH regLider AS (
						  SELECT peri.cod_peri val_camp_ingr,
								 hger.cod_regi,
								 hger.cod_zona,
								 hger.cod_secc,
								 hger.gere cod_lide,
								 clie.val_ape1 || ' ' || clie.val_ape2 || ' ' || clie.val_nom1 || ' ' || clie.val_nom2 nom_lide,
								 CASE
								   WHEN hger.ind_orig_regi = 'C' THEN 'SISTEMA COMERCIAL'
								   WHEN hger.ind_orig_regi = 'W' THEN 'WEB'
								   ELSE 'SISTEMA COMERCIAL'
								  END val_orig_regi,
								 null val_obse,
								 DECODE(hger.perd_oid_peri_hast,NULL,NULL,peri2.cod_peri) val_camp_desv,
								 DECODE(hger.perd_oid_peri_hast,NULL,NULL,ltde.des_tipo_desv) val_moti_desv,
								 CASE
								   WHEN peri.cod_peri = $P{codigoPeriodoInicio} THEN 1
								   WHEN peri.cod_peri < $P{codigoPeriodoInicio} THEN 0
								 END ind_nuev_lide,
								 CASE
								   WHEN hger.perd_oid_peri_hast IS NOT NULL THEN 1
								   ELSE 0
								 END ind_desv
							FROM zon_histo_geren hger,
								 zon_tipo_desvi ltde,
								 mae_clien clie,
								 cra_perio perd,
								 seg_perio_corpo peri,
								 cra_perio perd2,
								 seg_perio_corpo peri2
						   WHERE hger.ind_desv_auto = ltde.cod_tipo_desv(+)
							 AND hger.gere = clie.cod_clie
							 AND hger.perd_oid_peri_desd = perd.oid_peri
							 AND perd.peri_oid_peri = peri.oid_peri
							 AND NVL( hger.perd_oid_peri_hast,( SELECT cra_perio.oid_peri
																  FROM cra_perio,
																	   seg_perio_corpo
																 WHERE cra_perio.peri_oid_peri = seg_perio_corpo.oid_peri
																   AND seg_perio_corpo.cod_peri = $P{codigoPeriodoInicio} ) ) = perd2.oid_peri
							 AND perd2.peri_oid_peri = peri2.oid_peri
							 --
							 AND hger.cod_regi IS NOT NULL
							 AND hger.cod_zona IS NOT NULL
							 AND hger.cod_secc IS NOT NULL
							 AND $P{codigoPeriodoInicio} BETWEEN peri.cod_peri AND peri2.cod_peri
						)
		SELECT
		VAL_CAMP_INGR,
		COD_REGI,
		COD_ZONA,
		COD_SECC,
		COD_LIDE,
		NOM_LIDE,
		VAL_ORIG_REGI,
		VAL_OBSE,
		VAL_CAMP_DESV,
		VAL_MOTI_DESV,
		IND_NUEV_LIDE,
		IND_DESV
		FROM regLider
		 WHERE 1=1
		   AND ((regLider.val_camp_ingr BETWEEN $P{codigoPeriodoInicio} AND $P{codigoPeriodoFin}) OR
				(regLider.val_camp_desv BETWEEN $P{codigoPeriodoInicio} AND $P{codigoPeriodoFin} OR regLider.val_camp_desv IS NULL))
		   AND (($P{tipoEstado} = 1 /*Nombradas*/ AND regLider.ind_desv = 0) OR
				($P{tipoEstado} = 2 /*Desvinculadas*/ AND regLider.ind_desv = 1) OR
				 $P{tipoEstado} = 0 /*Todas*/)
		 ORDER BY 1,2,3,4]]></queryString>


	</variable>
	<variable name="totalPorPend" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($V{totalImpMovi}.intValue() ==0  )?new BigDecimal(0):new BigDecimal( $V{totalImpPend}.doubleValue()  /  $V{totalImpMovi}.doubleValue()  )]]></variableExpression>

	<queryString><![CDATA[SELECT to_char(a.fec_fact,'dd/MM/yyyy')fec_fact, 
       e.cod_regi, 
       d.cod_zona, 
       c.cod_clie,
       (TRIM(c.val_nom1) || ' ' || TRIM(c.val_nom2) || ' ' || TRIM(c.val_ape1) || ' ' || TRIM(c.val_ape2)) nombre,
       DECODE ((SELECT NVL (COUNT (oid_soli_cabe), 0)
                FROM ped_solic_cabec
                WHERE soca_oid_soli_cabe = a.oid_soli_cabe 
                AND ind_oc = 1),1, 'NO','SI' ) pedido_servicio,
	a.val_nume_soli BOLETA_DESPACHO,
	(select dees.val_i18n ESTADO
      	from mae_clien_datos_adici clda,mae_estat_clien esta,
           gen_i18n_sicc_comun dees, mae_clien clie
    	 where clda.esta_oid_esta_clie = esta.oid_esta_clie
       	and clda.esta_oid_esta_clie = dees.val_oid
       	and clda.clie_oid_clie = clie.oid_clie
       	and dees.attr_enti = 'MAE_ESTAT_CLIEN'
       	and clie.cod_clie = c.Cod_Clie) ESTADO 
FROM ped_solic_cabec a, 
     mae_clien c, 
     zon_zona d, 
     zon_regio e
WHERE a.clie_oid_clie = c.oid_clie
AND a.zzon_oid_zona = d.oid_zona
AND d.zorg_oid_regi = e.oid_regi
AND a.perd_oid_peri = (SELECT x.oid_peri
                       FROM cra_perio x, seg_perio_corpo y
                       WHERE x.peri_oid_peri = y.oid_peri 
                       AND y.cod_peri = $P{codigoPeriodo})
AND a.tspa_oid_tipo_soli_pais IN (SELECT tspa_oid_tipo_soli_pais FROM int_lar_tipo_solici_pedido_dis)
$P!{condicion}
ORDER BY 1, 2, 3, 4]]></queryString>


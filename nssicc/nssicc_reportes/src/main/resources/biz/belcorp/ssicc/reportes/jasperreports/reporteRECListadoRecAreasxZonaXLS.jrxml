	<queryString><![CDATA[SELECT  
       gen_pkg_gener.GEN_FN_CLIEN_DATOS(REC_REPOR_OPERA_UNIDA_ADMIN.COD_CLIE,'COD_ZONA') AS ZONA,		
	    REC_REPOR_OPERA_UNIDA_ADMIN.DES_ZONA,
	   gen_pkg_gener.GEN_FN_CLIEN_DATOS(REC_REPOR_OPERA_UNIDA_ADMIN.COD_CLIE,'NOM_CLIE') AS NOMBRE,
	   SUM(NUM_UNID_RECL) AS UNIDAES,-- unidades

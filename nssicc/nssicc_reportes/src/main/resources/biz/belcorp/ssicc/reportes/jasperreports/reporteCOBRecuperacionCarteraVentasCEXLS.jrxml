	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoSociedad" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoSociedad")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicion")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[WITH TEMP1
AS
(SELECT 
   est.oid_peri,
   est.cod_peri,   
   est.oid_clie,      
   SUM(est.imp_fact_neto) imp_fact_neto, 
   SUM(est.cob_dias_999)  imp_recup,
   SUM(est.imp_sald_pend_sac) imp_pend
FROM cob_repor_estad_recup_cobra est
WHERE est.cod_pais= $P{codigoPais}
AND est.cod_soci = $P{codigoSociedad}
AND est.cod_peri>= $P{codigoPeriodoInicial}
AND est.cod_peri <= $P{codigoPeriodoFinal}
AND est.imp_pend > 0
GROUP BY 
   est.oid_peri,
   est.cod_peri,   
   est.oid_clie          
$P!{condicion})
SELECT 
     t1.oid_peri,
     t1.cod_peri,
     zr.des_regi,
     ch.cod_zona des_zona,
     ch.cod_secc,
     t1.oid_clie,
     ch.cod_clie,
     ch.val_nom1 || ' ' || ch.val_nom2 || ' ' || ch.val_ape1 || ' ' || ch.val_ape2 nom_clie, 
     ch.num_docu_iden,
     ch.val_nomb_via || ' ' || ch.num_ppal val_dire,     
     ch.val_obse val_refe,
     ch.val_tele_fijo num_tele_fijo,
     ch.val_tele_ofic num_tele_trab,
     ch.val_tele_celu num_tele_movi,
     ch.cam_ulti_pedi camp_ulti_pedi,
     FIN_PKG_GENER.FIN_FN_OBTIE_DESCR_ESTAT_CLIEN(t1.oid_clie) val_desc_esta_clie,
     FIN_PKG_GENER.FIN_FN_OBTIE_FECHA_FACTU($P{codigoPais},zz.oid_zona,oid_peri+6) fec_cier,
     imp_fact_neto,
     imp_recup,
     imp_pend,
     CASE
        WHEN imp_fact_neto > 0 THEN
         ROUND((imp_recup*100/imp_fact_neto),2)
        ELSE 0
     END por_recu,
     NVL((SELECT a.des_acci_cobr
      FROM cob_detal_asign_carte d,
           cob_accio_cobra_pais a
      WHERE d.cod_ulti_gest_cobr = a.cod_acci_cobr 
      AND d.cod_etap_deud = a.cod_etap_deud
      AND d.cod_peri = t1.cod_peri
      AND d.cod_etap_deud = 'TEL'
      AND d.cod_clie = ch.cod_clie
      AND ROWNUM< 2),'') ult_gest_tele,    
     FIN_PKG_GENER.FIN_FN_OBTIE_REFER_CLIEN(ch.cod_clie,'1') ref_fami,
     FIN_PKG_GENER.FIN_FN_OBTIE_REFER_CLIEN(ch.cod_clie,'2') ref_nofa,
     FIN_PKG_GENER.FIN_FN_OBTIE_REFER_CLIEN(ch.cod_clie,'3') ref_aval,
     FIN_PKG_GENER.FIN_FN_OBTIE_REFER_OBSER_CLIEN(ch.cod_clie) ref_pers     
FROM TEMP1 t1,
     int_reu_consu_histo ch,
     zon_zona zz,
     zon_regio zr
WHERE t1.oid_clie = ch.oid_clie(+)
  AND ch.COD_ZONA = zz.cod_zona
  AND ch.COD_REGI = zr.COD_REGI    
ORDER BY t1.cod_peri , zr.cod_regi, zz.cod_zona , ch.cod_clie ASC]]></queryString>


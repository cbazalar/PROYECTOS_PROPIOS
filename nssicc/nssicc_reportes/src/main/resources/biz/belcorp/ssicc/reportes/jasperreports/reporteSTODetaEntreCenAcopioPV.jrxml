	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoRegion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoRegion")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoCiaTransporte" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoCiaTransporte")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodo" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select xx.cod_peri,
       xx.nom_cent_acop as cod_regi,
       xx.BOLETAS_TOTALES,       
       yy.BOLETAS_A_TIEMPO,
       yy.BOLETAS_A_TIEMPO/xx.BOLETAS_TOTALES porc_A_tiempo,
       zz.BOLETAS_CON_NOVEDAD,
       zz.BOLETAS_CON_NOVEDAD/xx.BOLETAS_TOTALES porc_novedad
  from (select j.cod_peri, k.nom_cent_acop, count(*) BOLETAS_TOTALES
          FROM ped_solic_cabec a,
               zon_terri_admin d,
               zon_secci       f,
               zon_zona        g,
               zon_regio       h,
               cra_perio       i,
               seg_perio_corpo j,
               STO_CENTR_ACOPI k
         WHERE a.ztad_oid_terr_admi = d.oid_terr_admi
           AND d.zscc_oid_secc = f.oid_secc
           and a.esso_oid_esta_soli <> 4
           AND f.zzon_oid_zona = g.oid_zona
           AND g.zorg_oid_regi = h.oid_regi
           AND a.perd_oid_peri = i.oid_peri
           and i.peri_oid_peri = j.oid_peri
           and j.cod_peri like nvl($P{codigoPeriodo},to_char(sysdate, 'YYYY')) || '%' --Periodos que van del a√±o en curso
--           and h.cod_regi || g.cod_zona || f.cod_secc =               k.cod_regi || k.cod_zona || k.cod_secc
           and k.cod_comp_tran = nvl($P{codigoCiaTransporte},k.cod_comp_tran) --Codigo de Compania de Transporte seleccionado                                   
           AND a.tspa_oid_tipo_soli_pais IN
               (SELECT tspa_oid_tipo_soli_pais
                  FROM int_lar_tipo_solici_pedido_dis)
           and (select sum(ind_oc)
                  from ped_solic_cabec
                 where soca_oid_soli_cabe = a.oid_soli_cabe) = 0 --Si el tipo de orden es BER colocar =0
         group by j.cod_peri, k.nom_cent_acop) xx,
       (select a.cod_peri,
               b.nom_cent_acop,
               count(distinct num_docu) BOLETAS_A_TIEMPO
          from int_solic_conso_orden_trans a, sto_centr_acopi b
         where a.tip_orde = 'BER' --Tipo de Orden seleccionado
           and nvl(a.cod_esta_ent2, 0) = '01'
           and a.cod_peri like nvl($P{codigoPeriodo},to_char(sysdate , 'YYYY')) || '%' --Periodos que van del ano en curso
           and a.cod_comp_tran = b.cod_comp_tran
           and a.cod_comp_tran = nvl($P{codigoCiaTransporte},a.cod_comp_tran) -- Compania de Transporte Seleccionada
           and a.fec_proc <= (select w.fec_inic
                                from cra_crono       w,
                                     cra_activ       x,
                                     seg_pais        y,
                                     zon_zona        z,
                                     cra_perio       z1,
                                     seg_perio_corpo z2
                               where w.CACT_OID_ACTI = x.OID_ACTI
                                 and x.PAIS_OID_PAIS = y.OID_PAIS
                                 and w.ZZON_OID_ZONA = z.OID_ZONA
                                 and x.cod_acti = 'FA'
                                 and y.COD_PAIS = a.cod_pais
                                 and z.COD_ZONA = a.cod_zona
                                 and w.PERD_OID_PERI = z1.oid_peri
                                 and z1.peri_oid_peri = z2.oid_peri
                                 and z2.cod_peri = a.cod_peri)
         group by a.cod_peri, b.nom_cent_acop) yy,
       (select a.cod_peri,
               b.nom_cent_acop,
               count(distinct num_docu) BOLETAS_CON_NOVEDAD
          from int_solic_conso_orden_trans a, sto_centr_acopi b
         where a.tip_orde = 'BER' --Tipo de Orden seleccionado
           and a.cod_peri like nvl($P{codigoPeriodo},to_char(sysdate, 'YYYY')) || '%' --Periodos que van del ano en curso
           and nvl(a.ind_nove, 0) = '1'
           and a.cod_nove not in ('01', '04')
           and a.cod_comp_tran = b.cod_comp_tran
           and a.cod_cent_acop = b.cod_cent_acop
           and a.cod_comp_tran = nvl($P{codigoCiaTransporte},a.cod_comp_tran) -- Compania de Transporte Seleccionada
         group by a.cod_peri, b.nom_cent_acop) zz 
         where xx.cod_peri = yy.cod_peri 
         and xx.nom_cent_acop = yy.nom_cent_acop 
         and xx.cod_peri = zz.cod_peri 
         and xx.nom_cent_acop = zz.nom_cent_acop
order by 1]]></queryString>


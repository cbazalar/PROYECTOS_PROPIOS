	</parameter>
	<queryString><![CDATA[SELECT 
   cb.des_banc,     
   GEN_PKG_GENER.GEN_FN_CLIEN_DATOS_OID(MB.CLIE_OID_CLIE, 'DES_REGI') region,
   GEN_PKG_GENER.GEN_FN_CLIEN_DATOS_OID(MB.CLIE_OID_CLIE, 'COD_ZONA') zona,
   mb.cod_cons cod_clie,
   mb.cod_ofic_reca dig_chequeo,   
   (SELECT TRIM(mc.val_ape1) || ' ' || TRIM(mc.val_ape2) || ' '  || TRIM(mc.val_nom1) || ' ' || TRIM(mc.val_nom2)  
    FROM mae_clien mc
    WHERE mc.oid_clie = mb.clie_oid_clie) nom_clie,
   CASE
      WHEN    mb.val_hora_norm_adic='P'  THEN 'Por Regularizar'
      WHEN mb.val_hora_norm_adic='R'     THEN 'Regularizado'
      ELSE 'Normal'	
    END tipo,
   mb.fec_pago,
   mb.fec_proc,      
   mb.imp_pago
FROM 
     ccc_movim_banca mb,
     ccc_cuent_corri_banca ccb,
     ccc_proce cp,
     ccc_subpr cs,
     ccc_sucur cs,
     ccc_banco cb,
     seg_pais sp,
     seg_socie ss
WHERE mb.pais_oid_pais=sp.oid_pais
AND mb.soci_oid_soci = ss.oid_soci
AND mb.ccba_oid_cc_banc=ccb.oid_cuen_corr_banc
AND ccb.sucu_oid_sucu=cs.oid_sucu
AND cs.cban_oid_banc=cb.oid_banc 
AND mb.subp_oid_marc_crea=cs.oid_subp
AND cp.oid_proc = cs.ccpr_oid_proc
AND sp.cod_pais=$P{codigoPais}
AND ss.cod_soci=$P{codigoSociedad}
AND mb.val_hora_norm_adic=$P{tipoAbono}
AND mb.fec_pago >= DECODE(TO_DATE($P{fechaPagoDesde},'DD/MM/YYYY'),null,mb.fec_pago,'',mb.fec_pago,TO_DATE($P{fechaPagoDesde},'DD/MM/YYYY'))
AND mb.fec_pago <= DECODE(TO_DATE($P{fechaPagoHasta},'DD/MM/YYYY'),null,mb.fec_pago,'',mb.fec_pago,TO_DATE($P{fechaPagoHasta},'DD/MM/YYYY'))
AND mb.fec_proc >= DECODE(TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'),null,mb.fec_proc,'',mb.fec_proc,TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'))
AND mb.fec_proc <= DECODE(TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'),null,mb.fec_proc,'',mb.fec_proc,TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'))
AND cb.cod_banc = DECODE($P{codigoBanco},null,cb.cod_banc,'Todos',cb.cod_banc,'',cb.cod_banc,$P{codigoBanco})
ORDER BY 5,1,2,3]]></queryString>


	<queryString><![CDATA[SELECT 
 mc.cod_clie, 
 spc.cod_peri,
 TO_CHAR(mcc.fec_docu, 'DD/MM/YYYY') fec_docu,
 su.des_subp,
 mcc.imp_movi,
 mcc.imp_pend
FROM 
 mae_clien mc,
 ccc_movim_cuent_corri mcc,
 ccc_subpr su,
 cra_perio cp,
 seg_perio_corpo spc
WHERE mc.oid_clie = mcc.clie_oid_clie
  AND mcc.perd_oid_peri = cp.oid_peri
  AND cp.peri_oid_peri = spc.oid_peri
  AND mcc.subp_oid_subp_crea = su.oid_subp
  AND mcc.imp_pend > 0
$P!{condicionConsultoras}
ORDER BY mc.cod_clie, spc.cod_peri ASC]]></queryString>


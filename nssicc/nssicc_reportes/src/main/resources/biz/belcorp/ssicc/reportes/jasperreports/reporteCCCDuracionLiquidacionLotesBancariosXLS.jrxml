	<parameter name="codigoBanco" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoBanco")]]></defaultValueExpression>
	</parameter>
	<parameter name="tipoOrigen" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("tipoOrigen")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaProcDesde" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaProcDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaProcHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaProcHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT  
 ccb.cod_cc cod_banc,
 ccb.des_cc des_banc,             
 tl.des_tipo_orig tipo,
 br.num_lote,
 TRUNC(br.fec_proc) fec_proc,
 br.val_cant_regi_tota tot_reg_lote,
 br.cod_usua_liqu,
 TO_CHAR(br.fec_inic_liqu,'HH:MI:SS AM') fec_inic_liqu,
 TO_CHAR(br.fec_fina_liqu,'HH:MI:SS AM') fec_fina_liqu,
 TO_CHAR(trunc(mod((br.fec_fina_liqu-br.fec_inic_liqu)*24*60,60)),'09') || ':' || TO_CHAR(TRUNC(mod((br.fec_fina_liqu-br.fec_inic_liqu)*24*60*60,60)),'09') val_dura_liqu
FROM ccc_numer_lote_banca_recep br,
     ccc_cuent_corri_banca ccb,
     ccc_tipos_orige_lotes_banca tl
WHERE br.cod_cuen_corr_banc =  ccb.cod_cc 
  AND br.cod_tipo_orig = tl.cod_tipo_orig
  AND br.cod_tipo_orig = DECODE($P{tipoOrigen},null,br.cod_tipo_orig,'Todos',br.cod_tipo_orig,'T',br.cod_tipo_orig,$P{tipoOrigen})
  AND ccb.cod_cc = DECODE($P{codigoBanco},null,ccb.cod_cc,'Todos',ccb.cod_cc,'T',ccb.cod_cc,$P{codigoBanco})
  AND TRUNC(br.fec_proc) >= DECODE(TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'),null,br.fec_proc,'',br.fec_proc,TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'))
  AND TRUNC(br.fec_proc) <= DECODE(TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'),null,br.fec_proc,'',br.fec_proc,TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'))
  AND br.cod_iden_proc = 'P'
  ORDER BY br.fec_fina_liqu-br.fec_inic_liqu DESC]]></queryString>


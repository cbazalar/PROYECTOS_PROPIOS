	<queryString><![CDATA[SELECT    g18n.val_i18n AS pais, 
          pais.cod_pais, 
          p.*, 
          perfiles.NAME AS perfil 
FROM      memberof m, 
          v_usu_largo p, 
          propertyvalues pv, 
          seg_pais pais, 
          gen_i18n_sicc_comun g18n, 
          (
            SELECT    p0.idprincipal, 
                      p0.NAME 
            FROM      principals p0, 
                      principals p2, 
                      memberof m 
            WHERE     p2.NAME = 'PerfilesSICC' 
              AND     m.idmember = p2.idprincipal 
              AND     m.idrole = p0.idprincipal
          ) perfiles 
WHERE     m.idrole = perfiles.idprincipal 
  AND     p.idusuario = m.idmember 
  AND     p.idusuario = m.idmember 
  AND     pv.idprincipal = p.idusuario 
  AND     pv.idproperty = 31 
  AND     pais.oid_pais = pv.stringvalue 
  AND     g18n.val_oid = pv.stringvalue 
  AND     g18n.attr_enti = 'SEG_PAIS' 
ORDER BY  pais, 
          cod_pais, 
          idusuario, 
          perfil]]></queryString>


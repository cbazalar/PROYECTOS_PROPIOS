	<queryString><![CDATA[SELECT cod_pais,
       cod_peri,
       cod_regi,
       cod_zona,
       cod_clie,
       IND_VALI_PROL VALIDADO_PROL,
       ind_ocs_proc FACTURADO,
       (select a.cod_vali || ' ' || b.des_vali
          from sto_histo_audit_excep       a,
               sto_param_valid             b,
               ped_histo_solic_conso_cabec x
         where a.cod_vali = b.cod_vali
           and a.sec_nume_docu = x.sec_nume_docu
           and rownum = 1) RECHAZO
  FROM ped_histo_solic_conso_cabec
 WHERE cod_peri = $P{codigoPeriodo}
  AND soci_oid_soci=(select oid_soci from seg_socie 
                                where cod_soci = $P{codigoSociedad} 
                                and pais_oid_pais = $P{oidPais}) 
  AND ind_vali_prol_anul = 1
 ORDER BY cod_pais, cod_peri, cod_regi, cod_zona, cod_clie]]></queryString>


	</parameter>
	<queryString><![CDATA[select 
case when IND_RECE_OCR+IND_RECE_WEB+IND_RECE_DIGI+IND_RECE_DD>1 then 'MIXTO'
when IND_RECE_OCR=1 then 'OCR'
when IND_RECE_WEB=1 then 'WEB'
when IND_RECE_DIGI=1 then 'Digitado'
when IND_RECE_DD=1 then 'DD'
end MEDIO,
FEC_SOLI, 
FEC_FACT, 
COD_PERI, 
COD_ZONA,
COD_SECC, 
COD_TERR, 
EXPOOFERTAS, 
CATALOGO, 
TOTAL_PEDIDO, 
COD_CLIE, 
NOM_CLIE, 
ESTATUS_ANTERIOR 
, decode(NUMERO_PEDIDOS,2,'SI', null) SEGUNDO_PEDIDO
, decode(NUMERO_PEDIDOS,3,'SI', null) TERCER_PEDIDO
, decode(NUMERO_PEDIDOS,4,'SI', null) CUARTO_PEDIDO
, decode(ESTATUS_ANTERIOR,'Posible Egreso / Egresante','SI', null) POSIBLE_EGRESO
, saldo-total_pedido as saldo
, telefono
from 
(
select FEC_SOLI, FEC_FACT, COD_CLIE, NOM_CLIE, COD_PERI, COD_ZONA,COD_SECC, COD_TERR, ESTATUS_ANTERIOR, EXPOOFERTAS, CATALOGO, TOTAL_PEDIDO 
, (
select count(distinct x.oid_soli_cabe) from ped_solic_cabec x, ped_tipo_solic_pais y, ped_tipo_solic z where 
x.TSPA_OID_TIPO_SOLI_PAIS=y.OID_TIPO_SOLI_PAIS and y.TSOL_OID_TIPO_SOLI=z.OID_TIPO_SOLI and z.COD_TIPO_SOLI='SOC'
and x.CLIE_OID_CLIE=xx.CLIE_OID_CLIE
) NUMERO_PEDIDOS
, (
select sum(imp_pend) from ccc_movim_cuent_corri where clie_oid_clie=xx.clie_oid_clie and imp_pend>0
) saldo,
(
(select val_text_comu from mae_clien_comun where ticm_oid_tipo_comu=1 and clie_oid_clie=xx.clie_oid_clie) || ' / ' ||
(select val_text_comu from mae_clien_comun where ticm_oid_tipo_comu=6 and clie_oid_clie=xx.clie_oid_clie) || ' / ' ||
(select val_text_comu from mae_clien_comun where ticm_oid_tipo_comu=7 and clie_oid_clie=xx.clie_oid_clie)
) telefono
, IND_RECE_OCR, IND_RECE_WEB, IND_RECE_DIGI, IND_RECE_DD
from
(
select FEC_SOLI, FEC_FACT, COD_CLIE, NOM_CLIE, COD_PERI, COD_ZONA,COD_SECC, COD_TERR, ESTATUS_ANTERIOR, clie_oid_clie, IND_RECE_OCR, IND_RECE_WEB, IND_RECE_DIGI, IND_RECE_DD
, sum(case when OCAT_OID_CATA in (2005) then VAL_PREC_FACT_TOTA_LOCA else 0 end) EXPOOFERTAS
, sum(case when OCAT_OID_CATA not in (2005) then VAL_PREC_FACT_TOTA_LOCA else 0 end) CATALOGO
, sum(VAL_PREC_FACT_TOTA_LOCA) TOTAL_PEDIDO
from 
(
select a.clie_oid_clie, f.FEC_SOLI, a.FEC_FACT, f.COD_CLIE, f.NOM_CLIE, e.COD_PERI, g.COD_ZONA,j.COD_SECC, k.COD_TERR
, l.OCAT_OID_CATA, b.VAL_CODI_VENT, decode(b.VAL_PREC_CATA_UNIT_LOCA,0,0,b.VAL_PREC_FACT_TOTA_LOCA) VAL_PREC_FACT_TOTA_LOCA
, m.VAL_I18N ESTATUS_ANTERIOR, f.IND_RECE_OCR, f.IND_RECE_WEB, f.IND_RECE_DIGI, f.IND_RECE_DD
from ped_solic_cabec a, ped_solic_posic b, pre_ofert_detal c, cra_perio d, seg_perio_corpo e
, int_solic_conso_cabec f, zon_zona g, mae_clien h, zon_terri_admin i, zon_secci j, zon_terri k, pre_ofert l
, gen_i18n_sicc_comun m
where a.OID_SOLI_CABE=f.SOCA_OID_SOLI_CABE_REFE 
and a.OID_SOLI_CABE=b.SOCA_OID_SOLI_CABE
and b.OFDE_OID_DETA_OFER=c.OID_DETA_OFER 
and a.PERD_OID_PERI=d.OID_PERI
and d.PERI_OID_PERI=e.OID_PERI 
and e.COD_PERI= $P{codigoPeriodo}
and a.ZZON_OID_ZONA=g.OID_ZONA
and g.COD_ZONA= $P{codigoZona} 
and a.CLIE_OID_CLIE=h.OID_CLIE 
and a.FEC_FACT is not null
and a.ZTAD_OID_TERR_ADMI=i.OID_TERR_ADMI 
and i.ZSCC_OID_SECC=j.OID_SECC 
and i.TERR_OID_TERR=k.OID_TERR
and c.OFER_OID_OFER=l.OID_OFER 
and m.ATTR_ENTI='MAE_ESTAT_CLIEN' 
and m.VAL_OID=f.ESTA_OID_ESTA_CLIE
) 
group by FEC_SOLI, FEC_FACT, COD_CLIE, NOM_CLIE, COD_PERI, COD_ZONA,COD_SECC, COD_TERR, ESTATUS_ANTERIOR, clie_oid_clie, IND_RECE_OCR, IND_RECE_WEB, IND_RECE_DIGI, IND_RECE_DD
) xx
)]]></queryString>


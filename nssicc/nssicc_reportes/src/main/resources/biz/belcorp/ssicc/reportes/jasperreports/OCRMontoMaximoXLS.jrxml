	<queryString><![CDATA[SELECT a.cod_peri, 
       a.cod_regi,
       a.des_regi,
       a.cod_zona, 
       a.des_zona,
       a.cod_terr, 
       a.cod_clie, 
       a.nom_clie, 
       a.val_mont_pedi,
       a.val_sald_deud,
       c.cod_vent AS "Cod_Vent",
       rec_pkg_repor.OCR_FN_DEV_DES_PROD_OFER(a.cod_peri, c.cod_vent) AS "Descripcion",
       c.val_unid_dem AS "Unid_Demandadas",
       c.val_prec_cata_unit_loca AS "Precio"
FROM   int_solic_conso_cabec a,
       int_solic_conso_detal c,
       bas_ctrl_fact b
WHERE  a.cod_pais=b.cod_pais
and a.cod_clie=c.cod_clie
and a.cod_peri=c.cod_peri
and a.num_lote=c.num_lote
AND a.cod_peri = b.cod_peri
AND    a.ind_ocs_proc = 0
AND    a.ind_erro_mtma = 1
AND A.COD_PAIS = $P{codigoPais}       
       AND A.COD_CLIE > '000000000000000'
       AND A.COD_PERI = Decode ($P{codigoPeriodo},null, A.COD_PERI, 'Todos', A.COD_PERI,'', A.COD_PERI,$P{codigoPeriodo} )
       AND A.COD_REGI = Decode ($P{codigoRegion},null, A.COD_REGI, 'Todos', A.COD_REGI,'', A.COD_REGI,$P{codigoRegion} )
       AND A.COD_ZONA = Decode ($P{codigoZona},null, A.COD_ZONA, 'Todos', A.COD_ZONA,'', A.COD_ZONA,$P{codigoZona} )
       AND A.fec_prog_fact >= Decode ($P{fechaInicial},null, A.fec_prog_fact, null, A.fec_prog_fact,'', A.fec_prog_fact,TO_DATE($P{fechaInicial},'dd/mm/yyyy') )
       AND A.fec_prog_fact <= Decode ($P{fechaFinal},null, A.fec_prog_fact, null, A.fec_prog_fact,'', A.fec_prog_fact,TO_DATE($P{fechaFinal},'dd/mm/yyyy') )
       AND B.STA_CAMP = '0'
ORDER BY a.cod_regi, 
         a.cod_zona, 
     a.cod_terr, 
         a.cod_clie]]></queryString>


	<queryString><![CDATA[SELECT 
       zr.cod_regi,
       zz.cod_zona,
       zs.cod_secc,
       zt.cod_terr,
       mc.oid_clie,
       mc.cod_clie, 
       mc.cod_digi_ctrl,        
       TRIM(mc.val_nom1) ||' ' || 
       TRIM(mc.val_nom2) ||' ' || 
       TRIM(mc.val_ape1) ||' ' || 
       TRIM(mc.val_ape2) nom_clie,
       mci.num_docu_iden,
       tf.val_text_comu val_tele_fijo,
       tt.val_text_comu val_tele_ofic,
       tm.val_text_comu val_tele_celu,
       (SELECT zon_valor_estru_geopo.des_geog
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 IS NULL
       AND zon_valor_estru_geopo.orde_3 IS NULL
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1) nom_depa, 
       (SELECT zon_valor_estru_geopo.des_geog 
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
       AND zon_valor_estru_geopo.orde_3 IS NULL
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1) nom_prov,
       (SELECT zon_valor_estru_geopo.des_geog
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
       AND zon_valor_estru_geopo.orde_3 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,13,6)))
       AND zon_valor_estru_geopo.orde_4 IS NULL
       AND ROWNUM = 1) nom_dist,
       (SELECT zon_valor_estru_geopo.des_geog
       FROM zon_valor_estru_geopo
       WHERE zon_valor_estru_geopo.orde_1 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,1,6)))
       AND zon_valor_estru_geopo.orde_2 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,7,6)))
       AND zon_valor_estru_geopo.orde_3 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,13,6)))
       AND zon_valor_estru_geopo.orde_4 = LTRIM(RTRIM(SUBSTR(mcd.COD_UNID_GEOG,19,6)))
       AND ROWNUM = 1) des_urba,                 
       DECODE(TRIM(stv.des_abrv_tipo_via),NULL,'',stv.des_abrv_tipo_via || ' ' ) ||
      DECODE(TRIM(mcd.val_nomb_via),NULL,'',mcd.val_nomb_via || ' ' ) ||
      DECODE(TRIM(mcd.num_ppal),NULL,'',mcd.num_ppal || ' ' ) ||
      DECODE(TRIM(mcd.val_inte),NULL,'',mcd.val_inte || ' ' ) ||
      DECODE(TRIM(mcd.val_manz),NULL,'',mcd.val_manz || ' ' ) ||
      DECODE(TRIM(mcd.val_lote),NULL,'',mcd.val_lote || ' ' ) ||
      DECODE(TRIM(mcd.val_Km),NULL,'',mcd.val_km || ' ' ) || 
      DECODE(TRIM(mcd.val_obse),NULL,'',mcd.val_obse) AS val_dire,
       mce.camp_ulti_pedi cam_ulti_pedi,
       ABS(mc.sal_deud_ante) imp_sald_nega
FROM 
 mae_clien mc,
 mae_clien_unida_admin mcua,
 mae_clien_estat mce,
 mae_clien_ident mci,
 seg_tipo_via stv,
 mae_clien_direc mcd, 
 zon_terri_admin zta,
 zon_terri zt,
 zon_secci zs,
 zon_zona zz,
 zon_regio zr,
 (SELECT mtf.clie_oid_clie, mtf.val_text_comu
  FROM mae_clien_comun mtf
  WHERE  mtf.ticm_oid_tipo_comu = 1) tf,
 (SELECT mtt.clie_oid_clie, mtt.val_text_comu
  FROM mae_clien_comun mtt         
  WHERE mtt.ticm_oid_tipo_comu = 7) tt,
 (SELECT mtm.clie_oid_clie, mtm.val_text_comu
  FROM mae_clien_comun mtm             
  WHERE mtm.ticm_oid_tipo_comu = 6) tm
WHERE mc.oid_clie = mci.clie_oid_clie
AND mci.val_iden_docu_prin = 1 
AND mc.oid_clie = mcua.clie_oid_clie
AND mcua.ind_acti = 1 
AND mc.oid_clie = mce.oid_clie
AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
AND zta.terr_oid_terr = zt.oid_terr
AND zta.zscc_oid_secc = zs.oid_secc
AND zs.zzon_oid_zona = zz.oid_zona
AND zz.zorg_oid_regi  = zr.oid_regi
AND mcd.clie_oid_clie = mc.oid_clie
AND stv.oid_tipo_via = mcd.tivi_oid_tipo_via
AND mcd.ind_dire_ppal = 1
AND mcd.ind_elim = 0  
AND mc.oid_clie = tf.clie_oid_clie(+)    
AND mc.oid_clie = tt.clie_oid_clie(+)
AND mc.oid_clie = tm.clie_oid_clie(+) 
AND mc.sal_deud_ante < 0 
AND ABS(mc.sal_deud_ante) >=  NVL($P{importeDesde},0)
 $P!{condicion}
ORDER BY 1,2,3]]></queryString>


	<queryString><![CDATA[SELECT 
   c.fec_proc , 
   c.cod_ccba cod_banc, 
   ccb.des_cc des_banc,
   zr.cod_regi,
   zr.des_regi,
  COUNT(*) can_pago_banc, 
  SUM(D.VAL_PAGO) imp_pago_banc
FROM
  per_movim_banca_cabec c, 
  per_movim_banca_detal d,
  ccc_cuent_corri_banca ccb,
  ccc_banco cb,
  ccc_sucur cs,
  mae_clien_unida_admin mcua,
  mae_clien mc,
  zon_terri_Admin zta,
  zon_secci zs,
  zon_zona zz,
  zon_regio zr
WHERE c.pais_cod_pais = D.pais_cod_pais 
AND c.num_lote_inte = d.moca_num_lote_inte
AND d.cod_cons = mc.cod_clie
AND mc.oid_clie = mcua.clie_oid_clie
AND cb.oid_banc = cs.cban_oid_banc
AND cs.oid_sucu = ccb.sucu_oid_sucu
AND mcua.ind_acti = 1
AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
AND zta.zscc_oid_secc = zs.oid_secc
AND zs.zzon_oid_zona = zz.oid_zona
AND zz.zorg_oid_regi = zr.oid_regi
AND c.cod_ccba = ccb.cod_cc
AND c.pais_cod_pais = $P{codigoPais}
AND d.pais_cod_pais = $P{codigoPais}
AND c.cod_soci = $P{codigoSociedad}
AND c.sta_lote='C'
AND cb.cod_banc = DECODE($P{codigoBanco},$P{codigoBanco},cb.cod_banc,'',cb.cod_banc,$P{codigoBanco})
AND ccb.cod_cc = DECODE($P{codigoCuentaCorriente},$P{codigoCuentaCorriente},ccb.cod_cc,'',ccb.cod_cc,$P{codigoCuentaCorriente})
AND c.tior_tipo_orig_dato = DECODE($P{tipoLote}, $P{tipoLote},c.tior_tipo_orig_dato,'',c.tior_tipo_orig_dato,$P{tipoLote})
AND c.fec_proc >= TO_DATE($P{fechaProcDesde},'DD/MM/YYYY')
AND c.fec_proc <= TO_DATE($P{fechaProcHasta},'DD/MM/YYYY')
GROUP BY c.fec_proc , c.cod_ccba, ccb.des_cc,zr.cod_regi,zr.des_regi
ORDER BY 1 ASC
]]></queryString>


	</parameter>
	<queryString><![CDATA[select cgz.nom_grup GRUPO_DE_ZONA, 
       (SELECT rowconcat('select z.cod_zona 
               from cra_detal_grupo_zona det, zon_zona z 
               where z.oid_zona = det.zzon_oid_zona  and det.cgzo_oid_cabe_grup_zona = '||cgz.oid_cabe_grup_zona)  
         FROM dual) zonas,      
       ca.cod_acti, 
       ca.nom_acti actividad,
       TO_CHAR(cc.fec_inic, 'Day', 'NLS_DATE_LANGUAGE = SPANISH') dia,       
       cc.fec_inic fecha
from cra_crono cc,
     cra_perio cp,
     seg_perio_corpo sp,
     cra_activ ca,
     zon_zona zz,
     cra_cabec_grupo_zona cgz,
     cra_detal_grupo_zona dgz
where cc.perd_oid_peri = cp.oid_peri
and cp.peri_oid_peri = sp.oid_peri
and sp.cod_peri = $P{codigoPeriodo}
and ca.oid_acti = cc.cact_oid_acti
and zz.oid_zona = cc.zzon_oid_zona
and cgz.oid_cabe_grup_zona = dgz.cgzo_oid_cabe_grup_zona
and dgz.zzon_oid_zona = zz.oid_zona
group by cgz.nom_grup , 
       ca.nom_acti , 
       cc.fec_inic ,
       ca.cod_acti,
       cgz.oid_cabe_grup_zona
order by cgz.nom_grup,fecha]]></queryString>


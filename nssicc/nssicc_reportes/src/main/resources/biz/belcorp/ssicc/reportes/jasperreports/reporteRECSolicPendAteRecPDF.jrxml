	<queryString><![CDATA[SELECT /*+ NO_CPU_COSTING */  *
FROM (
SELECT  rec.cod_usua_ingr as usuario, 
    r.cod_regi,
        r.des_regi,
        zon.cod_zona,
        zon.des_zona, 
        te.cod_terr, 
    cli.cod_clie,
        trim(cli.val_ape1)||' '||trim(cli.val_ape2)||' '||trim(cli.val_nom1)||' '||trim(cli.val_nom2) as nom_clie, 
        to_char(rec.num_recl) as num_recl, 
        op.cod_oper,
        tiop.val_tipo_oper, 
        rec.FEC_INGR AS fecha_reclamo,        
        to_char(refer.val_nume_soli) solic_refer, 
        sc.fec_fact,  
    ped.cod_grup_proc,
        (select cons.val_nume_soli
      from ped_solic_cabec cons
          where cons.oid_soli_cabe = sc.soca_oid_soli_cabe) AS solic_generada,
        sc.perd_oid_peri,
        sc.oid_soli_cabe,
        'REC' Ind_RecInc,
        ocr_solic_pedidos.GEN_FN_DESC_PROD(PED_SOLIC_POSIC.Prod_Oid_Prod) AS des_prod,
        ocr_solic_pedidos.GEN_FN_CODSAP_PROD(PED_SOLIC_POSIC.Prod_Oid_Prod) AS cod_prod,
        VAL_CODI_VENT,
        VAL_CODI_VENT_FICT,
        case sc.grpr_oid_grup_proc
        when 1 then NUM_UNID_DEMA 
        else
        0
        end
        uni_gp1,
        case sc.grpr_oid_grup_proc
        when 2 then NUM_UNID_DEMA 
        else
        0
        end
        uni_gp2,
        case sc.grpr_oid_grup_proc
        when 3 then NUM_UNID_POR_ATEN 
        else
        0
        end
        uni_gp3,
        case sc.grpr_oid_grup_proc
        when 4 then NUM_UNID_COMPR 
        else
        0
        end
        uni_gp4,
        case sc.grpr_oid_grup_proc
        when 5 then NUM_UNID_ATEN 
        else
        0
        end
        uni_gp5,
        decode(VAL_PREC_CATA_UNIT_LOCA, 0, prod.VAL_COST_ESTD, nvl(VAL_PREC_CATA_UNIT_LOCA, prod.VAL_COST_ESTD)) as VAL_PREC_CATA_UNIT_LOCA,
        VAL_IMPO_DESC_UNIT_LOCA,
	gen_pkg_gener.gen_fn_devuelve_des_perio(rec.PERD_OID_PERI_DOCU_REFE) peri_refer  
FROM     ped_solic_cabec sc,
         mae_clien cli,
         zon_terri te,
         zon_terri_admin ter_adm,
         zon_zona zon,
         zon_regio r,
         ped_grupo_proce ped,
         rec_solic_opera solop,
         rec_opera_recla orec,
         rec_opera op,
         rec_tipos_opera tiop,
         rec_cabec_recla rec,
         ped_solic_cabec refer,
         mae_produ prod,
         PED_SOLIC_POSIC,
     PED_ESTAD_SOLIC EST,
         (SELECT fec_fina, fec_inic FROM cra_perio WHERE oid_peri =gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoInicio}) ORDER BY fec_fina) peri_i,
          (SELECT fec_fina FROM cra_perio WHERE oid_peri =gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFin}) ORDER BY fec_fina) peri_f
   WHERE cli.oid_clie = sc.clie_oid_clie
    AND SC.ESSO_OID_ESTA_SOLI = EST.OID_ESTA_SOLI
    AND EST.COD_ESTA_SOLI IN ('VA','LI')
    AND te.oid_terr = ter_adm.terr_oid_terr
    AND ter_adm.oid_terr_admi = sc.ztad_oid_terr_admi
    AND zon.oid_zona = sc.zzon_oid_zona
    AND (r.oid_regi = zon.zorg_oid_regi)
    AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) >= $P{codigoPeriodoInicio}
    AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) <= $P{codigoPeriodoFin}          
    AND (ped.oid_grup_proc = sc.grpr_oid_grup_proc)
    AND (sc.oid_soli_cabe = solop.soca_oid_soli_cabe)
    AND (orec.oid_oper_recl = solop.opre_oid_oper_recl)
    AND (op.oid_oper = tiop.rope_oid_oper)
    AND (tiop.oid_tipo_oper = orec.tiop_oid_tipo_oper)
    AND (rec.oid_cabe_recl = orec.care_oid_cabe_recl)
    AND op.oid_oper = tiop.rope_oid_oper
    AND (rec.soca_oid_soli_cabe = refer.oid_soli_cabe)     
    and (ped_solic_posic.prod_oid_prod = prod.oid_prod)
    AND  ped_solic_posic.Soca_Oid_Soli_Cabe= sc.oid_soli_cabe
    $P!{condicion}  
    AND ((ped.COD_GRUP_PROC<>'GP5'  AND sc.FEC_FACT is NULL)             
                OR( (ped.COD_GRUP_PROC='GP5' 
                   AND (peri_i.fec_inic <= sc.fec_fact)
                  AND (peri_f.fec_fina >= sc.fec_fact)          
                   AND Decode($P{fechaFacturacionInicio}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionInicio}, 'dd/mm/yyyy'))  <=sc.FEC_FACT
                   AND Decode($P{fechaFacturacionFin}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionFin}, 'dd/mm/yyyy'))  >=sc.FEC_FACT)))
    AND (sc.MODU_OID_MODU = 15)            
UNION
SELECT /*+ NO_CPU_COSTING */ sc.val_usua as usuario, 
    r.cod_regi,
        r.des_regi,
        zon.cod_zona,
        zon.des_zona, 
        te.cod_terr, 
    cli.cod_clie,
        trim(cli.val_ape1)||' '||trim(cli.val_ape2)||' '||trim(cli.val_nom1)||' '||trim(cli.val_nom2) as nom_clie, 
        '00000000' as num_recl, 
        op.cod_oper,
        tiop.val_tipo_oper, 
        sc.fec_cron AS fecha_reclamo,        
        '' as solic_refer, 
        sc.fec_fact,  
    ped.cod_grup_proc,
        (select cons.val_nume_soli
      from ped_solic_cabec cons
          where cons.oid_soli_cabe = sc.soca_oid_soli_cabe) AS solic_generada,
        sc.perd_oid_peri,
        sc.oid_soli_cabe,
        'REC' Ind_RecInc,
        ocr_solic_pedidos.GEN_FN_DESC_PROD(PED_SOLIC_POSIC.Prod_Oid_Prod) AS des_prod,
        ocr_solic_pedidos.GEN_FN_CODSAP_PROD(PED_SOLIC_POSIC.Prod_Oid_Prod) AS cod_prod,
        VAL_CODI_VENT,
        VAL_CODI_VENT_FICT,
        case sc.grpr_oid_grup_proc
        when 1 then NUM_UNID_DEMA 
        else
        0
        end
        uni_gp1,
        case sc.grpr_oid_grup_proc
        when 2 then NUM_UNID_DEMA 
        else
        0
        end
        uni_gp2,
        case sc.grpr_oid_grup_proc
        when 3 then NUM_UNID_POR_ATEN 
        else
        0
        end
        uni_gp3,
        case sc.grpr_oid_grup_proc
        when 4 then NUM_UNID_COMPR 
        else
        0
        end
        uni_gp4,
        case sc.grpr_oid_grup_proc
        when 5 then NUM_UNID_ATEN 
        else
        0
        end
        uni_gp5,
        decode(VAL_PREC_CATA_UNIT_LOCA, 0, prod.VAL_COST_ESTD, nvl(VAL_PREC_CATA_UNIT_LOCA, prod.VAL_COST_ESTD)) as VAL_PREC_CATA_UNIT_LOCA,
        VAL_IMPO_DESC_UNIT_LOCA,
	null as peri_refer  
FROM     ped_solic_cabec sc,
         mae_clien cli,
         zon_terri te,
         zon_terri_admin ter_adm,
         zon_zona zon,
         zon_regio r,
         rec_opera op,
         rec_tipos_opera tiop,
         ped_grupo_proce ped,
         mae_produ prod,
         PED_SOLIC_POSIC,
     PED_ESTAD_SOLIC EST,
         (SELECT fec_fina, fec_inic FROM cra_perio WHERE oid_peri =gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoInicio}) ORDER BY fec_fina) peri_i,
          (SELECT fec_fina FROM cra_perio WHERE oid_peri =gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFin}) ORDER BY fec_fina) peri_f
   WHERE cli.oid_clie = sc.clie_oid_clie
    AND SC.ESSO_OID_ESTA_SOLI = EST.OID_ESTA_SOLI
    AND EST.COD_ESTA_SOLI IN ('VA','LI')
    AND te.oid_terr = ter_adm.terr_oid_terr
    AND ter_adm.oid_terr_admi = sc.ztad_oid_terr_admi
    AND zon.oid_zona = sc.zzon_oid_zona
    AND (r.oid_regi = zon.zorg_oid_regi)
    AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) >= $P{codigoPeriodoInicio}
    AND gen_pkg_gener.GEN_FN_DEVUELVE_DES_PERIO(sc.perd_oid_peri) <= $P{codigoPeriodoFin}
    AND (ped.oid_grup_proc = sc.grpr_oid_grup_proc)
    and (ped_solic_posic.prod_oid_prod = prod.oid_prod)
    AND  ped_solic_posic.Soca_Oid_Soli_Cabe= sc.oid_soli_cabe
    and tiop.val_tipo_oper = '01'
    AND TIOP.ROPE_OID_OPER = OP.OID_OPER
    AND OP.TSPA_OID_SOLI_CON_STOC = sc.tspa_oid_tipo_soli_pais
    $P!{condicion}  
    AND ((ped.COD_GRUP_PROC<>'GP5'  AND sc.FEC_FACT is NULL)             
                OR( (ped.COD_GRUP_PROC='GP5' 
                   AND (peri_i.fec_inic <= sc.fec_fact)
                  AND (peri_f.fec_fina >= sc.fec_fact)          
                   AND Decode($P{fechaFacturacionInicio}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionInicio}, 'dd/mm/yyyy'))  <=sc.FEC_FACT
                   AND Decode($P{fechaFacturacionFin}, NULL,sc.FEC_FACT,to_date($P{fechaFacturacionFin}, 'dd/mm/yyyy'))  >=sc.FEC_FACT))) 
    AND (sc.MODU_OID_MODU = 1 and sc.tspa_oid_tipo_soli_pais in (
            SELECT int_pkg_recla.gen_fn_devue_oid_tipo_solpa(z.TIP_SOLI)
            FROM REP_REC_PARAM_ATENC z
        )           )
) t
order by cod_regi ASC, cod_zona ASC, cod_terr ASC, COD_CLIE ASC]]></queryString>


	<queryString><![CDATA[SELECT a.OBS_ADMI_CART AS obs_prub,
COD_PERI,
COD_REGI,
DES_REGI,
COD_ZONA,
COD_SECC,
COD_TERR,
DES_ZONA,
COD_CLIE,
NOM_CLIE,
IND_ADMI_CART,
VAL_SALD_DEUD,
OBS_ADMI_CART,
IND_GP5,
IND_ANUL,
FEC_ADMI_CART,
FEC_ULTI_PAGO,
PAG_REGU_DEUD_ANT,
VAL_SALD_DEUD_PEND,
STA_DEUD_PEND,
DIF_DIAS_DEUD_PEND,
FEC_FACT_PEDI,
FEC_ULTI_PAGO_PEDI,
PAG_REGU_PEDI,
MONT_PEDI_PEND,
STA2,
DIF_DIAS_PEDI_
FROM   JFLORENCIO.CCC_SEGU_LEVA_DEUD  A
where  A.COD_PERI = Decode ($P{codigoPeriodo},null, A.COD_PERI, 'Todos', A.COD_PERI,'', A.COD_PERI,$P{codigoPeriodo} )
   AND A.COD_REGI = Decode ($P{codigoRegion},null, A.COD_REGI, 'Todos', A.COD_REGI,'', A.COD_REGI,$P{codigoRegion} )
   AND A.COD_ZONA = Decode ($P{codigoZona},null, A.COD_ZONA, 'Todos', A.COD_ZONA,'', A.COD_ZONA,$P{codigoZona} )
AND A.FEC_FACT_PEDI >= Decode ($P{fechaInicial},null, A.FEC_FACT_PEDI, 'Todos', A.FEC_FACT_PEDI,'', A.FEC_FACT_PEDI,TO_DATE($P{fechaInicial},'dd/mm/yyyy') )
   AND A.FEC_FACT_PEDI <= Decode ($P{fechaFinal},null, A.FEC_FACT_PEDI, 'Todos', A.FEC_FACT_PEDI,'', A.FEC_FACT_PEDI,TO_DATE($P{fechaFinal},'dd/mm/yyyy') )
       ]]></queryString>


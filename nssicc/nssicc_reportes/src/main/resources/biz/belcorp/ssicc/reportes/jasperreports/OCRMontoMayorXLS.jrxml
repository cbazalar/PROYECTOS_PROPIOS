	<queryString><![CDATA[SELECT PSC.FEC_FACT,
	   $P{codigoPeriodo},
     ZR.COD_REGI,
	   ZR.DES_REGI,	 
	   ZZ.COD_ZONA,
     ZZ.DES_ZONA,
	   ZT.COD_TERR,
	   GEN_PKG_GENER.GEN_FN_DEVUELVE_COD_CLIE(PSC.CLIE_OID_CLIE) AS COD_CLIE,
	   GEN_PKG_GENER.GEN_FN_CLIEN_DATOS_OID(PSC.CLIE_OID_CLIE, 'NOM_CLIE') AS DES_CLIENTE,  
	   PSC.PERD_OID_PERI,

GEN_PKG_GENER.GEN_FN_CLIEN_MONTO_PEDID(PSC.CLIE_OID_CLIE,GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,4)) AS CAMPANIA_ANTE_4,
     	(SELECT A.VAL_NOMB_PERI FROM CRA_PERIO A WHERE A.OID_PERI = GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,4)) AS CAMPANIA_NOMBRE_4, 

GEN_PKG_GENER.GEN_FN_CLIEN_MONTO_PEDID(PSC.CLIE_OID_CLIE,GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,3)) AS CAMPANIA_ANTE_3,
     	(SELECT A.VAL_NOMB_PERI FROM CRA_PERIO A WHERE A.OID_PERI = GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,3)) AS CAMPANIA_NOMBRE_3, 

	   GEN_PKG_GENER.GEN_FN_CLIEN_MONTO_PEDID(PSC.CLIE_OID_CLIE,GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,2)) AS CAMPANIA_ANTE_2,
     (SELECT A.VAL_NOMB_PERI FROM CRA_PERIO A WHERE A.OID_PERI = GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,2)) AS CAMPANIA_NOMBRE_2, 

	   GEN_PKG_GENER.GEN_FN_CLIEN_MONTO_PEDID(PSC.CLIE_OID_CLIE,GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,1)) AS CAMPANIA_ANTE_1,
     (SELECT A.VAL_NOMB_PERI FROM CRA_PERIO A WHERE A.OID_PERI = GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_NANTE_CAMPA(PSC.PERD_OID_PERI,1)) AS CAMPANIA_NOMBRE_1 ,
	   PSC.VAL_NUME_SOLI,
	   PSC.VAL_TOTA_PAGA_LOCA,
     PSC.VAL_GLOS_OBSE    
FROM PED_SOLIC_CABEC PSC,
     ZON_REGIO ZR,
     ZON_ZONA ZZ,
     ZON_TERRI ZT
WHERE ZZ.ZORG_OID_REGI=ZR.OID_REGI
AND PSC.ZZON_OID_ZONA=ZZ.OID_ZONA
AND PSC.TERR_OID_TERR=ZT.OID_TERR
AND PSC.PERD_OID_PERI=$P{oidPeriodo}
AND PSC.PAIS_OID_PAIS = $P{oidPais}
AND PSC.VAL_TOTA_PAGA_LOCA >= $P{montoMayor}
AND PSC.IND_OC=1
AND PSC.FEC_FACT IS NOT NULL
AND PSC.FEC_FACT  >= Decode ($P{fechaInicial},null, PSC.FEC_FACT, null, PSC.FEC_FACT,'', PSC.FEC_FACT,TO_DATE($P{fechaInicial},'dd/mm/yyyy') )
AND PSC.FEC_FACT  <= Decode ($P{fechaFinal},null, PSC.FEC_FACT, null, PSC.FEC_FACT,'', PSC.FEC_FACT,TO_DATE($P{fechaFinal},'dd/mm/yyyy') )  
AND ZZ.COD_ZONA = DECODE($P{codigoZona}, null,ZZ.COD_ZONA,'',ZZ.COD_ZONA,'Todos',ZZ.COD_ZONA,$P{codigoZona})
AND ZR.COD_REGI = DECODE($P{codigoRegion}, null,ZR.COD_REGI,'',ZR.COD_REGI,'Todos',ZR.COD_REGI,$P{codigoRegion})
ORDER BY ZR.COD_REGI, ZZ.COD_ZONA, 8]]></queryString>


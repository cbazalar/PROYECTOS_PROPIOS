	<queryString><![CDATA[select    cl.COD_CLIE, 
          rg.COD_REGI,  
          zo.COD_ZONA, 
          sc.COD_SECC, 
          te.COD_TERR
from      mae_clien cl, 
          MAE_CLIEN_UNIDA_ADMIN ua, 
          zon_terri_admin za, 
          zon_terri te,
          zon_secci sc, 
          zon_zona zo, 
          zon_regio rg
where     cl.OID_CLIE = ua.CLIE_OID_CLIE 
and       ua.IND_ACTI = 1
and       ua.ZTAD_OID_TERR_ADMI = za.OID_TERR_ADMI
and       za.ZSCC_OID_SECC = sc.OID_SECC
and       sc.ZZON_OID_ZONA = zo.OID_ZONA
and       cl.COD_CLIE in (select cs.COD_CLIE from ZON_TMP_TERRI_CONSU cs)
and       za.TERR_OID_TERR = te.OID_TERR
and       zo.ZORG_OID_REGI = rg.OID_REGI
order by  zo.COD_ZONA]]></queryString>


	<queryString><![CDATA[SELECT   
   gen.val_i18n des_pais,
   socie.val_deno des_soci,
   etapa.val_desc des_etap_deud,
   cobra.val_nomb_usua_cobr nom_cobr,              
   carte.cod_usua_cobr,
   carte.cod_peri, 
   carte.des_regi_clie des_regi,   
   carte.cod_zona_clie cod_zona,
   carte.cod_secc,
   carte.cod_terr,
   carte.oid_clie,
   carte.cod_clie,
      NVL((SELECT TRIM(MC.VAL_NOM1) || ' ' || TRIM(MC.VAL_NOM2)
        FROM MAE_CLIEN MC
        WHERE MC.OID_CLIE=CARTE.OID_CLIE),' ') nom_clie, 
   NVL((SELECT TRIM(MC.VAL_APE1)
        FROM MAE_CLIEN MC
        WHERE MC.OID_CLIE=CARTE.OID_CLIE),' ') val_ape1_clie, 
       NVL((SELECT TRIM(MC.VAL_APE2)
        FROM MAE_CLIEN MC
        WHERE MC.OID_CLIE=CARTE.OID_CLIE),' ') val_ape2_clie,   
    carte.cod_tipo_docu_iden,
    carte.des_tipo_docu_iden,
    carte.num_docu_iden,
    NVL(gen_pkg_gener.gen_fn_clien_datos_oid(carte.oid_clie,'DES_DPTO'),' ') des_dpto,
    NVL(gen_pkg_gener.gen_fn_clien_datos_oid(carte.oid_clie,'DES_PROV'),' ') des_prov,
    NVL(gen_pkg_gener.gen_fn_clien_datos_oid(carte.oid_clie,'DES_DIST'),' ') des_dist,
    NVL(gen_pkg_gener.gen_fn_clien_datos_oid(carte.oid_clie,'DES_URBA'),' ') des_urba,
    FIN_PKG_GENER.FIN_FN_OBTIE_DIREC_CLIEN(carte.oid_clie)val_dire ,
    FIN_PKG_GENER.FIN_FN_OBTIE_DIREC_CLIEN_REFER(carte.oid_clie) val_refe,
    FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(carte.oid_clie,'TF') num_tele_fijo,
    FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(carte.oid_clie,'TT') num_tele_trab,
    FIN_PKG_GENER.FIN_FN_OBTIE_NUMER_TELEF_CLIEN(carte.oid_clie,'TM') num_tele_movi,
    carte.fec_docu fec_docu,
    carte.fec_venc fec_venc,
    carte.fec_asig fec_asig,
    carte.fec_cier fec_cier,
    carte.num_dias_mora num_dias_mora,
   TO_CHAR(carte.num_iden_cuot) num_bole_desp,
    (
       SELECT COUNT(*) val_cant_pedi
         FROM ped_solic_cabec_acum2 sca2
       WHERE sca2.clie_oid_clie  = carte.oid_clie   
           AND sca2.perd_oid_peri <= (select cp.oid_peri from cra_perio cp,seg_perio_corpo sp where cp.peri_oid_peri=sp.oid_peri and sp.cod_peri=carte.cod_peri)
      GROUP BY sca2.clie_oid_clie
    )Nro_Pedido,
  (SELECT mc.val_text_comu 
           FROM mae_clien_comun mc 
          WHERE mc.ticm_oid_tipo_comu=3
            AND mc.clie_oid_clie  = carte.oid_clie
      )Correo_Elect,
   gen_pkg_gener.gen_fn_devue_pedid_orig (carte.cod_clie,carte.cod_peri) origen_pedido,
   carte.imp_deud_orig imp_deud_orig,
   carte.imp_deud_asig imp_deud_asig,
   carte.imp_deud_canc imp_deud_canc,
   carte.imp_deud_pend imp_deud_pend,
   ROUND(carte.imp_deud_canc/carte.imp_deud_asig*100,2) imp_porc_recu,
   gen_pkg_gener.gen_fn_devue_prome_pagos_ultim(carte.oid_clie,$P{codigoPeriodo})DIAS_PAGOS
FROM cob_detal_asign_carte carte,
     cob_etapa_deuda_pais etapa,
     cob_usuar_cobra_Pais cobra,
     seg_socie socie,
     seg_pais pais,
     zon_regio regio,
     zon_zona zona,
     gen_i18n_sicc_comun gen
WHERE carte.cod_soci = socie.cod_soci 
AND carte.cod_pais = pais.cod_pais
AND pais.oid_pais=socie.pais_oid_pais
AND carte.cod_etap_deud =etapa.cod_etap_deud
AND carte.cod_regi_clie=regio.cod_regi
and carte.cod_zona_clie=zona.cod_zona
AND regio.oid_regi=zona.zorg_oid_regi
AND gen.attr_enti ='SEG_PAIS'
AND gen.val_oid =pais.oid_pais
AND carte.cod_usua_cobr =cobra.cod_usua_cobr
AND carte.cod_pais=$P{codigoPais}
AND carte.cod_soci=$P{codigoSociedad}
AND carte.cod_etap_deud=$P{codigoEtapaDeuda}
AND carte.cod_peri=$P{codigoPeriodo}
AND carte.cod_usua_cobr=$P{codigoCobrador}
AND carte.cod_Regi_clie=$P{codigoRegion}
AND carte.cod_zona_clie=DECODE($P{codigoZona},null,carte.cod_zona_clie,$P{codigoZona})]]></queryString>


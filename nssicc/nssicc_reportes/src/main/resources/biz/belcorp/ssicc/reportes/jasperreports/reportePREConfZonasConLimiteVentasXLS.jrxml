	<queryString><![CDATA[SELECT DISTINCT F.COD_SAP, G.VAL_I18N DESCRIPCION, H.COD_TIPO_OFER, A.PRECIO_UNITARIO, A.VAL_FACT_REPE,
A.VAL_CODI_VENT,I.COD_REGI, E.COD_ZONA, L.NUM_OFER, A.IND_DIGI CODIGO_PRINCIPAL, 
--CASE WHEN ( B.ZZON_OID_ZONA = E.OID_ZONA AND  B.ZORG_OID_REGI = I.OID_REGI) THEN 'True' end As Existe
( select count(*) from PED_GESTI_STOCK where A.OID_DETA_OFER = OFDE_OID_DETA_OFER and ZZON_OID_ZONA = E.OID_ZONA AND ZORG_OID_REGI = I.OID_REGI ) as EXISTE
FROM PRE_OFERT_DETAL A,
--PED_GESTI_STOCK B,
PRE_MATRI_FACTU C,
PRE_MATRI_FACTU_CABEC D,
ZON_ZONA E,
ZON_REGIO I,
MAE_PRODU F,
GEN_I18N_SICC_PAIS G,
CRA_PERIO J,
SEG_PERIO_CORPO K,
PRE_OFERT L,
PRE_TIPO_OFERT H
WHERE 
--A.OID_DETA_OFER = B.OFDE_OID_DETA_OFER (+)
--AND 
A.OID_DETA_OFER = C.OFDE_OID_DETA_OFER
AND A.OFER_OID_OFER = L.OID_OFER
AND H.OID_TIPO_OFER = A.TOFE_OID_TIPO_OFER
AND C.MFCA_OID_CABE = D.OID_CABE
AND D.PERD_OID_PERI = J.OID_PERI
AND J.PERI_OID_PERI = K.OID_PERI
--AND E.OID_ZONA (+) = B.ZZON_OID_ZONA
--AND B.ZORG_OID_REGI = I.OID_REGI (+)
AND E.ZORG_OID_REGI = I.OID_REGI
AND A.PROD_OID_PROD = F.OID_PROD
AND G.VAL_OID = F.OID_PROD
AND G.ATTR_ENTI = 'MAE_PRODU'
AND A.VAL_CODI_VENT IS NOT NULL

AND ( K.COD_PERI >= $P{periodoIni} AND K.COD_PERI <= $P{periodoFin})
AND H.COD_TIPO_OFER = $P{tipoOferta}
AND ( F.COD_SAP = $P{codigoSAP} or $P{codigoSAP} = 'TODOS' )
AND ( A.VAL_CODI_VENT = $P{codigoVenta} or $P{codigoVenta} = 'TODOS' ) ]]></queryString>


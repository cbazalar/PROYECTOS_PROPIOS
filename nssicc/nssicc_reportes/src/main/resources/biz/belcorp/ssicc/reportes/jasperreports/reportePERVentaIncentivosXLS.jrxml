	<queryString><![CDATA[select 
to_char(a.FEC_FACT,'dd/mm/yyyy') AS FEC_FACT,c.cod_sap, d.val_i18n, sum(b.NUM_UNID_ATEN), sum(b.VAL_PREC_NETO_UNIT_LOCA) 
from fac_docum_conta_cabec a, fac_docum_conta_linea b, mae_produ c, gen_i18n_sicc_pais d
where
(a.FEC_FACT>=TO_DATE($P{fechaDesde},'dd/mm/yyyy') and a.FEC_FACT<(to_date($P{fechaHasta},'dd/mm/yyyy')+1))
and d.attr_enti='MAE_PRODU' and b.PROD_OID_PROD=d.val_oid
and a.OID_CABE=b.DCCA_OID_CABE
and b.PROD_OID_PROD=c.OID_PROD and c.VAL_GRUP_ARTI='0502' 
and b.NUM_UNID_ATEN>0
group by a.FEC_FACT,c.cod_sap, d.val_i18n]]></queryString>


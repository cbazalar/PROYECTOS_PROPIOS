	</parameter>
	<parameter name="fechaDesde" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaHasta" isForPrompting="false" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select to_char(a.fec_emis, 'dd/mm/yyyy') fecha,
       c.DES_TIPO_DOCU tipo_documento,
       a.val_seri_docu_lega serie_legal,
       count(*) as cantidad,
       sum(ABS(a.VAL_BASE_IMPO)) base_imponible,
       sum(ABS(a.VAL_DESC)) descuento ,
       sum(ABS(a.VAL_BASE_IMPO_NETO)) base_imponible_neto,
       sum(ABS(a.IMP_IMPU))impuesto ,
       sum(ABS(a.IMP_TOTA)) importe_total
  from fac_regis_unico_venta a,
       seg_subac             b,
       fac_tipo_docum        c,
       GEN_I18N_SICC_COMUN   d
 where a.SBAC_OID_SBAC = b.OID_SBAC
   and a.SBAC_OID_SBAC = d.VAL_OID
   and d.ATTR_ENTI = 'SEG_SUBAC'
   and a.TIDO_OID_TIPO_DOCU = c.OID_TIPO_DOCU
   and to_char(a.FEC_EMIS, 'yyyymmdd') >= $P{fechaDesde}
   and to_char(a.FEC_EMIS, 'yyyymmdd') <= $P{fechaHasta}
   and a.SBAC_OID_SBAC in (888)
   AND a.IND_ESTA = 0
 group by a.fec_emis, c.DES_TIPO_DOCU, a.val_seri_docu_lega
 order by a.fec_emis, c.DES_TIPO_DOCU, a.val_seri_docu_lega
]]></queryString>


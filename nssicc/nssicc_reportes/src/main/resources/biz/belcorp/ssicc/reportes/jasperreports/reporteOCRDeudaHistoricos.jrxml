	<queryString><![CDATA[WITH DATOS AS(
SELECT A.OID_CLIE,
       fin_pkg_gener.FIN_FN_OBTIE_CAMPA_PRIME_PEDID(a.oid_clie) AS CAMPANA_INICIAL,
       fin_pkg_gener.FIN_FN_OBTIE_CAMPA_ULTIM_PEDID(a.oid_clie) AS CAMPANA_FINAL
FROM MAE_CLIEN a)

SELECT A.COD_REGI,
       A.DES_REGI,
       A.COD_ZONA,
       A.DES_ZONA,
       (gen_pkg_gener.GEN_FN_CLIEN_DATOS(a.Cod_Clie, 'COD_SECC')) as COD_SECC,
       A.COD_TERR,
       A.COD_CLIE,
       A.NOM_CLIE,
       A.VAL_MONT_PEDI,
       IND_ADMI_CART,     
       A.USU_ADMI_CART, 
       VAL_SALD_DEUD,
       A.OBS_PRUB,
       D.CAMPANA_INICIAL,
       D.CAMPANA_FINAL
FROM   PED_HISTO_SOLIC_CONSO_CABEC A,
       BAS_CTRL_FACT B,
       DATOS D
WHERE  B.COD_PAIS  =A.COD_PAIS 
       AND B.COD_PERI = A.COD_PERI 
       AND A.COD_PAIS = $P{codigoPais}       
       AND A.COD_PERI = Decode ($P{codigoPeriodo},null, A.COD_PERI, 'Todos', A.COD_PERI,'', A.COD_PERI,$P{codigoPeriodo} )
       AND A.COD_CLIE > '000000000000000'
       AND A.CLIE_OID_CLIE = D.OID_CLIE (+)
       AND A.FEC_SOLI = A.FEC_SOLI     
       AND A.COD_REGI = Decode ($P{codigoRegion},null, A.COD_REGI, 'Todos', A.COD_REGI,'', A.COD_REGI,$P{codigoRegion} )
       AND A.COD_ZONA = Decode ($P{codigoZona},null, A.COD_ZONA, 'Todos', A.COD_ZONA,'', A.COD_ZONA,$P{codigoZona} )     
       AND B.STA_CAMP = '1'
       AND A.IND_ERRO_DEUD = '2'   
       AND A.IND_ERRO_REMP = 0
       AND A.fec_prog_fact >= Decode ($P{fechaInicial},null, A.fec_prog_fact, 'Todos', A.fec_prog_fact,'', A.fec_prog_fact,TO_DATE($P{fechaInicial},'dd/mm/yyyy') )
       AND A.fec_prog_fact <= Decode ($P{fechaFinal},null, A.fec_prog_fact, 'Todos', A.fec_prog_fact,'', A.fec_prog_fact,TO_DATE($P{fechaFinal},'dd/mm/yyyy') )
         $P!{condicion}
ORDER BY COD_REGI, COD_ZONA, (gen_pkg_gener.GEN_FN_CLIEN_DATOS(a.Cod_Clie, 'COD_SECC')), COD_TERR, COD_CLIE]]></queryString>


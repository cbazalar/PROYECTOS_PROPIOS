	<queryString><![CDATA[SELECT    
   c.cod_regi,
   c.cod_zona,
   c.cod_clie,
   c.val_nom1 || ' ' ||
   c.val_nom2 || ' ' ||
   c.val_ape1 || ' ' ||
   c.val_ape2 nom_clie,
   c.num_docu_iden,
   c.fec_cast,
   c.imp_deud_cast,
   c.imp_deud_actu
FROM 
   ccc_consu_casti_cabec c
WHERE  c.fec_cast  >= TO_DATE($P{fechaCastigoDesde},'DD/MM/YYYY')
AND c.fec_cast  <= TO_DATE($P{fechaCastigoHasta},'DD/MM/YYYY')
$P!{condicion}
ORDER BY cod_regi, cod_zona ,cod_clie ASC]]></queryString>


	<queryString><![CDATA[SELECT   decode (Substr(per_ant.des_regi, 0, 1), 'L', 1, 'O' , 1, 2) AS orden,
	per_ant.periodo_act, 
		 per_ant.periodo_ant, 
		 per_ant.oid_subg_vent,
         per_ant.des_subg_vent, 
		 per_ant.oid_regi, 
		 per_ant.des_regi,
		 des_zona,
         per_ant.moneda, 
		 per_ant.estim ESTIM, 
		 per_ant.factur,
         ROUND (DECODE (per_ant.estim, 0, 0,  (per_ant.factur / per_ant.estim) * 100 )-100,2) var,
         NVL (diario.n_pedido, 0) pedid, 
		 NVL (diario.monto_neto, 0) neto,
         DECODE (NVL (diario.n_pedido, 0), 0, 0, diario.monto_neto / diario.n_pedido ) psp_mov_diario,
         ROUND (DECODE (NVL (diario.monto_neto, 0), 0, 0, (faltantes.falta / diario.monto_neto) * 100), 1) por_faltantes_mov_diario,
         per_ant.estim_act pedidos_estimados,
         NVL (acum.n_pedido, 0) pedidos_fact,
         (NVL (acum.n_pedido, 0) - per_ant.estim_act) diferencia,
         ROUND (DECODE (per_ant.estim_act, 0, 0, (NVL (acum.n_pedido, 0) / per_ant.estim_act) * 100)-100,2) por_f_e,
         DECODE (NVL (acum.n_pedido, 0),0, 0,acum.num_unid_tota / acum.n_pedido) pup,
         per_ant.venta_neta_act monto_neto_estimado,
         NVL (acum.monto_neto, 0) monto_neto_fact,
         ROUND (DECODE (per_ant.venta_neta_act,0, 0,(NVL (acum.monto_neto, 0) / per_ant.venta_neta_act)* 100)-100,2) por_variacion,
         DECODE (NVL (acum.n_pedido, 0),0, 0,NVL (acum.monto_neto, 0) / acum.n_pedido) psp,
         ROUND (DECODE (NVL (acum.monto_neto, 0),0, 0,(faltantes_acum.falta / acum.monto_neto) * 100),1) por_faltantes,
         NVL (faltantes_acum.falta, 0) faltantes_acum,
         faltantes.falta falta_diario, 
		 acum.num_unid_tota num_unid_tota
    FROM (SELECT per.val_nomb_peri periodo_act,
                 per_ant.val_nomb_peri periodo_ant, sgv.oid_subg_vent,
                 sgv.des_subg_vent, reg.oid_regi, reg.des_regi,
				 zona.oid_zona, zona.des_zona,
                 gen_moned.val_i18n moneda,
                 (SELECT COUNT (*)
                    FROM ped_solic_cabec psc,
                         zon_zona zon,
                         seg_subac sa,
                         ped_estad_solic pes,
                         ped_tipo_solic_pais psp,
                         ped_tipo_solic pts
                   WHERE psc.perd_oid_peri = per_ant.oid_peri
                     AND zon.zorg_oid_regi = reg.oid_regi
                     AND psc.fec_fact IS NOT NULL
                     AND psc.ind_ts_no_conso = 1
                     AND psc.ind_pedi_prue <> 1
                     AND ((NVL (pes.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
                     AND psc.ind_oc = 1
                     AND pes.oid_esta_soli(+) = psc.esso_oid_esta_soli
                     AND psc.tspa_oid_tipo_soli_pais = psp.oid_tipo_soli_pais
                     AND pts.oid_tipo_soli = psp.tsol_oid_tipo_soli
                     AND pts.ind_anul <> 1
                     AND pts.ind_devo <> 1
                     AND psc.zzon_oid_zona = zon.oid_zona
					 AND psc.zzon_oid_zona = zona.oid_zona
                     AND psc.sbac_oid_sbac = sa.oid_sbac) factur,
                 (SELECT NVL (SUM (fvp.num_pedi), 0)
                    FROM int_fuent_venta_previ_sap fvp
                   WHERE fvp.perd_oid_peri = per_ant.oid_peri
                     AND fvp.zorg_oid_regi = reg.oid_regi
					 and fvp.ZZON_OID_ZONA = zona.oid_zona) estim,
                 (SELECT NVL (SUM (fvp.num_pedi), 0)
                    FROM int_fuent_venta_previ_sap fvp
                   WHERE fvp.perd_oid_peri = per.oid_peri
                     AND fvp.zorg_oid_regi = reg.oid_regi
					 and fvp.ZZON_OID_ZONA = zona.oid_zona) estim_act,
                 (SELECT NVL (SUM (fvp.val_vent_neta_esta), 0)
                    FROM int_fuent_venta_previ_sap fvp
                   WHERE fvp.perd_oid_peri = per.oid_peri
                     AND fvp.zorg_oid_regi = reg.oid_regi
					 and fvp.ZZON_OID_ZONA = zona.oid_zona) venta_neta_act
            FROM cra_perio per,
                 zon_regio reg,
				 zon_zona zona,
                 zon_sub_geren_venta sgv,
                 seg_pais pais,
                 (SELECT gen.val_oid, gen.val_i18n
                    FROM v_gen_i18n_sicc gen
                   WHERE gen.attr_enti = 'SEG_MONED' AND gen.idio_oid_idio = 1) gen_moned,
                 (SELECT p.oid_peri, p.val_nomb_peri
                    FROM cra_perio p
                   WHERE p.oid_peri = $P{oidPeriodoAnterior}) per_ant
           WHERE sgv.oid_subg_vent = reg.zsgv_oid_subg_vent
		   and zona.ZORG_OID_REGI = reg.oid_regi
             AND pais.oid_pais = per.pais_oid_pais
             AND pais.mone_oid_mone = gen_moned.val_oid(+)
             AND per.pais_oid_pais = $P{oidPais}
             AND per.marc_oid_marc = to_number($P{oidMarca})
             AND per.cana_oid_cana = to_number($P{oidCanal})
			 $P!{condicionZona1} 
             AND per.oid_peri = $P{oidPeriodo}
             $P!{condicion1} 
		) per_ant,
         (		 
		 SELECT   zon.zorg_oid_regi oid_regi, zon.oid_zona,
                   SUM (val_prec_neto_unit_loca * decode(pto.IND_ESTA,1,num_unid_aten,0)) monto_neto,
                   COUNT (DISTINCT psc.oid_soli_cabe) n_pedido
              FROM ped_solic_cabec psc,
                   ped_solic_posic posic,
                   ped_solic_cabec z,
                   zon_zona zon,
                   seg_subac sa,
                   ped_estad_solic pes,
                   ped_tipo_solic_pais psp,
                   ped_tipo_solic pts, 
				   pre_ofert_detal pod, 
				   pre_tipo_ofert pto
             WHERE 1 = 1
               AND psc.oid_soli_cabe(+) = posic.soca_oid_soli_cabe
               AND z.oid_soli_cabe = psc.soca_oid_soli_cabe
               AND posic.espo_oid_esta_posi <> 2
               AND psc.fec_fact = TO_DATE ($P{fechaFinFacturacion}, 'dd/MM/yyyy')
               AND psc.ind_ts_no_conso = 1
               AND psc.ind_pedi_prue <> 1
               AND ((NVL (pes.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
               AND psc.ind_oc = 1
               AND pes.oid_esta_soli(+) = z.esso_oid_esta_soli
               AND psc.tspa_oid_tipo_soli_pais = psp.oid_tipo_soli_pais
               AND pts.oid_tipo_soli = psp.tsol_oid_tipo_soli
               AND pts.ind_anul <> 1
               AND pts.ind_devo <> 1
               AND psc.perd_oid_peri = $P{oidPeriodo}
			   $P!{condicionZona}
               AND psc.zzon_oid_zona = zon.oid_zona
               $P!{condicion}
               AND psc.sbac_oid_sbac = sa.oid_sbac
			   AND posic.OFDE_OID_DETA_OFER=pod.OID_DETA_OFER
			   AND pod.TOFE_OID_TIPO_OFER=pto.OID_TIPO_OFER
          GROUP BY zon.zorg_oid_regi,zon.oid_zona		  
		  ) diario,
         (SELECT   conso.zorg_oid_regi, conso.oid_zona,
                   SUM (  decode(VAL_PREC_CATA_UNIT_LOCA,0,0,val_prec_neto_unit_loca)  * (decode(pto.IND_ESTA,1,num_unid_dema_real,0) - decode(pto.IND_ESTA,1,num_unid_aten,0))) falta
              FROM ped_solic_cabec psc,
                   ped_solic_posic psp,
                   ped_estad_solic pes,
                   ped_tipo_solic_pais psp,
                   ped_tipo_solic pts,
                   (SELECT zon.zorg_oid_regi, zon.oid_zona,psc.oid_soli_cabe, pes.cod_esta_soli
                      FROM ped_solic_cabec psc, zon_zona zon, seg_subac sa, ped_estad_solic pes
                     WHERE psc.fec_fact = TO_DATE ($P{fechaFinFacturacion}, 'dd/MM/yyyy')
                       AND psc.ind_ts_no_conso <> 1
                       AND psc.ind_pedi_prue <> 1
                       AND psc.perd_oid_peri = $P{oidPeriodo}
					   $P!{condicionZona}
                       AND psc.zzon_oid_zona = zon.oid_zona
                       $P!{condicion} 
					   and psc.esso_oid_esta_soli=pes.oid_esta_soli
                       AND psc.sbac_oid_sbac = sa.oid_sbac) conso,
				   pre_ofert_detal pod, 
				   pre_tipo_ofert pto					   
             WHERE psc.soca_oid_soli_cabe = conso.oid_soli_cabe
               AND psc.ind_ts_no_conso = 1
               AND psc.ind_pedi_prue <> 1
               AND psp.soca_oid_soli_cabe = psc.oid_soli_cabe
               AND psp.espo_oid_esta_posi <> 2
               AND ((NVL (conso.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
               AND psc.ind_oc = 1
               AND pes.oid_esta_soli(+) = psc.esso_oid_esta_soli
               AND psc.tspa_oid_tipo_soli_pais = psp.oid_tipo_soli_pais
               AND pts.oid_tipo_soli = psp.tsol_oid_tipo_soli
               AND pts.ind_anul <> 1
               AND pts.ind_devo <> 1
			   AND psp.OFDE_OID_DETA_OFER=pod.OID_DETA_OFER
			   AND pod.TOFE_OID_TIPO_OFER=pto.OID_TIPO_OFER
          GROUP BY zorg_oid_regi,conso.oid_zona) faltantes,
         (SELECT   zon.zorg_oid_regi oid_regi,oid_zona,
                   SUM (val_prec_neto_unit_loca * decode(pto.IND_ESTA,1,num_unid_aten,0)) monto_neto,
                   COUNT (DISTINCT psc.oid_soli_cabe) n_pedido,
                   SUM (decode(pto.IND_ESTA,1,num_unid_aten,0)) num_unid_tota
              FROM ped_solic_cabec psc,
			  	   ped_solic_cabec z,
                   zon_zona zon,
                   ped_estad_solic pes,
                   ped_tipo_solic_pais psp,
                   ped_tipo_solic pts,
                   ped_solic_posic posic,
				   pre_ofert_detal pod, 
				   pre_tipo_ofert pto
             WHERE 1 = 1
               AND z.oid_soli_cabe = psc.soca_oid_soli_cabe
               AND psc.oid_soli_cabe = posic.soca_oid_soli_cabe
               AND posic.espo_oid_esta_posi <> 2
               AND psc.ind_ts_no_conso = 1
               AND psc.ind_pedi_prue <> 1
               AND ((NVL (pes.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
               AND psc.ind_oc = 1
               AND pes.oid_esta_soli(+) = z.esso_oid_esta_soli
               AND psc.tspa_oid_tipo_soli_pais = psp.oid_tipo_soli_pais
               AND pts.oid_tipo_soli = psp.tsol_oid_tipo_soli
               AND pts.ind_anul <> 1
               AND pts.ind_devo <> 1
               AND psc.fec_fact IS NOT NULL
               AND psc.fec_fact >= TO_DATE ($P{fechaInicioFacturacion}, 'dd/MM/yyyy')
               AND psc.fec_fact <= TO_DATE ($P{fechaFinFacturacion}, 'dd/MM/yyyy')
               AND psc.perd_oid_peri = $P{oidPeriodo}
			   $P!{condicionZona}
               AND psc.zzon_oid_zona = zon.oid_zona
               $P!{condicion} 
			   AND posic.OFDE_OID_DETA_OFER=pod.OID_DETA_OFER
			   AND pod.TOFE_OID_TIPO_OFER=pto.OID_TIPO_OFER
          GROUP BY zon.zorg_oid_regi,oid_zona) acum,
         (SELECT   conso.zorg_oid_regi,oid_zona,
                   SUM (  decode(VAL_PREC_CATA_UNIT_LOCA,0,0,val_prec_neto_unit_loca)  * (decode(pto.IND_ESTA,1,num_unid_dema_real,0) - decode(pto.IND_ESTA,1,num_unid_aten,0))) falta
              FROM ped_solic_cabec psc,
                   ped_solic_posic psp,
                   ped_estad_solic pes,
                   ped_tipo_solic_pais psp,
                   ped_tipo_solic pts,
                   (SELECT zon.zorg_oid_regi, oid_zona,psc.oid_soli_cabe, pes.cod_esta_soli 
                      FROM ped_solic_cabec psc, zon_zona zon, seg_subac sa, ped_estad_solic pes
                     WHERE psc.ind_ts_no_conso <> 1
                       AND psc.ind_pedi_prue <> 1
                       AND psc.fec_fact IS NOT NULL
                       AND psc.fec_fact >= TO_DATE ($P{fechaInicioFacturacion}, 'dd/MM/yyyy')
                       AND psc.fec_fact <= TO_DATE ($P{fechaFinFacturacion}, 'dd/MM/yyyy')
                       AND psc.perd_oid_peri = $P{oidPeriodo}
                       AND psc.zzon_oid_zona = zon.oid_zona
					   $P!{condicionZona}
					   and psc.esso_oid_esta_soli=pes.oid_esta_soli
					   $P!{condicion} 
					   AND psc.sbac_oid_sbac = sa.oid_sbac) conso,
				    pre_ofert_detal pod, 
				    pre_tipo_ofert pto
             WHERE psc.soca_oid_soli_cabe = conso.oid_soli_cabe
               AND psc.ind_ts_no_conso = 1
               AND psc.ind_pedi_prue <> 1
               AND psp.soca_oid_soli_cabe = psc.oid_soli_cabe
               AND psp.espo_oid_esta_posi <> 2
               AND ((NVL (conso.cod_esta_soli, 1) <> 'AN' AND $P{indSolPedFactAn}=1) OR ($P{indSolPedFactAn}=0))
               AND psc.ind_oc = 1
               AND pes.oid_esta_soli(+) = psc.esso_oid_esta_soli
               AND psc.tspa_oid_tipo_soli_pais = psp.oid_tipo_soli_pais
               AND pts.oid_tipo_soli = psp.tsol_oid_tipo_soli
               AND pts.ind_anul <> 1
               AND pts.ind_devo <> 1
			   AND psp.OFDE_OID_DETA_OFER=pod.OID_DETA_OFER
			   AND pod.TOFE_OID_TIPO_OFER=pto.OID_TIPO_OFER
          GROUP BY zorg_oid_regi,oid_zona) faltantes_acum
   WHERE 
   per_ant.oid_regi = diario.oid_regi(+)
   AND per_ant.oid_regi = faltantes.zorg_oid_regi(+)
   AND per_ant.oid_regi = acum.oid_regi(+)
   AND per_ant.oid_regi = faltantes_acum.zorg_oid_regi(+)
   and per_ant.oid_zona = diario.oid_zona(+)
   AND per_ant.oid_zona = faltantes.oid_zona(+)
   AND per_ant.oid_zona = acum.oid_zona(+)
   AND per_ant.oid_zona = faltantes_acum.oid_zona(+)   
ORDER BY per_ant.des_regi,des_zona]]></queryString>


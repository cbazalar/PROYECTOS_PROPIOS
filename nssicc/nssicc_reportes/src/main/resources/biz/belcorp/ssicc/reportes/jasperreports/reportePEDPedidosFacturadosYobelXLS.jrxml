	</parameter>
	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select to_char(FEC_FACT, 'dd/MM/yyyy') FEC_FACT,
       sum(decode(indicadorpedidoservicio, 0, 1, 0)) NORMALES,
       sum(decode(indicadorpedidoservicio, 0, 0, 1)) SERVICIO,
       sum(1) TOTAL
  from (SELECT ped_solic_cabec.fec_fact,
               ped_solic_cabec.val_nume_soli numeropedido,
               decode(nvl(sto_pkg_gener.sto_fn_obten_param_ocr($P{codigoPais},'LAR_TIPO_PEDSERV'),'1'),
                      1,
                      lar_fn_ind_serv_recl(ped_solic_cabec.oid_soli_cabe,
                                           (SELECT (PERIODO.OID_PERI)
                                              FROM CRA_PERIO PERIODO
                                             WHERE PERIODO.OID_PERI = gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodo}) 
                                            )),
                      int_pkg_laris.lar_fn_ind_serv_recl2(ped_solic_cabec.oid_soli_cabe,
                                           (SELECT (PERIODO.OID_PERI)
                                              FROM CRA_PERIO PERIODO
                                             WHERE PERIODO.OID_PERI = gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodo})
                                            ))) indicadorpedidoservicio
          FROM ped_solic_cabec, int_lar_tipo_solici_pedido_dis
         WHERE ped_solic_cabec.perd_oid_peri =
               (SELECT (PERIODO.OID_PERI)
                  FROM CRA_PERIO PERIODO
                 WHERE PERIODO.OID_PERI = gen_pkg_gener.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodo}))                       
           AND ped_solic_cabec.tspa_oid_tipo_soli_pais =
               int_lar_tipo_solici_pedido_dis.tspa_oid_tipo_soli_pais
           $P!{condicion}
           AND ped_solic_cabec.num_unid_aten_tota > 0
           AND NOT EXISTS
         (SELECT m.sbti_oid_subt_clie
                  FROM mav_param_geren_zona m, zon_zona z
                 WHERE (ped_solic_cabec.sbti_oid_subt_clie =
                       m.sbti_oid_subt_clie)
                   AND ped_solic_cabec.clie_oid_clie = z.clie_oid_clie))
 	group by fec_fact]]></queryString>


	</parameter>
	<queryString><![CDATA[select b.cod_regi,
       b.cod_zona,
       a.uni_admi,
       decode(b.ind_envi,
              '1',
              'APROBADO',
              decode(B.IND_RECH, '1', 'RECHAZADO', 'EN GESTION')) estado,
       A.cod_clie,
       A.NUM_DOCU,
       A.NUM_DOCU_IDEN,
       A.VAL_APE1,
       A.VAL_APE2,
       A.VAL_NOM1,
       A.VAL_NOM2,
       A.VAL_TELF_CLIE,
       B.USU_MODI,
       replace(b.VAL_OBSE_RECH_DEFI, ',', '') VAL_OBSE_RECH_DEFI,
       (select des_moti_rech
          from sto_recha_motiv mot
         where MOT.COD_MOTI_RECH = B.COD_MOTI_RECH
           and MOT.COD_TIPO_DOCU = 'SCC') motrech,
       B.COD_ULTI_VALI_ERRO codvali,
       (select SPV.DES_VALI
          from sto_param_valid spv
         where SPV.COD_VALI = B.COD_ULTI_VALI_ERRO
           and SPV.COD_TIPO_DOCU = 'SCC') desvali
  from INT_SOLIC_CONSO_CREDI a, sto_docum_digit b
 where A.SEC_NUME_DOCU = B.SEC_NUME_DOCU
   and trunc(B.FEC_MODI) >= to_Date($P{fechaInicio}, 'DD/MM/YYYY')
   and trunc(B.FEC_MODI) <= to_Date($P{fechaFin}, 'DD/MM/YYYY')
   AND  ($P{codigoRegion} is null or b.cod_regi=$P{codigoRegion})
   AND  ($P{codigoZona} is null or b.cod_zona=$P{codigoZona})]]></queryString>


	<queryString><![CDATA[SELECT 
CLIENTE.COD_CLIE CUENTA,
CLIENTE.VAL_NOM1|| ' '|| CLIENTE.VAL_NOM2|| ' '|| CLIENTE.VAL_APE1|| ' '|| CLIENTE.VAL_APE2 NOMBRE,
PICA.NUM_TOTA_CAJA CAJAS,
SOL.FEC_FACT FECHA, 
SOL.VAL_NUME_SOLI FACTURA,
ETIQ.NUM_CAJA CAJA,
(SELECT ET.VAL_ESTA_CAJA FROM APE_ESTAD_PEDID ET WHERE ET.OID_ESTA_PEDI= ETIQ.ESTP_OID_ESTA_PEDI) ESTADO,
ETIQ.VAL_USUA_CHEQ CHEQUEADORA,
(SELECT S.COD_SECC FROM ZON_SECCI S WHERE S.OID_SECC = PICA.ZSCC_OID_SECC) SECC,
(SELECT R.COD_REGI FROM ZON_REGIO R WHERE R.OID_REGI = PICA.ZORG_OID_REGI) ||
(SELECT Z.COD_ZONA FROM ZON_ZONA Z WHERE Z.OID_ZONA = PICA.ZZON_OID_ZONA) RGZN,
(ET.VAL_ESTA_CAJA) ESTADO,
(SELECT GEN.VAL_I18N
          FROM GEN_I18N_SICC_PAIS GEN
         WHERE GEN.VAL_OID = (SELECT CE.OID_CONF_CENT_DIST
                                FROM APP_CONFI_CENTR_DISTR CE
                               WHERE CE.COD_CENT_DIST = $P{centro})
           AND GEN.ATTR_ENTI = 'APP_CONFI_CENTR_DISTR') CENTRO
FROM 
APE_LISTA_PICAD_CABEC PICA,
PED_SOLIC_CABEC		  SOL,
APE_ETIQU			  ETIQ,
MAE_CLIEN 			  CLIENTE,
APE_ESTAD_PEDID ET
WHERE PICA.FEC_FACTU=TO_DATE($P{fecha},'DD/MM/YYYY')
AND PICA.SOCA_OID_SOLI_CABE = SOL.OID_SOLI_CABE	 AND
PICA.OID_LIST_PICA_CABE = ETIQ.LPCA_OID_LIST_PICA_CABE  
AND PICA.CLIE_OID_CLIE = CLIENTE.OID_CLIE
AND ETIQ.ESTP_OID_ESTA_PEDI =$P{estado}
ORDER BY NOMBRE]]></queryString>


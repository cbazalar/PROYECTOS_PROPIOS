AND mcc.imp_movi > 0
AND mcc.fec_docu >= TO_DATE($P{fechaDocumentoDesde},'DD/MM/YYYY')
AND mcc.fec_docu <= TO_DATE($P{fechaDocumentoHasta},'DD/MM/YYYY')
GROUP BY mcc.fec_docu
ORDER BY 1 ASC]]></queryString>


	<parameter name="codigoPeriodo" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT PedNOAnul.NroPediSAnul    AS Pedidos_Sin_Anulaciones,
       PedAnul.NroPediCAnul      AS Pedidos_Con_Anuladas,
       PedNOAnul.NroOrdenesSAnul AS Ordenes_Sin_Anulaciones,
       PedAnul.NroOrdenesCAnul   AS Ordenes_Con_Anuladas
  FROM (SELECT COUNT(DISTINCT sc.CLIE_OID_CLIE) NroPediSAnul,
               COUNT(sc.OID_SOLI_CABE) NroOrdenesSAnul
          FROM ped_solic_cabec     sc,
               ped_solic_cabec     scc,
               ped_tipo_solic_pais tsp,
               ped_tipo_solic      ts,
               ped_estad_solic     pes
         WHERE sc.SOCA_OID_SOLI_CABE = scc.OID_SOLI_CABE
           AND sc.PERD_OID_PERI = $P{codigoPeriodo}
           and sc.TSPA_OID_TIPO_SOLI_PAIS = tsp.OID_TIPO_SOLI_PAIS
           and tsp.TSOL_OID_TIPO_SOLI = ts.OID_TIPO_SOLI
           and ts.COD_TIPO_SOLI = 'SOC'
           AND scc.ESSO_OID_ESTA_SOLI = pes.OID_ESTA_SOLI
           and pes.COD_ESTA_SOLI <> 'AN') PedNOAnul,
       (SELECT COUNT(DISTINCT sc.CLIE_OID_CLIE) NroPediCAnul,
               COUNT(sc.OID_SOLI_CABE) NroOrdenesCAnul
          FROM ped_solic_cabec     sc,
               ped_solic_cabec     scc,
               ped_tipo_solic_pais tsp,
               ped_tipo_solic      ts,
               ped_estad_solic     pes
         WHERE sc.SOCA_OID_SOLI_CABE = scc.OID_SOLI_CABE
           AND sc.PERD_OID_PERI = $P{codigoPeriodo}
           and sc.TSPA_OID_TIPO_SOLI_PAIS = tsp.OID_TIPO_SOLI_PAIS
           and tsp.TSOL_OID_TIPO_SOLI = ts.OID_TIPO_SOLI
           and ts.COD_TIPO_SOLI = 'SOC'
           AND scc.ESSO_OID_ESTA_SOLI = pes.OID_ESTA_SOLI
           and pes.COD_ESTA_SOLI <> 'ZZ') PedAnul]]></queryString>


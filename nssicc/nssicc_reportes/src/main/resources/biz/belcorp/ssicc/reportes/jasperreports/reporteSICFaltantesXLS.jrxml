		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodo")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select pecf.cod_peri as CmpRef,
peco.cod_peri as CmpProc,
cod_zona as Zona,
cli.cod_clie as CodClie,
val_ape1||' '||val_ape2||', '||val_nom1||' '||val_nom2 as Nombres,
pos.val_codi_vent as CVta,
prd.cod_sap as Producto,
gen.val_i18n as Descripcion,
pos.num_unid_aten as Unid,
care.num_recl as Recl
from ped_solic_cabec con, -- Consolidado
ped_solic_cabec sol,  -- Solicitud
ped_solic_posic pos,  -- Posición
mae_produ prd, -- Productos
gen_i18n_sicc_pais gen, -- Descripciones
mae_clien cli, -- Clientes
rec_solic_opera soloper, -- Solicitud Operación
rec_opera_recla oprec, -- Operación Reclamo
rec_tipos_opera toper, -- Tipos Operación
rec_opera rope, -- Operación
rec_cabec_recla care, -- Cabecera Reclamo
zon_zona zon, -- Zonas
cra_perio peri, -- Periodo
seg_perio_corpo peco, -- Periodo Corporativo
cra_perio perf, -- Periodo
seg_perio_corpo pecf, -- Periodo Corporativo
zon_regio regi -- Región
where con.perd_oid_peri = peri.oid_peri -- Link Consolidado <--> Periodo
and peri.peri_oid_peri = peco.oid_peri  -- Link Periodo <--> Periodo Corporativo
and peco.cod_peri = $P{codigoPeriodo} -- Campaña PARM
and care.perd_oid_peri_docu_refe = perf.oid_peri -- Link Consolidado <--> Periodo
and perf.peri_oid_peri = pecf.oid_peri  -- Link Periodo <--> Periodo Corporativo
and con.tspa_oid_tipo_soli_pais not in (select tspa_oid_tipo_soli_pais from
int_lar_tipo_solici_pedido_dis) -- no Enviado a Yobel
and con.fec_fact is not null  -- Facturado
and con.oid_soli_cabe = sol.soca_oid_soli_cabe -- Link Consolidado <-> Solicitud
and sol.modu_oid_modu = 15 -- Origen reclamos
and sol.oid_soli_cabe = pos.soca_oid_soli_cabe -- Link Solicitud <-> Posición
and pos.prod_oid_prod = prd.oid_prod -- Link Posición <-> Producto
and pos.prod_oid_prod = gen.val_oid -- Link Posición <-> Descripciones
and gen.attr_enti = 'MAE_PRODU' -- Descripción de Maestro de Productos
and pos.num_unid_aten < 0 -- Productos atendidos menor a cero
and con.clie_oid_clie = cli.oid_clie -- Link Consolidado <-> Cliente
and sol.oid_soli_cabe = soloper.soca_oid_soli_cabe -- Link Solicitud <-> Solicitud Operación
and soloper.opre_oid_oper_recl = oprec.oid_oper_recl -- Link Solicitud Operación <-> Operación Reclamo
and oprec.tiop_oid_tipo_oper=toper.oid_tipo_oper -- Link Operación Reclamo <-> Tipo de Operación
and toper.rope_oid_oper=rope.oid_oper -- Link Tipo de Operación <-> Operación
and oprec.care_oid_cabe_recl = care.oid_cabe_recl -- Link Operación Reclamos <--> Cabecera Reclamo
and rope.cod_oper in (select cod_oper from rec_opera where ind_falt_merc=1 and ind_devu_fisi_fact=1)  -- Operación de faltante
and con.zzon_oid_zona=zon.oid_zona -- Link Consolidado <--> Zona
and zon.zorg_oid_regi=regi.oid_regi -- Link Zona <--> Región]]></queryString>


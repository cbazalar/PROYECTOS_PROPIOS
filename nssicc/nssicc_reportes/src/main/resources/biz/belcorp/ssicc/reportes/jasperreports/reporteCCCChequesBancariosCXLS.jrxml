	</parameter>
	<parameter name="codigoBanco" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoBanco")]]></defaultValueExpression>
	</parameter>
	<parameter name="tipoCheque" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("tipoCheque")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaProcDesde" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaProcDesde")]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaProcHasta" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaProcHasta")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT
   mb.fec_proc,       
   cb.des_banc,   
   CASE
      WHEN mb.ind_tipo_cheq ='F' THEN
        'Cheque a Fecha'
      ELSE  
        'Cheque a Dia'
   END tipo,
   mb.fec_cobr_cheq,   
   SUM(mb.imp_pago) imp_pago,
   COUNT(1) cant
FROM 
 ccc_movim_banca mb,
 ccc_subpr su,
 ccc_proce cp,
 ccc_banco_chequ cb   
WHERE mb.subp_oid_marc_crea = su.oid_subp
AND su.ccpr_oid_proc = cp.oid_proc 
AND mb.cod_banc_cheq = cb.cod_banc
AND mb.fec_proc >= DECODE(TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'),null,mb.fec_proc,'',mb.fec_proc,TO_DATE($P{fechaProcDesde},'DD/MM/YYYY'))
AND mb.fec_proc <= DECODE(TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'),null,mb.fec_proc,'',mb.fec_proc,TO_DATE($P{fechaProcHasta},'DD/MM/YYYY'))
AND mb.fec_cobr_cheq >= DECODE(TO_DATE($P{fechaCobroDesde},'DD/MM/YYYY'),null,mb.fec_cobr_cheq,'',mb.fec_cobr_cheq,TO_DATE($P{fechaCobroDesde},'DD/MM/YYYY'))
AND mb.fec_cobr_cheq <= DECODE(TO_DATE($P{fechaCobroHasta},'DD/MM/YYYY'),null,mb.fec_cobr_cheq,'',mb.fec_cobr_cheq,TO_DATE($P{fechaCobroHasta},'DD/MM/YYYY'))
AND mb.cod_banc_cheq = DECODE($P{codigoBanco},null,mb.cod_banc_cheq,'T',mb.cod_banc_cheq,'',mb.cod_banc_cheq,$P{codigoBanco})
AND mb.ind_tipo_cheq = DECODE($P{tipoCheque},null,mb.ind_tipo_cheq,'T',mb.ind_tipo_cheq,'',mb.ind_tipo_cheq,$P{tipoCheque})
AND cp.cod_proc = 'TES001'
AND su.cod_subp = '4'
AND mb.cod_iden_proc='P'
GROUP BY mb.fec_proc,cb.des_banc,mb.ind_tipo_cheq,mb.fec_cobr_cheq   
ORDER BY 1,2 ASC]]></queryString>


	<queryString><![CDATA[SELECT 
r.oid_clie, 
r.cod_clie, 
(select decode(count(1),1,'*','') from flx_consu_habil_flexi ch where ch.cod_clie= r.cod_clie
and ch.cod_peri_fact = $P{codigoPeriodo}
and ch.ind_habi = '1'
and ch.ind_acti = '0') || r.nom_clie  nom_clie, 
r.cod_zona, 
r.cod_secc, 
r.nom_lide, 
r.cod_lide, 
r.cod_terr, 
r.cam_ingr, 
r.fec_fact, 
r.num_fact, 
r.num_fac1, 
r.num_fac2, 
r.num_fac3, 
r.num_fac4, 
r.sal_pend, 
nvl(r.per_rela,0) per_rela, 
r.fec_venc, 
nvl(r.pag_dife,0) pag_dife,  
nvl(r.sal_pen1,0) sal_pen1, 
nvl(r.per_rel1,0) per_rel1, 
r.fec_ven1, 
nvl(r.pag_dif1,0) pag_dif1, 
nvl(r.sal_pen2,0) sal_pen2, 
nvl(r.per_rel2,0) per_rel2, 
r.fec_ven2, 
nvl(r.pag_dif2,0)pag_dif2, 
nvl(r.sal_pen3,0) sal_pen3, 
nvl(r.per_rel3,0) per_rel3, 
r.fec_ven3, 
nvl(r.pag_dif3,0) pag_dif3, 
nvl(r.sal_pen4,0) sal_pen4, 
nvl(r.per_rel4,0) per_rel4, 
r.fec_ven4, 
nvl(r.pag_dif4,0) pag_dif4,  
r.fec_ini4, 
r.mon_perc, 
nvl(r.per_rel5,0) per_rel5,
nvl(r.sal_unic,0) sal_unic,
(nvl(r.sal_tota,0) - REC_PKG_PROCE.REC_FN_ABONO_PENDI_CONSU(r.oid_clie,null)) sal_tota,
CASE
    WHEN r.SAL_PAGO_MIN_FLX < 0 THEN 'PAGADO'
    WHEN r.SAL_PAGO_MIN_FLX IS NULL THEN 'NO APLICA'
     ELSE TO_CHAR(r.SAL_PAGO_MIN_FLX)
   END SAL_PAGO_MIN_FLX,
nvl( CASE
     WHEN r.sal_pago_min_flx < 0 THEN 0
	ELSE r.sal_pago_min_flx
     END
    , r.sal_tota) - nvl(r.sal_pen1,0) - nvl(r.sal_pen2,0) - nvl(r.pag_dif2,0) - nvl(r.sal_pen3,0) - nvl(r.pag_dif3,0) mini_pend
FROM 
  PER_REPOR_FACTU_PENDI_SECCI r
where (nvl(r.sal_tota,0) - nvl(REC_PKG_PROCE.REC_FN_ABONO_PENDI_CONSU(r.oid_clie,null), 0)) > 0
order by 
r.cod_zona, r.cod_secc, r.cod_terr, r.cod_lide, r.cod_clie]]></queryString>


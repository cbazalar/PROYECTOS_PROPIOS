	<queryString><![CDATA[SELECT A.CAM_PARA_MAV CAMPANHA,
       TO_CHAR(A.FEC_CREA, 'dd/mm/yyyy') FECHA,
       A.COR_PARA_CONF NUMEROMAV,
       A.COD_ACTI CODACTIVIDAD,
       G.VAL_I18N ACTIVIDAD,
       A.FOR_COBR FORMACOBRO,
       A.FOR_PAGO FORMAPAGO,
       CASE A.IND_ORIG_MAV
         WHEN 'D' THEN
          'DIGITACION'
         ELSE
          'CARGA MASIVA'
       END ORIGENMAV,
       CASE A.IND_ENVI_MENS
         WHEN 'S' THEN
          'SI'
         ELSE
          'NO'
       END INDMENSAJE,       
       CASE A.IND_UNID_DIF
         WHEN 'S' THEN
          'SI'
         ELSE
          'NO'
       END INDUNDDIFE,
       CASE A.IND_UNID_DIF
         WHEN 'N' THEN
          A.NUM_UNID_PROD
         ELSE
          NULL
       END NUM_UNID_PROD,
       A.ACT_TIPO_DESP TIPODESPA,
       A.VAL_BASE_ESTI_DEST ESTIMADO,
       A.COD_TIPO_OFER CODTOFERTA,
       G1.VAL_I18N TIPOOFERTA,
       A.COD_SAP CODIGOSAP,
       G2.VAL_I18N DESCPRODUCTO,
       CASE A.COD_ESTA_MAV
          WHEN 1
             THEN 'PENDIENTE'
          WHEN 2
             THEN 'COMPLETADO'
          WHEN 3
             THEN 'INICIADO'
          WHEN 4
             THEN 'GENERADO'
          ELSE 'DESACTIVADO'
       END ESTADOMAV,
	   A.VAL_OBSE,
		A.VAL_PREC, 
		A.VAL_BASE_ESTI_DEST,
    A.USU_CREA,
    TO_CHAR(A.FEC_CREA,'dd/MM/yyyy') FEC_CREA,
    A.USU_MODI,
    TO_CHAR(A.FEC_MODI,'dd/MM/yyyy') FEC_MODI	   
  FROM MAV_PARAM_CONFI A,
       MAV_ACTIV_TIPO_OFERT ACTIV,
       GEN_I18N_SICC_PAIS G,
       (SELECT *
          FROM GEN_I18N_SICC_COMUN
         WHERE ATTR_ENTI = 'PRE_TIPO_OFERT') G1,
       (SELECT *
          FROM GEN_I18N_SICC_PAIS
         WHERE ATTR_ENTI = 'MAE_PRODU') G2
 WHERE A.CAM_PARA_MAV = $P{codigoPeriodo}
   AND A.TICL_OID_TIPO_CLIE = $P{codigoTipoMav}
   AND G.VAL_OID = A.ATOF_ACTI_OID_ACTI
   AND G.ATTR_ENTI = 'MAV_ACTIV'
   AND A.ATOF_OID_TIPO_OFER_ACTI = ACTIV.OID_TIPO_OFER_ACTI
   AND ACTIV.TOFE_OID_TIPO_OFER IN (SELECT A.OID_TIPO_OFER
                                     FROM PRE_TIPO_OFERT A
                                    WHERE A.COD_TIPO_OFER = NVL($P{codigoTipoOferta}, A.COD_TIPO_OFER))
   AND ACTIV.TOFE_OID_TIPO_OFER = G1.VAL_OID
   AND A.PROD_OID_PROD = G2.VAL_OID
   AND A.EST_REGI = 1
   AND A.COD_SAP = NVL($P{codigoProducto}, A.COD_SAP)
   AND ACTIV.ACTI_OID_ACTI = NVL($P{codigoActividad}, ACTIV.ACTI_OID_ACTI)
   $P!{condicionConfiguracion}]]></queryString>


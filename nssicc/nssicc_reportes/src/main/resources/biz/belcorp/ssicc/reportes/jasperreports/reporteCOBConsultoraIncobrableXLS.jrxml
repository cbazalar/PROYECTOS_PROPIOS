	<queryString><![CDATA[SELECT  
        COD_SOCI soc,
        COD_CLIE,
        num_docu_iden cedula,
        VAL_NOM1||' '||VAL_NOM2||' '||VAL_APE1 ||' '||VAL_APE2 Nombre,
        cod_camp_deud camp_deuda,            
        IMP_DEUD_CAST vlr_castigado,            
        IMP_DEUD_PAGA vlr_pagos,             
        IMP_DEUD_ACTU deuda_actual,
        CASE IND_ACTI
                  WHEN 1 THEN 'Deudora' 
                  WHEN 8 THEN 'Condonada'  
                  WHEN 9 THEN 'Cancelada' 
         END  INDICA
     FROM
        CCC_CONSU_CASTI_CABEC
     WHERE ind_acti in (1,8,9) 
     AND COD_SOCI  = $P{codigoSociedad}
     $P!{condicion}]]></queryString>


	<queryString><![CDATA[SELECT cbz.cod_peri CAMPAÑA,
       cbz.cod_regi COD_REGION,
       cbz.cod_zona COD_ZONA,
       cbz.cod_secc COD_SECCION,
       SUM(cbz.imp_fact) FACTURADO,
       SUM(cbz.imp_carg_dire) CARGO_DIRECTO,
       SUM(cbz.imp_abon_nmon) ABONO_MOMEN,
       SUM(cbz.imp_fact_neto) FACTURA_NETO,
       SUM(cbz.cob_dias_21) COBRANZA_21,
       SUM(cbz.cob_dias_31) COBRANZA_31,
       SUM(cbz.cob_dias_42) COBRANZA_42,
       SUM(cbz.cob_dias_63) COBRANZA_63,
       SUM(cbz.cob_dias_84) COBRANZA_84,
       SUM(cbz.cob_dias_126) COBRANZA_126,
       SUM(cbz.cob_dias_189) COBRANZA_189,
       SUM(cbz.cob_dias_999) COBRANZA_999,
       SUM(cbz.imp_sald_pend_sac) SALDO
  FROM cob_repor_estad_recup_cobra cbz
  WHERE cbz.ind_cicl_nuev = $P{codigoCicloNueva}
   AND cbz.cod_peri >= $P{codPeriodoInicio}
   AND cbz.cod_peri <= $P{codPeriodoFin} 
   $P!{condicion}
 GROUP BY cbz.cod_peri, cbz.cod_regi, cbz.cod_zona, cbz.cod_secc
 ORDER BY cbz.cod_peri, cbz.cod_regi, cbz.cod_zona, cbz.cod_secc ASC]]></queryString>


	</parameter>
	<queryString><![CDATA[WITH temp1 AS (
       SELECT mcc.clie_oid_clie, MIN(spc.cod_peri) cod_peri
       FROM 
            ccc_movim_cuent_corri mcc,
            cra_perio cp,
            seg_perio_corpo spc    
      WHERE mcc.perd_oid_peri = cp.oid_peri
         AND cp.peri_oid_peri = spc.oid_peri 
        AND mcc.subp_oid_subp_crea = 2001
        AND mcc.imp_movi > 0
     HAVING MIN(spc.cod_peri) >= $P{codigoPeriodoInicio} 
        AND MIN(spc.cod_peri) <= $P{codigoPeriodoFin}
    GROUP BY mcc.clie_oid_clie)
    SELECT 
        zr.des_regi,
        zz.cod_zona,
   zs.cod_secc,
   mc.cod_clie,
   mci.num_docu_iden,
   mc.val_nom1 || ' ' || mc.val_nom2 || ' ' || mc.val_ape1 || ' ' || mc.val_ape2 nom_clie,
   t1.cod_peri
    FROM 
   temp1 t1,
   mae_clien mc,
   mae_clien_ident mci,
   mae_clien_unida_admin mcua,
   zon_terri_admin zta,
   zon_secci zs,
   zon_zona zz,
   zon_regio zr
    WHERE t1.clie_oid_clie = mc.oid_clie
      AND mc.oid_clie = mci.clie_oid_clie
      AND t1.clie_oid_clie = mci.clie_oid_clie
    AND mci.val_iden_docu_prin = 1
    AND mcua.ind_acti = 1
    and mc.oid_clie = mcua.clie_oid_clie
    AND mcua.ztad_oid_terr_admi = zta.oid_terr_admi
    AND zta.zscc_oid_secc = zs.oid_secc
    AND zs.zzon_oid_zona = zz.oid_zona
    AND zz.zorg_oid_regi = zr.oid_regi
    $P!{condicionRegion}
    $P!{condicionZona}
    $P!{condicionSeccion}
 ORDER BY zr.cod_regi,zz.cod_zona,zs.cod_secc]]></queryString>


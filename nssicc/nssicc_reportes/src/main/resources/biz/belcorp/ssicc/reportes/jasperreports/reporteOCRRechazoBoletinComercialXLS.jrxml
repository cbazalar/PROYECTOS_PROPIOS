	<queryString><![CDATA[select substr(cc.uni_admi,1,2) CODREGION,
       substr(cc.uni_admi,3,4) CODZONA,
       substr(cc.uni_admi,7,1) CODSECCION,
       substr(cc.uni_admi,8,6) CODTERRITORIO,
       cc.num_docu_iden NRORUT,
       cc.val_ape1 || ' ' || cc.val_ape2 || ' ' || cc.val_nom1 || ' ' || cc.val_nom2 NOMBRES,
       cc.cod_peri CAM_RECHAZO,
       to_char(dd.fec_modi,'dd/MM/yyyy') FEC_RECHAZO
from sto_docum_digit dd,
     sto_detal_docum_excep de,
     int_solic_conso_credi cc
where de.sec_nume_docu = dd.sec_nume_docu
and dd.cod_tipo_docu = de.cod_tipo_docu
and dd.cod_tipo_docu = 'SCC'   
and de.cod_vali = 'SCC-44'
and cc.sec_nume_docu = dd.sec_nume_docu
and dd.ind_rech = '1'
-- filtros
$P!{condicion}
union all
select substr(cc.uni_admi,1,2) CODREGION,
       substr(cc.uni_admi,3,4) CODZONA,
       substr(cc.uni_admi,7,1) CODSECCION,
       substr(cc.uni_admi,8,6) CODTERRITORIO,
       cc.num_docu_iden NRORUT ,
       cc.val_ape1 || ' ' || cc.val_ape2 || ' ' || cc.val_nom1 || ' ' || cc.val_nom2 NOMBRES,
       cc.cod_peri CAM_RECHAZO,
       to_char(dd.fec_modi,'dd/MM/yyyy') FEC_RECHAZO
from sto_histo_docum_digit dd,
     sto_histo_detal_docum_excep de,
     int_histo_solic_conso_credi cc
where de.sec_nume_docu = dd.sec_nume_docu
and dd.cod_tipo_docu = de.cod_tipo_docu
and dd.cod_tipo_docu = 'SCC'   
and de.cod_vali = 'SCC-44'
and cc.sec_nume_docu = dd.sec_nume_docu
and dd.ind_rech = '1'
$P!{condicion}
ORDER BY 1,2]]></queryString>


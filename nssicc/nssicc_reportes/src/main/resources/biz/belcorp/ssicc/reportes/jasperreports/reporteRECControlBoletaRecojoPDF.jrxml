	<parameter name="parameterMap" isForPrompting="false" class="java.util.Map"/>
	<parameter name="codigoPeriodoInicial" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoInicial")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPeriodoFinal" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPeriodoFinal")]]></defaultValueExpression>
	</parameter>
	<parameter name="codigoPais" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("codigoPais")]]></defaultValueExpression>
	</parameter>
	<parameter name="recojoList" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("recojoList")]]></defaultValueExpression>
	</parameter>
	<parameter name="resultadoList" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("resultadoList")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionRegion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionRegion")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT T.PERIODO,
	   T.REGION,
	   T.ZONA,
	   T.CLIENTE,
	   T.BOLETA_RECOJO,
	   TO_CHAR(T.FECHA_EMISION,'DD/MM/YYYY') AS FECHA_EMISION,
	   TO_CHAR(T.FECHA_RECOJO,'DD/MM/YYYY') AS FECHA_RECOJO,
	   T.NUM_RECOJO,
	   T.ESTADO,
	   T.NOVEDAD,
	   T.COD_NOVEDAD,
	   T.UNIDADES_RECLAMADAS,
	   T.BOLETA_DESPACHO,
	   T.LOTE_ENVIO,
	   T.LOTE_DEVUELVE,
       T.FECHA_GENER,
       T.CON_PEDIDO
		FROM (
		SELECT 
		CB.COD_REGI REGION, -- CODIGO DE REGION
		CB.COD_ZONA ZONA, -- CODIGO DE ZONA
		MC.COD_CLIE CLIENTE, -- CODIGO DE CLEITNE DE BOLETA DE RECOJO
		CB.VAL_NUME_BORE BOLETA_RECOJO, -- NUMERO DE BOLETA DE RECOJO
		TO_DATE(CB.FEC_INGR) FECHA_EMISION, -- FECHA DE EMISION DE BOLETA DE RECOJO
		CB.FEC_RECO FECHA_RECOJO,
		DECODE(CB.NUM_RECO,1,'PRIMER RECOJO') NUM_RECOJO, -- NUMERO DE RECOJO
		REB1.DES_ESTA_BORE ESTADO,
		REB2.DES_ESTA_BORE NOVEDAD,
		REB2.COD_ESTA_BORE COD_NOVEDAD,
		CB.NUM_TOTA_UNID_RECL UNIDADES_RECLAMADAS, -- NUMERO TOTAL DE UNIDADES RECLAMADAS
		CB.VAL_NUME_BOLE_DESP BOLETA_DESPACHO, -- NUMERO DE BOLETA DE DESPACHO (NROFAC)
		CB.COD_PERI_proc PERIODO, -- CAMPAÑA DE PROCESO DE BOLETA DE RECOJO
		CB.NUM_LOTE_ENVI LOTE_ENVIO,
		CB.NUM_LOTE_DEVU LOTE_DEVUELVE,
		TO_CHAR(CB.FEC_INGR,'DD/MM/YYYY')  FECHA_GENER,
		CB.VAL_NUME_SOLI_PED1  CON_PEDIDO
		FROM INT_REC_CABEC_BOREC CB,
			 MAE_CLIEN MC	,
			 INT_REC_ESTAD_BOREC REB1,
			 INT_REC_ESTAD_BOREC REB2	  
		WHERE CB.COD_PAIS = $P{codigoPais}
		AND CB.NUM_RECO IN (1)
		AND MC.OID_CLIE = CB.CLIE_OID_CLIE
		AND CB.ESBO_OID_ESTA_BOR1 = REB1.OID_ESTA_BORE
		AND CB.ESBO_OID_ESTA_BOR2 = REB2.OID_ESTA_BORE(+)
		-- filtro de periodo
		AND CB.COD_PERI_proc >= $P{codigoPeriodoInicial}
		AND CB.COD_PERI_proc <= $P{codigoPeriodoFinal}
		UNION
		SELECT 
		CB.COD_REGI REGION, -- CODIGO DE REGION
		CB.COD_ZONA ZONA, -- CODIGO DE ZONA
		MC.COD_CLIE CLIENTE, -- CODIGO DE CLEITNE DE BOLETA DE RECOJO
		CB.VAL_NUME_BORE BOLETA_RECOJO, -- NUMERO DE BOLETA DE RECOJO
		TRUNC(CB.FEC_ING2) FECHA_EMISION, -- FECHA DE EMISION DE BOLETA DE RECOJO
		TRUNC(CB.FEC_REC2) FECHA_RECOJO,
		DECODE(CB.NUM_RECO,2,'SEGUNDO RECOJO') NUM_RECOJO, -- NUMERO DE RECOJO
		REB1.DES_ESTA_BORE ESTADO,
		REB2.DES_ESTA_BORE NOVEDAD,
		REB2.COD_ESTA_BORE COD_NOVEDAD,
		CB.NUM_TOTA_UNID_RECL UNIDADES_RECLAMADAS, -- NUMERO TOTAL DE UNIDADES RECLAMADAS
		CB.VAL_NUME_BOLE_DESP BOLETA_DESPACHO, -- NUMERO DE BOLETA DE DESPACHO (NROFAC)
		CB.COD_PERI_proc PERIODO, -- CAMPAÑA DE PROCESO DE BOLETA DE RECOJO
		CB.NUM_LOTE_ENV2  LOTE_ENVIO,
		CB.NUM_LOTE_DEV2 LOTE_DEVUELVE,
		TO_CHAR(CB.FEC_INGR,'DD/MM/YYYY')  FECHA_GENER,
		CB.VAL_NUME_SOLI_PED1  CON_PEDIDO
		FROM INT_REC_CABEC_BOREC CB,
			 MAE_CLIEN MC	,
			 INT_REC_ESTAD_BOREC REB1,
			 INT_REC_ESTAD_BOREC REB2	  
		WHERE CB.COD_PAIS = $P{codigoPais}
		AND CB.NUM_RECO IN (2)
		AND MC.OID_CLIE = CB.CLIE_OID_CLIE
		AND CB.ESBO_OID_ESTA_BOR1 = REB1.OID_ESTA_BORE
		AND CB.ESBO_OID_ESTA_BOR2 = REB2.OID_ESTA_BORE(+)
		-- filtro de periodo
		AND CB.COD_PERI_proc >= $P{codigoPeriodoInicial}
		AND CB.COD_PERI_proc <= $P{codigoPeriodoFinal}
		) T	
		WHERE 
			  (T.NUM_RECOJO= $P{recojoList} or $P{recojoList}='TODOS')
			   AND (T.COD_NOVEDAD=$P{resultadoList} or $P{resultadoList}='TODOS')
			  $P!{condicionRegion}
		ORDER BY T.PERIODO,T.REGION, T.ZONA, T.FECHA_EMISION]]></queryString>


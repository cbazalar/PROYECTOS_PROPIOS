	<queryString><![CDATA[select 
    r.des_regi
    ,z.cod_zona
    ,z.des_zona
    ,(select count(*)
      from zon_secci s
      where gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_inic)<= $P{codigoPeriodo} 
      and (s.perd_oid_peri_fina is null or gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_fina)>= $P{codigoPeriodo} ) 
      and s.zzon_oid_zona = z.oid_zona ) secciones
    ,(select count(*)
      from zon_secci s
      where gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_inic)<= $P{codigoPeriodo} 
      and (s.perd_oid_peri_fina is null or gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_fina)>= $P{codigoPeriodo} ) 
      and s.clie_oid_clie is not null
      and s.zzon_oid_zona = z.oid_zona ) cantidad_lideres
    ,(select count(*)
      from zon_secci s
      where gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_inic)<= $P{codigoPeriodo} 
      and (s.perd_oid_peri_fina is null or gen_pkg_gener.gen_fn_devuelve_des_perio(s.perd_oid_peri_fina)>= $P{codigoPeriodo} ) 
      and s.clie_oid_clie is null
      and s.zzon_oid_zona = z.oid_zona ) secciones_sin_lider
    ,(select count(*)
     from lid_criti_campa cc, mae_clien c, zon_secci s1
     where cc.cod_clie_lide = c.cod_clie
     and c.oid_clie = s1.clie_oid_clie
     and cc.cod_peri = $P{codigoPeriodo}
     and cc.ind_prod = 'P'
     and s1.zzon_oid_zona = z.oid_zona ) tot_lider_produ
    ,(select count(*)
     from lid_criti_campa cc, mae_clien c, zon_secci s1
     where cc.cod_clie_lide = c.cod_clie
     and c.oid_clie = s1.clie_oid_clie
     and cc.cod_peri = $P{codigoPeriodo}
     and cc.ind_prod <> 'P'
     and s1.zzon_oid_zona = z.oid_zona ) tot_lider_no_produ
from 
    zon_zona z
    ,zon_regio r 
where gen_pkg_gener.gen_fn_devuelve_des_perio(z.perd_oid_peri_inic) <= $P{codigoPeriodo}
and (z.perd_oid_peri_fina is null or gen_pkg_gener.gen_fn_devuelve_des_perio(z.perd_oid_peri_fina) >= $P{codigoPeriodo})
and z.zorg_oid_regi = r.oid_regi
$P!{condicionRegion}
$P!{condicionZona}
order by des_regi, cod_zona]]></queryString>


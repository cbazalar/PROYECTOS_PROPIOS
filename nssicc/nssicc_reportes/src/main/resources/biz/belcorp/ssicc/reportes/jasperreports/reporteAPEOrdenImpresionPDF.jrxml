	<parameter name="fechaFacturacion" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("fechaFacturacion")]]></defaultValueExpression>
	</parameter>
	<parameter name="condicionTipoSolicitud" isForPrompting="true" class="java.lang.String">
		<defaultValueExpression ><![CDATA[$P{parameterMap}.get("condicionTipoSolicitud")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT 1,
       'TODAS LAS LINEAS'  LINEA,
       orden               ORDEN,
       cod_region          REGION,
		   cod_zona            ZONA,
		   a                   SEC_A,
		   b                   SEC_B,
		   c                   SEC_C,
		   d                   SEC_D,
		   e                   SEC_E,
		   f                   SEC_F,
		   g                   SEC_G,
		   h                   SEC_H,
		   i                   SEC_I,
		   j                   SEC_J,
		   totalzona           TOT_ZONA,
       (SELECT TRIM(cliente.val_nom1)|| ' '|| TRIM(cliente.val_nom2) || ' '|| TRIM(cliente.val_ape1) || ' '|| TRIM(cliente.val_ape2)
          FROM mae_clien cliente 
         WHERE cliente.oid_clie = impresion.oid_cliente) GERENTE
  FROM (SELECT secuencia.num_secu_zona_ruta     ORDEN,
               r.cod_regi                       COD_REGION,
       				 z.cod_zona                       COD_ZONA,
       				 z.clie_oid_clie                  OID_CLIENTE,
       				 SUM(DECODE(cod_secc, 'A', 1,0))  A,
       				 SUM(DECODE(cod_secc, 'B', 1,0))  B,
       				 SUM(DECODE(cod_secc, 'C', 1,0))  C,
       				 SUM(DECODE(cod_secc, 'D', 1,0))  D,
       				 SUM(DECODE(cod_secc, 'E', 1,0))  E,
               SUM(DECODE(cod_secc, 'F', 1,0))  F,
               SUM(DECODE(cod_secc, 'G', 1,0))  G,
               SUM(DECODE(cod_secc, 'H', 1,0))  H,
               SUM(DECODE(cod_secc, 'I', 1,0))  I,
               SUM(DECODE(cod_secc, 'J', 1,0))  J,
              (SUM(DECODE(cod_secc, 'A', 1,0)) + SUM(DECODE(cod_secc, 'F', 1,0)) +
               SUM(DECODE(cod_secc, 'B', 1,0)) + SUM(DECODE(cod_secc, 'G', 1,0)) +
       			   SUM(DECODE(cod_secc, 'C', 1,0)) + SUM(DECODE(cod_secc, 'H', 1,0)) +
       			   SUM(DECODE(cod_secc, 'D', 1,0)) + SUM(DECODE(cod_secc, 'I', 1,0)) +
       			   SUM(DECODE(cod_secc, 'E', 1,0)) + SUM(DECODE(cod_secc, 'J', 1,0)) ) TOTALZONA
          FROM zon_zona z,
          		 zon_regio r,
          		 ped_solic_cabec sol,
          		 zon_secci s,
          		 ped_tipo_solic_pais tip,
          		 ped_tipo_solic solic,
          		 zon_terri_admin ad,
               seg_subac sub,
          		 ped_solic_cabec_secue secuencia,
               ape_lista_picad_cabec lista
         WHERE z.zorg_oid_regi = r.oid_regi
           AND sol.zzon_oid_zona = z.oid_zona
        	 AND sol.ztad_oid_terr_admi = ad.oid_terr_admi
        	 AND sol.fec_fact = TO_DATE($P{fechaFacturacion},'DD/MM/YYYY')
        	 AND tip.oid_tipo_soli_pais = sol.tspa_oid_tipo_soli_pais
         	 AND solic.oid_tipo_soli = tip.tsol_oid_tipo_soli
        	 AND (sol.num_docu_orig IS NULL OR sol.num_docu_orig = 0)
           AND ad.zscc_oid_secc = s.oid_secc
			     AND (sol.num_docu_orig = 0 OR length(sol.num_docu_orig)< 2 OR sol.num_docu_orig IS NULL)
        	 AND sub.oid_sbac = sol.sbac_oid_sbac
        	 AND secuencia.soca_oid_soli_cabe = sol.oid_soli_cabe
           AND sol.oid_soli_cabe = lista.soca_oid_soli_cabe
          
           $P!{condicionTipoSolicitud}
           
         GROUP BY cod_regi,cod_zona,z.clie_oid_clie,secuencia.num_secu_zona_ruta) IMPRESION
UNION
SELECT 2,
       (SELECT i18.val_i18n
          FROM gen_i18n_sicc_pais i18
         WHERE i18.attr_enti = 'APE_LINEA_ARMAD'
           AND i18.attr_num_atri = 1
           AND i18.idio_oid_idio = 1
           AND i18.val_oid = oid_linea) LINEA,
       orden               ORDEN,
       cod_region          REGION,
		   cod_zona            ZONA,
		   a                   SEC_A,
		   b                   SEC_B,
		   c                   SEC_C,
		   d                   SEC_D,
		   e                   SEC_E,
		   f                   SEC_F,
		   g                   SEC_G,
		   h                   SEC_H,
		   i                   SEC_I,
		   j                   SEC_J,
		   totalzona           TOT_ZONA,
       (SELECT TRIM(cliente.val_nom1)|| ' '|| TRIM(cliente.val_nom2) || ' '|| TRIM(cliente.val_ape1) || ' '|| TRIM(cliente.val_ape2)
          FROM mae_clien cliente 
         WHERE cliente.oid_clie = impresion.oid_cliente) GERENTE
  FROM (SELECT secuencia.num_secu_zona_ruta     ORDEN,
               r.cod_regi                       COD_REGION,
       				 z.cod_zona                       COD_ZONA,
       				 z.clie_oid_clie                  OID_CLIENTE,
               lista.liar_oid_line_arma         OID_LINEA,
       				 SUM(DECODE(cod_secc, 'A', 1,0))  A,
       				 SUM(DECODE(cod_secc, 'B', 1,0))  B,
       				 SUM(DECODE(cod_secc, 'C', 1,0))  C,
       				 SUM(DECODE(cod_secc, 'D', 1,0))  D,
       				 SUM(DECODE(cod_secc, 'E', 1,0))  E,
               SUM(DECODE(cod_secc, 'F', 1,0))  F,
               SUM(DECODE(cod_secc, 'G', 1,0))  G,
               SUM(DECODE(cod_secc, 'H', 1,0))  H,
               SUM(DECODE(cod_secc, 'I', 1,0))  I,
               SUM(DECODE(cod_secc, 'J', 1,0))  J,
              (SUM(DECODE(cod_secc, 'A', 1,0)) + SUM(DECODE(cod_secc, 'F', 1,0)) +
               SUM(DECODE(cod_secc, 'B', 1,0)) + SUM(DECODE(cod_secc, 'G', 1,0)) +
       			   SUM(DECODE(cod_secc, 'C', 1,0)) + SUM(DECODE(cod_secc, 'H', 1,0)) +
       			   SUM(DECODE(cod_secc, 'D', 1,0)) + SUM(DECODE(cod_secc, 'I', 1,0)) +
       			   SUM(DECODE(cod_secc, 'E', 1,0)) + SUM(DECODE(cod_secc, 'J', 1,0)) ) TOTALZONA
          FROM zon_zona z,
          		 zon_regio r,
          		 ped_solic_cabec sol,
          		 zon_secci s,
          		 ped_tipo_solic_pais tip,
          		 ped_tipo_solic solic,
          		 zon_terri_admin ad,
               seg_subac sub,
          		 ped_solic_cabec_secue secuencia,
               ape_lista_picad_cabec lista
         WHERE z.zorg_oid_regi = r.oid_regi
           AND sol.zzon_oid_zona = z.oid_zona
        	 AND sol.ztad_oid_terr_admi = ad.oid_terr_admi
        	 AND sol.fec_fact = TO_DATE($P{fechaFacturacion},'DD/MM/YYYY')
        	 AND tip.oid_tipo_soli_pais = sol.tspa_oid_tipo_soli_pais
         	 AND solic.oid_tipo_soli = tip.tsol_oid_tipo_soli
        	 AND (sol.num_docu_orig IS NULL OR sol.num_docu_orig = 0)
           AND ad.zscc_oid_secc = s.oid_secc
			     AND (sol.num_docu_orig = 0 OR length(sol.num_docu_orig)< 2 OR sol.num_docu_orig IS NULL)
        	 AND sub.oid_sbac = sol.sbac_oid_sbac
        	 AND secuencia.soca_oid_soli_cabe = sol.oid_soli_cabe
           AND sol.oid_soli_cabe = lista.soca_oid_soli_cabe
           
           $P!{condicionTipoSolicitud}
           
         GROUP BY cod_regi,cod_zona,z.clie_oid_clie,secuencia.num_secu_zona_ruta, lista.liar_oid_line_arma) IMPRESION
ORDER BY 1, region, ORDEN]]></queryString>


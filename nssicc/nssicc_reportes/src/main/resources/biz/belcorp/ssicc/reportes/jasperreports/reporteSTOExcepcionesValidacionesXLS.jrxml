	<queryString><![CDATA[SELECT *
FROM
(
SELECT A.COD_PERI PERIODO,
            A.COD_TIPO_DOCU DOCUMENTO,
            A.COD_VALI VALIDACION,
            D.DES_VALI DESCRIPCION,            
            TO_CHAR(A.FEC_DIGI, 'DD/MM/YYYY') FECHA,            
            'INGRESO' TIPO,
            C.NUM_DOCU_IDEN CEDULA,
            A.USU_DIGI USUARIO
  FROM STO_EXCLU_VALID a, 
            MAE_CLIEN b, 
            MAE_CLIEN_IDENT c,
            STO_PARAM_VALID d
 WHERE A.COD_VALI = D.COD_VALI 
      and A.COD_CLIE = B.COD_CLIE
      and B.OID_CLIE = C.CLIE_OID_CLIE
      and C.VAL_IDEN_DOCU_PRIN = 1
      and A.COD_TIPO_DOCU = $P{documento}
      $P!{validacion}
      $P!{periodo}
      $P!{codigoCliente}
      $P!{fecha}
UNION
SELECT A.COD_PERI PERIODO,
            A.COD_TIPO_DOCU DOCUMENTO,
            A.COD_VALI VALIDACION,
            D.DES_VALI DESCRIPCION,
            TO_CHAR(A.FEC_DIGI, 'DD/MM/YYYY') FECHA,            
            'ELIMINADO' TIPO,
            C.NUM_DOCU_IDEN CEDULA,
            A.USU_DIGI USUARIO
   FROM STO_HISTO_EXCLU_VALID a, 
             MAE_CLIEN b, 
             MAE_CLIEN_IDENT c,
             STO_PARAM_VALID d
  WHERE A.COD_VALI = D.COD_VALI 
       and A.COD_CLIE = B.COD_CLIE
       and B.OID_CLIE = C.CLIE_OID_CLIE
       and C.VAL_IDEN_DOCU_PRIN = 1
       and A.COD_TIPO_DOCU = $P{documento}
       $P!{validacion}
       $P!{periodo}
       $P!{codigoCliente}
       $P!{fecha}
)
ORDER BY PERIODO, DOCUMENTO, VALIDACION, FECHA, TIPO, CEDULA]]></queryString>


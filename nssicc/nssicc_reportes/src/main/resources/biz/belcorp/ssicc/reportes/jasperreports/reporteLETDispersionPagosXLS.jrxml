	<queryString><![CDATA[WITH temp AS
(
  SELECT pago.cam_proc CampannaProceso,
         pago.cod_lide CodigoLider,
         DECODE(pago.val_ape1,NULL,'',pago.val_ape1) || DECODE(pago.val_ape2,NULL,'',' ') ||
         DECODE(pago.val_ape2,NULL,'',pago.val_ape2) || DECODE(pago.val_nom1,NULL,'',' ') ||
         DECODE(pago.val_nom1,NULL,'',pago.val_nom1) || DECODE(pago.val_nom2,NULL,'',' ') ||
         DECODE(pago.val_nom2,NULL,'',pago.val_nom2) nombreLider,
         unad.cod_regi || ' - ' || unad.des_regi Region,
         unad.cod_zona || ' - ' || unad.des_zona Zona,
         unad.cod_secc || ' - ' || unad.des_secci Seccion,
         (
           SELECT UPPER( llni.lniv_cod_nive || ' - ' || lniv.des_nive )
             FROM lec_lider_nivel llni,
                  lec_nivel lniv
            WHERE llni.lniv_cod_nive = lniv.cod_nive
              AND llni.pais_cod_pais = pago.pais_cod_pais
              AND llni.lpro_cod_prog = pago.lpro_cod_prog
              AND llni.cod_lide = pago.cod_lide
              AND llni.ind_tipo_nive = 'R'
              AND pago.cam_proc BETWEEN llni.cam_inic AND NVL(llni.cam_fin,pago.cam_proc )
         ) CodigoNivel,
         ltpg.cod_tarj CodigoTarjeta,
         ltpg.num_tarj NumeroTarjeta,
         ltpg.lest_cod_esta || ' - ' || UPPER(lest.des_esta) EstadoTarjeta,
         pago.ltpa_cod_tipo_pago || ' - ' || UPPER(pago.des_tipo_pago) TipoPago,
         pago.num_secu NumeroComprobante,
         pago.val_mont_brut MontoBruto,
         pago.val_impu MontoImpuesto,
         pago.val_mont_neto Montoneto,
         TO_CHAR(pago.fec_proc_pago, 'dd/MM/yyyy') FechaPago,
         (
          SELECT lesp.cod_esta_pago || ' - ' || lesp.des_esta_pago
            FROM SSICC_COMUN.Lec_Estad_Pagos lesp
           WHERE lesp.cod_esta_pago = pago.ind_proc_pago
         ) EstadoPago,
         unad.cod_regi codigoRegion,
         unad.cod_zona codigoZona,
         unad.cod_secc codigoSeccion,
         pago.ind_proc_pago codigoEstadoPago
    FROM (
           SELECT lcom.pais_cod_pais,
                  lcom.lpro_cod_prog,
                  lcom.ltpa_cod_tipo_pago,
                  ltpa.des_tipo_pago,
                  lcom.num_secu,
                  lcom.cod_lide,
                  lcom.cod_regi,
                  lcom.cod_zona,
                  lcom.cod_secc,
                  lcom.val_mont_brut,
                  lcom.val_impu,
                  lcom.val_mont_neto,
                  lcom.fec_proc_pago,
                  lcom.fec_proc_neto,
                  lcom.cam_proc,
                  lcom.cam_refe,
                  lcom.cam_reca,
                  lcom.ind_proc_pago,
                  clie.val_ape1,
                  clie.val_ape2,
                  clie.val_nom1,
                  clie.val_nom2,
                  lcom.ltpg_num_tarj
             FROM lec_lider_pago_comis lcom,
                  lec_tipo_pago ltpa,
                  mae_clien clie
            WHERE lcom.ltpa_cod_tipo_pago = ltpa.cod_tipo_pago
              AND lcom.cod_lide = clie.cod_clie
              AND lcom.pais_cod_pais = $P{codigoPais}
              AND (
                   (
                     $P{fechaPagoInicial} IS NOT NULL AND
                     $P{fechaPagoFinal} IS NOT NULL AND
                     (lcom.fec_proc_pago >= TO_DATE($P{fechaPagoInicial},'DD/MM/YYYY') AND
                      lcom.fec_proc_pago <  TO_DATE($P{fechaPagoFinal},'DD/MM/YYYY') + 1
                     )
                   ) OR
                   (
                     $P{fechaPagoInicial} IS NULL AND
                     $P{fechaPagoFinal} IS NULL AND
                     lcom.cam_proc >= $P{codigoPeriodo} AND 
                     lcom.cam_proc <= $P{codigoPeriodoFinal}
                   )
                  )
              $P!{codigoRegion}
              $P!{codigoZona}
              $P!{condicionIndProcPago} 
	      $P!{condicionSoloTarjeta}
         ) pago,
         (
          SELECT zorg.cod_regi, zorg.des_regi,
                 zzon.cod_zona, zzon.des_zona,
                 zscc.cod_secc, zscc.des_secci
            FROM zon_secci zscc,
                 zon_zona zzon,
                 zon_regio zorg,
                 cra_perio perd,
                 seg_perio_corpo peri,
                 cra_perio perd2,
                 seg_perio_corpo peri2
           WHERE zscc.zzon_oid_zona = zzon.oid_zona
             AND zzon.zorg_oid_regi = zorg.oid_regi
             AND zscc.perd_oid_peri_inic = perd.oid_peri
             AND perd.peri_oid_peri = peri.oid_peri
             AND NVL(zscc.perd_oid_peri_fina,GEN_PKG_GENER.GEN_FN_DEVUELVE_ID_CRA_PERIO2($P{codigoPeriodoFinal})) = perd2.oid_peri
             AND perd2.peri_oid_peri = peri2.oid_peri
             AND $P{codigoPeriodoFinal} >= peri.cod_peri 
             AND $P{codigoPeriodoFinal} <= peri2.cod_peri
         ) unad,
         lec_tarje_pagos ltpg,
         lec_estad_tarje lest
   WHERE pago.ltpg_num_tarj = ltpg.num_tarj(+)
     AND ltpg.lest_cod_esta = lest.cod_esta(+)
     AND pago.cod_regi || pago.cod_zona || pago.cod_secc = unad.cod_regi || unad.cod_zona || unad.cod_secc
)
SELECT CAMPANNAPROCESO,
       CODIGOLIDER,
       NOMBRELIDER,
       REGION,
       ZONA,
       SECCION,
       CODIGONIVEL,
       CODIGOTARJETA,
       NUMEROTARJETA,
       ESTADOTARJETA,
       TIPOPAGO,
       NUMEROCOMPROBANTE,
       MONTOBRUTO,
       MONTOIMPUESTO,
       MONTONETO,
       FECHAPAGO,
       ESTADOPAGO
  FROM temp
ORDER BY campannaProceso,
          Region,
          Zona,
          Seccion,
          tipoPago]]></queryString>


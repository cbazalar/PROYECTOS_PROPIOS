	<queryString>
		<![CDATA[SELECT C.COD_CLIE,
             GEN_PKG_GENER.GEN_FN_CLIEN_DATOS(C.COD_CLIE, 'NOM_CLIE') NOM_CLIE,
             GEN_PKG_GENER.GEN_FN_CLIEN_DATOS(C.COD_CLIE, 'COD_ZONA') COD_ZONA,
             GEN_PKG_GENER.GEN_FN_CLIEN_DATOS(C.COD_CLIE, 'COD_SECC') COD_SECC,
             D.VAL_CODI_VENT CUV,
             B.VAL_NOMB_PERI CAMPANA,
             D.NUM_UNID_POR_ATEN,
             D.NUM_UNID_ATEN,
             to_char(A.FEC_FACT,'dd/MM/yyyy') FEC_FACT
   FROM PED_SOLIC_CABEC A, 
            CRA_PERIO B,
            MAE_CLIEN C,
            PED_SOLIC_POSIC D,
            MAE_PRODU E
WHERE A.PERD_OID_PERI = B.OID_PERI
     AND A.CLIE_OID_CLIE = C.OID_CLIE
     AND A.OID_SOLI_CABE = D.SOCA_OID_SOLI_CABE
     AND D.PROD_OID_PROD = E.OID_PROD
     AND A.FEC_FACT IS NOT NULL 
     AND D.NUM_UNID_POR_ATEN>NVL(D.NUM_UNID_ATEN,0)
     AND D.VAL_CODI_VENT = $P{codigoVenta}
     AND A.GRPR_OID_GRUP_PROC = 5
     AND A.TSPA_OID_TIPO_SOLI_PAIS = fin_pkg_gener.FIN_FN_OBTIE_OID_SOLIC_PAIS('SOC') 
     AND a.perd_oid_peri = (SELECT  cp.oid_peri FROM cra_perio cp,seg_perio_corpo sp WHERE cp.peri_oid_peri=sp.oid_peri AND sp.cod_peri=$P{codigoPeriodo})
     and b.pais_oid_pais = gen_pkg_gener.gen_fn_devuelve_id_pais($P{codigoPais})]]>
	</queryString>
